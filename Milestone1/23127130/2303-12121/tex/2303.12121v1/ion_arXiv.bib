@article{Martinez2020,
abstract = {By means of particle-in-cell numerical simulations, we investigate the emission of high-energy photons in laser-plasma interactions under ultrahigh-intensity conditions relevant to multi-petawatt laser systems. We first examine the characteristics of synchrotron radiation from laser-driven plasmas of varying density and size. In particular, we show and explain the dependence of the angular distribution of the radiated photons on the transparency or opacity of the plasma. We then study the competition of the synchrotron and Bremsstrahlung emissions in copper foil targets irradiated by {\$}10{\^{}}{\{}22{\}}\backslash,\backslashrm W\backslash,cm{\^{}}{\{}-2{\}}{\$}, {\$}50 \backslash, \backslashrm fs{\$} laser pulses. Synchrotron emission is observed to be maximized for target thicknesses of a few {\$}10 \backslash, \backslashrm nm{\$}, close to the relativistic transparency threshold, and to be superseded by Bremsstrahlung in targets a few {\$}\backslashmu{\$}m thick. At their best efficiency, both mechanisms are found to radiate about one percent of the laser energy into photons with energies above {\$}10\backslash,\backslashrm keV{\$}. Their energy and angular spectra are thoroughly analyzed in light of the ultrafast target dynamics.},
archivePrefix = {arXiv},
arxivId = {2006.16603},
author = {Martinez, B. and D'Humi{\`{e}}res, E. and Gremillet, L.},
doi = {10.1103/PhysRevResearch.2.043341},
eprint = {2006.16603},
issn = {2643-1564},
journal = {Physical Review Research},
keywords = {doi:10.1103/PhysRevResearch.2.043341 url:https://d},
mendeley-groups = {Ion Acceleration/Solid Ions},
month = {dec},
number = {4},
pages = {043341},
publisher = {American Physical Society},
title = {{Synchrotron radiation from ultrahigh-intensity laser-plasma interactions and competition with Bremsstrahlung in thin foil targets}},
url = {https://link.aps.org/doi/10.1103/PhysRevResearch.2.043341},
volume = {2},
year = {2020}
}






