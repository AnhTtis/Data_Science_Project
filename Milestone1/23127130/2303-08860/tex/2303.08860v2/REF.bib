@article{Bauschlicher2009,
  author={{Bauschlicher}, Jr., C.~W. and {Peeters}, E. and {Allamandola}, L.~J.},
  title={The Infrared Spectra of Very Large Irregular Polycyclic Aromatic Hydrocarbons (PAHs): Observational Probes of Astronomical PAH Geometry, Size, and Charge},
  journal={The Astrophysical Journal},
  volume={697},
  number={1},
  pages={311},
  url={http://stacks.iop.org/0004-637X/697/i=1/a=311},
  year={2009},
  abstract={The mid-infrared (IR) spectra of six large, irregular polycyclic aromatic hydrocarbons (PAHs) with formulae (C 84 H 24 -C 120 H 36 ) have been computed using density functional theory (DFT). Trends in the dominant band positions and intensities are compared to those of large, compact PAHs as a function of geometry, size, and charge. Irregular edge moieties that are common in terrestrial PAHs, such as bay regions and rings with quartet hydrogens, are shown to be uncommon in astronomical PAHs. As for all PAHs comprised solely of C and H reported to date, mid-IR emission from irregular PAHs fails to produce a strong CC str band at 6.2 m, the position characteristic of the important, class A astronomical PAH spectra. Earlier studies showed that inclusion of nitrogen within a PAH shifts this to 6.2 m for PAH cations. Here we show that this band shifts to 6.3 m in nitrogenated PAH anions, close to the position of the CC stretch in class B astronomical PAH spectra. Thus, nitrogenated PAHs may be important in all sources and the peak position of the CC stretch near 6.2 m appears to directly reflect the PAH cation to anion ratio. Large irregular PAHs exhibit features at 7.8 m but lack them near 8.6 m. Hence, the 7.7 m astronomical feature is produced by a mixture of small and large PAHs while the 8.6 m band can only be produced by large compact PAHs. As with the CC str , the position and profile of these bands reflect the PAH cation to anion ratio.}
}

