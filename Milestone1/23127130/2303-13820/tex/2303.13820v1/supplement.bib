  copyright = {2022 The Author(s)},
  langid = {english},
  keywords = {Condensed-matter physics,Electronic and spintronic devices,Electronics,Materials science,photonics and device physics,Physics,Spintronics},
  file = {C\:\\Users\\User\\Zotero\\storage\\Z974MFZ7\\Sato et al. - 2022 - Large magnetocapacitance beyond 420% in epitaxial .pdf;C\:\\Users\\User\\Zotero\\storage\\E8ZZP2DN\\s41598-022-11545-6.html}
}

