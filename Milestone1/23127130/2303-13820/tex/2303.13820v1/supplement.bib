  langid = {english},
  keywords = {Condensed-matter physics,Photonic crystals,Quantum physics},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Condensed-matter physics;Photonic crystals;Quantum physics Subject\_term\_id: condensed-matter-physics;photonic-crystals;quantum-physics},
