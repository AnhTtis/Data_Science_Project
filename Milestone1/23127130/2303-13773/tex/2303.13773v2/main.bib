	author = {Mansour, Mohamed A. A. and Dessouky, Maged M.},
	month = apr,
	year = {2010},
	keywords = {Analysis of variance, Daily photograph selection, Earth observation satellite, Genetic algorithms, Genome coding, Multi-criteria constrained optimization},
