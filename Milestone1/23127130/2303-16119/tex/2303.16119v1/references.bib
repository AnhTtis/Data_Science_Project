@article{Abraham2003,
author = {Abraham, C. and Cornillon, P.A. and Matzner-Løber, E. and Molinari, N.},
title = {Unsupervised Curve Clustering using {B}-Splines},
journal = {Scandinavian Journal of Statistics},

volume = {30},
number = {3},
pages = {581-595},
keywords = {B-splines, clustering, epi-convergence, functional data, k-means, partitioning},
doi = {10.1111/1467-9469.00350},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/1467-9469.00350},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/1467-9469.00350},
abstract = {Abstract Data in many different fields come to practitioners through a process naturally described as functional. Although data are gathered as finite vector and may contain measurement errors, the functional form have to be taken into account. We propose a clustering procedure of such data emphasizing the functional nature of the objects. The new clustering method consists of two stages: fitting the functional data by B-splines and partitioning the estimated model coefficients using a k-means algorithm. Strong consistency of the clustering method is proved and a real-world example from food industry is given.},
year = {2003}
}



