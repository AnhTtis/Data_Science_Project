@article{Cox1981,
 jstor_articletype = {research-article},
 title = {Statistical analysis of time series: Some recent developments},
 author = {Cox, D.R. and Gudmundsson, Gudmundur and Lindgren, Georg and Bondesson, Lennart and Harsaae, Erik and Laake, Petter and Juselius, Katarina and Lauritzen, Steffen L.},
 journal = {Scandinavian Journal of Statistics},
