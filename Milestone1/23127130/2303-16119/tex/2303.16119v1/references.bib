@article {Scheltensetal2016,
	author = {Scheltens, N.M.E. and 
	Galindo-Garre, F. and 
	Pijnenburg, Y.A.L. and 
	van der Vlies, A.E. and 
	Smits, L.L. and {et al.}},
	title = {The identification of cognitive subtypes in {A}lzheimer{\textquoteright}s disease dementia using latent class analysis},
	volume = {87},
	number = {3},
	pages = {235--243},
	year = {2016},
	doi = {10.1136/jnnp-2014-309582},
	publisher = {BMJ Publishing Group Ltd},
	abstract = {Objective Alzheimer{\textquoteright}s disease (AD) is a heterogeneous disorder with complex underlying neuropathology that is still not completely understood. For better understanding of this heterogeneity, we aimed to identify cognitive subtypes using latent class analysis (LCA) in a large sample of patients with AD dementia. In addition, we explored the relationship between the identified cognitive subtypes, and their demographical and neurobiological characteristics.Methods We performed LCA based on neuropsychological test results of 938 consecutive probable patients with AD dementia using Mini-Mental State Examination as the covariate. Subsequently, we performed multinomial logistic regression analysis with cluster membership as dependent variable and dichotomised demographics, APOE genotype, cerebrospinal fluid biomarkers and MRI characteristics as independent variables.Results LCA revealed eight clusters characterised by distinct cognitive profile and disease severity. Memory-impaired clusters{\textemdash}mild-memory (MILD-MEM) and moderate-memory (MOD-MEM){\textemdash}included 43\% of patients. Memory-spared clusters mild-visuospatial-language (MILD-VILA), mild-executive (MILD-EXE) and moderate-visuospatial (MOD-VISP) {\textemdash}included 29\% of patients. Memory-indifferent clusters mild-diffuse (MILD-DIFF), moderate-language (MOD-LAN) and severe-diffuse (SEV-DIFF) {\textemdash}included 28\% of patients. Cognitive clusters were associated with distinct demographical and neurobiological characteristics. In particular, the memory-spared MOD-VISP cluster was associated with younger age, APOE e4 negative genotype and prominent atrophy of the posterior cortex.Conclusions Using LCA, we identified eight distinct cognitive subtypes in a large sample of patients with AD dementia. Cognitive clusters were associated with distinct demographical and neurobiological characteristics.},
	issn = {0022-3050},
	URL = {https://jnnp.bmj.com/content/87/3/235},
	eprint = {https://jnnp.bmj.com/content/87/3/235.full.pdf},
	journal = {Journal of Neurology, Neurosurgery \& Psychiatry}
}

