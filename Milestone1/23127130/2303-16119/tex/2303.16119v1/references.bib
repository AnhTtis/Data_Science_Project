 number = {2},
 jstor_formatteddate = {1981},
 pages = {93--115},
 url = {http://www.jstor.org/stable/4615819},
 ISSN = {03036898},
 abstract = {A selective review of the statistical analysis of time series is given, attention being concentrated on methods for the interpretation of data as contrasted with methods for empirical forecasting.Ways of describing second-order properties are compared and the distinction between short-range and long-range dependency outlined.Estimation of second-order functions is reviewed in model-free and model-specific contexts.Criteria for model choice and goodness of fit are discussed.Analysis of replicated series is mentioned.Some aspects of nonGaussian theory are developed.The possibility of an exponential family theory of time series is outlined and also some aspects of nonlinear processes.The distinction between reversible and irreversible processes is emphasized.Techniques for handling nonstationary processes are compared, dealing both with nonstationarity in mean and nonstationarity in structure.Finally there are some fairly brief remarks on multiple time series analysis, especially on the regression analysis of one response series on several explanatory series.},
 language = {English},
 year = {1981},
 publisher = {Blackwell Publishing on behalf of Board of the Foundation of the Scandinavian Journal of Statistics},
 copyright = {Copyright 1981 Board of the Foundation of the Scandinavian Journal of Statistics},
}

