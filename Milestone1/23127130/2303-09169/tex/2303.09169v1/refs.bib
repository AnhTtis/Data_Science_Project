@article{Huke:19,
author = {P. Huke and M. Debus and M. Zechmeister and A. Reiners},
journal = {J. Opt. Soc. Am. B},
keywords = {Frequency combs; High resolution spectroscopy; Linewidth; Nd:YAG lasers; Power spectra; Spectrometers},
number = {7},
pages = {1899--1906},
publisher = {OSA},
title = {Characterization and calibration of a Fourier-transform spectrometer using a laser frequency comb},
volume = {36},
month = {Jul},
year = {2019},
url = {http://opg.optica.org/josab/abstract.cfm?URI=josab-36-7-1899},
doi = {10.1364/JOSAB.36.001899},
abstract = {We have used a laser frequency comb with a repetition frequency of \&\#x03BD;rep\&\#x2248;1\&\#x2009;\&\#x2009;GHz to measure the drift and dispersion of a Fourier-transform spectrometer (FTS). We used the electronic measurements of \&\#x03BD;rep and \&\#x03BD;CEO to create a reference line list. We measured 28 interferograms and computed the phase and power spectra. The analysis of the interferograms and phase spectra allowed for compensation of several spectroscopic artifacts. In the computed power spectra, we detected \&\#x223C;64.000 suitable lines in the near-infrared bandwidth \&\#x0394;\&\#x03BD;$=$308.79\&\#x2013;374.74\&\#x2009;\&\#x2009;THz. The residual dispersion of the FTS can be described by two factors, a linear dispersion and a constant offset. Both are highly correlated and need to be computed simultaneously. The factors were computed from the comparison of a reference with measured line lists. The linear dispersion factor is found to be varying on the order of 10\&\#x2212;8\&\#x2009;\&\#x2009;Hz/Hz, while the constant offset is of the order of 107\&\#x2009;\&\#x2009;Hz. Using two factors for calibration, the difference between the reference and the measured line list can be removed completely with an uncertainty of \&\#x223C;65\&\#x2009;\&\#x2009;kHz corresponding to a precision of 0.5\&\#x00B7;10\&\#x2212;9\&\#x2009;\&\#x2009;Hz/Hz.},
}

