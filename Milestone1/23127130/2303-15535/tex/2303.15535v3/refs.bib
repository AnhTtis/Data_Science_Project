@article{Suri2014,
   abstract = {The tangent bundle $T^kM$ of order $k$, of a smooth Banach manifold $M$ consists of all equivalent classes of curves that agree up to their accelerations of order $k$. For a Banach manifold $M$ and a natural number $k$ first we determine a smooth manifold structure on $T^kM$ which also offers a fiber bundle structure for $(\pi_k,T^kM,M)$. Then we introduce a particular lift of linear connections on $M$ to geometrize $T^kM$ as a vector bundle over $M$. More precisely based on this lifted nonlinear connection we prove that $T^kM$ admits a vector bundle structure over $M$ if and only if $M$ is endowed with a linear connection. As a consequence applying this vector bundle structure we lift Riemannian metrics and Lagrangians from $M$ to $T^kM$. Also, using the projective limit techniques, we declare a generalized Fr\'echet vector bundle structure for $T^\infty M$ over $M$.},
   author = {Ali Suri},
   doi = {10.1007/s00009-016-0812-7},
   keywords = {banach manifold,connection map,echet manifold,fr,higher order tangent bundle,lagrangians,lifting of riemannian,linear connection,metrics},
   pages = {1-14},
   title = {Higher order tangent bundles},
   url = {http://arxiv.org/abs/1403.3111%0Ahttp://dx.doi.org/10.1007/s00009-016-0812-7},
   year = {2014},
}
@article{Saccon2017,
   abstract = {In the literature on robotics and multibody dynamics, the concept of average angular velocity has received considerable attention in recent years. We address the question of whether the average angular velocity defines an orientation frame that depends only on the current robot configuration and provide a simple algebraic condition to check whether this holds. In the language of geometric mechanics, this condition corresponds to requiring the flatness of the mechanical connection associated with the robotic system. Here, however, we provide both a reinterpretation and a proof of this result accessible to readers with a background in rigid body kinematics and multibody dynamics but not necessarily acquainted with differential geometry, still providing precise links to the geometric mechanics literature. This should help spreading the algebraic condition beyond the scope of geometric mechanics, contributing to a proper utilization and understanding of the concept of average angular velocity.},
   author = {Alessandro Saccon and Silvio Traversaro and Francesco Nori and Henk Nijmeijer},
   doi = {10.1109/LRA.2017.2655560},
   issn = {23773766},
   issue = {2},
   journal = {IEEE Robotics and Automation Letters},
   keywords = {Direct/inverse dynamics formulation,humanoid robots,kinematics,multilegged robots,space robotics},
   pages = {943-950},
   publisher = {IEEE},
   title = {On Centroidal Dynamics and Integrability of Average Angular Velocity},
