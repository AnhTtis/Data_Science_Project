}
@article{Ruggiero2018,
   abstract = {Aerial manipulation aims at combining the versatility and the agility of some aerial platforms with the precision and the dexterity of robotic arms. This letter tries to collect the results reached by the research community so far within the field of aerial manipulation, specially from the technological and control point of view. A brief literature review about general aerial robotics and space manipulation is carried out as well.},
