	journal = {Nature Communications},
	author = {Maaß, Henriette and Bentmann, Hendrik and Seibel, Christoph and Tusche, Christian and Eremeev, Sergey V. and Peixoto, Thiago R. F. and Tereshchenko, Oleg E. and Kokh, Konstantin A. and Chulkov, Evgueni V. and Kirschner, Jürgen and Reinert, Friedrich},
	month = may,
	year = {2016},
	pages = {11621},
	file = {Full Text PDF:/home/matyilona/edu/Zotero/storage/SZ4R6ZTH/Maaß et al. - 2016 - Spin-texture inversion in the giant Rashba semicon.pdf:application/pdf;Snapshot:/home/matyilona/edu/Zotero/storage/SASFB29B/ncomms11621.html:text/html},
}

@article{akrap_optical_2014-1,
	title = {Optical properties of {BiTeBr} and {BiTeCl}},
