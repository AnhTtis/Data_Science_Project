	pages = {85--101},
	file = {Eremko et al. - 2016 - General solution of the Dirac equation for quasi-t.pdf:/home/matyilona/edu/Zotero/storage/7GXFQLNA/Eremko et al. - 2016 - General solution of the Dirac equation for quasi-t.pdf:application/pdf},
}

