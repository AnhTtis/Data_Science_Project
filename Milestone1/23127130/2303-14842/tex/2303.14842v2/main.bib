	file = {Full Text PDF:/home/matyilona/edu/Zotero/storage/Y8ED6EQ4/Eremeev et al. - 2014 - Spin-helical Dirac states in graphene induced by p.pdf:application/pdf;Snapshot:/home/matyilona/edu/Zotero/storage/GRZ32I4B/srep06900.html:text/html},
}

@article{maas_spin-texture_2016-1,
	title = {Spin-texture inversion in the giant {Rashba} semiconductor {BiTeI}},
