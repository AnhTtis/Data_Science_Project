	author = {Bahramy, M. S. and Yang, B.-J. and Arita, R. and Nagaosa, N.},
	month = feb,
	year = {2012},
	pages = {679},
	file = {Full Text PDF:/home/matyilona/edu/Zotero/storage/XA7SJU7Q/Bahramy et al. - 2012 - Emergence of non-centrosymmetric topological insul.pdf:application/pdf;ncomms1679-s1.pdf:/home/matyilona/edu/Zotero/storage/QNRRT9X8/ncomms1679-s1.pdf:application/pdf;Snapshot:/home/matyilona/edu/Zotero/storage/3KPWTVMS/ncomms1679.html:text/html},
}

