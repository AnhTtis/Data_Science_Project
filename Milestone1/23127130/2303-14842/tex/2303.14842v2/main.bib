	author = {Fülöp, Bálint and Tajkov, Zoltán and Pet{\textbackslash}Ho, János and Kun, Péter and Koltai, János and Oroszlány, László and Tóvári, Endre and Murakawa, Hiroshi and Tokura, Yoshinori and Bordács, Sándor and Tapasztó, Levente and Csonka, Szabolcs},
