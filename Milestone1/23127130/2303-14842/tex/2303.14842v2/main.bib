	file = {arXiv\:1608.05721 PDF:/home/matyilona/edu/Zotero/storage/5NXZVCTL/Hamo et al. - 2016 - Electron Attraction Mediated by Coulomb Repulsion.pdf:application/pdf;arXiv.org Snapshot:/home/matyilona/edu/Zotero/storage/TA75R2PL/1608.html:text/html},
}

