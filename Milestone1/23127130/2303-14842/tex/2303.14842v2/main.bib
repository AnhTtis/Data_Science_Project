	file = {West et al. - 2018 - Gate-based single-shot readout of spins in silicon.pdf:/home/matyilona/edu/Zotero/storage/EIVFBWBB/West et al. - 2018 - Gate-based single-shot readout of spins in silicon.pdf:application/pdf},
}

@article{yuan_liquid-gated_2011-1,
