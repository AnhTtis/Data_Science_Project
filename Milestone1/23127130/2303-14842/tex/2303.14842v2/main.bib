	journal = {arXiv:1802.04448 [cond-mat, physics:physics]},
	author = {Liang, L. and Chen, Q. H. and Lu, J. M. and Talsma, W. and Shan, J. and Blake, G. R. and Palstra, T. T. M. and Ye, J. T.},
