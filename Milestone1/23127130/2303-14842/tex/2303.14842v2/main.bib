@article{camenzind_hyperfine-phonon_2018-1,
	title = {Hyperfine-phonon spin relaxation in a single-electron {GaAs} quantum dot},
	volume = {9},
	issn = {2041-1723},
	url = {http://arxiv.org/abs/1711.01474},
	doi = {10.1038/s41467-018-05879-x},
	abstract = {Understanding and control of the spin relaxation time \$T\_1\$ is among the key challenges for spin based qubits. A larger \$T\_1\$ is generally favored, setting the fundamental upper limit to the qubit coherence and spin readout fidelity. In GaAs quantum dots at low temperatures and high in-plane magnetic fields \$B\$, the spin relaxation relies on phonon emission and spin-orbit coupling. The characteristic dependence \$T\_1 {\textbackslash}propto B{\textasciicircum}\{-5\}\$ and pronounced \$B\$-field anisotropy were already confirmed experimentally. However, it has also been predicted 15 years ago that at low enough fields, the spin-orbit interaction is replaced by the coupling to the nuclear spins, where the relaxation becomes isotropic, and the scaling changes to \$T\_1 {\textbackslash}propto B{\textasciicircum}\{-3\}\$. We establish these predictions experimentally, by measuring \$T\_1\$ over an unprecedented range of magnetic fields -- made possible by lower temperature -- and report a maximum \$T\_1 = 57{\textbackslash}pm15\$ s at the lowest fields, setting a new record for the electron spin lifetime in a nanostructure.},

	urldate = {2018-11-27},
	journal = {Nature Communications},
	author = {Camenzind, Leon C. and Yu, Liuqi and Stano, Peter and Zimmerman, Jeramy and Gossard, Arthur C. and Loss, Daniel and Zumbühl, Dominik M.},
	month = dec,
	year = {2018},


	file = {arXiv\:1711.01474 PDF:/home/matyilona/edu/Zotero/storage/ZU3ZIV3J/Camenzind et al. - 2018 - Hyperfine-phonon spin relaxation in a single-elect.pdf:application/pdf;arXiv.org Snapshot:/home/matyilona/edu/Zotero/storage/7492RLH3/1711.html:text/html},
}

