	pages = {11621},
	file = {Full Text PDF:/home/matyilona/edu/Zotero/storage/XF9NAGLI/Maaß et al. - 2016 - Spin-texture inversion in the giant Rashba semicon.pdf:application/pdf;Snapshot:/home/matyilona/edu/Zotero/storage/NHH5IBAI/ncomms11621.html:text/html},
}

@article{zutic_spintronics_2004,
