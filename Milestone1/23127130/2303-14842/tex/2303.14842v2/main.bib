@article{gyenis_experimental_2019,
	title = {Experimental realization of an intrinsically error-protected superconducting qubit},
	url = {http://arxiv.org/abs/1910.07542},
	abstract = {Encoding a qubit in logical quantum states with wavefunctions characterized by disjoint support and robust energies can offer simultaneous protection against relaxation and pure dephasing. Using a circuit-quantum-electrodynamics architecture, we experimentally realize a superconducting \$0-{\textbackslash}pi\$ qubit, which hosts protected states suitable for quantum-information processing. Multi-tone spectroscopy measurements reveal the energy level structure of the system, which can be precisely described by a simple two-mode Hamiltonian. We find that the parity symmetry of the qubit results in charge-insensitive levels connecting the protected states, allowing for logical operations. The measured relaxation (1.6 ms) and dephasing times (25 \${\textbackslash}mu\$s) demonstrate that our implementation of the \$0-{\textbackslash}pi\$ circuit not only broadens the family of superconducting qubits, but also represents a promising candidate for the building block of a fault-tolerant quantum processor.},

