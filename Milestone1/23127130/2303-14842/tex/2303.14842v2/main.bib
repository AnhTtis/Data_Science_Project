	doi = {10.1103/PhysRevB.64.121202},
	abstract = {The Rashba Hamiltonian describes the splitting of the conduction band as a result of spin-orbit coupling in the presence of an external field and is commonly used to model the electronic structure of confined narrow-gap semiconductors. Due to the mixing of spin states some care has to be exercised in the calculation of transport properties. We derive the velocity operator for the Rashba-split conduction band and demonstrate that the transmission of an interface between a ferromagnet and a Rashba-split semiconductor does not depend on the magnetization direction, in contrast with previous assertions in the literature.},

	urldate = {2018-01-27},
	journal = {Physical Review B},
	author = {Molenkamp, Laurens W. and Schmidt, Georg and Bauer, Gerrit E. W.},
