 file = {Why a Right to Explanation of Automated:D\:\\Nicolas Scharowski\\Documents\\Citavi 6\\Projects\\Masterarbeit_MMI_NS\\Citavi Attachments\\Why a Right to Explanation of Automated.pdf:pdf}
