@inbook{Mucha,
author = {Mucha, Henrik and Robert, Sebastian and Breitschwerdt, Ruediger and Fellmann, Michael},
title = {Interfaces for Explanations in Human-AI Interaction: Proposing a Design Evaluation Approach},
year = {2021},
isbn = {9781450380959},
publisher = {ACM},
