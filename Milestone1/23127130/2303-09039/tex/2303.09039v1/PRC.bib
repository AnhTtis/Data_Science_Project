@Article{ESEBBAG1993205,
  author   = {C. Esebbag and J.L. Egido},
  journal  = {Nucl. Phys. A},
  title    = {Number projected statistics and the pairing correlations at high excitation energies},
  year     = {1993},
  issn     = {0375-9474},
  number   = {2},
  pages    = {205-231},
  volume   = {552},
  abstract = {We analyze the use of particle-number projected statistics (PNPS) as an effective way to include the quantum and statistical fluctuations, associated with the pairing degree of freedom, left out in finite-temperature mean-field theories. As a numerical application the exact-soluble degenerate model is worked out. In particular, we find that the sharp temperature-induced superfluid-normal phase transition, predicted in the mean-field approximations, is washed out in the PNPS. Some approximations as well as the Landau prescription to include statistical fluctuations are also discussed. We find that the Landau prescription provides a reasonable approximation to the PNPS.},
  doi      = {https://doi.org/10.1016/0375-9474(93)90464-9},
  url      = {https://www.sciencedirect.com/science/article/pii/0375947493904649},
}

