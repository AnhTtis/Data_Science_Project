@Article{Egido1985,
  author   = {J.L. Egido and P. Ring and S. Iwasaki and H.J. Mang},
  journal  = {Phys. Lett. B},
  title    = {On the validity of the mean field approach for the description of pairing collapse in finite nuclei},
