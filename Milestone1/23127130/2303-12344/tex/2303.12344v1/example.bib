@article{Meisner2017,
	title = {Atom {Tunneling} in the {Water} {Formation} {Reaction} {H2} + {OH} → {H2O} + {H} on an {Ice} {Surface}},
	volume = {1},
	issn = {24723452},
	url = {http://pubs.acs.org/doi/10.1021/acsearthspacechem.7b00052},
	doi = {10.1021/acsearthspacechem.7b00052},
	abstract = {OH radicals play a key role as an intermediate in the water formation chemistry of the interstellar medium. For example, the reaction of OH radicals with H2 molecules is among the final steps in the astrochemical reaction network starting from O, O2, and O3. Experimentally, it was shown that, even at 10 K, this reaction occurs on ice surfaces. Because the reaction has a high activation energy, only atom tunneling can explain such experimental findings. In this study, we calculated reaction rate constants for the title reaction on a water-ice Ih surface. To our knowledge, low-temperature rate constants on a surface are not available in the literature. All surface calculations were performed using a quantum mechanics/molecular mechanics framework (BHLYP/TIP3P) after a thorough benchmark of different density functionals and basis sets to highly accurate correlation methods. Reaction rate constants are obtained using the instanton theory, which takes atom tunneling into account inherently, with reaction rate constants down to 110 K for the Eley-Rideal mechanism and down to 60 K for the Langmuir-Hinshelwood mechanism. We found that the reaction rate is nearly temperature-independent below 80 K. We give kinetic isotope effects for all possible deuteration patterns for both reaction mechanisms. For the implementation in astrochemical networks, we also give fit parameters to a modified Arrhenius equation. Finally, several different binding sites and binding energies of OH radicals on the Ih surface are discussed, and the corresponding rate constants are compared to the gas-phase case.},
	number = {7},
	urldate = {2018-09-17},
	journal = {ACS Earth and Space Chemistry},
	author = {Meisner, Jan and Lamberts, Thanja and Kästner, Johannes},
	month = sep,
	year = {2017},
	note = {arXiv: 1708.05559
Publisher: American Chemical Society},
	keywords = {astrochemistry, interstellar medium molecules, isotopes, kinetics, tunneling, water formation},
	pages = {399--410},
	file = {PDF:/home/molpeceres/Zotero/storage/U36QNE6R/full-text.pdf:application/pdf;PDF:/home/molpeceres/Zotero/storage/YXZ29SE8/full-text.pdf:application/pdf},
