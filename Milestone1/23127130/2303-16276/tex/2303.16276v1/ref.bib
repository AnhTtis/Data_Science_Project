@article{Roy17,
abstract = {This paper describes program LEVEL, which can solve the radial or one-dimensional Schr{\"{o}}dinger equation and automatically locate either all of, or a selected number of, the bound and/or quasibound levels of any smooth single- or double-minimum potential, and calculate inertial rotation and centrifugal distortion constants and various expectation values for those levels. It can also calculate Franck–Condon factors and other off-diagonal matrix elements, either between levels of a single potential or between levels of two different potentials. The potential energy function may be defined by any one of a number of analytic functions, or by a set of input potential function values which the code will interpolate over and extrapolate beyond to span the desired range.},
author = {{Le Roy}, Robert J.},
doi = {10.1016/j.jqsrt.2016.05.028},
file = {:home/yuly/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Le Roy - 2017 - SI LEVEL A computer program for solving the radial Schr{\"{o}}dinger equation for bound and quasibound levels.pdf:pdf},
issn = {00224073},
journal = {Journal of Quantitative Spectroscopy and Radiative Transfer},
keywords = {Centrifugal distortion constants,Diatomic Einstein coefficients,Diatomic Franck-Condon factors,Diatomic matrix elements,Diatomic vib-rotational energies,Quasibound level widths,Quasibound levels,Radial Schroedinger solver},
month = {jan},
pages = {167--178},
publisher = {Elsevier Ltd},
title = {{LEVEL: A computer program for solving the radial Schr{\"{o}}dinger equation for bound and quasibound levels}},
volume = {186},
year = {2017}
}

