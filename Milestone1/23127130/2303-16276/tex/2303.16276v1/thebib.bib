@article{Churilov2002,
  author={S S Churilov and Y N Joshi},
  title={Analysis of the 4p 6 4d 8 4f and 4p 5 4d 10 Configurations of Xe X and Some Highly Excited Levels of Xe VIII and Xe IX Ions},
  journal={Physica Scripta},
  volume={65},
  number={1},
  pages={40},
  url={http://stacks.iop.org/1402-4896/65/i=1/a=006},
  year={2002},
  abstract={The spectra of various ions of xenon were investigated in the 70??170 ? region. The earlier analysis of the 4d 9 ??4d 8 5p transitions Kaufman et al. , J. Opt. Soc. Am. B 73 , 691 (1983) in Rh-like Xe X was confirmed except for the two highest 4d 8 ( 1 S)5p 2 P levels. The 4p 6 4d 9 ??(4p 6 4d 8 5p 8 4f +4p 5 4d 10 ) transitions arrays were studied. We have established 28 new levels belonging to the odd parity system and classified 40 additional lines in Xe X. The spectrum is adequately described by the Hartree?Fock and parametric least-squares-fitting calculations. The resonance transitions from the highly excited 4d 9 (6p+5f+7p+6f) levels in Pd-like Xe IX and the 4 10 5s 2 S??4d 9 5s4f 2 P transitions in Ag-like Xe VIII have also been identified. The ionization potential of Xe IX has been calculated to be 1450500Â±1000 cm -1 (179.85Â±0.10 eV). The results from all three spectra are in good agreement with the Rh I, Pd I and Ag I isoelectronic sequences data.}
}
	

@article{Churilov2000,
  author={S S Churilov and Y N Joshi},
  title={Revised and Extended Analysis of the Odd Parity Configurations of Five-Times Ionized Xenon: Xe VI},
  journal={Physica Scripta},
  volume={62},
  number={5},
  pages={358},
  url={http://stacks.iop.org/1402-4896/62/i=5/a=002},
  year={2000},
  abstract={Xenon spectra were recorded in the 300??1240 ? region on a 3 m and a 10.7 normal incidence spectrograph using a modified triggered spark source. The spectrum of five-times ionized xenon (Xe VI) was investigated. The previous analysis of the 5s 2 5p, 5s5p 2 , 5s 2 5d and 5s 2 6s configurations (V Kaufman and J Sugar 1987 J. Opt. Soc. Am. B 4 1924, A Tauheed et al 1992 J. Phys. B: At. Mol. Opt. Phys. 25 L561) was confirmed. Three of the five levels of the 5p 3 configurations (A Tauheed et al 1995 Div. At. Mol. Opt. Phys. (APS) & DAMP(CAP) Joint Meeting, Toronto, May 1995 ) and all the 5p 3 , 5s5p5d and 5s5p6s configurations levels (R Sarmiento et al 1999 J. Phys. B: At. Mol. Opt. Phys. 32 2853) have been found to be erroneous. 53 new lines have been classified in the Xe VI spectrum. Twenty nine additional levels belonging to the 5p 3 , 5f, 6p and 5s5p5d odd configurations have been established. Hartree?Fock calculations with relativistic corrections (HFR) and least-square-fitted calculations (LSF) were carried out to interpret the spectrum.}
}
	



@article{Hansen1987,
  author={JÃ¸rgen E Hansen and Willy Persson},
  title={Revised analysis of singly ionized xenon, Xe II},
  journal={Physica Scripta},
  volume={36},
  number={4},
  pages={602},
  url={http://stacks.iop.org/1402-4896/36/i=4/a=005},
  year={1987},
  abstract={We present a revised analysis of the spectrum of singly ionized xenon, Xe II. This spectrum has been reanalyzed on the basis of the wavelength material published by Drs J. C. Boyce and C. J. Humphreys. The latter has kindly placed the original wavelength list covering the wavelength range 10 220-390 ? at our disposal. We report 161 energy levels which have been identified on the basis of classifications of 950 lines. We report the first f and g levels in Xe II. Also a number of g -factors have been determined for the first time and we give in total 75 g -factors. We have carried out least-squares fits to the even configurations and report the resulting parameter values and eigenvector compositions. A least-squares fit to the 5 p 4 6 p configuration is also reported. The levels have been named in jK and for many levels also in LS coupling. The former is the better coupling scheme for Xe II. We present an analysis of the 5 s photoelectron satellite spectrum of Xe based on our calculated eigenvector compositions and calculations of transition probabilities for ground state transitions as well as lifetimes for the 6 p levels. The latter are compared to recent experimental measurements. A list of wavelengths for observed laser transitions showing the present classifications and a discussion of the determination of the ionization potential of Xe II concludes the paper.}
}
	


