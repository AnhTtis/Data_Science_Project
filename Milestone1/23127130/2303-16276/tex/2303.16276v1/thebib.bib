@article{McCarron2015,
  author={D J McCarron and E B Norrgard and M H Steinecker and D DeMille},
  title={Improved magneto?optical trapping of a diatomic molecule},
  journal={New Journal of Physics},
  volume={17},
  number={3},
  pages={035014},
  url={http://stacks.iop.org/1367-2630/17/i=3/a=035014},
  year={2015},
  abstract={We present experimental results from a new scheme for magneto?optically trapping strontium monofluoride (SrF) molecules, which provides increased confinement compared to our original work. The improved trap employs a new approach to magneto?optical trapping presented by Tarbutt (2015 New J. Phys. 17 [http://dx.doi.org/10.1088/1367-2630/17/1/015007] 015007 ), which provided insight for the first time into the source of the restoring force in magneto?optical traps (MOTs) where the cycling transition includes dark Zeeman sublevels (known as type-II MOTs). We measure a radial spring constant ##IMG## [http://ej.iop.org/images/1367-2630/17/3/035014/njp510712ieqn1.gif] {$20\times $} greater than in our original work with SrF, comparable to the spring constants reported in atomic type-II MOTs. We achieve a trap lifetime ##IMG## [http://ej.iop.org/images/1367-2630/17/3/035014/njp510712ieqn2.gif] {${{\tau }_{{\rm MOT}}}=136(2)$} ms, over ##IMG## [http://ej.iop.org/images/1367-2630/17/3/035014/njp510712ieqn3.gif] {$2\times $} longer than originally reported for SrF. Finally, we demonstrate further cooling of the trapped molecules by briefly increasing the trapping lasers?? detunings. Our trapping scheme remains a straightforward extension of atomic techniques and marks a step towards the direct production of large, dense, ultracold molecular gases via laser cooling.}
}

