@article{Theocharous2014,
author = {Evangelos Theocharous and Christopher J. Chunnilall and Ryan Mole and David Gibbs and Nigel Fox and Naigui Shang and Guy Howlett and Ben Jensen and Rosie Taylor and Juan R. Reveles and Oliver B. Harris and Naseer Ahmed},
journal = {Opt. Express},
keywords = {Remote sensing and sensors ; Metrological instrumentation; Infrared},
number = {6},
pages = {7290--7307},
publisher = {OSA},
title = {The partial space qualification of a vertically aligned carbon nanotube coating on aluminium substrates for EO applications},
volume = {22},
month = {Mar},
year = {2014},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-22-6-7290},
doi = {10.1364/OE.22.007290},
abstract = {The fabrication of NanoTube Black, a Vertically Aligned carbon NanoTube Array (VANTA) on aluminium substrates is reported for the first time. The coating on aluminium was realised using a process that employs top down thermal radiation to assist growth, enabling deposition at temperatures below the substrate's melting point. The NanoTube Black coatings were shown to exhibit directional hemispherical reflectance values of typically less than 1\% across wavelengths in the 2.5 {\textmu}m to 15 {\textmu}m range. VANTA-coated aluminium substrates were subjected to space qualification testing (mass loss, outgassing, shock, vibration and temperature cycling) before their optical properties were re-assessed. Within measurement uncertainty, no changes to hemispherical reflectance were detected, confirming that NanoTube Black coatings on aluminium are good candidates for Earth Observation (EO) applications.},
}


