@article{Collopy2015,
  author={Alejandra L Collopy and Matthew T Hummon and Mark Yeo and Bo Yan and Jun Ye},
  title={Prospects for a narrow line MOT in YO},
  journal={New Journal of Physics},
  volume={17},
  number={5},
  pages={055008},
  url={http://stacks.iop.org/1367-2630/17/i=5/a=055008},
  year={2015},
  abstract={In addition to being suitable for laser cooling and trapping in a magneto-optical trap (MOT) using a relatively broad ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn1.gif] {$(\sim 5\;{\rm MHz})$} transition, the molecule YO possesses a narrow-line transition. This forbidden transition between the ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn2.gif] {${{X}^{2}}\Sigma $} and ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn3.gif] {$A{{^{\prime} }^{2}}{{\Delta }_{3/2}}$} states has linewidth ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn4.gif] {$\sim 2\pi \times 160$} kHz. After cooling in a MOT on the 614 nm ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn5.gif] {${{X}^{2}}\Sigma $} to ##IMG## [http://ej.iop.org/images/1367-2630/17/5/055008/njp512651ieqn6.gif] {${{A}^{2}}{{\Pi }_{1/2}}$} (orange) transition, the narrow 690 nm (red) transition can be used to further cool the sample, requiring only minimal additions to the first stage system. We estimate that the narrow line cooling stage will bring the temperature from ??1 mK to ??10 Î¼ K, significantly advancing the frontier on direct cooling achievable for molecules.}
}
	


