@Article{Leach1991,
author ="Leach, Christine A. and Tsekouras, Athanassios A. and Vaccaro, Patrick H. and Zare, Richard N. and Zhao, Daqing",
title  ="Indirect information on reactive transition states from conservation of angular momentum",
journal  ="Faraday Discuss. Chem. Soc.",
year  ="1991",
volume  ="91",
issue  ="0",
pages  ="183-190",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/DC9919100183",
url  ="http://dx.doi.org/10.1039/DC9919100183",
abstract  ="By choosing a kinematically constrained bimolecular reaction of the type H + H[prime or minute]L [rightward arrow] HH[prime or minute]+ L{,} where H and H[prime or minute] are heavy atoms and L is a light atom{,} the orbital angular momentum{,} {,} of the reagents appears almost exclusively as HH[prime or minute] product rotation{,} . It follows that the magnitude of is given by ||={,} where is the reduced mass of the reagent collision partners{,} is their relative velocity and is the impact parameter{,} the distance of closest approach if the reagents moved in undeflected straight-line paths. Hence{,} by measuring both the product rotational distribution and the relative velocity distribution of the reagents{,} it is possible to deduce the range of impact parameters resulting in specific vibrational levels of the HH[prime or minute] product. A prototype system is Ba + HI [rightward arrow] BaI + H. Experimental results for BaI (= 0) are presented for this kinematically constrained reaction showing that the range of is restricted{,} peaking at or near the energetic cut-off."}


