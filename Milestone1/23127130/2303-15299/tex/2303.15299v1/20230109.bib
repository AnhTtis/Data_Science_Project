@Article{DLW06,
  Title                    = {Robust Partial Pole Assignment for Vibrating Systems With Aerodynamic Effects},
  Author                   = {Datta, B.N. and Wen Wei Lin and Jenn Nan Wang},
  Journal                  = {Automatic Control, IEEE Transactions on},
  Year                     = {2006},

  Month                    = {Dec.},
  Number                   = {12},
  Pages                    = {1979 -1984},
  Volume                   = {51},

  Abstract                 = {This note proposes a novel algorithm for robust partial eigenvalue assignment (RPEVA) problem for a cubic matrix pencil arising from modeling of vibrating systems with aerodynamic effects. The RPEVA problem for a cubic pencil is the one of choosing suitable feedback matrices to reassign a few (say k lt;3n) unwanted eigenvalues while leaving the remaining large number (3n-k) of them unchanged, in such a way that the the eigenvalues of the closed-loop matrix are as insensitive as possible to small perturbation of the data. The latter amounts to minimizing the condition number of the closed-loop eigenvector matrix. The problem is solved directly in the cubic matrix polynomial setting without making any transformation to a standard first-order state-space system. This allows us to take advantage of the exploitable structures such as the sparsity, definiteness, bandness, etc., very often offered by large practical problems. The major computational requirements are: i) solution of a small Sylvester equation, ii) QR factorizations, and iii) solution of a standard least squares problem. The least-squares problem result from matrix rank-two update techniques used in the algorithm for reassigning complex eigenvalues. The practical effectiveness of the method is demonstrated by implementational results on simulated data provided by the Boeing company},
  Doi                      = {10.1109/TAC.2006.886543},
  File                     = {:C\:\\Users\\Admin\\Documents\\PolePlace\\DLW06.pdf:PDF},
  Groups                   = {poleplace},
  ISSN                     = {0018-9286},
  Keywords                 = {Aerodynamics;Control systems;Damping;Eigenvalues and eigenfunctions;Equations;Feedback;Force control;Mathematics;Polynomials;Robustness;aerodynamics;closed loop systems;eigenvalues and eigenfunctions;least squares approximations;pole assignment;polynomial matrices;robust control;vibration control;QR factorization;Sylvester equation;aerodynamic effects;closed-loop matrix;cubic matrix pencil;cubic matrix polynomials setting;feedback matrices;least square problem;robust partial eigenvalue assignment;robust partial pole assignment;vibrating systems;Cubic matrix ploynomial;least-squares problem;robust partial ploe;vibrating systems;}
}

@InProceedings{DS02,
