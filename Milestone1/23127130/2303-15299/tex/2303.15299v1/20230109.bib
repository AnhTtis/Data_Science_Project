@Article{A04,
  Title                    = {Pole-placement for SISO linear systems by state-derivative feedback},
  Author                   = {Abdelaziz, T.H.S. and Valasek, M.},
  Journal                  = {Control Theory and Applications, IEE Proceedings -},
  Year                     = {2004},

  Month                    = {Jul.},
  Number                   = {4},
  Pages                    = {377 - 385},
  Volume                   = {151},

  Abstract                 = {A procedure for solving the pole-placement problem for a linear single-input/single-output (SISO) systems by state-derivative feedback is described. This pole-placement problem is always solvable for controllable systems if all eigenvalues of the original system are nonzero. Then any arbitrary closed-loop poles can be placed to achieve the desired system performance. The solution procedure results in a formula similar to the Ackermann one. Its derivation is based on the transformation of a linear SISO system into Frobenius canonical form by co-ordinate transformation, then solving the pole-placement problem by state-derivative feedback and transforming the solution into the original co-ordinates. The solution is also extended to time-varying systems.},
  Doi                      = {10.1049/ip-cta:20040660},
  File                     = {:C\:\\Users\\Admin\\Documents\\PolePlace\\A04.pdf:PDF},
  Groups                   = {poleplace},
  ISSN                     = {1350-2379},
  Keywords                 = { closed loop systems; control system synthesis; linear systems; poles and zeros; state feedback; time-varying systems; Frobenius canonical form; closed-loop poles; linear systems; pole-placement; single-input single-output system; state-derivative feedback; time-varying systems;}
}

