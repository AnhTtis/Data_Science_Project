@article{AMSLER2002494,
title = {Temperature dependence of pure CsI: scintillation light yield and decay time},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {480},
number = {2},
pages = {494-500},
year = {2002},
issn = {0168-9002},
doi = {https://doi.org/10.1016/S0168-9002(01)01239-6},
url = {https://www.sciencedirect.com/science/article/pii/S0168900201012396},
author = {C Amsler and D Grögler and W Joffrain and D Lindelöf and M Marchesotti and P Niederberger and H Pruys and C Regenfus and P Riedler and A Rotondi},
keywords = {Pure CsI scintillator, Light yield, Decay time, Temperature dependence},
abstract = {The temperature dependence of the light emission for pure CsI crystals has been measured with photomultipliers, and photodiodes with wavelength shifters from 80–300K. The light yield at 80K is Nγ=50,000±5000 photons/MeV. This number was deduced from the number of electron–hole pairs produced in the photodiode, Neh=39,600±1200. The light yield at room temperature is lower by a factor of 15.8±1.0, giving 3200±400 photons/MeV. Decay times were measured with a photomultiplier. At room temperature two fast decay components were observed with decay times of 6±1 and 28±2ns. Below 180K only one component is observed and at 80K the decay time is 1015±17ns.}
}

