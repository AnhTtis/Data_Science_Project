issn = {00103616},
journal = {Communications in Mathematical Physics},
keywords = {concentration of measure,entanglement,superdense coding},
number = {1},
pages = {95--117},
primaryClass = {quant-ph},
title = {{Aspects of generic entanglement}},
volume = {265},
year = {2006}
}

