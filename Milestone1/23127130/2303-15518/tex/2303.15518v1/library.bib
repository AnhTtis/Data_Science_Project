file = {:Users/z5189882/Documents/papers/1801.04934.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {evolution,formation,galaxies,general,groups,high-redshift,large-scale structure of universe,mass function},
number = {1},
