arxivId = {1402.0510},
author = {Masters, Daniel and McCarthy, Patrick and Siana, Brian and Malkan, Mathew and Mobasher, Bahram and Atek, Hakim and Henry, Alaina and Martin, Crystal L. and Rafelski, Marc and Hathi, Nimish P. and Scarlata, Claudia and Ross, Nathaniel R. and Bunker, Andrew J. and Blanc, Guillermo and Bedregal, Alejandro G. and Dom{\'{i}}nguez, Alberto and Colbert, James and Teplitz, Harry and Dressler, Alan},
