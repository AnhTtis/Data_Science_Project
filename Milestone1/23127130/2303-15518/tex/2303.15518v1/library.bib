}
@article{Kuchner2021,
abstract = {Galaxy clusters grow by accreting galaxies from the field and along filaments of the cosmic web. As galaxies are accreted they are affected by their local environment before they enter (pre-processing), and traverse the cluster potential. Observations that aim to constrain pre-processing are challenging to interpret because filaments comprise a heterogeneous range of environments including groups of galaxies embedded within them and backsplash galaxies that contain a record of their previous passage through the cluster. This motivates using modern cosmological simulations to dissect the population of galaxies found in filaments that are feeding clusters, to better understand their history, and aid the interpretation of observations. We use zoom-in simulations from The ThreeHundred project to track haloes through time and identify their environment. We establish a benchmark for galaxies in cluster infall regions that supports the reconstruction of the different modes of pre-processing. We find that up to 45 per cent of all galaxies fall into clusters via filaments (closer than 1 h−1Mpc from the filament spine). 12 per cent of these filament galaxies are long-established members of groups and between 30 and 60 per cent of filament galaxies at R200 are backsplash galaxies. This number depends on the cluster's dynamical state and sharply drops with distance. Backsplash galaxies return to clusters after deflecting widely from their entry trajectory, especially in relaxed clusters. They do not have a preferential location with respect to filaments and cannot collapse to form filaments. The remaining pristine galaxies (∼30–60 per cent) are environmentally affected by cosmic filaments alone.},
archivePrefix = {arXiv},
arxivId = {2111.11467},
author = {Kuchner, Ulrike and Haggar, Roan and Arag{\'{o}}n-Salamanca, Alfonso and Pearce, Frazer R and Gray, Meghan E and Rost, Agust{\'{i}}n and Cui, Weiguang and Knebe, Alexander and Yepes, Gustavo},
doi = {10.1093/mnras/stab3419},
eprint = {2111.11467},
file = {:Users/z5189882/Documents/papers/stab3419.pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {1 i n t,clusters,cosmology,data analysis,evolution,galaxies,general,in a cold dark,large-scale structure of universe,low-mass haloes form,matter universe model,methods,observations,ro d u c,t i o n},
number = {1},
pages = {581--592},
title = {{An inventory of galaxies in cosmic filaments feeding galaxy clusters: galaxy groups, backsplash galaxies, and pristine galaxies}},
volume = {510},
year = {2021}
}
@article{Contini2019,
abstract = {We study the roles of stellar mass and environment in quenching the star formation activity of a large set of simulated galaxies by taking advantage of an analytic model coupled to the merger tree extracted from an N-body simulation. The analytic model has been set to match the evolution of the global stellar mass function since redshift {\$}z\backslashsim 2.3{\$} and give reasonable predictions of the star formation history of galaxies at the same time. We find that stellar mass and environment play different roles: the star formation rate/specific star formation rate-{\$}M{\_}*{\$} relations are independent of the environment (defined as the halo mass) at any redshift probed, {\$}0{\textless}z{\textless}1.5{\$}, for both star forming and quiescent galaxies, while the star formation rate-{\$}M{\_}{\{}halo{\}}{\$} relation strongly depends on stellar mass in the same redshift range, for both star forming and quiescent galaxies. Moreover, the star formation rate and the specific star formation rate are strongly dependent on stellar mass even when the distance from the cluster core is used as a proxy for the environment, rather than the halo mass. We then conclude that stellar mass is the main driver of galaxy quenching at any redshift probed in this study, not just at {\$}z{\textgreater}1{\$} as generally claimed, while the environment has a minimal role. All the physical processes linked to the environment must act on very short timescales, such that they do not influence the star formation of active galaxies, but increase the probability of a given galaxy to become quiescent.},
author = {Contini, E and Gu, Q and Kang, X and Rhee, J and Yi, S K},
doi = {10.3847/1538-4357/ab3b03},
file = {:Users/z5189882/Documents/papers/Contini_2019_ApJ_882_167.pdf:pdf},
