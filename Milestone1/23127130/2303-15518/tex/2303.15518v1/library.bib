@article{Cole2020,
abstract = {In this letter, we investigate the impact of environment on integrated and spatially-resolved stellar kinematics of a sample of massive, quiescent galaxies at intermediate redshift ({\$}0.6{\textless}z{\textless}1.0{\$}). For this analysis, we combine photometric and spectroscopic parameters from the UltraVISTA and Large Early Galaxy Astrophysics Census (LEGA-C) surveys in the COSMOS field and environmental measurements. We analyze the trends with overdensity (1+{\$}\backslashdelta{\$}) on the rotational support of quiescent galaxies and find no universal trends at either fixed mass or fixed stellar velocity dispersion. This is consistent with previous studies of the local Universe; rotational support of massive galaxies depends primarily on stellar mass. We highlight two populations of massive galaxies ({\$}\backslashlog M{\_}\backslashstar/M{\_}\backslashodot\backslashgeq11{\$}) that deviate from the average mass relation. First, the most massive galaxies in the most under-dense regions ({\$}(1+\backslashdelta)\backslashleq1{\$}) exhibit elevated rotational support. Similarly, at the highest masses ({\$}\backslashlog M{\_}\backslashstar/M{\_}\backslashodot\backslashgeq11.25{\$}) the range in rotational support is significant in all but the densest regions. This corresponds to an increasing slow-rotator fraction such that only galaxies in the densest environments ({\$}(1+\backslashdelta)\backslashgeq3.5{\$}) are primarily (90{\$}\backslashpm{\$}10$\backslash${\%}) slow-rotators.This effect is not seen at fixed velocity dispersion, suggesting minor merging as the driving mechanism: only in the densest regions have the most massive galaxies experienced significant minor merging, building stellar mass and diminishing rotation without significantly affecting the central stellar velocity dispersion. In the local Universe, most massive galaxies are slow-rotators, regardless of environment, suggesting minor merging occurs at later cosmic times {\$}(z\backslashlesssim0.6){\$} in all but the most dense environments.},
archivePrefix = {arXiv},
arxivId = {2001.02695},
author = {Cole, Justin and Bezanson, Rachel and van der Wel, Arjen and Bell, Eric and D'Eugenio, Francesco and Franx, Marijn and Gallazzi, Anna and van Houdt, Josha and Muzzin, Adam and Pacifici, Camilla and van de Sande, Jesse and Sobral, David and Straatman, Caroline and Wu, Po-Feng},
doi = {10.3847/2041-8213/ab7241},
eprint = {2001.02695},
file = {:Users/z5189882/Documents/papers/Cole_2020_ApJL_890_L25.pdf:pdf},
issn = {2041-8205},
journal = {The Astrophysical Journal},
number = {2},
pages = {L25},
title = {{ Stellar Kinematics and Environment at z ∼ 0.8 in the LEGA-C Survey: Massive Slow Rotators Are Built First in Overdense Environments }},
volume = {890},
year = {2020}
}
@article{Santini2007a,
abstract = {The goal of this work is to infer the star formation properties and the mass assembly process of high-redshift galaxies from their IR emission using the 24 $\mu$m band of MIPS-Spitzer. We used the GOODS-MUSIC catalog, with multiwavelength coverage from 0.3 to 8 $\mu$m and spectroscopic or accurate photometric redshifts. We describe how this catalog has been extended with the addition of mid-IR fluxes derived from MIPS 24 $\mu$m image. As for the Star Formation Rate (hereafter SFR), we compared two different tracers: IR emission by reprocessed UV photons and UV luminosity corrected for dust reddening. The former has been estimated using both synthetic and empirical templates, the latter has been derived from a SED fitting technique over optical-to-IR bands. We show that, apart from a few outliers, there is a general consistency among these indicators. The relation between the specific SFR (SSFR) and the total stellar mass confirms the well-known downsizing scenario, as we find that the most massive galaxies have higher star formation activity at higher redshifts. A preliminary comparison with the theoretical predictions of the Millennium Simulation shows that such model provides an overall consistency with the SSFR distribution, including the existence of quiescent galaxies even at z > 1.5. However, the average SSFR observed in the more active galaxies is systematically underpredicted by a factor up to ∼ 2-3 increasing with redshift. {\textcopyright} Societ{\`{a}} Italiana di Fisica.},
author = {Santini, P. and Fontana, A. and Grazian, A. and Pentericci, L. and Salimbeni, S.},
doi = {10.1393/ncb/i2008-10474-1},
file = {:Users/z5189882/Documents/papers/aa11434-08.pdf:pdf},
issn = {15949982},
journal = {A Century of Cosmology: Past, Present and Future},
keywords = {evolution,fundamental parameters,galaxies,galaxies: evolution, galaxies: high-redshift, gala,high-redshift,photometry},
pages = {1295--1299},
title = {{Star formation and mass assembly in high-redshift galaxies}},
volume = {767},
year = {2007}
}
@article{Gupta2021,
abstract = {Using the TNG100 (100 Mpc)3 simulation of the IllustrisTNG project, we demonstrate a strong connection between the onset of star formation quenching and the stellar size of galaxies. We do so by tracking the evolutionary history of extended and normal-size galaxies selected at z = 2 with log(M∗/M☉)= 10.2−11 and stellar-half-mass-radii above and within 1-sigma of the stellar size–stellar mass relation, respectively. We match the stellar mass and star formation rate distributions of the two populations. By z = 1, only 36 of the extended massive galaxies have quenched, in contrast to a quenched fraction of 69 for the normal-size massive galaxies. We find that normal-size massive galaxies build up their central stellar mass without a significant increase in their stellar size between z = 2−4, whereas the stellar size of the extended massive galaxies almost doubles in the same time. In IllustrisTNG, lower black hole masses and weaker kinetic-mode feedback appears to be responsible for the delayed quenching of star formation in the extended massive galaxies. We show that relatively gas-poor mergers may be responsible for the lower central stellar density and weaker supermassive black hole feedback in the extended massive galaxies.},
