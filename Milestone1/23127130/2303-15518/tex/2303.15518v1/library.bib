keywords = {clusters,evolution,galaxies,general - galaxies},
month = {sep},
number = {September},
pages = {1--26},
title = {{The GOGREEN survey: Post-infall environmental quenching fails to predict the observed age difference between quiescent field and cluster galaxies at z &gt; 1}},
url = {http://arxiv.org/abs/2009.03953%0Ahttp://dx.doi.org/10.1093/mnras/staa2752 https://academic.oup.com/mnras/advance-article/doi/10.1093/mnras/staa2752/5903709},
