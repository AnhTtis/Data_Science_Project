title = {{The IllustrisTNG simulations: public data release}},
url = {http://dx.doi.org/10.1186/s40668-019-0028-x http://arxiv.org/abs/1812.05609 https://comp-astrophys-cosmol.springeropen.com/articles/10.1186/s40668-019-0028-x},
volume = {6},
