primaryClass = {astro-ph},
title = {{Galactic Stellar and Substellar Initial Mass Function}},
url = {http://arxiv.org/abs/astro-ph/0304382 http://dx.doi.org/10.1086/376392 http://iopscience.iop.org/article/10.1086/376392},
