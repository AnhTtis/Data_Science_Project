title = {{THE DISTRIBUTION OF SATELLITES AROUND MASSIVE GALAXIES AT 1 {\textless} z {\textless} 3 IN ZFOURGE/CANDELS: DEPENDENCE ON STAR FORMATION ACTIVITY}},
url = {https://iopscience.iop.org/article/10.1088/0004-637X/792/2/103},
