arxivId = {1410.3485},
author = {Furlong, M. and Bower, R. G. and Theuns, T. and Schaye, J. and Crain, R. A. and Schaller, M. and {Dalla Vecchia}, C. and Frenk, C. S. and McCarthy, I. G. and Helly, J. and Jenkins, A. and Rosas-Guevara, Y. M.},
doi = {10.1093/mnras/stv852},
