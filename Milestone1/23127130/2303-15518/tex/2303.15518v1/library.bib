journal = {Astronomy and Astrophysics},
keywords = {cosmology: observations,galaxies: evolution,galaxies: groups: general},
title = {{A journey from the outskirts to the cores of groups: I. Color-and mass-segregation in 20K-zCOSMOS groups}},
volume = {539},
year = {2012}
}
