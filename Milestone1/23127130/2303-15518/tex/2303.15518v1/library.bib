pages = {80},
title = {{Physical conditions of the interstellar medium in star-forming galaxies at z$\sim$1.5}},
url = {https://academic.oup.com/pasj/article-lookup/doi/10.1093/pasj/psv041 http://arxiv.org/abs/1504.05589 http://dx.doi.org/10.1093/pasj/psv041},
volume = {67},
