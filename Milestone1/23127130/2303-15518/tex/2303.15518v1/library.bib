doi = {10.3847/2041-8213/aa77f3},
eprint = {1705.03062},
file = {:Users/z5189882/Documents/papers/Noble_2017_ApJL_842_L21.pdf:pdf},
issn = {20418213},
journal = {The Astrophysical Journal},
keywords = {clusters,evolution,galaxies,galaxies: clusters: general,galaxies: evolution,ga,general,high-redshift,infrared,ism,star formation},
number = {2},
pages = {L21},
