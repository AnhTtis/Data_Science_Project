issn = {2041-8213},
journal = {The Astrophysical Journal},
month = {jul},
number = {2},
pages = {L20},
title = {{A rise in the ionizing photons in star-forming galaxies over the past 5 billion years}},
url = {http://arxiv.org/abs/1507.079320Ahttp://dx.doi.org/10.1088/2041-8205/812/2/L20 http://stacks.iop.org/2041-8205/812/i=2/a=L20?key=crossref.7f0a093cc02d9a21be65a7006b49b19c http://arxiv.org/abs/1507.07932 http://dx.doi.org/10.1088/2041-8205/812/2/L20},
