arxivId = {1801.04934},
author = {Papovich, Casey and Kawinwanichakij, Lalitwadee and Quadri, Ryan F. and Glazebrook, Karl and Labb{\'{e}}, Ivo and Tran, Kim-Vy H. and Forrest, Ben and Kacprzak, Glenn G. and Spitler, Lee R. and Straatman, Caroline M. S. and Tomczak, Adam R.},
doi = {10.3847/1538-4357/aaa766},
