doi = {10.1111/j.1365-2966.2003.07154.x},
file = {:Users/z5189882/Documents/papers/346-4-1055.pdf:pdf},
issn = {00358711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: Seyfert,Galaxies: active,Galaxies: starburst,Galaxies: stellar content,Galaxies: structure},
