keywords = {Galaxies: elliptical and lenticular, cD,Galaxies: formation,Galaxies: fundamental parameters,Galaxies: statistics,Galaxies: structure,cD},
month = {oct},
number = {1 PART 2},
pages = {120--123},
title = {{Major Merging: The Way to Make a Massive, Passive Galaxy}},
url = {http://arxiv.org/abs/0910.4018 http://dx.doi.org/10.1088/0004-637X/706/1/L120},
volume = {706},
