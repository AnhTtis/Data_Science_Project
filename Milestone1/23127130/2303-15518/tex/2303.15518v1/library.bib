@article{Pillepich2017,
abstract = {The IllustrisTNG project is a new suite of cosmological magneto-hydrodynamical simulations of galaxy formation performed with the Arepo code and updated models for feedback physics. Here we introduce the first two simulations of the series, TNG100 and TNG300, and quantify the stellar mass content of about 4000 massive galaxy groups and clusters ({\$}10{\^{}}{\{}13{\}} \backslashbackslashleq M{\_}{\{}\backslashbackslashrm 200c{\}}/M{\_}{\{}\backslashbackslashrm sun{\}} \backslashbackslashleq 10{\^{}}{\{}15{\}}{\$}) at recent times ({\$}z \backslashbackslashleq 1{\$}). The richest clusters have half of their total stellar mass bound to satellite galaxies, with the other half being associated with the central galaxy and the diffuse intra-cluster light. The exact ICL fraction depends sensitively on the definition of a central galaxy's mass and varies in our most massive clusters between 20 to 40{\%} of the total stellar mass. Haloes of {\$}5\backslashbackslashtimes 10{\^{}}{\{}14{\}}M{\_}{\{}\backslashbackslashrm sun{\}}{\$} and above have more diffuse stellar mass outside 100 kpc than within 100 kpc, with power-law slopes of the radial mass density distribution as shallow as the dark matter's ( {\$}-3.5 {\textless}\backslashbackslashalpha{\{}\backslash{\_}{\}}{\{}\backslashbackslashrm 3D{\}} {\textless}-3{\$}). Total halo mass is a very good predictor of stellar mass, and vice versa: at {\$}z=0{\$}, the 3D stellar mass measured within 30 kpc scales as {\$}\backslashbackslashpropto (M{\_}{\{}\backslashbackslashrm 500c{\}}){\^{}}{\{}0.49{\}}{\$} with a {\$}\backslashbackslashsim 0.12{\$} dex scatter. This is possibly too steep in comparison to the available observational constraints, even though the abundance of TNG less massive galaxies ({\$}{\textless}10{\^{}}{\{}11{\}}M{\_}{\{}\backslashbackslashrm sun{\}}{\$} in stars) is in good agreement with the measured galaxy stellar mass functions at recent epochs. The 3D sizes of massive galaxies fall too on a tight ({\$}\backslashbackslashsim{\{}\backslash{\$}{\}}0.16 dex scatter) power-law relation with halo mass, with {\$}r{\^{}}{\{}\backslashbackslashrm stars{\}}{\_}{\{}\backslashbackslashrm 0.5{\}} \backslashbackslashpropto (M{\_}{\{}\backslashbackslashrm 500c{\}}){\^{}}{\{}0.53{\}}{\$}. Even more fundamentally, halo mass alone is a good predictor for the whole stellar mass profiles beyond the inner few kpc, and we show how on average these can be precisely recovered given a single mass measurement of the galaxy or its halo.},
archivePrefix = {arXiv},
arxivId = {1707.03406},
author = {Pillepich, Annalisa and Nelson, Dylan and Hernquist, Lars and Springel, Volker and Pakmor, R{\"{u}}diger and Torrey, Paul and Weinberger, Rainer and Genel, Shy and Naiman, Jill P and Marinacci, Federico and Vogelsberger, Mark},
