issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {Galaxy clusters},
month = {jan},
number = {2},
pages = {74},
publisher = {IOP Publishing},
title = {{The Environmental Dependence of Gas Properties in Dense Cores of a Protocluster at z ∼ 2.5 Revealed with ALMA}},
