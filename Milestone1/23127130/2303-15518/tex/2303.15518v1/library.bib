eprint = {2101.06273},
file = {:Users/z5189882/Documents/papers/Fu_2021_ApJ_908_188.pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {Circumgalactic medium,Starburst galaxies},
month = {jan},
number = {2},
pages = {188},
publisher = {IOP Publishing},
title = {{A Long Stream of Metal-Poor Cool Gas around a Massive Starburst Galaxy at z = 2.67}},
url = {http://dx.doi.org/10.3847/1538-4357/abdb32 http://arxiv.org/abs/2101.06273},
volume = {908},
