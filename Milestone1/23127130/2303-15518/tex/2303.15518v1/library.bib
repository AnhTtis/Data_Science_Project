issn = {1538-4357},
journal = {The Astrophysical Journal},
month = {jan},
number = {2},
pages = {118},
title = {{THE SFR– M * RELATION AND EMPIRICAL STAR FORMATION HISTORIES FROM ZFOURGE AT 0.5 {\textless} z {\textless} 4}},
url = {https://iopscience.iop.org/article/10.3847/0004-637X/817/2/118 http://arxiv.org/abs/1510.06072 http://dx.doi.org/10.3847/0004-637X/817/2/118},
volume = {817},
