journal = {Monthly Notices of the Royal Astronomical Society: Letters},
keywords = {1 i n t,2004,and,baldry et al,blue,classification into,e,evolution,formation,g,galaxies,of nearby galaxies leads,red,ro d u c,star formation,t i o n,the bimodal colour distribution,to a natural},
number = {1},
pages = {L59--L63},
title = {{Are star formation rates of galaxies bimodal?}},
volume = {470},
year = {2017}
}
