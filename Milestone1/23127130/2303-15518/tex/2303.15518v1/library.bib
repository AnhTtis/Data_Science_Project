file = {:Users/z5189882/Library/Application Support/Mendeley Desktop/Downloaded/Kewley et al. - 2013 - Theoretical Evolution of Optical Strong Lines across Cosmic Time(2).pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {galaxies: abundances,galaxies: fundamental parameters,galaxies: starburst},
month = {jul},
number = {2},
pages = {100},
title = {{Theoretical Evolution of Optical Strong Lines across Cosmic Time}},
url = {http://stacks.iop.org/0004-637X/774/i=2/a=100?key=crossref.0a429c38f8568616a2ecc31ea5ffd272 http://arxiv.org/abs/1307.0508 http://dx.doi.org/10.1088/0004-637X/774/2/100},
volume = {774},
