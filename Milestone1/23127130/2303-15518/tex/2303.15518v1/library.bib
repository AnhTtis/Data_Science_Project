eprint = {1504.05589},
file = {:Users/z5189882/Documents/papers/1504.05589.pdf:pdf},
issn = {0004-6264},
journal = {Publications of the Astronomical Society of Japan},
keywords = {galaxies: ISM,galaxies: evolution,galaxies: high-redshift,galaxies: star formation},
month = {apr},
