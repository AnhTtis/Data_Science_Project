arxivId = {1504.05589},
author = {Hayashi, Masao and Ly, Chun and Shimasaku, Kazuhiro and Motohara, Kentaro and Malkan, Matthew A. and Nagao, Tohru and Kashikawa, Nobunari and Goto, Ryosuke and Naito, Yoshiaki},
