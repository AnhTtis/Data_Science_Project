journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Galaxies: Evolution,Galaxies: Groups: General,Galaxies: ISM,Galaxies: Spiral,Gravitational lensing: Weak,Intergalactic medium},
number = {1},
pages = {1015--1034},
