author = {Bellstedt, Sabine and Driver, Simon P and Robotham, Aaron S G and Davies, Luke J M and Bogue, Cameron R J and Cook, Robin H W and Hashemizadeh, Abdolhosein and Koushan, Soheil and Taylor, Edward N and Thorne, Jessica E and Turner, Ryan J and Wright, Angus H},
doi = {10.1093/mnras/staa1466},
eprint = {2005.11215},
file = {:Users/z5189882/Documents/papers/2005.11215.pdf:pdf},
issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {astronomical data bases,cata-,miscellaneous,photometric,techniques},
