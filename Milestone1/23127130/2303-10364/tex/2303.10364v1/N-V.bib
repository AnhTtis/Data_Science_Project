@article{hall_ion-neutral_2013,
	title = {Ion-neutral chemistry at ultralow energies: {Dynamics} of reactive collisions between laser-cooled {Ca}{\textasciicircum}+ ions and {Rb} atoms in an ion-atom hybrid trap},
	volume = {111},
	issn = {0026-8976, 1362-3028},
	shorttitle = {Ion-neutral chemistry at ultralow energies},
	url = {http://arxiv.org/abs/1302.4682},
	doi = {10.1080/00268976.2013.780107},
	abstract = {Cold chemical reactions between laser-cooled Ca{\textasciicircum}+ ions and Rb atoms were studied in an ion-atom hybrid trap. Reaction rate constants were determined in the range of collision energies {\textless}E\_\{coll\}{\textgreater}/k\_B = 20 mK-20 K. The lowest energies were achieved in experiments using single localized Ca{\textasciicircum}+ ions. Product branching ratios were studied using resonant-excitation mass spectrometry. The dynamics of the reactive processes in this system (non-radiative and radiative charge transfer as well as radiative association leading to the formation of CaRb{\textasciicircum}+ molecular ions) have been analyzed using high-level quantum-chemical calculations of the potential energy curves of CaRb{\textasciicircum}+ and quantum-scattering calculations for the radiative channels. For the present low-energy scattering experiments, it is shown that the energy dependence of the reaction rate constants is governed by long-range interactions in line with the classical Langevin model, but their magnitude is determined by short-range non-adiabatic and radiative couplings which only weakly depend on the asymptotic energy. The quantum character of the collisions is predicted to manifest itself in the occurrence of narrow shape resonances at well-defined collision energies. The present results highlight both universal and system-specific phenomena in cold ion-neutral reactive collisions.},
