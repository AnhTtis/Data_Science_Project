@article{laforge_resonance-enhanced_2021,
	title = {Resonance-{Enhanced} {Multiphoton} {Ionization} in the {X}-{Ray} {Regime}},
	volume = {127},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.127.213202},
	doi = {10.1103/PhysRevLett.127.213202},
	abstract = {Here, we report on the nonlinear ionization of argon atoms in the short wavelength regime using ultraintense x rays from the European XFEL. After sequential multiphoton ionization, high charge states are obtained. For photon energies that are insufficient to directly ionize a 1s electron, a different mechanism is required to obtain ionization to Ar17+. We propose this occurs through a two-color process where the second harmonic of the FEL pulse resonantly excites the system via a 1s→2p transition followed by ionization by the fundamental FEL pulse, which is a type of x-ray resonance-enhanced multiphoton ionization (REMPI). This resonant phenomenon occurs not only for Ar16+, but also through lower charge states, where multiple ionization competes with decay lifetimes, making x-ray REMPI distinctive from conventional REMPI. With the aid of state-of-the-art theoretical calculations, we explain the effects of x-ray REMPI on the relevant ion yields and spectral profile.},
	number = {21},
	urldate = {2021-12-10},
	journal = {Physical Review Letters},
	author = {LaForge, Aaron C. and Son, Sang-Kil and Mishra, Debadarshini and Ilchen, Markus and Duncanson, Stephen and Eronen, Eemeli and Kukk, Edwin and Wirok-Stoletow, Stanislaw and Kolbasova, Daria and Walter, Peter and Boll, Rebecca and De Fanis, Alberto and Meyer, Michael and Ovcharenko, Yevheniy and Rivas, Daniel E. and Schmidt, Philipp and Usenko, Sergey and Santra, Robin and Berrah, Nora},
	month = nov,
	year = {2021},
	pages = {213202},
	file = {APS Snapshot:C\:\\Users\\bollre\\Zotero\\storage\\555955JR\\PhysRevLett.127.html:text/html;Ar_XFEL_supplement_10_19_21.pdf:C\:\\Users\\bollre\\Zotero\\storage\\BE7Z7FRK\\Ar_XFEL_supplement_10_19_21.pdf:application/pdf;Full Text PDF:C\:\\Users\\bollre\\Zotero\\storage\\8MCIDTGS\\LaForge et al. - 2021 - Resonance-Enhanced Multiphoton Ionization in the X.pdf:application/pdf},
