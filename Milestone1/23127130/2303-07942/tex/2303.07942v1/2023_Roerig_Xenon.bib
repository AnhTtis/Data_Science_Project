	year = {2016},
	pages = {1233--1260},
	file = {Santra and Young - 2016 - Interaction of Intense X-Ray Beams with Atoms.pdf:C\:\\Users\\bollre\\Zotero\\storage\\C4CAZB93\\Santra and Young - 2016 - Interaction of Intense X-Ray Beams with Atoms.pdf:application/pdf},
}

