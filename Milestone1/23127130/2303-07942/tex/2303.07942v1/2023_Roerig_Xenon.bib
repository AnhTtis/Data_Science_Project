	journal = {Physical Review Letters},
	author = {Kanter, E. P. and Krässig, B. and Li, Y. and March, A. M. and Ho, P. and Rohringer, N. and Santra, R. and Southworth, S. H. and DiMauro, L. F. and Doumy, G. and Roedig, C. A. and Berrah, N. and Fang, L. and Hoener, M. and Bucksbaum, P. H. and Ghimire, S. and Reis, D. A. and Bozek, J. D. and Bostedt, C. and Messerschmidt, M. and Young, L.},
	month = nov,
