@article{PROSEKOV201873,
title = {Food security: The challenge of the present},
journal = {Geoforum},
volume = {91},
pages = {73-77},
year = {2018},
issn = {0016-7185},
doi = {https://doi.org/10.1016/j.geoforum.2018.02.030},
url = {https://www.sciencedirect.com/science/article/pii/S0016718518300666},
author = {Alexander Y. Prosekov and Svetlana A. Ivanova},
keywords = {Food security, Population growth, Nutrition, BRICS countries},
abstract = {The group of basic problems that determine the existence of mankind involves the surplus of food for some and the malnutrition of others. There is an opinion that ensuring food security is an integrated task of agriculture and political will, combined with the logistics of product delivery. Despite joint efforts and various UN programs to combat hunger, only short-term local results have been achieved. Food security, especially in the global sense, has not yet been implemented, and there are reasons for this. The analytical review presents evaluation of the achieved result and points out the activities that require adjustments.}
}