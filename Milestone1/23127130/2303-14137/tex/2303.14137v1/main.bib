@article{B_ttcher_2009,
	doi = {10.1088/0004-637x/694/1/174},
	url = {https://doi.org/10.1088/0004-637x/694/1/174},
	year = 2009,
	month = {mar},
	publisher = {American Astronomical Society},
	volume = {694},
	number = {1},
	pages = {174--182},
	author = {M. Böttcher and K. Fultz and H. D. Aller and M. F. Aller and J. Apodaca and A. A. Arkharov and U. Bach and R. Bachev and A. Berdyugin and C. Buemi and P. Calcidese and D. Carosati and P. Charlot and S. Ciprini and A. Di Paola and M. Dolci and N. V. Efimova and E. Forn{\'{e}} Scurrats and A. Frasca and A. C. Gupta and V. A. Hagen-Thorn and J. Heidt and D. Hiriart and T. S. Konstantinova and E. N. Kopatskaya and A. Lähteenmäki and L. Lanteri and V. M. Larionov and J.-F. Le Campion and P. Leto and E. Lindfors and E. Marilli B. Mihov and E. Nieppola and K. Nilsson and J. M. Ohlert E. Ovcharov and P. Pääkkönen and M. Pasanen and B. Ragozzine and C. M. Raiteri and J. A. Ros and A. Sadun and A. Sanchez and E. Semkov and M. Sorcia and A. Strigachev and L. Takalo and M. Tornikoski and C. Trigilio and G. Umana and A. Valcheva and M. Villata and A. Volvach and J.-H. Wu and X. Zhou},
	title = {{THE} {WHOLE} {EARTH} {BLAZAR} {TELESCOPE} {CAMPAIGN} {ON} {THE} {INTERMEDIATE} {BL} {LAC} {OBJECT} 3C 66A {IN} 2007-2008},
	journal = {The Astrophysical Journal},
	abstract = {Prompted by a high optical state in 2007 September, the Whole Earth Blazar Telescope consortium organized an intensive optical, near-IR (JHK) and radio observing campaign on the intermediate BL Lac object 3C 66A throughout the fall and winter of 2007–2008. In this paper, we present data from 28 observatories in 12 countries, covering the observing season from late 2007 July through 2008 February. The source remained in a high optical state throughout the observing period and exhibited several bright flares on timescales of ∼10 days. This included an exceptional outburst around 2007 September 15–20, reaching a peak brightness at R∼ 13.4. Our campaign revealed microvariability with flux changes up to |dR/dt|∼ 0.02 mag hr−1. Our observations do not reveal evidence for systematic spectral variability in the overall high state covered by our campaign, in agreement with previous results. In particular, we do not find evidence for spectral hysteresis in 3C 66A for which hints were found in an earlier campaign in a somewhat lower flux state. We also did not find any evidence for spectral lags in the discrete correlation functions between different optical bands. We infer a value of the magnetic field in the emission region of B∼ 19   e2/7B τ−6/7h D13/71 G, where eB is the magnetic field equipartition fraction, τh is the shortest observed variability timescale in units of hours, and D1 is the Doppler factor in units of 10. From the lack of systematic spectral variability, we can derive an upper limit on the Doppler factor, D ⩽ 28   τ−1/8h e3/16B. This is in perfect agreement with superluminal motion measurements with the VLBI/VLBA of βapp ⩽ 27 and argues against models with very high Lorentz factors of Γ ≳ 50, required for a one-zone synchrotron-self-Compton interpretation of some high-frequency-peaked BL Lac objects detected at TeV γ-ray energies.}
}

