@article{zhong_critical_2018,
	title = {Critical dynamical exponent of the two-dimensional scalar
	$ \phi^4 $ model with local moves},
	volume = {98},
	url = {https://link.aps.org/doi/10.1103/PhysRevE.98.062128},
	doi = {10.1103/PhysRevE.98.062128},
	abstract = {We study the scalar one-component two-dimensional (2D) ϕ4 model by computer simulations, with local Metropolis moves. The equilibrium exponents of this model are well established, e.g., for the 2D ϕ4 model γ=1.75 and ν=1. The model has also been conjectured to belong to the Ising universality class. However, the value of the critical dynamical exponent zc is not settled. In this paper, we obtain zc for the 2D ϕ4 model using two independent methods: (a) by calculating the relative terminal exponential decay time τ for the correlation function ⟨Φ(t)Φ(0)⟩, and thereafter fitting the data as τ∼Lzc, where L is the system size, and (b) by measuring the anomalous diffusion exponent for the order parameter, viz., the mean-square displacement ⟨ΔΦ2(t)⟩∼tc as c=γ/(νzc), and from the numerically obtained value c≈0.80, we calculate zc. For different values of the coupling constant λ, we report that zc=2.17±0.03 and zc=2.19±0.03 for the two methods, respectively. Our results indicate that zc is independent of λ, and is likely identical to that for the 2D Ising model. Additionally, we demonstrate that the generalized Langevin equation formulation with a memory kernel, identical to those applicable for the Ising model and polymeric systems, consistently captures the observed anomalous diffusion behavior.},
	number = {6},
	urldate = {2019-05-21},
	journal = {Physical Review E},
	author = {Zhong, Wei and Barkema, Gerard T. and Panja, Debabrata and Ball, Robin C.},
	month = dec,
	year = {2018},
	pages = {062128},
	file = {APS Snapshot:/home/master/Zotero/storage/DF6W5UQX/PhysRevE.98.html:text/html;Full Text PDF:/home/master/Zotero/storage/P4LHJFVU/Zhong et al. - 2018 - Critical dynamical exponent of the two-dimensional.pdf:application/pdf}
}

