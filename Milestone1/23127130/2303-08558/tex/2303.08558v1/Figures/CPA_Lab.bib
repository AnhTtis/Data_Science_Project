@article{blanchardRealTimeSubwavelengthTerahertz2013,
  title = {Real-Time, Subwavelength Terahertz Imaging},
  author = {Blanchard, F. and Doi, A. and Tanaka, T. and Tanaka, K.},
  year = {2013},
  month = jul,
  journal = {Annu. Rev. Mater. Res.},
  volume = {43},
  number = {1},
  pages = {237--259},
  issn = {1531-7331, 1545-4118},
  doi = {10.1146/annurev-matsci-071312-121656},
  urldate = {2022-07-25},
  abstract = {The fields of biosensing, nanospectroscopy, and plasmonics have great potential for near-field terahertz (THz) technology. In this work, we demonstrate that electro-optic (EO) imaging combined with the brightness of recently developed intense THz sources permits the imaging of subwavelength-size samples without compromising spatial resolution or acquisition time. We report on recent advances in this field and current achievements in optimizing spatial resolution and acquisition time. Nearfield imaging demonstrations on field enhancement in metallic-based resonators and metamaterials are also discussed. This development will accelerate our comprehension of subwavelength light-matter interactions at THz frequencies and enable new spectroscopic applications.},
  langid = {english}
}

@article{blanchardRealtimeTerahertzNearfield2011,
  title = {Real-Time Terahertz near-Field Microscope},
  author = {Blanchard, F. and Doi, A. and Tanaka, T. and Hirori, H. and Tanaka, H. and Kadoya, Y. and Tanaka, K.},
  year = {2011},
  month = apr,
  journal = {Opt. Express},
  volume = {19},
  number = {9},
  pages = {8277},
  issn = {1094-4087},
  doi = {10.1364/OE.19.008277},
  urldate = {2022-07-25},
  abstract = {We report a terahertz near-field microscope with a high dynamic range that can capture images of a 370 x 740 {$\mu$}m2 area at 35 frames per second. We achieve high spatial resolution (14 {$\mu$}m corresponding to {$\lambda$}/30 for a center frequency at 0.7 THz) on a large area by combining two novel techniques: terahertz generation by tilted-pulse-front excitation and electrooptic balanced imaging detection using a thin crystal. To demonstrate the microscope capability, we reveal the field enhancement at the gap position of a dipole antenna after the irradiation of a terahertz pulse.},
  langid = {english}
}

