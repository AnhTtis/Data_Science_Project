  title = {Charge Density Waves in Nanocrystalline Thin Films of Blue Bronze K0.3MoO3},
  author = {Stare{\v s}ini{\'c}, D. and Dominko, D. and Salamon, K. and Biljakovi{\'c}, K. and Tomeljak, A. and Sch{\"a}fer, H. and Huber, T. and Demsar, J. and Socol, G. and Ristoscu, C. and Mihailescu, I.N. and Siketi{\'c}, Z. and Bogdanovi{\'c} Radovi{\'c}, I. and Pletikapi{\'c}, G. and Svetli{\v c}i{\'c}, V. and Deki{\'c}, M. and {\v S}ami{\'c}, H. and Marcus, J.},
  year = {2012},
