  author = {Clerici, M and Faccio, D and Caspani, L and Peccianti, M and Yaakobi, O and Schmidt, B E and Shalaby, M and Vidal, F and L{\'e}gar{\'e}, F and Ozaki, T and Morandotti, R},
  year = {2013},
