@article{gatiBreakdownHookeLaw2016,
  title = {Breakdown of Hooke's Law of Elasticity at the Mott Critical Endpoint in an Organic Conductor},
  author = {Gati, Elena and Garst, Markus and Manna, Rudra S. and Tutsch, Ulrich and Wolf, Bernd and Bartosch, Lorenz and Schubert, Harald and Sasaki, Takahiko and Schlueter, John A. and Lang, Michael},
  year = {2016},
  month = dec,
  journal = {Sci. Adv.},
  volume = {2},
  number = {12},
  pages = {e1601646},
  issn = {2375-2548},
  doi = {10.1126/sciadv.1601646},
  urldate = {2021-09-01},
  abstract = {The Mott metal-insulator transition, a paradigm of strong electron-electron correlations, has been considered as a source of intriguing phenomena. Despite its importance for a wide range of materials, fundamental aspects of the transition, such as its universal properties, are still under debate. We report detailed measurements of relative length changes {$\Delta$}               L               /               L               as a function of continuously controlled helium-gas pressure               P               for the organic conductor {$\kappa$}-(BEDT-TTF)               2               Cu[N(CN)               2               ]Cl across the pressure-induced Mott transition. We observe strongly nonlinear variations of {$\Delta$}               L               /               L               with pressure around the Mott critical endpoint, highlighting a breakdown of Hooke's law of elasticity. We assign these nonlinear strain-stress relations to an intimate, nonperturbative coupling of the critical electronic system to the lattice degrees of freedom. Our results are fully consistent with mean-field criticality, predicted for electrons in a compressible lattice with finite shear moduli. We argue that the Mott transition for all systems that are amenable to pressure tuning shows the universal properties of an isostructural solid-solid transition.},
  langid = {english},
  keywords = {h-Cl}
}

@article{gatiEvidenceElectronicallyDriven2018,
  title = {Evidence for Electronically Driven Ferroelectricity in a Strongly Correlated Dimerized BEDT-TTF Molecular Conductor},
  author = {Gati, Elena and Fischer, Jonas K. H. and Lunkenheimer, Peter and Zielke, David and K{\"o}hler, Sebastian and Kolb, Felizitas and {von Nidda}, Hans-Albrecht Krug and Winter, Stephen M. and Schubert, Harald and Schlueter, John A. and Jeschke, Harald O. and Valent{\'i}, Roser and Lang, Michael},
  year = {2018},
  month = jun,
  journal = {Phys. Rev. Lett.},
  volume = {120},
  number = {24},
  pages = {247601},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.120.247601},
  urldate = {2023-02-02},
  abstract = {Clear evidence is provided for an order-disorder type ferroelectric state in dimerized {$\kappa$}-\dh ET\TH 2Hg\dh SCN\TH 2Cl [42], driven by charge order within the \dh ET\TH 2 dimers and stabilized by a coupling to the anions. According to our ab initio density functional theory calculations, this material is characterized by a moderate strength of dimerization t1=t0 {$\sim$} 3. Our results highlight the role of intradimer degrees of freedom in dimerized \dh ET\TH 2X materials in promoting intriguing states. Besides the possibility for electronically driven multiferroicity, for the present material, we propose that the charge order in the presence of a strong frustration may induce a quasi-1D spin-liquid state as a consequence of dimensional reduction.},
  langid = {english}
}

@article{girlando73,
  title = {Polarized Raman Spectra of TCNQ and TCNQ-D4 Single Crystals},
  author = {Girlando, A. and Pecile, C.},
  year = {1973},
  month = oct,
  journal = {Spectrochimica Acta Part A: Molecular Spectroscopy},
  volume = {29},
  number = {10},
  pages = {1859--1878},
  issn = {0584-8539},
  doi = {10.1016/0584-8539(73)80170-9},
  urldate = {2023-02-10},
  abstract = {The polarized Raman spectra of TCNQ and TCNQ-d4 single crystals were measured by means of standard and less common scattering geometries. Considering the rather unusual optical properties of the TCNQ crystals, we carefully evaluated Roth Raman and infrared spectral predictions in terms of the oriented gas model. The Raman polarization data led to an almost complete assignment of the g intramolecular fundamentals. The discordant assignments of the infrared absorption spectra previously reported by different authors are reconciled in this work by removing some remarkable discrepancies in the attribution of the symmetry species. As a result, 43 out of 50 optically active vibrational modes of both TCNQ and TCNQ-d4 have found a well based and definite attribution. The frequencies of the in-plane normal modes were calculated using a 26 parameter Valence Force Field. Generally they were in very good agreement with the observed ones. The rather complicated spectral features of the low frequency region could be only partially interpreted by proposing the assignment of the six librational modes and of some low frequency intramolecular fundamentals.},
  langid = {english}
}

