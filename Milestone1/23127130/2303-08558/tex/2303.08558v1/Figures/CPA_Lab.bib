@article{adamReviewNearFieldTerahertz2011,
  title = {Review of Near-Field Terahertz Measurement Methods and Their Applications: How to Achieve Sub-Wavelength Resolution at THz Frequencies},
  shorttitle = {Review of Near-Field Terahertz Measurement Methods and Their Applications},
  author = {Adam, Aurele Joseph Louis},
  year = {2011},
  month = sep,
  journal = {J Infrared Milli Terahz Waves},
  volume = {32},
  number = {8-9},
  pages = {976--1019},
  issn = {1866-6892, 1866-6906},
  doi = {10.1007/s10762-011-9809-2},
  urldate = {2022-07-25},
  abstract = {In the last decades, many research teams working at Terahertz frequencies focused their efforts on surpassing the diffraction limit. Numerous techniques have been investigated, combining methods existing at optic wavelength with THz system such as Time Domain Spectroscopy. The actual development led on one side to a resolution as high as {$\lambda$}/3000 and one the other side to a video-rate recording. The purpose of this paper is to give an overview of the history of the field, to describe the different approaches, to give examples of existing applications and to draw the perspective for this research area.},
  langid = {english}
}

@article{adamTHzNearfieldMeasurements2008,
  title = {THz Near-Field Measurements of Metal Structures},
  author = {Adam, Aur{\`e}le J.L. and Brok, Janne M. and Planken, Paul C.M. and Seo, Min Ah and Kim, Dai Sik},
  year = {2008},
  month = mar,
  journal = {Comptes Rendus Physique},
  volume = {9},
  number = {2},
  pages = {161--168},
  issn = {16310705},
  doi = {10.1016/j.crhy.2007.07.005},
  urldate = {2022-07-25},
  abstract = {We report on electro-optic measurements of THz electric fields in the near-field of metal structures lying on GaP electro-optic sampling crystals. With (110) oriented crystals, the x or y component of the THz electric field is measured, whereas with a (001) oriented crystal, the z-component is measured. With an estimated spatial resolution of 20 \textmu m, the technique allows us to map the field on a scale, orders of magnitude smaller than the spatial dimensions of these structures. We illustrate the technique by showing results of measurements of the THz electric field in a plane underneath a metal sphere and a single square hole in a metal foil. The technique provides us with a wealth of information on the time-evolution of light fields near metal structures. To cite this article: A.J.L. Adam et al., C. R. Physique 9 (2008).},
  langid = {english}
}

