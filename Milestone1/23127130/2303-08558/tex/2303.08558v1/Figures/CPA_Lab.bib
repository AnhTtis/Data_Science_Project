@article{naito08,
  title = {Carrier Dynamics in {$\kappa$}-Type Organic Superconductors: Time-Resolved Observation},
  shorttitle = {Carrier Dynamics in {$\kappa$}-Type Organic Superconductors},
  author = {Naito, Toshio and Yamada, Yukari and Inabe, Tamotsu and Toda, Yasunori},
  year = {2008},
  month = jun,
  journal = {J. Phys. Soc. Jpn.},
  volume = {77},
  number = {6},
  pages = {064709},
  issn = {0031-9015, 1347-4073},
  doi = {10.1143/JPSJ.77.064709},
  urldate = {2023-02-06},
  abstract = {The  -type organic superconductors have unique physical properties, many of which are yet to be understood. Most of their physical properties are closely related to carrier dynamics, which can be directly observed by ultrafast optical measurements in their nonequilibrium states. Time-resolved temperature-dependent reflectivity changes were measured on single crystals of an isostructural pair of organic charge transfer salts  -(ET)2Cu[N(CN)2]X, which are ambient- (X {$\frac{1}{4}$} Br) or high-pressure (X {$\frac{1}{4}$} Cl) superconductors. The latter compound in its ground state is a Mott insulator under ambient pressure. The obtained temperature dependences of relaxation times for both salts indicated the existence of an intermediate electronic state between those of metals and insulators at all measurement temperatures. Such an unusual electronic state coexisted with the superconducting and Mott insulating states in X {$\frac{1}{4}$} Br and X {$\frac{1}{4}$} Cl, respectively. In addition, the results for X {$\frac{1}{4}$} Br revealed the formation of a temperature-dependent well-defined gap in the superconducting transition, while those for X {$\frac{1}{4}$} Cl suggested that the Mott transition in this salt does not depend on electron\textendash phonon interactions, consistent with the theory that the Mott transition should originate from electron correlation.},
  langid = {english},
  annotation = {NIR pump-probe}
}

@article{nakagawa18,
  title = {Fluctuating Superconductivity in {\emph{{$\kappa$}}} -Type Organic Compounds Probed by Polarized Time-Resolved Spectroscopy},
  author = {Nakagawa, Koichi and Tsuchiya, Satoshi and Yamada, Jun-ichi and Toda, Yasunori},
  year = {2018},
  month = aug,
  journal = {EPL},
  volume = {122},
  number = {6},
  pages = {67003},
  issn = {1286-4854},
  doi = {10.1209/0295-5075/122/67003},
  urldate = {2021-07-28},
  abstract = {In this work, polarized time-resolved spectroscopy was carried out to investigate a superconducting (SC) state of the organic compounds {$\kappa$}-(BEDT-TTF)2X (X = Cu(NCS)2 ({$\kappa$}-NCS, Tc {$\sim$} 9 K) and Cu[N(CN)2]Br ({$\kappa$}-Br, Tc {$\sim$} 12 K)), which have different effective electron correlations. We found that the SC response, which was characterized by probe polarization anisotropy with slow relaxation time, decreases steeply at approximately Tc in {$\kappa$}-NCS with increasing temperature, corresponding to the closing of the SC gap. On the other hand, in {$\kappa$}-Br the SC response persisted significantly above Tc. The results indicate that fluctuating superconductivity (FSC) emerges in {$\kappa$}-Br but not in {$\kappa$}-NCS, suggesting that strong electron correlation can play a critical role for FSC.},
  langid = {english},
  keywords = {h-Br},
  annotation = {OP-OP}
}

