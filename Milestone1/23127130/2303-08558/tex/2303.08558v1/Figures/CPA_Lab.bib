@article{vlasovaChargetransferProcessesRadical2009,
  title = {Charge-Transfer Processes in Radical Ion Molecular Conductors {$\kappa$}-(BEDT-TTF)2Cu[N(CN)2]Br x Cl1 - x : The Superconductor (x = 0.9) and the Conductor with the Metal-Insulator Transition (x = 0)},
  shorttitle = {Charge-Transfer Processes in Radical Ion Molecular Conductors {$\kappa$}-(BEDT-TTF)2Cu[N(CN)2]Br x Cl1 - x},
  author = {Vlasova, R. M. and Drichko, N. V. and Petrov, B. V. and Semkin, V. N. and Faltermeier, D. and Barz, J. and Dumm, M. and Dressel, M. and Mezier, C. and Batail, P.},
  year = {2009},
  month = may,
  journal = {Phys. Solid State},
  volume = {51},
  number = {5},
  pages = {1041--1053},
  issn = {1063-7834, 1090-6460},
  doi = {10.1134/S1063783409050242},
  urldate = {2022-10-28},
  abstract = {Optical spectral investigations of low-dimensional organic molecular conductors {$\kappa$}-(BEDT\textendash TTF)2Cu[N(CN)2]BrxCl1 \textendash{} x with x = 0.9 (the superconductor with Tc = 11.3 K) and x = 0 (the metal with the metal\textendash insulator transition at T {$<$} 50 K) are performed in the range 50\textendash 6000 cm\textendash 1 (6 meV\textendash 0.74 eV) at temperatures from 300 to 20 K. The optical conductivity spectra are quantitatively analyzed in terms of the proposed model, according to which the charge transfer involves two types of charge carriers, i.e., electrons (holes) localized on clusters (dimers and tetramers formed by BEDT\textendash TTF molecules) and quasi-free charge carriers, with the use of the tetramer ``cluster'' model based on the Hubbard Hamiltonian for correlated electrons and the Drude model for quasi-free charge carriers. Physical parameters of the model, such as the energy of Coulomb repulsion between two electrons (holes) in one molecule, the transfer integrals between molecules inside the dimer and between dimers, and the electron\textendash molecular vibration coupling constants, are determined. The anisotropy of the spectra in the conducting plane is explained. The inference is made that only electrons localized on clusters couple with intramolecular vibrations.},
  langid = {english}
}

