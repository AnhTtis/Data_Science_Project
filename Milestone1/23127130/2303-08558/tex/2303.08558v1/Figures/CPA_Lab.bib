@article{schutte93,
  title = {The Incommensurately Modulated Structures of the Blue Bronzes K{\textsubscript{0.3}}MoO{\textsubscript{3}} and Rb{\textsubscript{0.3}}MoO{\textsubscript{3}}},
  author = {Schutte, W. J. and {de Boer}, J. L.},
  year = {1993},
  month = aug,
  journal = {Acta Crystallogr B Struct Sci},
  volume = {49},
  number = {4},
  pages = {579--591},
  issn = {0108-7681},
  doi = {10.1107/S0108768192006578},
  urldate = {2023-02-10},
  abstract = {The incommensurately modulated structures of the isostructural blue bronzes of K and Rb with modulation wavevector q = a* + 0.748 (1)b* + le* at 100 K have been determined by X-ray diffraction. The lattice parameters of the C-centred monoclinic cell for Ko.3MoO 3 are: a = 18.162 (2), b = 7.554 (1), c = 9.816 (1)/\textasciitilde,/3 = 117.393(6) \textdegree, V = 1195.7/\textasciitilde 3, Z = 20, /.t=55.7cm-l, ,\textasciitilde =0.7107A, Mr=156.9. For Rb0.3MoO3: a = 18.536 (2), b = 7.556 (1), c = 10.035 (5) A, fl = 118.52 (1)\textdegree, V = 1234.9 A 3, Z = 20, /1. = 110.0 c m - i, A = 0.7107/\textasciitilde, Mr = 172.4. The symmetry of the structure can be described as consisting of a one-dimensionally modulated system with the four-dimensional superspace group C\textasciitilde/'\textasciitilde{} (Ofl\textasciitilde ). The final Re = 0.033 for 7985 reflections for the K bronze and 0.032 for 4458 reflections for the Rb bronze. In the modulated structure, valence calculations show that the phase transition to the semiconductor state is accompanied by ordering of Mo 5+ along the infinite-chain direction. The metallic conductivity, with delocalization of 4d electrons between clusters by overlapping Mo-O-Mo orbitals along the infinite-chain direction, turns into semiconductor properties by localization of 4d electrons on individual Mo(2) and Mo(3) octahedra (not on Mol0030 clusters as a whole), modulated with wavevector q. By a comparison of the structures of the blue and red bronzes, their physical properties can be interpreted.},
  langid = {english}
}

