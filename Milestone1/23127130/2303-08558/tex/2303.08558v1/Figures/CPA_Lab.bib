@article{menzelElectronphononInteractionSpectral2009,
  title = {Electron-Phonon Interaction and Spectral Weight Transfer in Fe\$\_\{1-x\}\$Co\$\_\{x\}\$Si},
  author = {Menzel, D. and Popovich, P. and Kovaleva, N. N. and Schoenes, J. and Doll, K. and Boris, A. V.},
