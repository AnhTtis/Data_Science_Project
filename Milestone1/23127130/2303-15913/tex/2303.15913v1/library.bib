@inproceedings{Yin2003,
	abstract = {We present an intuitive animation interface that uses a foot pressure sensor pad to interactively control avatars for video games, virtual reality, and low-cost performance-driven animation. During an offline training phase, we capture full body motions with a motion capture system, as well as the corresponding foot-ground pressure distributions with a pressure sensor pad, into a database. At run time, the user acts out the animation desired on the pressure sensor pad. The system then tries to "see" the motion only through the foot-ground interactions measured, and the most appropriate motions from the database are selected, and edited online to drive the avatar. We describe our motion recognition, motion blending, and inverse kinematics algorithms in detail. They are easy to implement, and cheap to compute. FootSee can control a virtual avatar in a fixed latency of 1 second with reasonable accuracy. Our system thus makes it possible to create interactive animations without the cost or inconveniences of a full body motion capture system.},
	author = {Yin, Kangkang and Pai, Dinesh K.},
	booktitle = {Proceedings of the 2003 ACM SIGGRAPH/Eurographics symposium on Computer animation},
	file = {:C$\backslash$:/Users/f{\_}m/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yin, Pai - 2003 - FootSee an Interactive Animation System(2).pdf:pdf},
	isbn = {1581136595},
	keywords = {I37 [Computer Graphics],Three-Dimensional Graphics and Realism−Animation},
	mendeley-groups = {Diss/CheesyFoot/Use Cases,Diss/CheesyFoot},
	pages = {385},
	publisher = {Association for Computing Machinery},
	title = {{FootSee: an Interactive Animation System}},
	url = {https://dl.acm.org/citation.cfm?id=846323},
	year = {2003}
}
