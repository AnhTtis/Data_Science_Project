@article{sears03,
	abstract = {Since Weiser introduced his vision of ubiquitous computing, computing$\backslash$ndevices have become$\backslash$n$\backslash$nlighter, smaller, cheaper, and more powerful. At the same time, pervasive$\backslash$ncomputing and the$\backslash$n$\backslash$nconcept of context have attracted significant attention, with the$\backslash$ngoal of supporting the use of$\backslash$n$\backslash$ncomputing devices anywhere, anytime, as computers fade into the environment.$\backslash$nWhile there has$\backslash$n$\backslash$nbeen some success inferring the users� intensions, reliably understanding$\backslash$nusers� general goal$\backslash$n$\backslash$nremains a significant challenge. Using limited context information,$\backslash$nsuch as location, can be useful,$\backslash$n$\backslash$nbut the benefits are limited. Context is more than location. As computers$\backslash$nare embedded into$\backslash$n$\backslash$neveryday things, the situations users encounter become more variable.$\backslash$nAs a result, situationally induced$\backslash$n$\backslash$nimpairment and disabilities (SIID) will become more common and user$\backslash$ninterfaces will$\backslash$n$\backslash$nplay an even more important role. Recent understandings on context$\backslash$nsuggested the importance of$\backslash$n$\backslash$napplications themselves as parts of the whole context space. This$\backslash$narticle will explain and discuss$\backslash$n$\backslash$nthe characteristics of SIID under a three-dimension (human, environment,$\backslash$nand applications)$\backslash$n$\backslash$ncontext model. We suggest integrating information from all dimensions$\backslash$nto have a whole picture of$\backslash$n$\backslash$ncontext. More studies are needed to understand the relationship among$\backslash$ndifferent dimensions, and$\backslash$n$\backslash$nto help design effective context-aware applications overcoming SIID.},
	author = {Sears, Andrew and Lin, Min and Jacko, Julie and Xiao, Yan},
	isbn = {0805849319},
	issn = {0023-7213 (Print)},
	journal = {Proceedings of HCI International 2003, 10th International Conference on Human-Computer Interaction},
	mendeley-groups = {Cheesyfoot2go/Interaction,Diss,Diss/WalkTheLine},
	pages = {1298--1302},
	pmid = {25310037},
	title = {{When computers fade: Pervasive computing and situationallyinduced impairments and disabilities}},
	volume = {2},
	year = {2003}
}
