@article{Chapman1987,
	abstract = {Foot preference has been given only superficial attention in studies of hemispheric lateralization, although it has potential utility for predicting hemispheric dominance. This paper reports the development of a reliable (alpha = 0.89) 11-item behavioral inventory of foot preference. Since footedness and handedness are only partially related, both must be measured reliably to identify individuals who have a consistent right or left side preference. It has yet to be determined whether footedness or handedness has the stronger relationship to other aspects of cerebral lateralization and whether both measures together predict lateralization better than one of them alone.},
	author = {Chapman, Jean and Chapman, Loren and Allen, John},
	doi = {10.1016/0028-3932(87)90082-0},
