	month = {may},
	number = {3},
	pages = {333--361},
	publisher = {Taylor {\&} Francis Group},
	title = {{Foot Operation of Controls}},
	url = {http://www.tandfonline.com/doi/abs/10.1080/00140137108931255},
	volume = {14},
	year = {1971}
