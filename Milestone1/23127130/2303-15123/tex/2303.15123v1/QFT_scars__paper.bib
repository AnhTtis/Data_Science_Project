	journal = {Journal of High Energy Physics},
	number = {7},
	pages = {123},
	title = {Black hole collapse in the 1/c expansion},
	ty = {JOUR},
	xurl = {https://doi.org/10.1007/JHEP07(2016)123},
	volume = {2016},
	year = {2016},
