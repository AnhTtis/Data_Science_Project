@article{Reagor2017,
author = {Matthew Reagor  and Christopher B. Osborn  and Nikolas Tezak  and Alexa Staley  and Guenevere Prawiroatmodjo  and Michael Scheer  and Nasser Alidoust  and Eyob A. Sete  and Nicolas Didier  and Marcus P. da Silva  and Ezer Acala  and Joel Angeles  and Andrew Bestwick  and Maxwell Block  and Benjamin Bloom  and Adam Bradley  and Catvu Bui  and Shane Caldwell  and Lauren Capelluto  and Rick Chilcott  and Jeff Cordova  and Genya Crossman  and Michael Curtis  and Saniya Deshpande  and Tristan El Bouayadi  and Daniel Girshovich  and Sabrina Hong  and Alex Hudson  and Peter Karalekas  and Kat Kuang  and Michael Lenihan  and Riccardo Manenti  and Thomas Manning  and Jayss Marshall  and Yuvraj Mohan  and William O’Brien  and Johannes Otterbach  and Alexander Papageorge  and Jean-Philip Paquette  and Michael Pelstring  and Anthony Polloreno  and Vijay Rawat  and Colm A. Ryan  and Russ Renzas  and Nick Rubin  and Damon Russel  and Michael Rust  and Diego Scarabelli  and Michael Selvanayagam  and Rodney Sinclair  and Robert Smith  and Mark Suska  and Ting-Wai To  and Mehrnoosh Vahidpour  and Nagesh Vodrahalli  and Tyler Whyland  and Kamal Yadav  and William Zeng  and Chad T. Rigetti },
title = {Demonstration of universal parametric entangling gates on a multi-qubit lattice},
journal = {Science Advances},
volume = {4},
number = {2},
pages = {eaao3603},
year = {2018},
doi = {10.1126/sciadv.aao3603},
URL = {https://www.science.org/doi/abs/10.1126/sciadv.aao3603},
eprint = {https://www.science.org/doi/pdf/10.1126/sciadv.aao3603},
abstract = {Harnessing techniques from analog signal processing, we establish a new path for large-scale quantum computation. We show that parametric coupling techniques can be used to generate selective entangling interactions for multi-qubit processors. By inducing coherent population exchange between adjacent qubits under frequency modulation, we implement a universal gate set for a linear array of four superconducting qubits. An average process fidelity of ℱ = 93\% is estimated for three two-qubit gates via quantum process tomography. We establish the suitability of these techniques for computation by preparing a four-qubit maximally entangled state and comparing the estimated state fidelity with the expected performance of the individual entangling gates. In addition, we prepare an eight-qubit register in all possible bitstring permutations and monitor the fidelity of a two-qubit gate across one pair of these qubits. Across all these permutations, an average fidelity of ℱ = 91.6 ± 2.6\% is observed. These results thus offer a path to a scalable architecture with high selectivity and low cross-talk.}}

