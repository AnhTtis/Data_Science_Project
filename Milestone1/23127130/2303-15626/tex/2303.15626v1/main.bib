@article{roland_quantum_2002,
	title = {Quantum search by local adiabatic evolution},
	volume = {65},
	url = {http://link.aps.org.ezp-prod1.hul.harvard.edu/abstract/PRA/v65/e042308},
	doi = {{10.1103/PhysRevA.65.042308}},
	abstract = {The adiabatic theorem has been recently used to design quantum algorithms of a new kind, where the quantum computer evolves slowly enough so that it remains near its instantaneous ground state, which tends to the solution. We apply this time-dependent Hamiltonian approach to Groverâs problem, i.e., searching a marked item in an unstructured database. We find that by adjusting the evolution rate of the Hamiltonian so as to keep the evolution adiabatic on each infinitesimal time interval, the total running time is of order {sqrt[N],} where N is the number of items in the database. We thus recover the advantage of Groverâs standard algorithm as compared to a classical search, scaling as N. This is in contrast with the constant-rate adiabatic approach of Farhi et al. (e-print quant-ph/0001106), where the requirement of adiabaticity is expressed only globally, resulting in a time of order N.},
