}

@article{ianniruberto_simulations_2012,
	title = {Simulations of {Fast} {Shear} {Flows} of {PS} {Oligomers} {Confirm} {Monomeric} {Friction} {Reduction} in {Fast} {Elongational} {Flows} of {Monodisperse} {PS} {Melts} {As} {Indicated} by {Rheooptical} {Data}},
