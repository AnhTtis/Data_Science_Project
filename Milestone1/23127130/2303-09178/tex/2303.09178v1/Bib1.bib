	doi = {10.1021/acs.nanolett.9b02224},
	abstract = {When compressed in a slit of width \$D\$, a \${\textbackslash}Theta\$-chain that displays the scaling of size \$R\_0\$ (diameter) with respect to the number of monomers \$N\$, \$R\_0{\textbackslash}sim aN{\textasciicircum}\{1/2\}\$, expands in the lateral direction as \$R\_\{{\textbackslash}parallel\}{\textbackslash}sim a N{\textasciicircum}\{{\textbackslash}nu\}(a/D){\textasciicircum}\{2{\textbackslash}nu-1\}\$. Provided that the \${\textbackslash}Theta\$ condition is strictly maintained throughout the compression, the well-known scaling exponent of \${\textbackslash}Theta\$-chain in 2 dimensions, \${\textbackslash}nu=4/7\$, is anticipated in a perfect confinement. However, numerics shows that upon increasing compression from \$R\_0/D{\textless}1\$ to \$R\_0/D{\textbackslash}gg 1\$, \${\textbackslash}nu\$ gradually deviates from \${\textbackslash}nu=1/2\$ and plateaus at \${\textbackslash}nu=3/4\$, the exponent associated with the self-avoiding walk in two dimensions. Using both theoretical considerations and numerics, we argue that it is highly nontrivial to maintain the \${\textbackslash}Theta\$ condition under confinement because of two major effects. First, as the dimension is reduced from 3 to 2 dimensions, the contributions of higher order virial terms, which can be ignored in 3 dimensions at large \$N\$, become significant. Second and more importantly, the geometrical confinement, which is regarded as an applied external field, alters the second virial coefficient (\$B\_2\$) changes from \$B\_2=0\$ (\${\textbackslash}Theta\$ condition) in free space to \$B\_2{\textgreater}0\$ (good-solvent condition) in confinement. Our study provides practical insight into how confinement affects the conformation of a single polymer chain.},
	language = {en},
	number = {8},
	urldate = {2022-09-18},
	journal = {Nano Letters},
	author = {Liu, Lei and Pincus, Philip A. and Hyeon, Changbong},
	month = aug,
	year = {2019},
	note = {arXiv:1905.13473 [cond-mat, physics:physics]},
	pages = {5667--5673},
	file = {1905.13473.pdf:G\:\\My Drive\\Zotero\\storage\\5T475ZSY\\1905.13473.pdf:application/pdf},
}

