@article{Cejnar2007,
  author={Pavel Cejnar and Francesco Iachello},
  title={Phase structure of interacting boson models in arbitrary dimension},
  journal={J. Phys. A: Math. Theor.},
  volume={40},
  number={4},
  pages={581},
  url={http://stacks.iop.org/1751-8121/40/i=4/a=001},
  year={2007},
  abstract={We analyse the phase structure of a class of interacting boson models with two types of bosons, one scalar and one non-scalar, subject to one- and two-body interactions and with dynamic algebra U ( n ). To these models, we associate a classical description in terms of f = n − 1 variables. We show that, if the system is invariant under two- or three-dimensional rotations (for f ≥ 2 even or odd), the models have both first- and second-order phase transitions only if f = 5, 9, 13, .... In the other f ≥ 2 cases, the system has only second-order transitions. All phase transitions of this class of models belong to the cusp catastrophe in the classification of structurally unstable potentials.}
}
@article{Cejnar2021,
	doi = {10.1088/1751-8121/abdfe8},
	url = {https://doi.org/10.1088/1751-8121/abdfe8},
	year = 2021,
	month = {mar},
	publisher = {{IOP} Publishing},
	volume = {54},
	number = {13},
	pages = {133001},
	author = {Pavel Cejnar and Pavel Str{\'{a}}nsk{\'{y}} and Michal Macek and Michal Kloc},
	title = {Excited-state quantum phase transitions},
	journal = {J. Phys. A: Math. Theor.},
	abstract = {We review the effects of excited-state quantum phase transitions (ESQPTs) in interacting many-body systems with finite numbers of collective degrees of freedom. We classify typical ESQPT signatures in the spectra of energy eigenstates with respect to the underlying classical dynamics and outline a variety of quantum systems in which they occur. We describe thermodynamic and dynamic consequences of ESQPTs, like those in microcanonical thermodynamics, quantum quench dynamics, and in the response to nearly adiabatic or periodic driving. We hint at some generalizations of the ESQPT concept in periodic lattices and in resonant tunneling systems.}
}
