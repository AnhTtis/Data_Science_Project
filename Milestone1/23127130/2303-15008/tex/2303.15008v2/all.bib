  EPRINT        = {1103.2370v1},
  journal  = {ApJ},
  MONTH         = {03},
  PAGES         = {L40},
  TITLE         = {Observational evidence for a metal rich atmosphere on the super-Earth GJ1214b},
  URL           = {http://arxiv.org/abs/1103.2370v1},
  VOLUME        = {731},
  YEAR          = {2011},
}

