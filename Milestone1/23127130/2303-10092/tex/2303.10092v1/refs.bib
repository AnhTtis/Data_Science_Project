journal = {Computational Materials Science},
keywords = {Composites,Electron microscopy,Finite element method,Microanalysis,Micromechanics,Tungsten-copper},
title = {{Modelling the elastic properties of bi-continuous composite microstructures captured with TriBeam serial-sectioning}},
volume={131},
