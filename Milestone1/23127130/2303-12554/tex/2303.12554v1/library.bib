issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Regularized Stokeslets,Stokes flow,Stokeslets},
month = {dec},
pages = {783--796},
publisher = {Elsevier Inc.},
title = {{Regularized Stokeslet segments}},
url = {https://doi.org/10.1016/j.jcp.2018.08.055 https://linkinghub.elsevier.com/retrieve/pii/S0021999118305886},
volume = {375},
year = {2018}
}
