issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Cartesian grid methods,Double-layer potential,Embedded domain methods,Fast multipole methods,Fast solvers,Fictitous domain methods,Immersed interface methods,Integral equations,Moving boundaries,Stokes equations},
month = {jan},
number = {1},
