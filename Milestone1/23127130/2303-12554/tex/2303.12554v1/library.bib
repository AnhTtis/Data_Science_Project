issn = {10902716},
journal = {J. Comput. Phys.},
keywords = {Ewald summation,Kernel independent fast multipole method,Periodic boundary conditions},
pages = {214--232},
publisher = {Elsevier Inc.},
title = {{Flexibly imposing periodicity in kernel independent FMM: A multipole-to-local operator approach}},
url = {https://doi.org/10.1016/j.jcp.2017.11.012},
