issn = {10902716},
journal = {J. Comput. Phys.},
keywords = {Boundary integral method,Fast multipole methods,Fluid membranes,Fluid-structure interaction,Particulate flows,Semi-implicit algorithms,Spectral collision detection,Stokes flow,Suspensions,Vesicle simulations},
pages = {245--267},
publisher = {Elsevier Inc.},
title = {{High-volume fraction simulations of two-dimensional vesicle suspensions}},
url = {http://dx.doi.org/10.1016/j.jcp.2014.06.013},
volume = {274},
year = {2014}
}
