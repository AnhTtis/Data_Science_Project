file = {:C\:/Users/Ludvig/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Barnett, Betcke - 2008 - Stability and convergence of the method of fundamental solutions for Helmholtz problems on analytic domains.pdf:pdf},
issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Analytic continuation,Boundary value problem,Helmholtz equation,High frequency waves,Method of fundamental solutions},
month = {jul},
number = {14},
pages = {7003--7026},
title = {{Stability and convergence of the method of fundamental solutions for Helmholtz problems on analytic domains}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0021999108002106},
volume = {227},
year = {2008}
}
@article{Mikkonen2012,
