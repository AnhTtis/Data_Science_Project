journal = {ACS Nano},
keywords = {Wenzel and Cassie states,contact angle,hysteresis,molecular dynamics simulation,nanopillared surface},
number = {9},
pages = {6834--6842},
title = {{Measurement of contact-angle hysteresis for droplets on nanopillared surface and in the Cassie and Wenzel states: A molecular dynamics simulation study}},
volume = {5},
year = {2011}
}
