journal = {Theor. Chim. Acta},
keywords = {Bonded atoms,Dipole moments,Electron density,Populations},
mendeley-groups = {Hirshfeld},
number = {2},
pages = {129--138},
primaryClass = {10.1007},
title = {{Bonded-atom fragments for describing molecular charge densities}},
volume = {44},
year = {1977}
}

