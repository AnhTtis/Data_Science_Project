issn = {0192-8651},
journal = {J. Comput. Chem.},
keywords = {ABEEM$\sigma \pi$ polarizable force field,charge distributions,dipole moments,transition metal atoms,valence state parameters},
month = {sep},
number = {23},
pages = {1690--1706},
publisher = {John Wiley {\&} Sons, Ltd},
title = {{Valence state parameters of all transition metal atoms in metalloproteins—development of ABEEM$\sigma \pi$ fluctuating charge force field}},
url = {https://doi.org/10.1002/jcc.23676},
volume = {35},
year = {2014}
}

