file = {:home/xhu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bayly et al. - 1993 - A well-behaved electrostatic potential based method using charge restraints for deriving atomic charges The RESP m.pdf:pdf},
isbn = {0022-3654},
issn = {00223654},
journal = {J. Phys. Chem.},
mendeley-groups = {ESP},
number = {40},
pages = {10269--10280},
