@article{kurosawa2006,
    author = {Kurosawa, Ryuichi and Harries, Tim J. and Symington, Neil H.},
    title = "{On the formation of Hα line emission around classical T Tauri stars}",
    journal = {Monthly Notices of the Royal Astronomical Society},
    volume = {370},
    number = {2},
    pages = {580-596},
    year = {2006},
    month = {06},
    abstract = "{We present radiative transfer models of the circumstellar environment of classical T Tauri stars, concentrating on the formation of the Hα emission. The wide varieties of line profiles seen in observations are indicative of both inflow and outflow, and we therefore employ a circumstellar structure that includes both magnetospheric accretion and a disc wind. We perform systematic investigations of the model parameters for the wind and the magnetosphere to search for possible geometrical and physical conditions which lead to the types of profiles seen in observations. We find that the hybrid models can reproduce the wide range of profile types seen in observations, and that the most common profile types observed occupy a large volume of parameter space. Conversely, the most infrequently observed profile morphologies require a very specific set of models parameters. We find our model profiles are consistent with the canonical value of the mass-loss rate to mass-accretion rate ratio (μ= 0.1) found in earlier magnetohydrodynamic calculations and observations, but the models with 0.05 \\&lt; μ \\&lt; 0.2 are still in accord with observed Hα profiles. We investigate the wind contribution to the line profile as a function of model parameters, and examine the reliability of Hα as a mass-accretion diagnostic. Finally, we examine the Hα spectroscopic classification used by Reipurth et al., and discuss the basic physical conditions that are required to reproduce the profiles in each classified type.}",
    issn = {0035-8711},
    doi = {10.1111/j.1365-2966.2006.10527.x},
    url = {https://doi.org/10.1111/j.1365-2966.2006.10527.x},
    eprint = {https://academic.oup.com/mnras/article-pdf/370/2/580/2897091/mnras0370-0580.pdf},
}
