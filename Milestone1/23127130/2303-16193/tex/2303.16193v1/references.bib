@article{Canas_2022,
doi = {10.3847/1538-3881/ac7804},
url = {https://dx.doi.org/10.3847/1538-3881/ac7804},
year = {2022},
month = {jul},
publisher = {The American Astronomical Society},
volume = {164},
number = {2},
pages = {50},
author = {Caleb I. Cañas and Shubham Kanodia and Chad F. Bender and Suvrath Mahadevan and Guđhmundur Stefánsson and William D. Cochran and Andrea S. J. Lin and Hsiang-Chih Hwang and Luke Powers and Andrew Monson and Elizabeth M. Green and Brock A. Parker and Tera N. Swaby and Henry A. Kobulnicky and John Wisniewski and Arvind F. Gupta and Mark E. Everett and Sinclaire Jones and Benjamin Anjakos and Corey Beard and Cullen H. Blake and Scott A. Diddams and Zehao Dong and Connor Fredrick and Elnaz Hakemiamjad and Leslie Hebb and Jessica E. Libby-Roberts and Sarah E. Logsdon and Michael W. McElwain and Andrew J. Metcalf and Joe P. Ninan and Jayadev Rajagopal and Lawrence W. Ramsey and Paul Robertson and Arpita Roy and Jacob Ruhle and Christian Schwab and Ryan C. Terrien and Jason T. Wright},
title = {TOI-3714 b and TOI-3629 b: Two Gas Giants Transiting M Dwarfs Confirmed with the Habitable-zone Planet Finder and NEID},
journal = {The Astronomical Journal},
abstract = {We confirm the planetary nature of two gas giants discovered by the Transiting Exoplanet Survey Satellite to transit M dwarfs. TOI-3714 (V = 15.24,  J = 11.74) is an M2 dwarf hosting a hot Jupiter (M  p  = 0.70 ± 0.03 M  J  and R  p  = 1.01 ± 0.03 R  J ) on an orbital period of 2.154849 ± 0.000001 days with a resolved white dwarf companion. TOI-3629 (V = 14.63,  J = 11.42) is an M1 dwarf hosting a hot Jupiter (M  p  = 0.26 ± 0.02 M  J  and R  p  =0.74 ± 0.02 R  J ) on an orbital period of  days. We characterize each transiting companion using a combination of ground-based and space-based photometry, speckle imaging, and high-precision velocimetry from the Habitable-zone Planet Finder and the NEID spectrographs. With the discovery of these two systems, there are now nine M dwarfs known to host transiting hot Jupiters. Among this population, TOI-3714 b (T eq = 750 ± 20 K and TSM = 98 ± 7) and TOI-3629 b (T eq = 690 ± 20 K and TSM = 80 ± 9) are warm gas giants amenable to additional characterization with transmission spectroscopy to probe atmospheric chemistry and, for TOI-3714, obliquity measurements to probe formation scenarios.}
}

