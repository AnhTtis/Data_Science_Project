	Doi = {10.1088/0004-637X/735/2/71},
	Eid = {71},
	Eprint = {1104.3897},
	Journal = {\apj},
	Keywords = {galaxies: abundances, galaxies: evolution, H II regions, ISM: kinematics and dynamics},
	Month = jul,
	Pages = {71},
	Primaryclass = {astro-ph.CO},
	Title = {{Metal Transport to the Gaseous Outskirts of Galaxies}},
	Volume = 735,
	Year = 2011,
