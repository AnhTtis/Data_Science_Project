@article{Noginov19_318,
    author = {Vanessa N. Peters and Md Omar Faruk and Joshua Asane and Rohan Alexander and D'angelo A. Peters and Srujana Prayakarao and Sangeeta Rout and M. A. Noginov},
    journal = {Optica},
    keywords = {Energy transfer; Infrared radiation; Organic photovoltaics; Raman scattering; Surface plasmon polaritons; Surface plasmons},
    number = {3},
    pages = {318--325},
    publisher = {OSA},
    title = {Effect of strong coupling on photodegradation of the semiconducting polymer P3HT},
    volume = {6},
    month = {Mar},
    year = {2019},
    url = {http://www.osapublishing.org/optica/abstract.cfm?URI=optica-6-3-318},
    doi = {10.1364/OPTICA.6.000318},
    abstract = {Photonic structures are commonly designed to manipulate scattered or emitted light. However, in recent years, they were demonstrated to affect a variety of phenomena, including chemical reactions, which lie outside the traditional electrodynamics domain. In this work, we have studied the effect of a Fabry\&\#x2013;Perot cavity on the chemical reaction of practical importance\&\#x2014;photodegradation of the semiconducting polymer 2,5-poly(3-hexylthiophene) (P3HT), which is the material of choice in organic photovoltaics. Experimentally, Fabry\&\#x2013;Perot cavities, composed of two silver mirrors and filled with P3HT polymer, were photoexposed over tens of hours, and the concentration of the remaining thiophene rings (composing P3HT) was studied as a function of time. It has been found that in the regime of strong coupling with the cavity, characterized by one of the largest values of the Rabi splitting reported in the literature (1.0\&\#x00A0;eV), the normalized rate of photodegradation is reduced threefold, substantially larger than in the weak coupling regime reported in our recent study. This result adds to the toolbox of strong coupling phenomena and paves the road towards long-range control of chemical reactions and catalysis.},
}



