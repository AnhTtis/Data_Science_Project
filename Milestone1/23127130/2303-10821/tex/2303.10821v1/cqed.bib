    title = {Calculation of Transition Density Matrices by Nonunitary Orbital Transformations},
    journal = {International Journal of Quantum Chemistry},
    volume = {30},
    number = {4},
    pages = {479-494},
    doi = {10.1002/qua.560300404},
    url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/qua.560300404},
