@article{waldner_simple_2018,
	title = {A simple mathematical model using centred loops and random perturbations accurately reconstructs search patterns observed in desert ants},
	volume = {204},
	issn = {0340-7594, 1432-1351},
	url = {http://link.springer.com/10.1007/s00359-018-1297-6},
	doi = {10.1007/s00359-018-1297-6},
	abstract = {This paper describes a new mathematical model that is based on centred loops to reconstruct the “Systematic Search” behaviour of Cataglyphis desert ants. The notable advantage of this model is the combination of simplicity, efficiency and performance. All model input is kept to a minimum, using only parameters that previous research has shown to be available to the animals at all times: distance from the origin, direction of the last step and home vector. Outbound and inbound search paths are being combined into loops that return to the origin, sampling this area more intensely. A stochastic element is added by random perturbations during the next step, mimicking unsystematic errors during the process of path integration and yielding the typical search patterns observed in Cataglyphis desert ants. The model output is compared to runs observed in the field.},
	language = {en},
	number = {12},
	urldate = {2021-08-26},
	journal = {Journal of Comparative Physiology A},
	author = {Waldner, Franz and Merkle, Tobias},
	month = dec,
	year = {2018},
	pages = {985--998},
	file = {Waldner and Merkle - 2018 - A simple mathematical model using centred loops an.pdf:/home/unimelb.edu.au/eflayac/Documents/Articles/ants/Waldner and Merkle - 2018 - A simple mathematical model using centred loops an.pdf:application/pdf}
}

