	note = {arXiv: 2103.12158},
	keywords = {Computer Science - Machine Learning, Electrical Engineering and Systems Science - Systems and Control},
	annote = {Comment: 32 pages, 12 figures. arXiv admin note: text overlap with arXiv:2010.07452},
	file = {Kara and Yuksel - 2021 - Convergence of Finite Memory Q-Learning for POMDPs.pdf:/home/unimelb.edu.au/eflayac/Documents/Articles/Optimisation/Kara and Yuksel - 2021 - Convergence of Finite Memory Q-Learning for POMDPs.pdf:application/pdf}
}

