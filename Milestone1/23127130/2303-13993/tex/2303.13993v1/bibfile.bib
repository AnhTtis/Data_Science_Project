	note = {arXiv: 2006.01397},
	keywords = {Mathematics - Optimization and Control, Computer Science - Machine Learning, Statistics - Machine Learning, Electrical Engineering and Systems Science - Systems and Control},
	file = {2006.01397.pdf:/home/unimelb.edu.au/eflayac/Documents/Articles/Optimisation/Online optimisation/2006.01397.pdf:application/pdf}
}

