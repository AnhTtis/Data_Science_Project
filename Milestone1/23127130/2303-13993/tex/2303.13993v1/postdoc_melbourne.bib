	author = {A. Brezhneva, Olga and A. Tret’yakov, Alexey and E. Marsden, Jerrold and {,Department of Mathematics and Statistics, 123 Bachelor Hall, Miami University, Oxford, OH 45056} and {,System Research Institute, Polish Academy of Sciences, Newelska 6, 01-447 Warsaw, Poland, University of Podlasie in Siedlce, 3 Maja 54, 08-110 Siedlce} and {,Control and Dynamical Systems 107-81, California Institute of Technology, Pasadena, CA 91125}},
	year = {2008},
