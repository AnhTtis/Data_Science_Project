	note = {arXiv: 1812.05509},
	keywords = {Mathematics - Optimization and Control},
	annote = {Comment: Some of the results in this paper are to be presented at the 2019 IEEE Conference on Decision and Control},
	file = {Kara et al. - 2019 - Weak Feller Property of Non-linear Filters.pdf:C\:\\Users\\eflayac\\Documents\\Articles\\Estimation\\Kara et al. - 2019 - Weak Feller Property of Non-linear Filters.pdf:application/pdf}
}

@article{mcdonald_stability_2020,
