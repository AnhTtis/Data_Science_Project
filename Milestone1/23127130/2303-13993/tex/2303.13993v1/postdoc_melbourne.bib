	note = {arXiv: 1908.03790},
	keywords = {Computer Science - Robotics},
	annote = {Comment: 16 pages},
	file = {Frey et al. - 2020 - Towards Online Observability-Aware Trajectory Opti.pdf:/home/unimelb.edu.au/eflayac/Documents/Articles/Optimisation/Frey et al. - 2020 - Towards Online Observability-Aware Trajectory Opti.pdf:application/pdf}
}

