	keywords = {Mathematics - Optimization and Control},
	annote = {Comment: 11 pages, 4 figures, submitted to a journal},
	file = {Hu - 2017 - Robust Stability of Optimization-based State Estim.pdf:C\:\\Users\\eflayac\\Documents\\Articles\\Optimisation\\MPC and MHE\\MHE\\Hu - 2017 - Robust Stability of Optimization-based State Estim.pdf:application/pdf}
