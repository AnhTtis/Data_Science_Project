@article{mcdonald_converse_2020,
	title = {Converse {Results} on {Filter} {Stability} {Criteria} and {Stochastic} {Non}-{Linear} {Observability}},
	url = {http://arxiv.org/abs/1812.01772},
	abstract = {Filter stability is a classical and extensively studied problem for partially observed Markov processes. For such a process, an incorrectly initialized non-linear ﬁlter is said to be stable if the ﬁlter eventually corrects itself with the arrival of new measurement information. This paper has three main contributions: (i) we introduce a general explicit deﬁnition of observability for stochastic non-linear dynamical systems which involves a functional characterization that is testable with little eﬀort. Under this observability deﬁnition we establish stability of one-step predictors under weak convergence. (ii) We establish new implications between various criteria for ﬁlter stability, notably in arriving from weak convergence of predictors to that of ﬁlters through weak Feller continuity of ﬁlter transitions, from weak merging to total variation merging for both the predictor and ﬁlters, and ﬁnally in establishing the equivalence of total variation merging and relative entropy merging. (iii) We then strengthen the stability results to involve weak merging and total variation merging, both in expectation and in an almost sure sense, as well as in relative entropy (which also has implications in the context of relative entropy convergence of Markov chains to invariant measures). Our conditions, and the examples we study, complement and generalize the existing results on ﬁlter stability and a detailed comparison is reported. Numerous examples are presented.},
	language = {en},
	urldate = {2021-04-08},
	journal = {arXiv:1812.01772 [math]},
	author = {McDonald, Curtis and Yuksel, Serdar},
	month = jul,
	year = {2020},
	note = {arXiv: 1812.01772},
	keywords = {Mathematics - Optimization and Control, Mathematics - Probability},
	annote = {Comment: Re-organized papers to better focus on important points},
	file = {McDonald and Yuksel - 2020 - Converse Results on Filter Stability Criteria and.pdf:/home/unimelb.edu.au/eflayac/Documents/Articles/Estimation/McDonald and Yuksel - 2020 - Converse Results on Filter Stability Criteria and.pdf:application/pdf}
}

@article{kara_convergence_2021,
