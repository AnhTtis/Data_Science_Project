@article{marxenDisturbanceEvolutionMach2010,
  title = {Disturbance Evolution in a {{Mach}} 4.8 Boundary Layer with Two-Dimensional Roughness-Induced Separation and Shock},
  author = {Marxen, Olaf and Iaccarino, Gianluca and Shaqfeh, Eric S. G.},
  year = {2010},
