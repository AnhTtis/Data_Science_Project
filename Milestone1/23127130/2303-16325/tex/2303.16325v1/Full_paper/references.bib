@article{rodriguezStudyLinearWavepacket2015,
  title = {A Study of Linear Wavepacket Models for Subsonic Turbulent Jets Using Local Eigenmode Decomposition of {{PIV}} Data},
  author = {Rodr{\'i}guez, Daniel and Cavalieri, Andr{\'e} V.G. and Colonius, Tim and Jordan, Peter},
