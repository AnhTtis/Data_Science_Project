@article{chuEnergyTransferSmall1965,
  title = {On the Energy Transfer to Small Disturbances in Fluid Flow ({{Part I}})},
  author = {Chu, Boa-Teh},
  year = {1965},
  month = sep,
  journal = {Acta Mechanica},
  volume = {1},
  number = {3},
  pages = {215--234},
  issn = {1619-6937},
  doi = {10.1007/BF01387235},
  abstract = {The ``energy in a small disturbance'' in a viscous compressible heat-conductive medium is defined as a positive definite quantity characterizing the mean level of fluctuation in the disturbance which, in the absence of heat transfer at the boundaries and of work done by boundary forces or body forces, and in the absence of heat and material sources, is a monotone non-increasing function of time. For small disturbances a quantity satisfying these requirements is found. When viscosity and heat conductivity are neglected, it reduces to the familiar acoustic energy in the theory of sound. Stability in the mean of such a fluid system can thus be discussed with reference to the growth and decay of the energy in the disturbance.}
