@article{cavalieriTransitionChaosReducedorder2022,
  title = {Transition to Chaos in a Reduced-Order Model of a Shear Layer},
  author = {Cavalieri, Andr{\'e} V. G. and Rempel, Erico L. and Nogueira, Petr{\^o}nio A. S.},
  year = {2022},
