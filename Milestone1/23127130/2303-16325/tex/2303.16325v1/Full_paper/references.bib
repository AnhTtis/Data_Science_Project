@article{towneEfficientGlobalResolvent2022,
  title = {Efficient Global Resolvent Analysis via the One-Way {{Navier}}\textendash{{Stokes}} Equations},
  author = {Towne, Aaron and Rigas, Georgios and Kamal, Omar and Pickering, Ethan and Colonius, Tim},
