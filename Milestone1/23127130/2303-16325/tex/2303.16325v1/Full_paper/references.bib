@article{caoTransitionTurbulenceHypersonic2022,
  title = {Transition to Turbulence in Hypersonic Flow over a Compression Ramp Due to Intrinsic Instability},
  author = {Cao, Shibin and Hao, Jiaao and Klioutchnikov, Igor and Wen, Chih-Yung and Olivier, Herbert and Heufer, Karl Alexander},
  year = {2022},
