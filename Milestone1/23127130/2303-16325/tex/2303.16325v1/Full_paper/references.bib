@article{dengFurtherStudiesGeometric2013,
  title = {Further Studies on {{Geometric Conservation Law}} and Applications to High-Order Finite Difference Schemes with Stationary Grids},
  author = {Deng, Xiaogang and Min, Yaobing and Mao, Meiliang and Liu, Huayong and Tu, Guohua and Zhang, Hanxin},
