@article{cookFreestreamReceptivityHypersonic2022,
  title = {Free-Stream Receptivity of a Hypersonic Blunt Cone Using Input\textendash Output Analysis and a Shock-Kinematic Boundary Condition},
  author = {Cook, David A. and Nichols, Joseph W.},
  year = {2022},
