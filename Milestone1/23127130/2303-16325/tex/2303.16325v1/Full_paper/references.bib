@article{hernandezSLEPcScalableFlexible2005,
  title = {{{SLEPc}}: {{A}} Scalable and Flexible Toolkit for the Solution of Eigenvalue Problems},
  shorttitle = {{{SLEPc}}},
  author = {Hernandez, Vicente and Roman, Jose E. and Vidal, Vicente},
  year = {2005},
