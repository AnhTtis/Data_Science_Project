@article{detullioInfluenceBoundarylayerDisturbances2015,
  title = {Influence of Boundary-Layer Disturbances on the Instability of a Roughness Wake in a High-Speed Boundary Layer},
  author = {De Tullio, Nicola and Sandham, Neil D.},
  year = {2015},
