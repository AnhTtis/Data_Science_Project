	doi = {https://doi.org/10.1016/j.jheap.2014.06.001},
	issn = {2214-4048},
	journal = {Journal of High Energy Astrophysics},
	pages = {1 - 8},
	title = {Millisecond pulsar interpretation of the Galactic center gamma-ray excess},
	url = {http://www.sciencedirect.com/science/article/pii/S2214404814000147},
	volume = {3-4},
	year = {2014},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S2214404814000147},
