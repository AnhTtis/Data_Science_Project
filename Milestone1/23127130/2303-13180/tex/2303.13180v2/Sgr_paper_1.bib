	issn = {0370-1573},
	journal = {Physics Reports},
	keywords = {Gamma-ray observations, Dark matter, Fermi-LAT},
	note = {Sensitivity Projections for Dark Matter Searches with the Fermi Large Area Telescope},
	pages = {1-46},
	title = {Sensitivity projections for dark matter searches with the Fermi large area telescope},
	url = {https://www.sciencedirect.com/science/article/pii/S0370157316300916},
	volume = {636},
