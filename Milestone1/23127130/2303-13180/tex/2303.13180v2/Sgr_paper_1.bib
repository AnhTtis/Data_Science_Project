	date-modified = {2022-10-05 16:22:02 +1100},
	doi = {10.1038/s41550-022-01777-x},
	id = {Crocker2022},
	isbn = {2397-3366},
	journal = {Nature Astronomy},
	title = {Gamma-ray emission from the Sagittarius dwarf spheroidal galaxy due to millisecond pulsars},
