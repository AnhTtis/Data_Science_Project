@article{tHooft:1998,
title = {The self-screening Hawking atmosphere — a different approach to quantum black hole microstates},
journal = {Nucl. Phys. B - Proc. Suppl.},
volume = {68},
number = {1},
pages = {174-184},
year = {1998},
note = {Strings '97},
@issn = {0920-5632},
doi = {10.1016/S0920-5632(98)00151-0},
url = {https://www.sciencedirect.com/science/article/pii/S0920563298001510},
author = {Gerard {'t Hooft}},
abstract = {A model is proposed in which the Hawking particles emitted by a black hole are treated as an envelope of matter that obeys an equation of state, and acts as a source in Einstein's equations. This is a crude but interesting way to accommodate for the back reaction. For large black holes, the solution can be given analytically, if the equation of state is p = κϱ, with 0 < κ < 1. The solution exhibits a singularity at the origin. If we assume N free particle types, we can use a Hartree-Fock procedure to compute the contribution of one such field to the entropy, and the result scales as expected as 1N. A slight mismatch is found that could be attributed to quantum corrections to Einstein's equations, but can also be made to disappear when κ is set equal to one. The case κ = 1 is further analysed.}
}

@incollection{tHooft:2006,
author = "G. {'t Hooft}",
title = "Black holes and the information paradox",
booktitle = "Frontiers of Fundamental Physics", 
year = "2006",
pages = "29--29",
doi = "https://doi.org/10.1007/1-4020-4339-2_4",
editor = " B. G. Sidharth and F. Honsella and A. {De Angelis}",
publisher = "Springer",
abstract = "In electromagnetism, like charges repel, opposite charges attract. A remarkable feature of the gravitational force is that like masses attract. This gives rise to an instability: the more mass you have, the stronger the attractive force, until an inevitable implosion follows, leading to a “black hole”. It is in the black hole where an apparent conflict between Einstein’s General Relativity and the laws of Quantum Mechanics becomes manifest. Most physicists now agree that a black hole should be described by a Schrödinger equation, with a Hermitean Hamiltonian, but this requires a modification of general relativity. Both General Relativity and Quantum mechanics are shaking on their foundations."
}

