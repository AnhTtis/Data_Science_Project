	note = {arXiv:2209.05209 [cond-mat]},
	keywords = {Condensed Matter - Materials Science, Condensed Matter - Strongly Correlated Electrons},
	file = {2209.05209.pdf:/Users/jneilson/Zotero/storage/6432ZN8K/2209.05209.pdf:application/pdf;arXiv Fulltext PDF:/Users/jneilson/Zotero/storage/3FDGMZ8H/Jin et al. - 2022 - Emergent magnetic states and tunable exchange bias.pdf:application/pdf;arXiv.org Snapshot:/Users/jneilson/Zotero/storage/EZU9T3T8/2209.html:text/html},
