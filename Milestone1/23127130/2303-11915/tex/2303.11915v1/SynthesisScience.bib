	number = {17},
	urldate = {2022-09-23},
	journal = {Inorganic Chemistry},
	author = {Shiraishi, Akihiro and Kimura, Shigeru and He, Xinyi and Watanabe, Naoto and Katase, Takayoshi and Ide, Keisuke and Minohara, Makoto and Matsuzaki, Kosuke and Hiramatsu, Hidenori and Kumigashira, Hiroshi and Hosono, Hideo and Kamiya, Toshio},
