@techreport{gabilondo_circumventing_2022,
	type = {preprint},
	title = {Circumventing {Thermodynamics} to {Synthesize} {Highly} {Metastable} {Tin}({II}) {Perovskites}: {Nano} {Eggshells} of {SnHfO3}},
