@article{liu_discovery_2021,
	title = {Discovery of the {Griffiths} phase in the quaternary nitrides {Ge} $_{\textrm{1−}}$ \textit{ $_{\textrm{x}}$ } {Sn} \textit{ $_{\textrm{x}}$ } {NFe} $_{\textrm{3}}$},
