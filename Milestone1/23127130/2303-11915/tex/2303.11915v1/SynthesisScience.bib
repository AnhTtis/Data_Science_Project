	file = {cm300662m_si_001.pdf:/Users/jneilson/Zotero/storage/WV38BSXF/cm300662m_si_001.pdf:application/pdf;Pourpoint et al. - 2012 - New Insights into the Crystal and Electronic Struc.pdf:/Users/jneilson/Zotero/storage/I8GMLI2P/Pourpoint et al. - 2012 - New Insights into the Crystal and Electronic Struc.pdf:application/pdf},
