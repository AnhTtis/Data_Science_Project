	pages = {6321--6327},
	file = {Liu et al. - 2020 - Evolution of Structural Transformation in γ′-Fe s.pdf:/Users/jneilson/Zotero/storage/GJKIUTT4/Liu et al. - 2020 - Evolution of Structural Transformation in γ′-Fe s.pdf:application/pdf},
