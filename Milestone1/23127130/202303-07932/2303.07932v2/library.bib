@article{Heertjes2009b,
abstract = {A nonlinear filter design is proposed to improve nanopositioning servo performances in high-speed (and generally linear) motion systems. The design offers a means to adapt fundamental control design tradeoffs-like disturbance suppression versus noise sensitivity-which are otherwise fixed. Typically performance-limiting oscillations in the feedback system that benefit from extra control are temporarily upscaled and subjected to nonlinear weighting. For sufficiently large amplitudes, this nonlinear filter operation induces extra controller gain. Oscillations that do not benefit from this extra control (typically because they represent noise contributions that should not be amplified) remain unscaled and, as such, do not induce extra controller gain. The combined usage of linear weighting filters with their exact inverses renders this part of the nonlinear filter design strictly performance based. The effective means to improve servo performance is demonstrated on a short-stroke wafer stage of an industrial wafer scanner. Since the nonlinear filter design is largely based on Lyapunov arguments, stability is guaranteed along the different design steps. {\textcopyright} 2009 IEEE.},
author = {Heertjes, Marcel Fran{\c{c}}ois and Schuurbiers, Xander G.P. and Nijmeijer, Henk},
doi = {10.1109/TIE.2009.2012420},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Heertjes, Schuurbiers, Nijmeijer/Heertjes, Schuurbiers, Nijmeijer - 2009 - Performance-improved design of N-PID controlled motion systems with applications to wafer s(2).pdf:pdf},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {Disturbance rejection,Gain,Loop shaping,Low pass filters,Lyapunov stability,Motion control systems,Noise,Noise sensitivity,Nonlinear filters,Nonlinear proportional-integral-differential (N-PI,Oscillators,Servomotors,Tuning},
number = {5},
pages = {1347--1355},
title = {{Performance-improved design of N-PID controlled motion systems with applications to wafer stages}},
volume = {56},
year = {2009}
}
@book{Vromans2018,
abstract = {Conventional iterative learning control (ILC) strategies are capable to achieve high motion control performance by learning complete feedforward control signals for repetitive motion tasks. Basis functions are introduced in ILC that incorporate linear combinations of different functions of state variables. By iteratively learning coefficients of these functions, ILC is capable of achieving high performance despite variations in the motion tasks. In this paper, an effective and generally applicable practical approach for selecting the basis functions using available models is presented. The approach is experimentally validated on a motion stage of a commercial wirebonder that features coupled and non-linear dynamics. By combining the conventional basis functions with ones that are based on the rigid-body equations of motion of the stage, a set of the basis functions is obtained which achieves significant reduction of the measured tracking errors. Practical guidelines for selection of the compact set of the basis functions is given together with results of experimental validation.},
author = {Vromans, R.J.P.M},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Vromans/Vromans - 2018 - Basis Function Iterative Learning Control of Bonding Machines Bridging Data Driven Learning and Physical Modeling (MSc.pdf:pdf},
pages = {18},
title = {{Basis Function Iterative Learning Control of Bonding Machines : Bridging Data Driven Learning and Physical Modeling (MSc Thesis)}},
year = {2018}
}
@article{Boeren2018a,
abstract = {Iterative control enables a significant control performance enhancement by learning feedforward command signals from previous tasks in a batch-to-batch fashion. The aim of this paper is to develop an approach to estimate the parameters of rational feedforward controllers that provide high performance and extrapolation capabilities towards varying tasks. An instrumental variable-based algorithm is developed that leads to unbiased parameter estimates and optimal accuracy in terms of variance. Furthermore, a noncausal implementation of rational feedforward controllers is proposed, aiming to improve performance by means of pre-actuation. Simulation and experimental results are presented to confirm that optimal accuracy is obtained with the proposed approach, and show the advantages of pre-actuation in terms of performance.},
author = {Boeren, Frank and Blanken, Lennart and Bruijnen, Dennis and Oomen, Tom},
doi = {10.1002/asjc.1595},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Boeren et al/Boeren et al. - 2018 - Optimal Estimation of Rational Feedforward Control via Instrumental Variables With Application to a Wafer Stage.pdf:pdf},
issn = {19346093},
journal = {Asian J. of Control},
keywords = {Feedforward control,learning control,mechatronics},
number = {3},
pages = {975--992},
title = {{Optimal Estimation of Rational Feedforward Control via Instrumental Variables: With Application to a Wafer Stage}},
