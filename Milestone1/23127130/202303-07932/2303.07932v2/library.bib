author = {Araki, Mituhiko and Ito, Yoshimichi and Hagiwara, Tomomichi},
doi = {10.1016/0005-1098(95)00162-X},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Araki, Ito, Hagiwara/Araki, Ito, Hagiwara - 1996 - Frequency response of sampled-data systems.pdf:pdf},
issn = {00051098},
journal = {Automatica},
keywords = {Frequency response,Frequency-domain analysis,Intersample behavior,L2-induced norm,Performance of digital control,Sampled-data systems,Sensitivity and complementary sensitivity},
month = {apr},
pages = {483--497},
title = {{Frequency response of sampled-data systems}},
