issn = {00051098},
journal = {Automatica},
keywords = {Basis functions,Instrumental variable,Iterative Learning Control,Varying reference},
pages = {295--302},
title = {{Optimality and flexibility in Iterative Learning Control for varying tasks}},
volume = {67},
year = {2016}
}
@article{Blanchini2010,
abstract = {The paper deals with the stabilizability of linear plants whose parameters vary with time in a compact set. First, necessary and sufficient conditions for the existence of a linear gain-scheduled stabilizing compensator are given. Next, it is shown that, if these conditions are satisfied, any compensator transfer function depending on the plant parameters and internally stabilizing the closed-loop control system when the plant parameters are constant, can be realized in such a way that the closed-loop asymptotic stability is guaranteed under arbitrary parameter variations. To this purpose, it is preliminarily proved that any transfer function that is stable for all constant parameters values admits a realization that is stable under arbitrary parameter variations (linear parameter-varying (LPV) stability). Then, the YoulaKucera parametrization of all stabilizing compensators is exploited; precisely, closed-loop LPV stability can be ensured by taking an LPV stable realization of the YoulaKucera parameter. To find one such realization, a reasonably simple and general algorithm based on Lyapunov equations and Cholesky's factorization is provided. These results can be exploited to apply linear time-invarient design to LPV systems, thus achieving both pointwise optimality (or pole placement) and LPV stability. Some potential applications in adaptive control and online tuning are pointed out. {\textcopyright} 2010 IEEE.},
author = {Blanchini, Franco and Casagrande, Daniele and Miani, Stefano and Viaro, Umberto},
doi = {10.1109/TAC.2010.2044259},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Blanchini et al/Blanchini et al. - 2010 - Stable LPV realization of parametric transfer functions and its application to gain-scheduling control design.pdf:pdf;:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Blanchini et al/Blanchini et al. - 2010 - Stable LPV realization of parametric transfer functions and its application to gain-scheduling control desi(2).pdf:pdf},
issn = {0018-9286},
journal = {IEEE Transactions on Automatic Control},
keywords = {Linear parameter-varying (LPV) systems,Lyapunov functions,YoulaKucera parametrization,stable LPV realization},
month = {oct},
number = {10},
pages = {2271--2281},
publisher = {IEEE},
title = {{Stable LPV realization of parametric transfer functions and its application to gain-scheduling control design}},
url = {http://ieeexplore.ieee.org/document/5422623/},
volume = {55},
year = {2010}
}
