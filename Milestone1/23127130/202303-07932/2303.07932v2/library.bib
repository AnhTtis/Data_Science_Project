file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Bhujwalla, Laurain, Gilson/Bhujwalla, Laurain, Gilson - 2017 - An RKHS Approach to Controlling Smoothness in Nonparametric LPV-IO Identification.pdf:pdf},
issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Derivatives in the RKHS,Kernel Methods,Linear Parameter-Varying Systems,Regularisation,Reproducing Kernel Hilbert Spaces,System Identification},
number = {1},
