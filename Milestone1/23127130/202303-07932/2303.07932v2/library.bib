keywords = {Hilbert space,Instrumental variables,Machine learning,Non-parametric estimation,Nonlinear identification,Reproducing kernel,Support vector machines},
pages = {340--347},
publisher = {Elsevier Ltd},
title = {{An instrumental least squares support vector machine for nonlinear system identification}},
url = {http://dx.doi.org/10.1016/j.automatica.2015.02.017},
volume = {54},
year = {2015}
}
