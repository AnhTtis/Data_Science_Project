file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Shahriari et al/Shahriari et al. - 2016 - Taking the Human Out of the Loop A Review of Bayesian Optimization.pdf:pdf},
issn = {0018-9219},
journal = {Proceedings of the IEEE},
month = {jan},
number = {1},
pages = {148--175},
title = {{Taking the Human Out of the Loop: A Review of Bayesian Optimization}},
url = {https://ieeexplore.ieee.org/document/7352306/},
volume = {104},
year = {2016}
}
@article{Bolder2014a,
abstract = {The increase of paper size and production speed in wide-format inkjet printing systems is limited by significant in-plane deformation of the paper during printing. To increase both the production speed and paper size, the compensation of paper deformation is essential. A potential approach to compensate the paper deformation is actively changing the longitudinal paper position during a lateral pass of the printheads. This paper aims at developing an Iterative Learning Control (ILC) algorithm suited for this compensation strategy. The paper position is measured directly, but in non-real-time using image data obtained with a scanner located at the printheads. The proposed controller is experimentally validated and compared with standard norm-optimal ILC in a reproducible experiment, where a set of benchmark trajectories is used that represents severe paper deformation. The results show that in contrast to standard ILC, the ILC with basis functions achieves good tracking performance for the reference set and is hence a proper candidate algorithm for the compensation strategy.},
author = {Bolder, Joost and Oomen, Tom and Koekebakker, Sjirk and Steinbuch, Maarten},
doi = {10.1016/j.mechatronics.2014.07.003},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Bolder et al/Bolder et al. - 2014 - Using iterative learning control with basis functions to compensate medium deformation in a wide-format inkjet pr.pdf:pdf},
issn = {09574158},
journal = {Mechatronics},
keywords = {Basis functions,ILC,Optimal control,Varying references,Visual servoing},
