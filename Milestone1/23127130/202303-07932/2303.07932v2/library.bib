@techreport{Eijsermans2014,
abstract = {The carriage, with printheads jetting ink onto the medium, performs a point-to-point motion from left-to-right and vice versa, which is called a pass. Between two successive passes, the gantry beam performs a point-to-point motion in longitudinal direction, which is called a step. By repeating this step along the complete length ofthe surface, the entire printing surface is covered. The productivity ofthe Arizona flatbed printer is related to the time required to perform the point-to- point motions ofboth the gantry and the carriage. To improve the productivity ofthe printing system more aggressive motion profiles are introduced. The focus ofthis work is on the point-to-point motion ofthe gantry beam. To achieve high tracking performance feed forward control is of crucial importance. It is common practice to manually tune feed forward controllers. However, this is a time-consuming process. There- fore, an auto-tuning feed forward control strategy is desired. To this end, iterative learning control (ILC) with basis functions is considerd. Both a SISO ILC controller as well as a MIMO ILC controller are designed. The accuracy require- ments consists of± 5 µm error bounds around the end position ofthe left and right end ofthe gantry beam. The experimental results show that the designed MIMO ILC controller satisfies the accuracy requirements, in contrast to the SISO ILC controller which only achieves satisfactory performance at the center ofthe gantry beam. Since the Arizona printing system has different printing modes, extrapolation properties are essential to guarantee satisfactory performance. Therefore, a set ofreferences is considered consisting ofthree different motion profiles. Experimental results show that the designed MIMO ILC controller with basis functions achieves good tracking performance for the entire set ofmotion profiles},
author = {Eijsermans, W},
file = {:C\:/Users/s152009/OneDrive - TU Eindhoven/Documents/Mendeley Desktop/Eijsermans/Eijsermans - 2014 - MIMO ILC with basis functions applied to a flatbed printing system (Internship Report).pdf:pdf},
title = {{MIMO ILC with basis functions applied to a flatbed printing system (Internship Report)}},
year = {2014}
}
@inproceedings{Kariwala2005,
abstract = {In many practical problems, the primary controlled variable Is not available for feedback and Is needed to be controlled Indirectly using secondary measurements. We derive bounds on the H2 and H∞ optimal achievable performance for systems under Indirect control, which have all scalar signals. These bounds are useful for gaining Insights Into the factors posing limitations on the achievable performance. As compared to direct control, the unstable poles can severely limit the control quality for Indirect control. The limiting effect of the unstable poles can be reduced using an additional feedforward controller, when the disturbance Is measurable. {\textcopyright} 2005 IEEE.},
author = {Kariwala, Vinay and Skogestad, Sigurd},
