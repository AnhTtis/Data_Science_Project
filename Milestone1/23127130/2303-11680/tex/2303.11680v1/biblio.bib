  doi =      {https://doi.org/10.1016/j.physletb.2020.135805},
  issn =     {0370-2693},
  keywords = {Tsallis thermostatistics, Barrow entropy, Logarithmically corrected-entropy approach, Equipartition theorem},
  url =      {https://www.sciencedirect.com/science/article/pii/S0370269320306080}
}

