@article{RICOLFI,
title = {Virtual classes and virtual motives of Quot schemes on threefolds},
journal = {Advances in Mathematics},
volume = {369},
pages = {107182},
year = {2020},
issn = {0001-8708},
doi = {https://doi.org/10.1016/j.aim.2020.107182},
url = {https://www.sciencedirect.com/science/article/pii/S0001870820302085},
author = {Andrea T. Ricolfi},
keywords = {Virtual classes, (Motivic) Donaldson–Thomas invariants, Quot schemes},
abstract = {For a simple, rigid vector bundle F on a Calabi–Yau 3-fold Y, we construct a symmetric obstruction theory on the Quot scheme QuotY(F,n), and we solve the associated enumerative theory. We discuss the case of other 3-folds. Exploiting the critical structure on the local model QuotA3(O⊕r,n), we construct a virtual motive (in the sense of Behrend–Bryan–Szendrői) for QuotY(F,n) for an arbitrary vector bundle F on a smooth 3-fold Y. We compute the associated motivic partition function. We obtain new examples of higher rank (motivic) Donaldson–Thomas invariants.}
}