}

@article{pommella_coupling_2019,
	title = {Coupling {Space}-{Resolved} {Dynamic} {Light} {Scattering} and {Rheometry} to {Investigate} {Heterogeneous} {Flow} and {Nonaffine} {Dynamics} in {Glassy} and {Jammed} {Soft} {Matter}},
