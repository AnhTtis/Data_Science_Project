	volume = {712},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/abs/geometric-scaling-of-a-purely-elastic-flow-instability-in-serpentine-channels/47A2DA7132B1197EA6A992E35024ADA8},
