	file = {Peng and Zhu - 2004 - Linear stability of Bingham fluids in spiral Couet.pdf:/Volumes/GoogleDrive/My Drive/Zotero/storage/B5TWZ9JG/Peng and Zhu - 2004 - Linear stability of Bingham fluids in spiral Couet.pdf:application/pdf},
}

@article{morozov_introductory_2007,
	title = {An introductory essay on subcritical instabilities and the transition to turbulence in visco-elastic parallel shear flows},
	volume = {447},
	issn = {03701573},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0370157307001469},
	doi = {10.1016/j.physrep.2007.03.004},
	abstract = {This paper is an pedagogical essay on the scenario of the instabilities and the transition to turbulence in visco-elastic polymer Ô¨Çows. When polymers are long, they get easily stretched by the shear present in Ô¨Çows, and the viscosity of the solution or melt is large. As a result, inertial effects are usually negligible as the Reynolds numbers are small but the Ô¨Çuid is strongly nonNewtonian due to the shear-induced elasticity and anistropy, and the slow relaxation effects. The dimensionless number governing these nonNewtonian effects is the Weissenberg number Wi. From a number of precise experiments and theoretical investigations in the last Ô¨Åfteen years, it has become clear that as the Weissenberg number increases, visco-elastic Ô¨Çuids exhibit Ô¨Çow instabilities driven by the anisotropy of the normal stress components and the curvature of the streamlines. The combination of these normal stress effects that drive laminar curved Ô¨Çow unstable and the possibilty of the elastic effects to store energy in high shear regions and to dump it elsewhere in less sheared regions, appears to be strongly self-enhancing: Instabilities and the transition to a turbulent regime driven by these elastic forces, are often found to be hysteretic and strongly subcritical (nonlinear). There are two main underlying themes of this introductory essay. First of all, that it is proÔ¨Åtable to let one be motivated by transition scenarios in Newtonian Ô¨Çuids as a function of Reynolds number, when investigating possible transition scenarios in visco-elastic Ô¨Çuids as a function of Weissenberg number. Secondly, that the self-enhancing effects of polymer stretching will also cause subcritical instabilities in visco-elastic parallel shear Ô¨Çows. The aim of this paper is to introduce and discuss these issues at a pictorial level which is accessible for a nonexpert. After introducing some of the basic ingredients of polymer rheology we follow a number of the important theoretical and experimental developments of the last Ô¨Åfteen years and discuss the picture that emerges from it. We then turn to a discussion of recent theoretical and numerical approaches aimed at establishing whether visco-elastic parallel shear Ô¨Çows indeed also exhibit a subcritical transition to elastic turbulence. We show how a simple extension of the well-known condition of Pakdel and McKinley for the instability threshold of curved Ô¨Çows, can be extended to the nonlinear (subcritical) instability scenario of parallel visco-elastic shear Ô¨Çows. This extension predicts the critical amplitude for the nonlinear instability to decrease as 1/Wi2 and to be independent of the wavenumber k of the perturbations. The fact that the threshold is k-independent over a large range of k‚Äôs suggest that many modes will be excited at the same time, and hence that the instability will generally drive the Ô¨Çow turbulent. Implications of these results and an outlook for the future are discussed as well.},
	language = {en},
	number = {3-6},
	urldate = {2021-04-26},
	journal = {Physics Reports},
	author = {Morozov, Alexander N. and van Saarloos, Wim},
	month = aug,
	year = {2007},
	pages = {112--143},
	file = {Morozov and van Saarloos - 2007 - An introductory essay on subcritical instabilities.pdf:/Volumes/GoogleDrive/My Drive/Zotero/storage/XVQNWHI8/Morozov and van Saarloos - 2007 - An introductory essay on subcritical instabilities.pdf:application/pdf},
}

