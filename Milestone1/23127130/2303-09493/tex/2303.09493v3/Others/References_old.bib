	urldate = {2020-12-09},
	journal = {Journal of Rheology},
	author = {Tassieri, Manlio and Laurati, Marco and Curtis, Dan J. and Auhl, Dietmar W. and Coppola, Salvatore and Scalfati, Andrea and Hawkins, Karl and Williams, Phylip Rhodri and Cooper, Jonathan M.},
