@article{Cui22,
title = {A \cal{G}aussian mixture regression model based adaptive filter for non-\cal{G}aussian noise without a priori statistic},
journal = {Signal Processing},
volume = {190},
pages = {108314},
year = {2022},
issn = {0165-1684},
doi = {10.1016/j.sigpro.2021.108314},
author = {Haoran Cui and Xiaoxu Wang and Shuaihe Gao and Tiancheng Li},
keywords = {Nonlinear adaptive filter, Unknown and non-\cal{G}aussian noises, Variational \cal{B}ayesian, \cal{G}aussian mixture model},
abstract = {In many engineering systems, the distribution of measurement noise is unknown and non-\cal{G}aussian, such as skewed, multimodal, time-varying distributions and we can not obtain these prior statistics in advance. How to achieve state estimation via this kind of non-\cal{G}aussian measurement noises without prior statistic information is a challenging problem. In this paper, a novel \cal{G}aussian mixture regression model (\cal{GM}RM) is proposed to model the unknown non-\cal{G}aussian measurement likelihood for \cal{B}ayesian update to achieve nonlinear state estimation. Without any prior assumption or limitation of measurement noises¡¯ statistics and distributions, the \cal{GM}RM can still describe the measurement likelihood accurately based on a group of parameters which are adjusted by maximizing the evidence lower bound. Based on the optimized \cal{GM}RM, a new variational \cal{B}ayesian \cal{G}aussian mixture filter is proposed by using the variational \cal{B}ayesian approach. To eliminate the influence of the initialization of the introduced parameters, a learning scheme is proposed to adaptively optimize their hyper-parameters based on the historical measurements. Finally, simulation examples are employed to illustrate the effectiveness of the filter.}
}

