keywords = {Turning points, \cal{M}arkov-switching, Forecast combination, \cal{B}ayesian model averaging},
abstract = {We propose new forecast combination schemes for predicting turning points of business cycles. The proposed combination schemes are based on the forecasting performances of a given set of models with the aim to provide better turning point predictions. In particular, we consider predictions generated by autoregressive (AR) and \cal{M}arkov-switching AR models, which are commonly used for business cycle analysis. In order to account for parameter uncertainty we consider a \cal{B}ayesian approach for both estimation and prediction and compare, in terms of statistical accuracy, the individual models and the combined turning point predictions for the United States and the Euro area business cycles.}
}

@article{Kim94,
