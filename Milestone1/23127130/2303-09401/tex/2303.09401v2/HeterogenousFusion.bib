@INPROCEEDINGS{Chen10,
author = {Chen, H. D. and Chang, K. C. and Smith, Chris},
title = {Constraint optimized weight adaptation for \cal{G}aussian mixture reduction},
booktitle = {Proc. SPIE},
volume = {7697},
number = {},
pages = {76970N-76970N-10},
abstract = {\cal{G}aussian mixture model (\cal{\cal{GM}M}) has been used in many applications for dynamic state estimation such as target tracking or distributed fusion. However, the number of components in the mixture distribution tends to grow rapidly when multiple \cal{\cal{GM}M}s are combined. In order to keep the computational complexity bounded, it is necessary to approximate a \cal{G}aussian mixture by one with reduced number of components. \cal{G}aussian mixture reduction is traditionally conducted by recursively selecting two components that appear to be most similar to each other and merging them. Different definitions on similarity measure have been used in literature. For the case of one-dimensional \cal{G}aussian mixtures, Kmeans algorithms and some variations are recently proposed to cluster \cal{G}aussian mixture components in groups, use a center component to represent all in each group, readjust parameters in the center components, and finally perform weight optimization. In this paper, we focus on multi-dimensional \cal{G}aussian mixture models. With a variety of reduction algorithms and possible combinations, we developed a hybrid algorithm with constraint optimized weight adaptation to minimize the integrated squared error (ISE). In additions, with extensive simulations, we showed that the proposed algorithm provides an efficient and effective \cal{G}aussian mixture reduction performance in various random scenarios.},
year = {2010},
doi = {10.1117/12.851993},
weburl = { 10.1117/12.851993},
month = {Oct},
}


