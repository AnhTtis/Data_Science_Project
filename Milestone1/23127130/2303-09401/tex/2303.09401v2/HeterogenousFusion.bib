@article{Biernack03,
title = {Choosing starting values for the \cal{EM} algorithm for getting the highest likelihood in multivariate \cal{G}aussian mixture models},
journal = {Computational Statistics \& Data Analysis},
volume = {41},
number = {3},
pages = {561-575},
year = {2003},
issn = {0167-9473},
doi = {10.1016/S0167-9473(02)00163-9},
author = {Christophe Biernacki and Gilles Celeux and GÃ©rard Govaert},
keywords = {Classification EM, \cal{EM} algorithm, Initialization strategies, Multivariate \cal{G}aussian mixture, Optimization, Stochastic EM},
abstract = {Simple methods to choose sensible starting values for the \cal{EM} algorithm to get maximum likelihood parameter estimation in mixture models are compared. They are based on random initialization, using a classification \cal{EM} algorithm (CEM), a Stochastic \cal{EM} algorithm (SEM) or previous short runs of \cal{EM} itself. Those initializations are included in a search/run/select strategy which can be compounded by repeating the three steps. They are compared in the context of multivariate \cal{G}aussian mixtures on the basis of numerical experiments on both simulated and real data sets in a target number of iterations. The main conclusions of those numerical experiments are the following. The simple random initialization which is probably the most employed way of initiating \cal{EM} is often outperformed by strategies using CEM, SEM or shorts runs of \cal{EM} before running EM. Also, it appears that compounding is generally profitable since using a single run of \cal{EM} can often lead to suboptimal solutions. Otherwise, none of the experimental strategies can be regarded as the best one and it is difficult to characterize situations where a particular strategy can be expected to outperform the other ones. However, the strategy initiating \cal{EM} with short runs of \cal{EM} can be recommended. This strategy, which as far as we know was not used before the present study, has some advantages. It is simple, performs well in a lot of situations presupposing no particular form of the mixture to be fitted to the data and seems little sensitive to noisy data.}
}

