@ARTICLE{Crooke2010,
  author = {Crooke, Philip S. and Parl, Fritz F.},
  title = {A mathematical model for DNA damage and repair.},
  journal = {J Nucleic Acids},
  year = {2010},
  volume = {2010},
  abstract = {In cells, DNA repair has to keep up with DNA damage to maintain the
	integrity of the genome and prevent mutagenesis and carcinogenesis.
	While the importance of both DNA damage and repair is clear, the
	impact of imbalances between both processes has not been studied.
	In this paper, we created a combined mathematical model for the formation
	of DNA adducts from oxidative estrogen metabolism followed by base
	excision repair (BER) of these adducts. The model encompasses a set
	of differential equations representing the sequence of enzymatic
	reactions in both damage and repair pathways. By combining both pathways,
	we can simulate the overall process by starting from a given time-dependent
	concentration of 17beta-estradiol (E(2)) and 2'-deoxyguanosine, determine
	the extent of adduct formation and the correction by BER required
	to preserve the integrity of DNA. The model allows us to examine
	the effect of phenotypic and genotypic factors such as different
	concentrations of estrogen and variant enzyme haplotypes on the formation
	and repair of DNA adducts.},
  doi = {10.4061/2010/352603},
  institution = {Department of Mathematics, Vanderbilt University, Nashville, TN 37240,
	USA.},
