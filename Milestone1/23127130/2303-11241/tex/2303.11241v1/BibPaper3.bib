@article{henry_2006,
	Abstract = {{Astrometric measurements for 25 red dwarf systems are presented,
        including the first definitive trigonometric parallaxes for 20
        systems within 10 pc of the Sun, the horizon of the RECONS
        sample. The three nearest systems that had no previous
