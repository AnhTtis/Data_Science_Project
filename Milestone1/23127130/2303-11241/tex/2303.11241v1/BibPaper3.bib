	Journal = {Communications in Asteroseismology},
	Keywords = {Astrophysics},
	Month = jul,
	Pages = {61},
	Primaryclass = {astro-ph},
	Title = {{On the origin of microturbulence in hot stars}},
	Volume = {158},
	Year = 2009}

@article{struve_1934,
	Abstract = {{The intensities of stellar absorption lines have been measured in the
        spectra of a Canis Majoris, a Lyrae, a Cygni, a Persei, Aurigae
        and i \raisebox{-0.5ex}\textasciitilde Leporis. With the help of
        theoretical multiplet intensities, curves have been constructed,
        for each star separately, showing the relation between total
        absorption and number of atoms. These curves differ
        considerably. All parts of the curves lie between the limits A =
        Const. and A o N. An application of the theory of Sch{\"u}tz
        based upon the simultaneous effects of radia- tion damping and
        thermal Doppler broadening shows agreement with the observations
        in a Canis Majoris, a Lyrae, and a Cygni, provided, however, the
        damping constant is assumed to be ten times that given by the
        classical theory. For the remaining three stars the hypothesis
        is advanced that in their atmospheres turbulent motions far
        exceed the motions caused by thermal agitation. The theory of
        Sch{\"u}tz is enlarged to include the case of turbulent motions,
        and excellent agreement with the observations is found. The most
        probable turbulent velocity is found to be 67 km/sec. in
        i\raisebox{-0.5ex}\textasciitilde Leporis, 20 km/sec. in
        Aurigae, and 7 km/sec. in a Persei. The interpretation of many
        other observational data in the light of the turbulence
        hypothesis is briefly discussed}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/1934ApJ....79..409S},
	Author = {{Struve}, Otto and {Elvey}, C.~T.},
	Date-Added = {2022-09-23 13:45:29 +0200},
	Date-Modified = {2022-09-23 13:45:33 +0200},
	Doi = {10.1086/143551},
	Journal = {\apj},
	Month = may,
	Pages = {409},
	Title = {{The Intensities of Stellar Absorption Lines}},
	Volume = {79},
	Year = 1934,
	Bdsk-Url-1 = {https://doi.org/10.1086/143551}}

@article{edmunds_1978,
	Abstract = {{It is suggested that the microturbulent broadening of spectral lines in
        late-type stellar atmospheres is caused by the propagation of
        acoustic waves, generated in the convection zones. It is shown
        that the wavelength of the waves obeys a condition required for
        the waves to affect the equivalent widths of strong lines. The
        variation of the predicted line broadening with temperature and
        surface gravity is in good agreement with published
        observations.}},
