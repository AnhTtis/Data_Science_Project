	Doi = {10.3847/1538-4357/ab39e5},
	Eid = {34},
	Eprint = {1906.05297},
	Journal = {\apj},
	Keywords = {Astronomy data analysis, Milky Way disk, Milky Way evolution, Milky Way formation, Stellar abundances, Galaxy chemical evolution, 1858, 1050, 1052, 1053, 1577, 580, Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
	Month = sep,
