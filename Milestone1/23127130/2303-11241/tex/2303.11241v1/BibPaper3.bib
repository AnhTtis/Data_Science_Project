	Archiveprefix = {arXiv},
	Author = {{Martioli}, E. and {H{\'e}brard}, G. and {Moutou}, C. and {Donati}, J. -F. and {Artigau}, {\'E}. and {Cale}, B. and {Cook}, N.~J. and {Dalal}, S. and {Delfosse}, X. and {Forveille}, T. and {Gaidos}, E. and {Plavchan}, P. and {Berberian}, J. and {Carmona}, A. and {Cloutier}, R. and {Doyon}, R. and {Fouqu{\'e}}, P. and {Klein}, B. and {Lecavelier des Etangs}, A. and {Manset}, N. and {Morin}, J. and {Tanner}, A. and {Teske}, J. and {Wang}, S.},
	Date-Added = {2022-09-29 10:51:34 +0200},
