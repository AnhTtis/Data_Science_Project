	Doi = {10.3847/1538-4357/ab9e67},
	Eid = {109},
	Eprint = {2006.10139},
	Journal = {\apj},
	Keywords = {Near infrared astronomy, Star formation, Pre-main sequence stars, Stellar magnetic fields, High resolution spectroscopy, Spectroscopy, Radiative transfer, Starspots, Protostars, 1093, 1569, 1290, 1610, 2096, 1558, 1335, 1572, 1302, Astrophysics - Solar and Stellar Astrophysics},
	Month = aug,
	Number = {2},
