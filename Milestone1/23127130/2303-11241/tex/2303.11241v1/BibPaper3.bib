	Keywords = {Astrophysics - Solar and Stellar Astrophysics},
	Month = nov,
	Pages = {arXiv:2111.03957},
	Primaryclass = {astro-ph.SR},
	Title = {{The Effects of Starspots on Spectroscopic Mass Estimates of Low-mass Young Stars}},
	Year = 2021,
