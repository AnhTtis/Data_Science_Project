	Keywords = {radiative transfer, atmospheric effects, instrumentation: spectrographs, methods: observational, methods: data analysis, techniques: spectroscopic, Astrophysics - Instrumentation and Methods for Astrophysics},
	Month = apr,
	Pages = {A77},
	Primaryclass = {astro-ph.IM},
	Title = {{Molecfit: A general tool for telluric absorption correction. I. Method and application to ESO instruments}},
	Volume = {576},
