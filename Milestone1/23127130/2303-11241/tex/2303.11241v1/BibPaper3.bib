	Bdsk-Url-1 = {https://doi.org/10.1093/mnras/stz1270}}

@inproceedings{kulenthirajah_2017,
	Abstract = {{We propose to create a high resolution spectral library in the optical
        and infrared range with PHOENIX model atmospheres code that can
        compute molecular lines in order to estimate stellar parameters.
        The chosen grid of stellar parameters will be as follows:
        {\ensuremath{\delta}} T\_\{eff\} =25K, {\ensuremath{\delta}} log
        g = 0.05, {\ensuremath{\delta}} [M/H] =0.05, ranging between:
        T\_\{eff\}=[2500K,4000K], log g=[4.0,-5.5], [M/H]= [-1,-1] but
        first, we need to calibrate on F to K stars. We present here the
        preliminary tests and calibration on the Sun and 18sco. The
        method yield respectively T\_\{eff\}=5770 {\ensuremath{\pm}} 3K,
        log g=4.458 {\ensuremath{\pm}} 0.006 and T\_\{eff\}=5763
        {\ensuremath{\pm}} 3.5K, log g=4.451 {\ensuremath{\pm}} 0.007}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
