	Archiveprefix = {arXiv},
	Author = {{Donati}, J. -F. and {Howarth}, I.~D. and {Jardine}, M.~M. and {Petit}, P. and {Catala}, C. and {Landstreet}, J.~D. and {Bouret}, J. -C. and {Alecian}, E. and {Barnes}, J.~R. and {Forveille}, T. and {Paletou}, F. and {Manset}, N.},
	Date-Added = {2022-09-22 16:23:24 +0200},
	Date-Modified = {2022-09-22 16:23:31 +0200},
	Doi = {10.1111/j.1365-2966.2006.10558.x},
	Eprint = {astro-ph/0606156},
	Journal = {\mnras},
	Keywords = {techniques: polarimetric: stars: early-type: stars: individual: {\ensuremath{\tau}} Sco: stars: magnetic fields: stars: rotation: stars: winds, outflows, techniques: polarimetric, stars: early-type, stars: individual: {\ensuremath{\tau}} Sco, stars: magnetic fields, stars: rotation, stars: winds, Astrophysics},
	Month = aug,
