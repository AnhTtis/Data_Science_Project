@article{stift_2003,
	Abstract = {{We present an in-depth investigation of the increase in equivalent width
        of saturated lines under the influence of strong magnetic
        fields, aptly called ``magnetic intensification''. Assuming 20
        different Zeeman patterns (with the number of components ranging
        from 3 to 45) for a fictitious Fe II line at 4500 {\r{A}} in a
        10 000 K, log g = 4.0 Kurucz atmosphere, we calculate equivalent
        widths as a function of magnetic field strength, field angle and
        line strength (abundance). The increase over the zero field
        equivalent width is found to always fall below the n/2 relation
        suggested by Babcock (\textbackslashcite\{bab49\}), where n is
        the number of Zeeman components. After a discussion of the
        behaviour of various Zeeman patterns we turn to the influence of
        anomalous dispersion on magnetic intensification. Changes in
        line blanketing due to magnetic desaturation appear to be of
        negligible size. Finally we show that magnetic intensification
        leads to apparent enhanced abundances and to virtual abundance
        variations in magnetic stars with dipolar surface magnetic field
        configurations.}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/2003A&A...398..411S},
	Author = {{Stift}, M.~J. and {Leone}, F.},
	Date-Added = {2022-09-23 08:56:22 +0200},
	Date-Modified = {2022-09-23 08:56:29 +0200},
	Doi = {10.1051/0004-6361:20021605},
	Journal = {\aap},
	Keywords = {stars: abundances, stars: chemically peculiar, stars: magnetic fields, polarization},
	Month = feb,
	Pages = {411-421},
	Title = {{Magnetic intensification of spectral lines}},
	Volume = {398},
	Year = 2003,
	Bdsk-Url-1 = {https://doi.org/10.1051/0004-6361:20021605}}

@article{landi_1981b,
	Abstract = {{Magnetic intensification is proposed as a mechanism to explain the
        general features of the variable broad-band linear polarization
        emerging from rotating magnetic stars. This mechanism is studied
        in detail, and some efforts are made to investigate the wide
