	Adsurl = {https://ui.adsabs.harvard.edu/abs/2013A&A...556A..15R},
	Archiveprefix = {arXiv},
	Author = {{Rajpurohit}, A.~S. and {Reyl{\'e}}, C. and {Allard}, F. and {Homeier}, D. and {Schultheis}, M. and {Bessell}, M.~S. and {Robin}, A.~C.},
	Date-Added = {2019-10-14 16:10:26 +0200},
