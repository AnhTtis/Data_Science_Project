	Doi = {10.1088/0004-637X/767/1/28},
	Eid = {28},
	Eprint = {1302.6231},
	Journal = {\apj},
	Keywords = {brown dwarfs, stars: fundamental parameters, stars: low-mass, Astrophysics - Solar and Stellar Astrophysics},
