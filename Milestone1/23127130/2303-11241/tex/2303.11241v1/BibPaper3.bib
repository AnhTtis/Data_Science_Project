@article{kulenthirarajah_2019,
	Abstract = {{The goal of the present study is to construct, test, and validate a
        high-resolution synthetic spectral library using PHOENIX model
        atmospheres and develop a reliable tool to estimate stellar
        parameters from high-resolution optical and/or near-infrared
        spectra of M dwarfs. We report here the preliminary results of
