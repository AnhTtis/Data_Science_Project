	Doi = {10.1086/321547},
	Eprint = {astro-ph/0104256},
	Journal = {\apj},
	Keywords = {Stars: Atmospheres, Stars: Fundamental Parameters, Stars: Low-Mass, Brown Dwarfs, Astrophysics},
	Month = {Jul},
	Number = {1},
	Pages = {357-372},
	Primaryclass = {astro-ph},
	Title = {{The Limiting Effects of Dust in Brown Dwarf Model Atmospheres}},
