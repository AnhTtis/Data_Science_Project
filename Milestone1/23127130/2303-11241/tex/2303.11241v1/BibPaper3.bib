	Archiveprefix = {arXiv},
	Author = {{Boyajian}, Tabetha S. and {von Braun}, Kaspar and {van Belle}, Gerard and {McAlister}, Harold A. and {ten Brummelaar}, Theo A. and {Kane}, Stephen R. and {Muirhead}, Philip S. and {Jones}, Jeremy and {White}, Russel and {Schaefer}, Gail and {Ciardi}, David and {Henry}, Todd and {L{\'o}pez-Morales}, Mercedes and {Ridgway}, Stephen and {Gies}, Douglas and {Jao}, Wei-Chun and {Rojas-Ayala}, B{\'a}rbara and {Parks}, J. Robert and {Sturmann}, Laszlo and {Sturmann}, Judit and {Turner}, Nils H. and {Farrington}, Chris and {Goldfinger}, P.~J. and {Berger}, David H.},
	Date-Added = {2021-04-21 11:55:54 +0200},
	Date-Modified = {2021-04-21 11:57:05 +0200},
	Doi = {10.1088/0004-637X/757/2/112},
	Eid = {112},
	Eprint = {1208.2431},
	Journal = {\apj},
	Keywords = {Hertzsprung-Russell and C-M diagrams, infrared: stars, planetary systems, stars: atmospheres, stars: fundamental parameters, stars: general, stars: late-type, stars: low-mass, techniques: high angular resolution, techniques: interferometric, Astrophysics - Solar and Stellar Astrophysics},
	Month = oct,
	Number = {2},
	Pages = {112},
	Primaryclass = {astro-ph.SR},
	Title = {{Stellar Diameters and Temperatures. II. Main-sequence K- and M-stars}},
