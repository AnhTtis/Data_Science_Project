	Date-Added = {2021-02-15 15:22:52 +0100},
	Date-Modified = {2021-02-15 15:23:00 +0100},
	Doi = {10.1051/0004-6361/201322261},
	Eid = {A19},
	Eprint = {1601.01877},
	Journal = {\aap},
	Keywords = {stars: low-mass, line: formation, stars: atmospheres, brown dwarfs, line: profiles, techniques: spectroscopic, Astrophysics - Solar and Stellar Astrophysics},
	Month = mar,
	Pages = {A19},
