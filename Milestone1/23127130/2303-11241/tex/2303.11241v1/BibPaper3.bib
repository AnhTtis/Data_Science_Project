@article{sarmento_2021,
	Abstract = {{The scientific community's interest on the stellar parameters of M
        dwarfs has been increasing over the last few years, with
        potential applications ranging from galactic characterization to
        exoplanet detection. The main motivation for this work is to
        develop an alternative and objective method to derive stellar
        parameters for M dwarfs using the H-band spectra provided by the
        Apache Point Observatory Galactic Evolution Experiment (APOGEE).
        Synthetic spectra generated with \textbackslashtextit\{iSpec\},
        \textbackslashtextit\{Turbospectrum\},
        \textbackslashtextit\{MARCS\} models atmospheres and a custom
        made line list including over 1 000 000 water lines, are
        compared to APOGEE observations, and parameters are determined
        through $\chi^2$ minimization. Spectroscopic parameters
        ($T_\mathrm{eff}$, $[M/H]$, $\log g$, $v_{mic}$) are presented
        for a sample of 313 M dwarfs, obtained from their APOGEE H-band
        spectra. The generated synthetic spectra reproduce observed
        spectra to a high accuracy level. The impact of the spectra
        normalization on the results are analyzed as well. Our output
        parameters are compared with the ones obtained with APOGEE
        Stellar Parameter and Chemical Abundances Pipeline (ASPCAP) for
        the same stellar spectrum, and we find that the values agree
        within the expected uncertainties. Comparisons with other
        previous near-infrared and optical literature are also
        available, with median differences within our estimated
        uncertainties found in most cases. Possible reasons for these
        differences are explored. The full H-band line list, the line
        selection for the synthesis, and the synthesized spectra are
        available for download, as are the calculated stellar
        parameters.}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/2021arXiv210304848S},
	Archiveprefix = {arXiv},
	Author = {{Sarmento}, Pedro and {Rojas-Ayala}, B{\'a}rbara and {Delgado Mena}, Elisa and {Blanco-Cuaresma}, Sergi},
	Date-Added = {2021-05-06 14:03:16 +0200},
	Date-Modified = {2021-05-06 14:03:26 +0200},
	Eid = {arXiv:2103.04848},
	Eprint = {2103.04848},
	Journal = {arXiv e-prints},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics},
	Month = mar,
	Pages = {arXiv:2103.04848},
	Primaryclass = {astro-ph.SR},
	Title = {{Determination of spectroscopic parameters for 313 M dwarf stars from their APOGEE Data Release 16 H-band spectra}},
	Year = 2021,
	Bdsk-File-1 = {}}

@article{kurucz_1970,
	Abstract = {{The computer program ATLAS calculates model stellar atmospheres in
        radiative and convective equilibrium for the complete range of
        stellar temperatures. The apporximations used limit the program
        to plane-parallel, horizontally homogeneous, stedy-state,
        nonmoving atmospheres with energy and abundances constant with
        depth. The program has been written to allow detailed
        statistical equillibrium calculations, but only hydrogen
        continua and H$^{-}$ are coded at present. Most of the published
        continuous opacities and hydrogen lines have been included, and
        provision is made for adding others easily. There is also
        provision for treating line opacity as line absorption
        distribution functions. In Sections 2 through 7, we discuss all
        aspects of model atmosphere calculations in considerable detail.
        These include the radiation field, statistical equilibrium,
        thermodynamic properties, opacity, convection, and the
        temperature correction. In Sections 8 and 9, we discuss the
        computer program itself, by going first through a sample
        calculation and second through a technical discussion of the
        program coding and operation. The program was written in FORTRAN
        IV and is essentially machine independent. A listing is
        available on magnetic tape.}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/1970SAOSR.309.....K},
	Author = {{Kurucz}, R.~L.},
	Date-Added = {2021-05-06 11:16:09 +0200},
	Date-Modified = {2021-05-06 11:16:17 +0200},
	Journal = {SAO Special Report},
	Month = feb,
	Title = {{Atlas: a Computer Program for Calculating Model Stellar Atmospheres}},
	Volume = {309},
	Year = 1970,
	Bdsk-File-1 = {}}

@article{hauschildt_1999,
	Abstract = {{We present our NextGen Model Atmosphere grid for low-mass stars for
        effective temperatures larger than 3000 K. These LTE models are
        calculated with the same basic model assumptions and input
        physics as the VLMS part of the NextGen grid so that the
        complete grid can be used, e.g., for consistent stellar
        evolution calculations and for internally consistent analysis of
        cool star spectra. This grid is also the starting point for a
        large grid of detailed NLTE model atmospheres for dwarfs and
        giants. The models were calculated from 3000 to 10,000 K (in
        steps of 200 K) for 3.5<=logg<=5.5 (in steps of 0.5) and
        metallicities of -4.0<=[M/H]<=0.0.
        \textbackslash\textbackslashWe discuss the results of the model
        calculations and compare our results to the Kurucz grid. Some
        comparisons to standard stars like Vega and the Sun are
        presented and compared with detailed NLTE calculations.}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/1999ApJ...512..377H},
