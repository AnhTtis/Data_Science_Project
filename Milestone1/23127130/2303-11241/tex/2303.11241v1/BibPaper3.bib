	Date-Modified = {2022-09-01 15:49:05 +0200},
	Doi = {10.1051/0004-6361/202037722},
	Eid = {A42},
	Eprint = {2006.13098},
	Journal = {\aap},
	Keywords = {techniques: spectroscopic, methods: numerical, methods: data analysis, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics},
	Month = aug,
