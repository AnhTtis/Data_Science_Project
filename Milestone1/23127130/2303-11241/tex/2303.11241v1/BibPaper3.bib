	Keywords = {PHYSICAL DATA AND PROCESSES: MOLECULAR DATA, STARS: ATMOSPHERES, STARS: AGB AND POST-AGB, STARS: VARIABLES: MIRAS, Astrophysics},
	Month = feb,
	Pages = {1109-1119},
	Primaryclass = {astro-ph},
	Title = {{Near-infrared narrow-band photometry of M-giant and Mira stars: models meet observations}},
