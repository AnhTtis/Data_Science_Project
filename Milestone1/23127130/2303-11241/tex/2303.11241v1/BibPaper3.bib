	Journal = {Living Reviews in Solar Physics},
	Keywords = {Stellar structures, Stellar evolution, Solar evolution, Solar interior, Astrophysics - Solar and Stellar Astrophysics},
	Month = dec,
	Number = {1},
	Pages = {2},
	Primaryclass = {astro-ph.SR},
	Title = {{Solar structure and evolution}},
