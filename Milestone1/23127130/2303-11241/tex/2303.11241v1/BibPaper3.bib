	Date-Added = {2022-05-04 16:11:33 +0200},
	Date-Modified = {2022-05-04 16:11:33 +0200},
	Doi = {10.1093/mnras/sty374},
	Eprint = {1802.04288},
	Journal = {\mnras},
	Keywords = {stars: activity, stars: evolution, stars: low-mass, stars: pre-main-sequence, open clusters and associations: general, Astrophysics - Solar and Stellar Astrophysics},
	Month = may,
	Number = {3},
	Pages = {3245-3262},
	Primaryclass = {astro-ph.SR},
	Title = {{The inflated radii of M dwarfs in the Pleiades}},
	Volume = {476},
	Year = 2018,
	Bdsk-Url-1 = {https://doi.org/10.1093/mnras/sty374}}

@article{barklem_2000,
	Abstract = {{A list of data for the broadening by neutral hydrogen collisions of many
        astrophysically important spectral lines, which has been
        incorporated into the Vienna Atomic Line Database (VALD), is
        presented. Data for lines of neutral atoms are interpolated from
        the tabulated data of Anstee \& O'Mara
        (\textbackslashcite\{ao:sp\}), Barklem \& O'Mara
        (\textbackslashcite\{bo:pd\}), and Barklem et al.
        (\textbackslashcite\{bor:df\}). Data for lines of singly ionised
        atoms are compiled from the calculations by Barklem \& O'Mara
        (\textbackslashcite\{bo:ion1,bo:ion2\}). The list at present
        contains data for 4891 lines between 2300 and 13000 {\"A} of
        elements from Li to Ni. We examine the statistical impact of the
        new theory by comparison with the previously available data. We
        also demonstrate the direct effect on spectral synthesis
        calculations. Table\raisebox{-0.5ex}\textasciitilde1 is only
        available in electronic form at the CDS via anonymous ftp to
        cdsarc.u-strasbg.fr (130.79.128.5) or via anonymous ftp to
        ftp.astro.uu.se/pub/Spectra/barklem/hlist/}},
