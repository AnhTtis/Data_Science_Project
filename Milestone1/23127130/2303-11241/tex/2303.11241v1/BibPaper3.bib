	Adsurl = {https://ui.adsabs.harvard.edu/abs/2019MNRAS.487.1335K},
	Author = {{Kulenthirarajah}, Logithan and {Donati}, Jean-Fran{\c{c}}ois and {Hussain}, Gaitee and {Morin}, Julien and {Allard}, France},
	Date-Added = {2019-10-17 14:23:24 +0200},
