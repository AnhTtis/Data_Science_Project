	Editor = {{Petit}, Pascal and {Jardine}, Moira and {Spruit}, Hendrik C.},
	Eprint = {1309.7668},
	Keywords = {binaries: eclipsing, stars: evolution, stars: interiors, stars: low-mass, stars: magnetic field, Astrophysics - Solar and Stellar Astrophysics},
	Month = aug,
	Pages = {150-153},
	Primaryclass = {astro-ph.SR},
