	Keywords = {stars: activity, magnetic fields, Spectral Type, Zeeman Splitting, Magnetic Field Measurement, Stellar Surface, Rossby Number, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics},
	Month = feb,
