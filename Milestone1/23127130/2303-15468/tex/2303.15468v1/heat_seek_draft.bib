	pages = {127--136},
}

@inproceedings{hansen_civic_2014,
	address = {New York, NY, USA},
	series = {{CSCW} '14},
	title = {Civic action brokering platforms: facilitating local engagement with {ACTion} {Alexandria}},
	isbn = {978-1-4503-2540-0},
