	author = {Daly, Elizabeth and Erete, Sheena and Farzan, Rosta and Hsieh, Gary and Lampe, Cliff and Lopez, Claudia and Monroy-Hernandez, Andres and Quercia, Daniele and Schwartz, Raz and Voida, Amy},
