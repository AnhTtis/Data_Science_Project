	doi = {10.1093/oxfordhb/9780190851187.013.42},
	pages = {373--392},
}

@article{rosenberger_variational_2020,
	title = {On variational cross-examination: a method for postphenomenological multistability},
	issn = {0951-5666, 1435-5655},
	shorttitle = {On variational cross-examination},
	url = {https://link.springer.com/10.1007/s00146-020-01050-7},
	doi = {10.1007/s00146-020-01050-7},
	abstract = {How should we understand postphenomenological methodology? Postphenomenology is a research perspective which builds on phenomenological and pragmatist philosophy to explore human–technology relations, but one with open methodological questions. Here, I offer some thoughts on the epistemological processes that should be (and often implicitly may be) at work in this research. In particular, I am concerned with postphenomenological research on technological “multistability,” i.e., a device’s ever-present capacity to be used for a variety of purposes, and to always be meaningful in multiple ways. I develop a methodology called “variational cross-examination,” which entails the critical contrast of a device’s various stabilities. As a set of instructive examples, I draw on my own line of research on the politics of public spaces, and especially the critique of anti-homeless design.},
	language = {en},
	urldate = {2022-12-16},
	journal = {AI \& Society},
	author = {Rosenberger, Robert},
