@article{StaffeldQuinn_1989,
	abstract = {We have developed a new experimental technique to observe particle movement driven by diffusiophoresis. This method-the stopped-flow diffusion cell (SFDC)-allows two particle suspensions to be brought into intimate contact while providing an initially steep, well-defined solute gradient at the junction between the suspensions. With this technique the manifestation of diffusiophoresis is the spontaneous formation of a concentrated band of particles, a band that moves in response to the transient decay of the initial step-function solute gradient. We have used the SFDC to measure electrolyte diffusiophoresis of 0.1- to 1-$\mu$m diameter charged latex particles in NaCl and KCl gradients. These measurements substantiate Anderson and Prieve's theory of diffusiophoresis. The spontaneous formation of colloidal particle bands has implications for separation processes as well as naturally occurring segregation phenomena, including biological colloids.},
	author = {P. O. Staffeld and J. A. Quinn},
	doi = {https://doi.org/10.1016/0021-9797(89)90079-9},
	issn = {0021-9797},
	journal = {J. Colloid Interface Sci.},
	number = {1},
	pages = {69-87},
	title = {Diffusion-induced banding of colloid particles via diffusiophoresis: 1. {E}lectrolytes},
	url = {https://www.sciencedirect.com/science/article/pii/0021979789900799},
	volume = {130},
	year = {1989},
