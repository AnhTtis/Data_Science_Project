@article{VelegolEtAl_2016,
	abstract = {Fluid transport that is driven by gradients of pressure{,} gravity{,} or electro-magnetic potential is well-known and studied in many fields. A subtler type of transport{,} called diffusiophoresis{,} occurs in a gradient of chemical concentration{,} either electrolyte or non-electrolyte. Diffusiophoresis works by driving a slip velocity at the fluid{\^a}solid interface. Although the mechanism is well-known{,} the diffusiophoresis mechanism is often considered to be an esoteric laboratory phenomenon. However{,} in this article we show that concentration gradients can develop in a surprisingly wide variety of physical phenomena {\^a} imposed gradients{,} asymmetric reactions{,} dissolution{,} crystallization{,} evaporation{,} mixing{,} sedimentation{,} and others {\^a} so that diffusiophoresis is in fact a very common transport mechanism{,} in both natural and artificial systems. We anticipate that in georeservoir extractions{,} physiological systems{,} drying operations{,} laboratory and industrial separations{,} crystallization operations{,} membrane processes{,} and many other situations{,} diffusiophoresis is already occurring {\^a} often without being recognized {\^a} and that opportunities exist for designing this transport to great advantage.},
	author = {Velegol, D. and Garg, A. and Guha, R. and Kar, A. and Kumar, M.},
	doi = {10.1039/C6SM00052E},
	issue = {21},
	journal = {Soft Matter},
	pages = {4686-4703},
