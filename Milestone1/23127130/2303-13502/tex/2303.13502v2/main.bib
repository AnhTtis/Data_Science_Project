@article{TAMANOI1999153,
title = "L-subalgebras, Milnor basis, and cohomology of Eilenberg-MacLane spaces",
journal = "Journal of Pure and Applied Algebra",
volume = "137",
number = "2",
pages = "153 - 198",
year = "1999",
issn = "0022-4049",
doi = "https://doi.org/10.1016/S0022-4049(97)00177-1",
url = "http://www.sciencedirect.com/science/article/pii/S0022404997001771",
author = "Hirotaka Tamanoi",
abstract = "We describe mod p cohomology rings of Eilenberg-MacLane spaces in terms of the Milnor basis rather than in terms of admissible monomials of the Steenrod algebra. We give a formula of excess for the Milnor basis elements, correcting Kraines' formula in odd prime case. Using the Milnor basis description, we study and characterize certain polynomial subalgebras generated by elements obtained by applying maximum number of Milnor primitives on mod p fundamental classes of Eilenberg-MacLane spaces. A simple and interesting unstable pattern emerges. These subalgebras are exact images of the BP-Thom map into mod p cohomology rings."
}

