@article{doi:10.1086/250049,
author = {Dewatripont, Mathias and Tirole, Jean},
title = {Advocates},
journal = {Journal of Political Economy},
volume = {107},
number = {1},
pages = {1-39},
year = {1999},
doi = {10.1086/250049},

URL = { 
        https://doi.org/10.1086/250049
    
},
eprint = { 
        https://doi.org/10.1086/250049
    
}
,
    abstract = { The paper's main contribution is to provide a rational for advocacy. After observing that many organizations (corporations, judiciary, and the executive and legislative branches of government) use competition among enfranchised advocates of special interests to improve policy making, it argues that advocacy has two major benefits. First, the advocates' rewards closely track their pursuing several conflicting causes at one time. Second, advocacy enhances the integrity of decision making by creating strong incentives to appeal in case of an abusive decision. The paper also analyzes the costs of advocacy in terms of manipulation and garbling of information. It further shows that it may be costly for both the organization and interested parties themselves to let these parties plead their own causes instead of being represented. The paper concludes with two applications to comparative legal systems and to the organization of Congress and with suggestions for future research. }
}

