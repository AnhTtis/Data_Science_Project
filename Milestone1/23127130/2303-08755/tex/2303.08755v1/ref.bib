@Inbook{vitagliano2018,
  author="Vitagliano, Giuseppe and Kl{\"o}ckl, Claude and Huber, Marcus and Friis, Nicolai",
  editor="Binder, Felix and Correa, Luis A. and Gogolin, Christian and Anders, Janet and Adesso, Gerardo",
