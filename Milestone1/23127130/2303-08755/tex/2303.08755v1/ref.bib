@article{gore2003,
	Abstract = {In 1997, Jarzynski proved a remarkable equality that allows one to compute the equilibrium free-energy difference ΔF between two states from the probability distribution of the nonequilibrium work W done on the system to switch between the states, e{\textendash}ΔF/kT = 〈e{\textendash}W/kT〉, [Jarzynski, C. (1997) Phys. Rev. Lett. 87, 2690{\textendash}2693]. The Jarzynski equality provides a powerful free-energy difference estimator from a set of N irreversible experiments and is closely related to free-energy perturbation, a common computational technique for estimating free-energy differences. Despite the many applications of the Jarzynski estimator, its behavior is only poorly understood. In this article we derive the large N limit for the Jarzynski estimator bias, variance, and mean square error that is correct for arbitrary perturbations. We then analyze the properties of the Jarzynski estimator for all N when the probability distribution of work values is Gaussian, as occurs, for example, in the near-equilibrium regime. This allows us to quantitatively compare it to two other free-energy difference estimators: the mean work estimator and the fluctuation{\textendash}dissipation theorem estimator. We show that, for near-equilibrium switching, the Jarzynski estimator is always superior to the mean work estimator and is even superior to the fluctuation{\textendash}dissipation estimator for small N. The Jarzynski-estimator bias is shown to be the dominant source of error in many cases. Our expression for the bias is used to develop a bias-corrected Jarzynski free-energy difference estimator in the near-equilibrium regime.},
	Author = {Gore, Jeff and Ritort, Felix and Bustamante, Carlos},
	Date-Added = {2019-12-01 18:13:05 -0300},
	Date-Modified = {2019-12-01 22:47:44 -0300},
	Doi = {10.1073/pnas.1635159100},
	Eprint = {https://www.pnas.org/content/100/22/12564.full.pdf},
	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {22},
	Pages = {12564--12569},
	Publisher = {National Academy of Sciences},
	Title = {Bias and error in estimates of equilibrium free-energy differences from nonequilibrium measurements},
	Url = {https://www.pnas.org/content/100/22/12564},
	Volume = {100},
	Year = {2003},
	Bdsk-Url-1 = {https://www.pnas.org/content/100/22/12564},
	Bdsk-Url-2 = {https://doi.org/10.1073/pnas.1635159100}}

