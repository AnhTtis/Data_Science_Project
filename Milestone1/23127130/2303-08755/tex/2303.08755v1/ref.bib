@article{margalit2015,
	Abstract = {The interference pattern arising from light or particles passing through a double slit is a simple experiment that belies the subtleties of interpretation when attempting to describe and understand the effect. For example, determining {\textquotedblleft}which path{\textquotedblright} the light or particles travel can result in the interference pattern disappearing. Margalit et al. present a new take on interferometry using time (see the Perspective by Arndt and Brand). A clock{\textemdash}i.e., the internal state of a cold atom condensate{\textemdash}was coherently split and brought back together to interfere. Making one-half of the clock tick at a different rate resulted in a change in the interference pattern, possibly as a consequence of the time being a {\textquotedblleft}which path{\textquotedblright} witness.Science, this issue p. 1205; see also p. 1168In Einstein{\textquoteright}s general theory of relativity, time depends locally on gravity; in standard quantum theory, time is global{\textemdash}all clocks {\textquotedblleft}tick{\textquotedblright} uniformly. We demonstrate a new tool for investigating time in the overlap of these two theories: a self-interfering clock, comprising two atomic spin states. We prepare the clock in a spatial superposition of quantum wave packets, which evolve coherently along two paths into a stable interference pattern. If we make the clock wave packets {\textquotedblleft}tick{\textquotedblright} at different rates, to simulate a gravitational time lag, the clock time along each path yields {\textquotedblleft}which path{\textquotedblright} information, degrading the pattern{\textquoteright}s visibility. In contrast, in standard interferometry, time cannot yield {\textquotedblleft}which path{\textquotedblright} information. This proof-of-principle experiment may have implications for the study of time and general relativity and their impact on fundamental effects such as decoherence and the emergence of a classical world.},
	Author = {Margalit, Yair and Zhou, Zhifan and Machluf, Shimon and Rohrlich, Daniel and Japha, Yonathan and Folman, Ron},
	Date-Added = {2019-12-01 22:37:28 -0300},
	Date-Modified = {2019-12-01 22:47:44 -0300},
	Doi = {10.1126/science.aac6498},
	Eprint = {https://science.sciencemag.org/content/349/6253/1205.full.pdf},
	Issn = {0036-8075},
	Journal = {Science},
	Number = {6253},
	Pages = {1205--1208},
	Publisher = {American Association for the Advancement of Science},
	Title = {A self-interfering clock as a ``which path'' witness},
	Url = {https://science.sciencemag.org/content/349/6253/1205},
	Volume = {349},
	Year = {2015},
	Bdsk-Url-1 = {https://science.sciencemag.org/content/349/6253/1205},
	Bdsk-Url-2 = {https://doi.org/10.1126/science.aac6498}}

