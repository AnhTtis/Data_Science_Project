	Issn = {0036-8075},
	Journal = {Science},
	Number = {5574},
	Pages = {1832--1835},
	Publisher = {American Association for the Advancement of Science},
	Title = {Equilibrium Information from Nonequilibrium Measurements in an Experimental Test of Jarzynski{\textquoteright}s Equality},
