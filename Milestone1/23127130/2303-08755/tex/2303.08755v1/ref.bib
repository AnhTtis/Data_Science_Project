@article{cerisola2017,
  Abstract = {Work is an essential concept in classical thermodynamics, and in
    the quantum regime, where the notion of a trajectory is not available, its
      definition is not trivial. For driven (but otherwise isolated) quantum
      systems, work can be defined as a random variable, associated with the
      change in the internal energy. The probability for the different values
      of work captures essential information describing the behaviour of the
      system, both in and out of thermal equilibrium. In fact, the work
      probability distribution is at the core of ``fluctuation theorems''in
      quantum thermodynamics. Here we present the design and implementation of
      a quantum work meter operating on an ensemble of cold atoms, which are
      controlled by an atom chip. Our device not only directly measures work
      but also directly samples its probability distribution. We demonstrate
      the operation of this new tool and use it to verify the validity of the
      quantum Jarzynksi identity.},
  Author = {Cerisola, Federico and Margalit, Yair and Machluf, Shimon and Roncaglia, Augusto J. and Paz, Juan Pablo and Folman, Ron},
	Da = {2017/11/01},
	Date-Added = {2020-05-03 20:24:32 -0300},
