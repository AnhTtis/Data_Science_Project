@misc{dong_beyond_2022,
	title = {Beyond {Heisenberg} {Limit} {Quantum} {Metrology} through {Quantum} {Signal} {Processing}},
	url = {http://arxiv.org/abs/2209.11207},
	doi = {10.48550/arXiv.2209.11207},
	abstract = {Leveraging quantum effects in metrology such as entanglement and coherence allows one to measure parameters with enhanced sensitivity. However, time-dependent noise can disrupt such Heisenberg-limited amplification. We propose a quantum-metrology method based on the quantum-signal-processing framework to overcome these realistic noise-induced limitations in practical quantum metrology. Our algorithm separates the gate parameter \${\textbackslash}varphi\${\textasciitilde}(single-qubit Z phase) that is susceptible to time-dependent error from the target gate parameter \${\textbackslash}theta\${\textasciitilde}(swap-angle between {\textbar}10{\textgreater} and {\textbar}01{\textgreater} states) that is largely free of time-dependent error. Our method achieves an accuracy of \$10{\textasciicircum}\{-4\}\$ radians in standard deviation for learning \${\textbackslash}theta\$ in superconducting-qubit experiments, outperforming existing alternative schemes by two orders of magnitude. We also demonstrate the increased robustness in learning time-dependent gate parameters through fast Fourier transformation and sequential phase difference. We show both theoretically and numerically that there is an interesting transition of the optimal metrology variance scaling as a function of circuit depth \$d\$ from the pre-asymptotic regime \$d {\textbackslash}ll 1/{\textbackslash}theta\$ to Heisenberg limit \$d {\textbackslash}to {\textbackslash}infty\$. Remarkably, in the pre-asymptotic regime our method's estimation variance on time-sensitive parameter \${\textbackslash}varphi\$ scales faster than the asymptotic Heisenberg limit as a function of depth, \${\textbackslash}text\{Var\}({\textbackslash}hat\{{\textbackslash}varphi\}){\textbackslash}approx 1/d{\textasciicircum}4\$. Our work is the first quantum-signal-processing algorithm that demonstrates practical application in laboratory quantum computers.},
	urldate = {2023-12-03},
	publisher = {arXiv},
	author = {Dong, Yulong and Gross, Jonathan and Niu, Murphy Yuezhen},
	month = sep,
	year = {2022},
	note = {arXiv:2209.11207 [quant-ph]},
	keywords = {Computer Science - Machine Learning, Quantum Physics},
}

@article{brennen_multiscale_2015,
