@misc{fowler_low_2019,
	title = {Low overhead quantum computation using lattice surgery},
	url = {http://arxiv.org/abs/1808.06709},
	doi = {10.48550/arXiv.1808.06709},
	abstract = {When calculating the overhead of a quantum algorithm made fault-tolerant using the surface code, many previous works have used defects and braids for logical qubit storage and state distillation. In this work, we show that lattice surgery reduces the storage overhead by over a factor of 4, and the distillation overhead by nearly a factor of 5, making it possible to run algorithms with \$10{\textasciicircum}8\$ T gates using only \$3.7{\textbackslash}times 10{\textasciicircum}5\$ physical qubits capable of executing gates with error \$p{\textbackslash}sim 10{\textasciicircum}\{-3\}\$. These numbers strongly suggest that defects and braids in the surface code should be deprecated in favor of lattice surgery.},
