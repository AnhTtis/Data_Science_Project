@misc{haah_optimal_2022,
	title = {Optimal learning of quantum {Hamiltonians} from high-temperature {Gibbs} states},
	url = {http://arxiv.org/abs/2108.04842},
	abstract = {We study the problem of learning a Hamiltonian \$H\$ to precision \${\textbackslash}varepsilon\$, supposing we are given copies of its Gibbs state \${\textbackslash}rho={\textbackslash}exp(-{\textbackslash}beta H)/{\textbackslash}operatorname\{Tr\}({\textbackslash}exp(-{\textbackslash}beta H))\$ at a known inverse temperature \${\textbackslash}beta\$. Anshu, Arunachalam, Kuwahara, and Soleimanifar (Nature Physics, 2021, arXiv:2004.07266) recently studied the sample complexity (number of copies of \${\textbackslash}rho\$ needed) of this problem for geometrically local \$N\$-qubit Hamiltonians. In the high-temperature (low \${\textbackslash}beta\$) regime, their algorithm has sample complexity poly\$(N, 1/{\textbackslash}beta,1/{\textbackslash}varepsilon)\$ and can be implemented with polynomial, but suboptimal, time complexity. In this paper, we study the same question for a more general class of Hamiltonians. We show how to learn the coefficients of a Hamiltonian to error \${\textbackslash}varepsilon\$ with sample complexity \$S = O({\textbackslash}log N/({\textbackslash}beta{\textbackslash}varepsilon){\textasciicircum}\{2\})\$ and time complexity linear in the sample size, \$O(S N)\$. Furthermore, we prove a matching lower bound showing that our algorithm's sample complexity is optimal, and hence our time complexity is also optimal. In the appendix, we show that virtually the same algorithm can be used to learn \$H\$ from a real-time evolution unitary \$e{\textasciicircum}\{-it H\}\$ in a small \$t\$ regime with similar sample and time complexity.},
	urldate = {2023-01-10},
	publisher = {arXiv},
	author = {Haah, Jeongwan and Kothari, Robin and Tang, Ewin},
	month = sep,
	year = {2022},
	note = {arXiv:2108.04842 [quant-ph]},
	keywords = {Computer Science - Data Structures and Algorithms, Computer Science - Machine Learning, Quantum Physics},
}

@misc{alhambra_quantum_2022,
	title = {Quantum many-body systems in thermal equilibrium},
	url = {http://arxiv.org/abs/2204.08349},
	abstract = {The thermal or equilibrium ensemble is one of the most ubiquitous states of matter. For models comprised of many locally interacting quantum particles, it describes a wide range of physical situations, relevant to condensed matter physics, high energy physics, quantum chemistry and quantum computing, among others. We give a pedagogical overview of some of the most important universal features about the physics and complexity of these states, which have the locality of the Hamiltonian at its core. We focus on mathematically rigorous statements, many of them inspired by ideas and tools from quantum information theory. These include bounds on their correlations, the form of the subsystems, various statistical properties, and the performance of classical and quantum algorithms. We also include a summary of a few of the most important technical tools, as well as some self-contained proofs.},
