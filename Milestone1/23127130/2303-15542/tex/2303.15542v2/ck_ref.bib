@article{huggins_virtual_2021,
	title = {Virtual {Distillation} for {Quantum} {Error} {Mitigation}},
	volume = {11},
	issn = {2160-3308},
	url = {http://arxiv.org/abs/2011.07064},
	doi = {10.1103/PhysRevX.11.041036},
	abstract = {Contemporary quantum computers have relatively high levels of noise, making it difficult to use them to perform useful calculations, even with a large number of qubits. Quantum error correction is expected to eventually enable fault-tolerant quantum computation at large scales, but until then it will be necessary to use alternative strategies to mitigate the impact of errors. We propose a near-term friendly strategy to mitigate errors by entangling and measuring \$M\$ copies of a noisy state \${\textbackslash}rho\$. This enables us to estimate expectation values with respect to a state with dramatically reduced error, \${\textbackslash}rho{\textasciicircum}M/ {\textbackslash}mathrm\{Tr\}({\textbackslash}rho{\textasciicircum}M)\$, without explicitly preparing it, hence the name "virtual distillation". As \$M\$ increases, this state approaches the closest pure state to \${\textbackslash}rho\$, exponentially quickly. We analyze the effectiveness of virtual distillation and find that it is governed in many regimes by the behavior of this pure state (corresponding to the dominant eigenvector of \${\textbackslash}rho\$). We numerically demonstrate that virtual distillation is capable of suppressing errors by multiple orders of magnitude and explain how this effect is enhanced as the system size grows. Finally, we show that this technique can improve the convergence of randomized quantum algorithms, even in the absence of device noise.},
	number = {4},
	urldate = {2023-08-01},
	journal = {Physical Review X},
	author = {Huggins, William J. and McArdle, Sam and O'Brien, Thomas E. and Lee, Joonho and Rubin, Nicholas C. and Boixo, Sergio and Whaley, K. Birgitta and Babbush, Ryan and McClean, Jarrod R.},
	month = nov,
	year = {2021},
	note = {arXiv:2011.07064 [quant-ph]},
	keywords = {Quantum Physics},
	pages = {041036},
}

