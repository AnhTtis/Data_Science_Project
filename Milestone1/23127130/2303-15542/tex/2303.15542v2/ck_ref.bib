	url = {https://pubs.rsc.org/en/content/articlelanding/2022/cc/d2cc01546c},
	doi = {10.1039/D2CC01546C},
	abstract = {Quantum technologies have seen a rapid developmental surge over the last couple of years. Though often overshadowed by quantum computation, quantum sensors show tremendous potential for widespread applications in chemistry and biology. One system stands out in particular: the nitrogen-vacancy (NV) center in diamond, an atomic-sized sensor allowing the detection of nuclear magnetic resonance (NMR) signals at unprecedented length scales down to a single proton. In this article, we review the fundamentals of NV center-based quantum sensing and its distinct impact on nano- and microscale NMR spectroscopy. Furthermore, we highlight possible future applications of this novel technology ranging from energy research, materials science, to single-cell biology, and discuss the associated challenges of these rapidly developing NMR sensors.},
	language = {en},
	number = {59},
	urldate = {2022-11-20},
	journal = {Chemical Communications},
	author = {Allert, Robin D. and Briegel, Karl D. and Bucher, Dominik B.},
	month = jul,
	year = {2022},
	note = {Publisher: The Royal Society of Chemistry},
	keywords = {sensing},
	pages = {8165--8181},
}

