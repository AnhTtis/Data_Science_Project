@article{tremblay_constant-overhead_2022,
	title = {Constant-overhead quantum error correction with thin planar connectivity},
	volume = {129},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/2109.14609},
	doi = {10.1103/PhysRevLett.129.050504},
	abstract = {Quantum LDPC codes may provide a path to build low-overhead fault-tolerant quantum computers. However, as general LDPC codes lack geometric constraints, na{\textbackslash}"ive layouts couple many distant qubits with crossing connections which could be hard to build in hardware and could result in performance-degrading crosstalk. We propose a 2D layout for quantum LDPC codes by decomposing their Tanner graphs into a small number of planar layers. Each layer contains long-range connections which do not cross. For any CSS code with a degree-\${\textbackslash}delta\$ Tanner graph, we design stabilizer measurement circuits with depth at most \$(2{\textbackslash}delta +2)\$ using at most \${\textbackslash}lceil {\textbackslash}delta/2 {\textbackslash}rceil\$ layers. We observe a circuit-noise threshold of 0.28{\textbackslash}\% for a positive-rate code family using 49 physical qubits per logical qubit. For a physical error rate of \$10{\textasciicircum}\{-4\}\$, this family reaches a logical error rate of \$10{\textasciicircum}\{-15\}\$ using fourteen times fewer physical qubits than the surface code.},
	number = {5},
	urldate = {2023-05-18},
	journal = {Physical Review Letters},
	author = {Tremblay, Maxime A. and Delfosse, Nicolas and Beverland, Michael E.},
	month = jul,
