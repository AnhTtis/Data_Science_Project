@misc{kliuchnikov_fast_2013,
	title = {Fast and efficient exact synthesis of single qubit unitaries generated by {Clifford} and {T} gates},
	url = {http://arxiv.org/abs/1206.5236},
	doi = {10.48550/arXiv.1206.5236},
	abstract = {In this paper, we show the equivalence of the set of unitaries computable by the circuits over the Clifford and T library and the set of unitaries over the ring \${\textbackslash}mathbb\{Z\}[{\textbackslash}frac\{1\}\{{\textbackslash}sqrt\{2\}\},i]\$, in the single-qubit case. We report an efficient synthesis algorithm, with an exact optimality guarantee on the number of Hadamard and T gates used. We conjecture that the equivalence of the sets of unitaries implementable by circuits over the Clifford and T library and unitaries over the ring \${\textbackslash}mathbb\{Z\}[{\textbackslash}frac\{1\}\{{\textbackslash}sqrt\{2\}\},i]\$ holds in the \$n\$-qubit case.},
