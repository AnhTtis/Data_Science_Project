	author = {Low, Guang Hao and Bauman, Nicholas P. and Granade, Christopher E. and Peng, Bo and Wiebe, Nathan and Bylaska, Eric J. and Wecker, Dave and Krishnamoorthy, Sriram and Roetteler, Martin and Kowalski, Karol and Troyer, Matthias and Baker, Nathan A.},
	month = apr,
