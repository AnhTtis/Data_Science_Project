@article{lucero_reduced_2010,
	title = {Reduced phase error through optimized control of a superconducting qubit},
	volume = {82},
	issn = {1050-2947, 1094-1622},
	url = {http://arxiv.org/abs/1007.1690},
	doi = {10.1103/PhysRevA.82.042339},
	abstract = {Minimizing phase and other errors in experimental quantum gates allows higher fidelity quantum processing. To quantify and correct for phase errors in particular, we have developed a new experimental metrology --- amplified phase error (APE) pulses --- that amplifies and helps identify phase errors in general multi-level qubit architectures. In order to correct for both phase and amplitude errors specific to virtual transitions and leakage outside of the qubit manifold, we implement "half derivative" an experimental simplification of derivative reduction by adiabatic gate (DRAG) control theory. The phase errors are lowered by about a factor of five using this method to \${\textbackslash}sim 1.6{\textasciicircum}\{{\textbackslash}circ\}\$ per gate, and can be tuned to zero. Leakage outside the qubit manifold, to the qubit \${\textbar}2{\textbackslash}rangle\$ state, is also reduced to \${\textbackslash}sim 10{\textasciicircum}\{-4\}\$ for \$20{\textbackslash}\%\$ faster gates.},
