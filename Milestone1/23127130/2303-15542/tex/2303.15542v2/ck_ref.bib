@article{low_hamiltonian_2016,
	title = {Hamiltonian {Simulation} by {Qubitization}},
	url = {https://arxiv.org/abs/1610.06546v3},
	doi = {10.22331/q-2019-07-12-163},
	abstract = {We present the problem of approximating the time-evolution operator \$e{\textasciicircum}\{-i{\textbackslash}hat\{H\}t\}\$ to error \${\textbackslash}epsilon\$, where the Hamiltonian \${\textbackslash}hat\{H\}=({\textbackslash}langle G{\textbar}{\textbackslash}otimes{\textbackslash}hat\{{\textbackslash}mathcal\{I\}\}){\textbackslash}hat\{U\}({\textbar}G{\textbackslash}rangle{\textbackslash}otimes{\textbackslash}hat\{{\textbackslash}mathcal\{I\}\})\$ is the projection of a unitary oracle \${\textbackslash}hat\{U\}\$ onto the state \${\textbar}G{\textbackslash}rangle\$ created by another unitary oracle. Our algorithm solves this with a query complexity \${\textbackslash}mathcal\{O\}{\textbackslash}big(t+{\textbackslash}log(\{1/{\textbackslash}epsilon\}){\textbackslash}big)\$ to both oracles that is optimal with respect to all parameters in both the asymptotic and non-asymptotic regime, and also with low overhead, using at most two additional ancilla qubits. This approach to Hamiltonian simulation subsumes important prior art considering Hamiltonians which are \$d\$-sparse or a linear combination of unitaries, leading to significant improvements in space and gate complexity, such as a quadratic speed-up for precision simulations. It also motivates useful new instances, such as where \${\textbackslash}hat\{H\}\$ is a density matrix. A key technical result is `qubitization', which uses the controlled version of these oracles to embed any \${\textbackslash}hat\{H\}\$ in an invariant \${\textbackslash}text\{SU\}(2)\$ subspace. A large class of operator functions of \${\textbackslash}hat\{H\}\$ can then be computed with optimal query complexity, of which \$e{\textasciicircum}\{-i{\textbackslash}hat\{H\}t\}\$ is a special case.},
	language = {en},
	urldate = {2022-10-21},
	author = {Low, Guang Hao and Chuang, Isaac L.},
	month = oct,
	year = {2016},
}

