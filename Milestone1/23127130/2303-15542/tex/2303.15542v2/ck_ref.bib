	author = {Klco, Natalie and Savage, Martin J.},
	month = may,
	year = {2019},
	note = {arXiv:1808.10378 [hep-lat, physics:hep-ph, physics:nucl-th, physics:quant-ph]},
	keywords = {High Energy Physics - Lattice, High Energy Physics - Phenomenology, Nuclear Theory, Quantum Physics},
