	pages = {015004},
}

@misc{kliuchnikov_shorter_2022,
	title = {Shorter quantum circuits},
	url = {http://arxiv.org/abs/2203.10064},
	abstract = {We give a novel procedure for approximating general single-qubit unitaries from a finite universal gate set by reducing the problem to a novel magnitude approximation problem, achieving an immediate improvement in sequence length by a factor of 7/9. Extending the works arXiv:1612.01011 and arXiv:1612.02689, we show that taking probabilistic mixtures of channels to solve fallback (arXiv:1409.3552) and magnitude approximation problems saves factor of two in approximation costs. In particular, over the Clifford+\${\textbackslash}sqrt\{{\textbackslash}mathrm\{T\}\}\$ gate set we achieve an average non-Clifford gate count of \$0.23{\textbackslash}log\_2(1/{\textbackslash}varepsilon)+2.13\$ and T-count \$0.56{\textbackslash}log\_2(1/{\textbackslash}varepsilon)+5.3\$ with mixed fallback approximations for diamond norm accuracy \${\textbackslash}varepsilon\$. This paper provides a holistic overview of gate approximation, in addition to these new insights. We give an end-to-end procedure for gate approximation for general gate sets related to some quaternion algebras, providing pedagogical examples using common fault-tolerant gate sets (V, Clifford+T and Clifford+\${\textbackslash}sqrt\{{\textbackslash}mathrm\{T\}\}\$). We also provide detailed numerical results for Clifford+T and Clifford+\${\textbackslash}sqrt\{{\textbackslash}mathrm\{T\}\}\$ gate sets. In an effort to keep the paper self-contained, we include an overview of the relevant algorithms for integer point enumeration and relative norm equation solving. We provide a number of further applications of the magnitude approximation problems, as well as improved algorithms for exact synthesis, in the Appendices.},
	urldate = {2023-05-01},
	publisher = {arXiv},
	author = {Kliuchnikov, Vadym and Lauter, Kristin and Minko, Romy and Paetznick, Adam and Petit, Christophe},
	month = mar,
	year = {2022},
	note = {arXiv:2203.10064 [quant-ph]},
	keywords = {Quantum Physics},
}

@misc{zhu_efficient_2023,
