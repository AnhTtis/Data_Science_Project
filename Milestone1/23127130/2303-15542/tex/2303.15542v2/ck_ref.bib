@article{bravyi_correcting_2018,
	title = {Correcting coherent errors with surface codes},
	volume = {4},
	issn = {2056-6387},
	url = {http://arxiv.org/abs/1710.02270},
	doi = {10.1038/s41534-018-0106-y},
	abstract = {We study how well topological quantum codes can tolerate coherent noise caused by systematic unitary errors such as unwanted \$Z\$-rotations. Our main result is an efficient algorithm for simulating quantum error correction protocols based on the 2D surface code in the presence of coherent errors. The algorithm has runtime \$O(n{\textasciicircum}2)\$, where \$n\$ is the number of physical qubits. It allows us to simulate systems with more than one thousand qubits and obtain the first error threshold estimates for several toy models of coherent noise. Numerical results are reported for storage of logical states subject to \$Z\$-rotation errors and for logical state preparation with general \$SU(2)\$ errors. We observe that for large code distances the effective logical-level noise is well-approximated by random Pauli errors even though the physical-level noise is coherent. Our algorithm works by mapping the surface code to a system of Majorana fermions.},
	number = {1},
	urldate = {2023-11-03},
	journal = {npj Quantum Information},
	author = {Bravyi, Sergey and Englbrecht, Matthias and Koenig, Robert and Peard, Nolan},
