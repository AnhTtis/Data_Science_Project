	keywords = {Condensed Matter - Quantum Gases, Quantum Physics},
	pages = {045020},
}

@misc{zemlevskiy_optimization_2023,
	title = {Optimization of {Algorithmic} {Errors} in {Analog} {Quantum} {Simulations}},
