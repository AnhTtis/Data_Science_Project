	abstract = {We present a quantum LDPC code family that has distance \${\textbackslash}Omega(N{\textasciicircum}\{3/5\}/{\textbackslash}operatorname\{polylog\}(N))\$ and \${\textbackslash}tilde{\textbackslash}Theta(N{\textasciicircum}\{3/5\})\$ logical qubits. This is the first quantum LDPC code construction which achieves distance greater than \$N{\textasciicircum}\{1/2\} {\textbackslash}operatorname\{polylog\}(N)\$. The construction is based on generalizing the homological product of codes to a fiber bundle.},
	urldate = {2024-02-28},
