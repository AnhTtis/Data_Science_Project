	pmid = {27294737},
	pmcid = {PMC4911317},
	pages = {444--456},
	file = {Accepted Version:/Users/carly_luptonsmith/Zotero/storage/6JXTTVWC/Barber et al. - 2016 - Neighborhood Disadvantage and Cumulative Biologica.pdf:application/pdf},
}

@article{aranda_protective_2011,
	title = {The protective effect of neighborhood composition on increasing frailty among older {Mexican} {Americans}: {A} barrio advantage?},
