	month = jul,
	year = {2021},
	note = {arXiv: 2011.08047},
	keywords = {Statistics - Methodology},
	file = {Colnet et al. - 2021 - Causal inference methods for combining randomized .pdf:/Users/carly_luptonsmith/Zotero/storage/VZTFSQHV/Colnet et al. - 2021 - Causal inference methods for combining randomized .pdf:application/pdf},
}

