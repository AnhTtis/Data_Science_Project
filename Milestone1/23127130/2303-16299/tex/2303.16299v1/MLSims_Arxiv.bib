	pmid = {29982701},
	pmcid = {PMC6483567},
	keywords = {Humans, Outcome Assessment, Health Care, Randomized Controlled Trials as Topic, Adult, Antipsychotic Agents, neuroleptic, nonremission, nonresponse, Schizophrenia, unresponsive},
	pages = {639--646},
