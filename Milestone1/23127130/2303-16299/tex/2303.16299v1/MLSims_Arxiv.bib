	pages = {1351--1364},
	file = {Debray et al. - 2018 - An overview of methods for network meta-analysis u.pdf:/Users/carly_luptonsmith/Zotero/storage/X66884EQ/Debray et al. - 2018 - An overview of methods for network meta-analysis u.pdf:application/pdf},
