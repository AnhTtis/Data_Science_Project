@article{Steidel2018,
	abstract = {We present results of a deep spectroscopic survey quantifying the statistics of the escape of ionizing radiation from star-forming galaxies at z ∼ 3. We measure the ratio of ionizing to non-ionizing UV flux density ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn1.gif] {$\langle {f}_{900}{/f}_{1500}{\rangle }_{\mathrm{obs}}$} , where f 900 is the mean flux density evaluated over the range [880, 910] {\AA}. We quantify the emergent ratio of ionizing to non-ionizing UV flux density by analyzing high signal-to-noise ratio composite spectra formed from subsamples with common observed properties and numbers sufficient to reduce the statistical uncertainty in the modeled IGM+CGM correction to obtain precise values of ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn2.gif] {$\langle {f}_{900}{/f}_{1500}{\rangle }_{\mathrm{out}}$} , including a full-sample average ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn3.gif] {$\langle {f}_{900}{/f}_{1500}{\rangle }_{\mathrm{out}}$} = 0.057 $\pm$ 0.006. We show that ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn4.gif] {$\langle {f}_{900}{/f}_{1500}{\rangle }_{\mathrm{out}}$} increases monotonically with ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn5.gif] {${W}_{\lambda }(\mathrm{Ly}\alpha )$} , inducing an inverse correlation with UV luminosity as a by-product. We fit the composite spectra using stellar spectral synthesis together with models of the ISM in which a fraction f c of the stellar continuum is covered by gas with column density ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn6.gif] {${N}_{{\rm{H}}{\rm{I}}}$} . We show that the composite spectra simultaneously constrain the intrinsic properties of the stars ( L 900 / L 1500 ) int along with f c , ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn7.gif] {${N}_{{\rm{H}}{\rm{I}}}$} , ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn8.gif] {$E(B-V)$} , and f esc,abs , the absolute escape fraction of ionizing photons. We find a sample-averaged f esc,abs  = 0.09 $\pm$ 0.01, with subsamples falling along a linear relation ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn9.gif] {$\langle {f}_{\mathrm{esc},\mathrm{abs}}\rangle \simeq 0.75[{W}_{\lambda }(\mathrm{Ly}\alpha )/110\,\mathring{\rm A} ]$} . Using the far-UV luminosity function, the distribution function n ( W (Ly α )), and the relationship between ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn10.gif] {${W}_{\lambda }(\mathrm{Ly}\alpha )$} and ##IMG## [http://ej.iop.org/images/0004-637X/869/2/123/apjaaed28ieqn11.gif] {$\langle {f}_{900}{/f}_{1500}{\rangle }_{\mathrm{out}}$} , we estimate the total ionizing emissivity of z  ∼ 3 star-forming galaxies with M uv ≤ −19.5, which exceeds the contribution of quasi-stellar objects by a factor of ∼3, and accounts for ∼50\% of the total ϵ LyC at z ∼ 3 estimated using indirect methods.},
	author = {Charles C. Steidel and Milan Bogosavljevi{\'c} and Alice E. Shapley and Naveen A. Reddy and Gwen C. Rudie and Max Pettini and Ryan F. Trainor and Allison L. Strom},
	date-added = {2018-12-29 14:30:32 +0100},
	date-modified = {2018-12-29 14:30:34 +0100},
	journal = {The Astrophysical Journal},
	number = {2},
	pages = {123},
	title = {The Keck Lyman Continuum Spectroscopic Survey (KLCS): The Emergent Ionizing Spectrum of Galaxies at z $\sim$ 3},
	url = {http://stacks.iop.org/0004-637X/869/i=2/a=123},
	volume = {869},
