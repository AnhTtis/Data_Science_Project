journal = {UbiComp 2016 Adjunct - Proceedings of the 2016 ACM International Joint Conference on Pervasive and Ubiquitous Computing},
keywords = {Fitness tracker,Heart-rate tracking.},
pages = {1101--1105},
title = {{A vision for heart rate health through wearables}},
year = {2016}
}

