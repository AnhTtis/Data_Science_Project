address = {New York, NY, USA},
url = {https://doi.org/10.1145/1576702.1576749},
doi = {10.1145/1576702.1576749},
abstract = {We present a real root isolation procedure for univariate elementary functions. The procedure finds the domain and the zero set of a function f in an arbitrary, possibly unbounded, interval as long as f is represented by a tame expression. An elementary expression is tame if the arguments of its trigonometric subexpressions are bounded. We discuss implementation of the procedure and give empirical results. The procedure requires the ability to determine signs of elementary functions at simple roots of other elementary functions. The currently known method to do this depends on Schanuel's conjecture [7].},
booktitle = {Proceedings of the 2009 International Symposium on Symbolic and Algebraic Computation},
pages = {341–350},
numpages = {10},
keywords = {solving equations, real root isolation, elementary functions},
location = {Seoul, Republic of Korea},
series = {ISSAC '09}
}

