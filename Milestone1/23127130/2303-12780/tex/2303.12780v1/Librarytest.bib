@article{katz_tensors_2005,
	title = {Tensors Mesons in {AdS}/{QCD}},
	url = {http://arxiv.org/abs/hep-ph/0510388},
	doi = {10.1103/PhysRevD.74.086004},
	abstract = {We explore tensor mesons in {AdS}/{QCD} focusing on f2 (1270), the lightest spin-two resonance in {QCD}. We find that the f2 mass and the partial width for f2 -{\textgreater} gamma gamma are in very good agreement with data. In fact, the dimensionless ratio of these two quantities comes out within the current experimental bound. The result for this ratio depends only on Nc and Nf, and the quark and glueball content of the operator responsible for the f2; more importantly, it does not depend on chiral symmetry breaking and so is both independent of much of the arbitrariness of {AdS}/{QCD} and completely out of reach of chiral perturbation theory. For comparison, we also explore f2 -{\textgreater} pi pi, which because of its sensitivity to the {UV} corrections has much more uncertainty. We also calculate the masses of the higher spin resonances on the Regge trajectory of the f2, and find they compare favorably with experiment.},
	journaltitle = {{arXiv}:hep-ph/0510388},
	author = {Katz, Emanuel and Lewandowski, Adam and Schwartz, Matthew D.},
	urldate = {2019-07-12},
	date = {2005-10-31},
	eprinttype = {arxiv},
