	keywords = {Condensed Matter - Soft Condensed Matter, Physics - Biological Physics, Physics - Computational Physics},
	file = {arXiv Fulltext PDF:/home/chen/Zotero/storage/PYJGMNB2/Shaebani et al. - 2020 - Computational models for active matter.pdf:application/pdf;arXiv.org Snapshot:/home/chen/Zotero/storage/SJVVT7AA/1910.html:text/html},
}

