@article{bonati_magnetic_2016,
	title = {Magnetic field effects on the static quark potential at zero and finite temperature},
	volume = {94},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/1607.08160},
	doi = {10.1103/PhysRevD.94.094007},
	abstract = {We investigate the static \$Q{\textbackslash}bar\{Q\}\$ potential at zero and finite temperature in the presence of a constant and uniform external magnetic field \${\textbackslash}vec\{B\}\$, for several values of the lattice spacing and for different orientations with respect to \${\textbackslash}vec\{B\}\$. As a byproduct, we provide continuum limit extrapolated results for the string tension, the Coulomb coupling and the Sommer parameter at \$T = 0\$ and \$B = 0\$. We confirm the presence in the continuum of a \$B\$-induced anisotropy, regarding essentially the string tension, for which it is of the order of 15{\textbackslash}\% at \${\textbar}e{\textbar} B {\textbackslash}sim 1{\textasciitilde}\{{\textbackslash}rm {GeV}\}{\textasciicircum}2\$ and would suggest, if extrapolated to larger fields, a vanishing string tension along the magnetic field for \${\textbar}e{\textbar} B {\textbackslash}gtrsim 4\$ {GeV}\${\textasciicircum}2\$. The angular dependence for \${\textbar}e{\textbar} B {\textbackslash}lesssim 1\$ {GeV}\${\textasciicircum}2\$ can be nicely parametrized by the first allowed term in an angular Fourier expansion, corresponding to a quadrupole deformation. Finally, for \$T {\textbackslash}neq 0\$, the main effect of the magnetic field is a general suppression of the string tension, leading to a precocious loss of the confining properties: this happens even before the appearance of inverse magnetic catalysis in the chiral condensate, supporting the idea that the influence of the magnetic field on the confining properties is the leading effect originating the decrease of \$T\_c\$ as a function of \$B\$.},
	number = {9},
	journaltitle = {Physical Review D},
	author = {Bonati, Claudio and D'Elia, Massimo and Mariti, Marco and Mesiti, Michele and Negro, Francesco and Rucci, Andrea and Sanfilippo, Francesco},
	urldate = {2019-03-10},
	date = {2016-11-10},
	eprinttype = {arxiv},
	eprint = {1607.08160},
	keywords = {High Energy Physics - Lattice, High Energy Physics - Phenomenology},
	file = {arXiv\:1607.08160 PDF:/home/chen/Zotero/storage/BSI8C46R/Bonati et al. - 2016 - Magnetic field effects on the static quark potenti.pdf:application/pdf;arXiv.org Snapshot:/home/chen/Zotero/storage/VICQFEED/1607.html:text/html},
}

