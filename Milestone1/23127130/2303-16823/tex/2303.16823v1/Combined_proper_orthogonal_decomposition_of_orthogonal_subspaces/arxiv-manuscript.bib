@article{Shen2003,
   abstract = {A modified SIMPLEC scheme for flow computations on collocated grids has been developed. It is demonstrated that the standard SIMPLEC scheme [1] is inconsistent when applied on collocated grids. Hence, for steady computations the computed solution depends on the velocity underrelaxation parameter f u , whereas the solutions of unsteady computations for small time steps are polluted by unphysical wiggles. A revised scheme is proposed that extends the capability of the SIMPLEC method to cope with collocated grids in a general and consistent way. The efficiency of the new scheme is demonstrated by computing flows past a circular cylinder and an airfoil. © 2003, Taylor & Francis Group, LLC. All rights reserved.},
   author = {Wen Zhong Shen and Jess A. Michelsen and Niels N. Sørensen and Jens Nørkær Sørensen},
   doi = {10.1080/713836202},
   issn = {15210626},
   issue = {3},
   journal = {Numerical Heat Transfer, Part B: Fundamentals},
   title = {An Improved Simplec Method On Collocated Grids For Steady And Unsteady Flow Computations},
   volume = {43},
   year = {2003},
}

