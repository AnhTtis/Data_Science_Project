@inproceedings{Lumley1967,
abstract = {A physical definition of "large eddy" is given which is optimal in a sense and which leads to the application of the proper orthogonal decomposition (Loeve) to turbulent velocity fields. This decomposition is in mutually uncorrelated eigenfunctions of the autocorrelation tensor, which are identified as the large eddies. In coordinate directions which are homogeneous the decomposition is the fustumary Fourier-Stieltjes integral while in coordinate directions in which the field is of finite extent it is discrete. A differential equation may be obtained for these eigenfunctions in parallel flow. By formal expansion in amplitude, the dynamics of these eigenfunctions recalls the work of Malkus (Spiegel). By taking the most energetic eigenfunction to be small and taking approximate account of the effect of all the rest with an undetermied parameter, an equation similar to the samll-disturbance stability equation is obtianed for the first eigenfunction; the "turbulent Reynolds number" (in a neutrally buoyant shear flow) is determined by requiring that only the first mode should be excited, and neutral. This gives results reminiscent of Townsend.},
address = {Moscow, USSR},
author = {Lumley, J. L.},
booktitle = {Proceedings of the international colloquium},
isbn = {9783937655239},
issn = {00976156},
mendeley-groups = {General turbulence},
pages = {166--167},
publisher = {Publishing house Nakua},
title = {{The structure of inhomogeneous turbulent flows}},
year = {1967}
}
