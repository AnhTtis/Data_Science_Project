@article{booth2016,
   title = {From plane waves to local {Gaussians} for the simulation of correlated periodic systems},
   volume = {145},
   issn = {0021-9606, 1089-7690},
   url = {http://scitation.aip.org/content/aip/journal/jcp/145/8/10.1063/1.4961301},
   doi = {10.1063/1.4961301},
   abstract = {We present a simple, robust, and black-box approach to the implementation and use of local, periodic, atom-centered Gaussian basis functions within a plane wave code, in a computationally efficient manner. The procedure outlined is based on the representation of the Gaussians within a finite bandwidth by their underlying plane wave coefficients. The core region is handled within the projected augment wave framework, by pseudizing the Gaussian functions within a cutoff radius around each nucleus, smoothing the functions so that they are faithfully represented by a plane wave basis with only moderate kinetic energy cutoff. To mitigate the effects of the basis set superposition error and incompleteness at the mean-field level introduced by the Gaussian basis, we also propose a hybrid approach, whereby the complete occupied space is first converged within a large plane wave basis, and the Gaussian basis used to construct a complementary virtual space for the application of correlated methods. We demonstrate that these pseudized Gaussians yield compact and systematically improvable spaces with an accuracy comparable to their non-pseudized Gaussian counterparts. A key advantage of the described method is its ability to efficiently capture and describe electronic correlation effects of weakly bound and low-dimensional systems, where plane waves are not sufficiently compact or able to be truncated without unphysical artifacts. We investigate the accuracy of the pseudized Gaussians for the water dimer interaction, neon solid, and water adsorption on a LiH surface, at the level of second-order Møller–Plesset perturbation theory.},
   number = {8},
   urldate = {2016-10-17},
   journal = {J. Chem. Phys.},
   author = {Booth, George H. and Tsatsoulis, Theodoros and Chan, Garnet Kin-Lic and Gr\"uneis, Andreas},
   month = aug,
   year = {2016},
   keywords = {Adsorption, Basis sets, Gaussians, Manifolds, Mean field theory, Wave functions},
   pages = {084111},
   file = {Booth et al. - 2016 - From plane waves to local Gaussians for the simula.pdf:/home/hummel/.mozilla/firefox/eg57sam5.default/zotero/storage/QXJ3PBPE/Booth et al. - 2016 - From plane waves to local Gaussians for the simula.pdf:application/pdf;Snapshot:/home/hummel/.mozilla/firefox/eg57sam5.default/zotero/storage/4H453JTD/1.html:text/html}
}
