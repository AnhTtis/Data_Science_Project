@article{gruneis11,
abstract = {We demonstrate that natural orbitals allow for reducing the computational cost of wave function based correlated calculations, especially for atoms and molecules in a large box, when a plane wave basis set under periodic boundary conditions is used. The employed natural orbitals are evaluated on the level of second-order M?ller?Plesset perturbation theory (MP2), which requires a computational effort that scales as (N5), where N is a measure of the system size. Moreover, we find that a simple approximation reducing the scaling to (N4) yields orbitals that allow for a similar reduction of the number of virtual orbitals. The MP2 natural orbitals are applied to coupled-cluster singles and doubles (CCSD) as well as full configuration interaction Quantum Monte Carlo calculations of the H2 molecule to test our implementation. Finally, the atomization energies of the LiH molecule and solid are calculated on the level of MP2 and CCSD. We demonstrate that natural orbitals allow for reducing the computational cost of wave function based correlated calculations, especially for atoms and molecules in a large box, when a plane wave basis set under periodic boundary conditions is used. The employed natural orbitals are evaluated on the level of second-order M?ller?Plesset perturbation theory (MP2), which requires a computational effort that scales as (N5), where N is a measure of the system size. Moreover, we find that a simple approximation reducing the scaling to (N4) yields orbitals that allow for a similar reduction of the number of virtual orbitals. The MP2 natural orbitals are applied to coupled-cluster singles and doubles (CCSD) as well as full configuration interaction Quantum Monte Carlo calculations of the H2 molecule to test our implementation. Finally, the atomization energies of the LiH molecule and solid are calculated on the level of MP2 and CCSD.},
author = {Gr{\"{u}}neis, Andreas and Booth, George H. and Marsman, Martijn and Spencer, James and Alavi, Ali and Kresse, Georg},
doi = {10.1021/ct200263g},
issn = {15499618},
journal = {J. Chem. Theory Comput.},
number = {9},
pages = {2780--2785},
pmid = {26605469},
title = {{Natural orbitals for wave function based correlated calculations using a plane wave basis set}},
volume = {7},
year = {2011}
