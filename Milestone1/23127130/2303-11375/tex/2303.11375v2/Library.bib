@article{atiaFastforwardingHamiltoniansExponentially2017,
  title = {Fast-Forwarding of {{Hamiltonians}} and Exponentially Precise Measurements},
  author = {Atia, Yosi and Aharonov, Dorit},
  year = {2017},
  month = nov,
  journal = {Nature Communications},
  volume = {8},
  number = {1},
  pages = {1572},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/s41467-017-01637-7},
  urldate = {2023-01-25},
  abstract = {The time-energy uncertainty relation (TEUR) \$\$\textbackslash Delta \{\textbackslash it\{t\}\}\textbackslash Delta \{\textbackslash it\{E\}\}\textbackslash,\{\textbackslash bf\{ \textbackslash ge \}\}\textbackslash,\{\textbackslash textstyle\{\{\textbackslash bf\{1\}\} \textbackslash over \{\textbackslash bf\{2\}\}\}\}\$\$holds if the Hamiltonian is completely unknown, but can be violated otherwise; here we initiate a rigorous study describing when and to what extent such violations can occur. To this end, we propose a computational version of the TEUR (cTEUR), in which {$\Delta$}t is replaced by the computational complexity of simulating the measurement. cTEUR violations are proved to occur if and only if the Hamiltonian can be fast forwarded (FF), namely, simulated for time t with complexity significantly smaller than t. Shor's algorithm provides an example of exponential cTEUR violations; we show that so do commuting local Hamiltonians and quadratic fermion Hamiltonians. A general FF method is ruled out, but finding further examples, as well as experimental demonstrations, are left for future work. We discuss possible connections to sensing and quantum gravity. This work initiates a rigorous theory of efficiency versus accuracy in energy measurements using computational complexity language.},
  copyright = {2017 The Author(s)},
  langid = {english},
  keywords = {Information theory and computation,Quantum information,Theoretical physics}
}

