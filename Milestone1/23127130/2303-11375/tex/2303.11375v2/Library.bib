  file = {C\:\\Users\\danie\\Zotero\\storage\\JIUBDWRT\\Arute et al_2020_Hartree-Fock on a superconducting qubit quantum computer.pdf;C\:\\Users\\danie\\Zotero\\storage\\UT732VQ2\\2004.html}
