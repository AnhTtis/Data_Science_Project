@misc{bultriniUnifyingBenchmarkingStateoftheart2021,
  title = {Unifying and Benchmarking State-of-the-Art Quantum Error Mitigation Techniques},
  author = {Bultrini, Daniel and Gordon, Max Hunter and Czarnik, Piotr and Arrasmith, Andrew and Coles, Patrick J. and Cincio, Lukasz},
