  keywords = {Quantum Physics},
  file = {C\:\\Users\\danie\\Zotero\\storage\\YGWIY87U\\Kuroiwa et al. - 2022 - Quantum Car-Parrinello Molecular Dynamics A Cost-.pdf;C\:\\Users\\danie\\Zotero\\storage\\VJZRSALU\\2212.html}
