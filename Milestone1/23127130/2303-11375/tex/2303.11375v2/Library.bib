@article{commeauVariationalHamiltonianDiagonalization2020,
  title = {Variational {{Hamiltonian Diagonalization}} for {{Dynamical Quantum Simulation}}},
  author = {Commeau, Benjamin and Cerezo, M. and Holmes, Zo{\"e} and Cincio, Lukasz and Coles, Patrick J. and Sornborger, Andrew},
  year = {2020},
  month = sep,
  journal = {arXiv:2009.02559 [quant-ph]},
  eprint = {2009.02559},
  primaryclass = {quant-ph},
  urldate = {2021-07-21},
  abstract = {Dynamical quantum simulation may be one of the first applications to see quantum advantage. However, the circuit depth of standard Trotterization methods can rapidly exceed the coherence time of noisy quantum computers. This has led to recent proposals for variational approaches to dynamical simulation. In this work, we aim to make variational dynamical simulation even more practical and near-term. We propose a new algorithm called Variational Hamiltonian Diagonalization (VHD), which approximately transforms a given Hamiltonian into a diagonal form that can be easily exponentiated. VHD allows for fast forwarding, i.e., simulation beyond the coherence time of the quantum computer with a fixed-depth quantum circuit. It also removes Trotterization error and allows simulation of the entire Hilbert space. We prove an operational meaning for the VHD cost function in terms of the average simulation fidelity. Moreover, we prove that the VHD cost function does not exhibit a shallow-depth barren plateau, i.e., its gradient does not vanish exponentially. Our proof relies on locality of the Hamiltonian, and hence we connect locality to trainability. Our numerical simulations verify that VHD can be used for fast-forwarding dynamics.},
  archiveprefix = {arxiv},
  keywords = {Dynamical simulation,Quantum Physics,variational fast forwarding},
  file = {C\:\\Users\\danie\\Zotero\\storage\\5LIP45HF\\Commeau et al_2020_Variational Hamiltonian Diagonalization for Dynamical Quantum Simulation.pdf;C\:\\Users\\danie\\Zotero\\storage\\4GESTGHZ\\2009.html}
}

@article{cortesQuantumKrylovSubspace2021,
  title = {Quantum {{Krylov}} Subspace Algorithms for Ground and Excited State Energy Estimation},
  author = {Cortes, Cristian L. and Gray, Stephen K.},
  year = {2021},
  month = sep,
  journal = {arXiv:2109.06868 [cond-mat, physics:physics, physics:quant-ph]},
  eprint = {2109.06868},
  primaryclass = {cond-mat, physics:physics, physics:quant-ph},
  urldate = {2021-09-15},
  abstract = {Quantum Krylov subspace diagonalization (QKSD) algorithms provide a low-cost alternative to the conventional quantum phase estimation algorithm for estimating the ground and excited-state energies of a quantum many-body system. While QKSD algorithms have typically relied on using the Hadamard test for estimating Krylov subspace matrix elements of the form, \$\textbackslash langle \textbackslash phi\_i|e\^\{-i\textbackslash hat\{H\}\textbackslash tau\}|\textbackslash phi\_j \textbackslash rangle\$, the associated quantum circuits require an ancilla qubit with controlled multi-qubit gates that can be quite costly for near-term quantum hardware. In this work, we show that a wide class of Hamiltonians relevant to condensed matter physics and quantum chemistry contain symmetries that can be exploited to avoid the use of the Hadamard test. We propose a multi-fidelity estimation protocol that can be used to compute such quantities showing that our approach, when combined with efficient single-fidelity estimation protocols, provides a substantial reduction in circuit depth. In addition, we develop a unified theory of quantum Krylov subspace algorithms and present three new quantum-classical algorithms for the ground and excited-state energy estimation problem, where each new algorithm provides various advantages and disadvantages in terms of total number of calls to the quantum computer, gate depth, classical complexity, and stability of the generalized eigenvalue problem within the Krylov subspace.},
  archiveprefix = {arxiv},
  keywords = {chemsitry,Condensed Matter - Materials Science,Krylov subspace,Kyrlov,physics,Physics - Chemical Physics,Physics - Computational Physics,QPE,quantum,Quantum,Quantum Physics},
  file = {C\:\\Users\\danie\\Zotero\\storage\\4RQD4CVH\\2109.html;C\:\\Users\\danie\\Zotero\\storage\\6PZJ5RFS\\2109.html}
}

