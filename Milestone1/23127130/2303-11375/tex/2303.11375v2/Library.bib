@article{mainaliComparisonMultilayerMulticonfiguration2021,
  title = {Comparison of the Multi-Layer Multi-Configuration Time-Dependent {{Hartree}} ({{ML-MCTDH}}) Method and the Density Matrix Renormalization Group ({{DMRG}}) for Ground State Properties of Linear Rotor Chains},
