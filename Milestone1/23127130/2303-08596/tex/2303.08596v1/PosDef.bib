	doi = {10.1007/s10955-016-1690-x},
	id = {Lis2017},
	isbn = {1572-9613},
	journal = {Journal of Statistical Physics},
	number = {1},
	pages = {72--89},
	title = {The Planar {I}sing Model and Total Positivity},
	ty = {JOUR},
	url = {https://doi.org/10.1007/s10955-016-1690-x},
	volume = {166},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1007/s10955-016-1690-x}}

@article{ADTW,
	abstract = {The known Pfaffian structure of the boundary spin correlations, and more generally order--disorder correlation functions, is given a new explanation through simple topological considerations within the model's random current representation. This perspective is then employed in the proof that the Pfaffian structure of boundary correlations emerges asymptotically at criticality in Ising models on {\$}{\$}{\{}{$\backslash$}mathbb {\{}Z{\}}{\}}\^{}2{\$}{\$}Z2with finite-range interactions. The analysis is enabled by new results on the stochastic geometry of the corresponding random currents. The proven statement establishes an aspect of universality, seen here in the emergence of fermionic structures in two dimensions beyond the solvable cases.},
