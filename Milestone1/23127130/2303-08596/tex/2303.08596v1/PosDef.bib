@article{LisT,
	abstract = {A matrix is called totally positive (resp. totally nonnegative) if all its minors are positive (resp. nonnegative). Consider the Ising model with free boundary conditions and no external field on a planar graph G. Let {\$}{\$}a{\_}1,{$\backslash$}dots ,a{\_}k,b{\_}k,{$\backslash$}dots ,b{\_}1{\$}{\$}a1,⋯,ak,bk,⋯,b1be vertices placed in a counterclockwise order on the outer face of G. We show that the {\$}{\$}k{$\backslash$}times k{\$}{\$}k×kmatrix of the two-point spin correlation functions {\$}{\$}{$\backslash$}begin{\{}aligned{\}} M{\_}{\{}i,j{\}} = {$\backslash$}langle {$\backslash$}sigma {\_}{\{}a{\_}i{\}} {$\backslash$}sigma {\_}{\{}b{\_}j{\}} {$\backslash$}rangle {$\backslash$}end{\{}aligned{\}}{\$}{\$}Mi,j=⟨σaiσbj⟩is totally nonnegative. Moreover, {\$}{\$}{$\backslash$}det M > 0{\$}{\$}detM>0if and only if there exist k pairwise vertex-disjoint paths that connect {\$}{\$}a{\_}i{\$}{\$}aiwith {\$}{\$}b{\_}i{\$}{\$}bi. We also compute the scaling limit at criticality of the probability that there are k parallel and disjoint connections between {\$}{\$}a{\_}i{\$}{\$}aiand {\$}{\$}b{\_}i{\$}{\$}biin the double random current model. Our results are based on a new distributional relation between double random currents and random alternating flows of Talaska {$[$}37{$]$}.},
