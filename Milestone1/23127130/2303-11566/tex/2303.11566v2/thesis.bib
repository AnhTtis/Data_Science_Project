@Article{Du2018,
author="Du, Huiqian
and Liu, Yilin",
title="Minmax-concave total variation denoising",
journal="Signal, Image and Video Processing",
year="2018",
month="Sep",
day="01",
volume="12",
number="6",
pages="1027--1034",
abstract="Total variation (TV) denoising is a commonly used method for recovering 1-D signal or 2-D image from additive white Gaussian noise observation. In this paper, we define the Moreau enhanced function of {\$}{\$}L{\_}1{\$}{\$}L1norm as {\$}{\$}{\{}{\backslash}varPhi {\}}{\_}{\backslash}alpha (x){\$}{\$}$\Phi$$\alpha$(x)and introduce the minmax-concave TV (MCTV) in the form of {\$}{\$}{\{}{\backslash}varPhi {\}}{\_}{\backslash}alpha (Dx){\$}{\$}$\Phi$$\alpha$(Dx), where D is the finite difference operator. We present that MCTV approaches {\$}{\$}{\backslash}Vert Dx{\backslash}Vert {\_}0{\$}{\$}{\textbardbl}Dx{\textbardbl}0if the non-convexity parameter {\$}{\$}{\backslash}alpha {\$}{\$}$\alpha$is chosen properly and apply it to denoising problem. MCTV can strongly induce the signal sparsity in gradient domain, and moreover, its form allows us to develop corresponding fast optimization algorithms. We also prove that although this regularization term is non-convex, the cost function can maintain convexity by specifying {\$}{\$}{\backslash}alpha {\$}{\$}$\alpha$in a proper range. Experimental results demonstrate the effectiveness of MCTV for both 1-D signal and 2-D image denoising.",
issn="1863-1711",
doi="10.1007/s11760-018-1248-2",
url="https://doi.org/10.1007/s11760-018-1248-2"
}

