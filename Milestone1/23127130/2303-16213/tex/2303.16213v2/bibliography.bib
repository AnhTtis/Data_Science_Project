@article{Fu_2014,
	doi = {10.1088/2041-8205/795/2/l39},
	url = {https://doi.org/10.1088/2041-8205/795/2/l39},
	year = 2014,
	month = {oct},
	publisher = {American Astronomical Society},
	volume = {795},
	number = {2},
	pages = {L39},
	author = {Wen Fu and Hui Li and Stephen Lubow and Shengtai Li and Edison Liang},
	title = {{EFFECTS} {OF} {DUST} {FEEDBACK} {ON} {VORTICES} {IN} {PROTOPLANETARY} {DISKS}},
	journal = {The Astrophysical Journal},
	abstract = {We carried out two-dimensional, high-resolution simulations to study the effect of dust feedback on the evolution of vortices induced by massive planets in protoplanetary disks. Various initial dust to gas disk surface density ratios (0.001–0.01) and dust particle sizes (Stokes number 4 × 10−4–0.16) are considered. We found that while dust particles migrate inward, vortices are very effective at collecting them. When dust density becomes comparable to gas density within the vortex, a dynamical instability is excited and it alters the coherent vorticity pattern and destroys the vortex. This dust feedback effect is stronger with a higher initial dust/gas density ratio and larger dust grain. Consequently, we found that the disk vortex lifetime can be reduced up to a factor of 10. We discuss the implications of our findings on the survivability of vortices in protoplanetary disks and planet formation.}
}


