@article{LI2022137225,
title = {One-neutron halo structure of 29Ne},
journal = {Phys. Lett. B},
pages = {137225},
year = {2022},
issn = {0370-2693},
doi = {https://doi.org/10.1016/j.physletb.2022.137225},
url = {https://www.sciencedirect.com/science/article/pii/S0370269322003598},
author = {J. G. Li and N. Michel and H. H. Li and W. Zuo},
keywords = {One-neutron halo, Continuum coupling, Gamow shell model, Neutron rms radius, Overlap function, Spectroscopic factor},
abstract = {We have applied the Gamow shell model to calculate nuclear observables of 26−31Ne isotopes pertaining to one-neutron halo structure, these nuclei being situated close to neutron drip-line. As both many-body correlations and continuum coupling are taken into account in that approach, halo structure can be analyzed properly. Our calculations provide good descriptions of 26−31Ne, where asymptotic behavior is crucial for that matter. One-body density, neutron root-mean-square radii of 26−31Ne, and one-neutron overlap functions of 29,31Ne have been calculated as well. Our results support the presence of a one-neutron p-wave halo in 31Ne, already pointed out experimentally. A similar situation also occurs in the ground state of 29Ne, which is mainly a p-wave valence neutron coupled to the inner 28Ne core. The 3/2+ excited state of 29Ne, which is dominated by a d-wave valence neutron, has also been considered. A larger radius and more extended wave function occur for the ground state of 29Ne when compared to its 3/2+ first excited state. The present results suggest that 29Ne is a good candidate for one-neutron p-wave halo in the medium-mass region.}
}

