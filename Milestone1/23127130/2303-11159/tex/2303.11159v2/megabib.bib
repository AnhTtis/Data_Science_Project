@article{Gruber,
 ISSN = {00358975},
 abstract = {The question raised by Wigner, as to whether the usual commutation relations of quantum mechanics are a consequence of the compatibility of the Lagrange and the Heisenberg equations of motion, is re-considered for the case of the simple harmonic oscillator. It is shown that the compatibility requirement allows somewhat more general commutations than the usual ones, and that the greater generality corresponds exactly to the possibility of quantizing the system according to Bose (the usual), Fermi and para-Fermi quantization.},
 author = {B. Gruber and L. O'Raifeartaigh},
 journal = {Proceedings of the Royal Irish Academy. Section A: Mathematical and Physical Sciences},
 pages = {69--73},
 publisher = {Royal Irish Academy},
 title = {Uniqueness of the Harmonic Oscillator Commutation Relation},
 volume = {63},
 year = {1963}
}


@Article{Boulware1963,
author={Boulware, D. G.
