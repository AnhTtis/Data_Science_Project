@article{contract_duration,
author = {Ravindran, Kiron and Susarla, Anjana and Mani, Deepa and Gurbaxani, Vijay},
title = {Social Capital and Contract Duration in Buyer-Supplier Networks for Information Technology Outsourcing},
journal = {Information Systems Research},
volume = {26},
number = {2},
