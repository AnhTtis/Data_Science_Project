url = {https://dx.doi.org/10.1088/0264-9381/26/13/135002},
year = {2009},
month = {jun},
publisher = {},
volume = {26},
number = {13},
pages = {135002},
author = {Ryuichi Fujita and Wataru Hikida},
title = {Analytical solutions of bound timelike geodesic orbits in Kerr spacetime},
journal = {Classical and Quantum Gravity},
abstract = {We derive the analytical solutions of the bound timelike geodesic orbits in Kerr spacetime. The analytical solutions are expressed in terms of the elliptic integrals using Mino time λ as the independent variable. Mino time decouples the radial and the polar motion of a particle and hence leads to forms more useful to estimate three fundamental frequencies, radial, polar and azimuthal motion, for the bound timelike geodesics in Kerr spacetime. This paper gives the first derivation of the analytical expressions of the fundamental frequencies. This paper also gives the first derivation of the analytical expressions of all coordinates for the bound timelike geodesics using Mino time. These analytical expressions should be useful not only to investigate physical properties of Kerr geodesics but more importantly to applications related to the estimation of gravitational waves from the extreme mass ratio inspirals.}
}

