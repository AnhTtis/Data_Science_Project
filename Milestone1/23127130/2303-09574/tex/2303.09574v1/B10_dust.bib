	langid = {english},
	eprinttype = {arxiv},
	eprint = {2104.08348},
	keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	file = {Booth et al. - 2021 - An inherited complex organic molecule reservoir in.pdf:/Users/sscibelli/Zotero/storage/F96B3VGJ/Booth et al. - 2021 - An inherited complex organic molecule reservoir in.pdf:application/pdf}
