@article{Mei2015,
author = {Liang Mei and Mikkel Brydegaard},
journal = {Opt. Express},
keywords = {Aerosol detection; Lidar; Remote sensing and sensors ; Atmospheric scattering; High power diode lasers; High power lasers; Laser beams; Line scan cameras; Remote sensing},
number = {24},
pages = {A1613--A1628},
publisher = {Optica Publishing Group},
title = {Atmospheric aerosol monitoring by an elastic Scheimpflug lidar system},
