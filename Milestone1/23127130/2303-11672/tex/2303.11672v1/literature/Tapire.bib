keywords = {ISM: jets and outflows,MHD,accretion,accretion discs,magnetic fields,protoplanetary discs},
month = {mar},
pages = {852--868},
title = {{Global evolution of the magnetic field in a thin disc and its consequences for protoplanetary systems}},
