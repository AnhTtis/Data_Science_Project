arxivId = {1402.7102},
author = {Bai, Xue-Ning and Xue-Ning},
doi = {10.1088/0004-637X/791/2/137},
eprint = {1402.7102},
issn = {0004-637X},
journal = {Astrophys. Journal, Vol. 791, Issue 2, Artic. id. 137, 22 pp. (2014).},
keywords = {instabilities,magnetohydrodynamics: MHD,methods: numerical,protoplanetary disks,turbulence},
month = {feb},
title = {{Hall-effect Controlled Gas Dynamics in Protoplanetary Disks: I. Wind Solutions at the Inner Disk}},
