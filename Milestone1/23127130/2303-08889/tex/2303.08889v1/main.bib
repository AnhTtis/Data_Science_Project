	Date-Modified = {2020-01-20 07:02:06 +0400},
	Doi = {10.1007/s11109-019-09533-0},
	Id = {Clayton2019},
	Isbn = {1573-6687},
	Journal = {Political {B}ehavior},
	Title = {Real Solutions for Fake News? {M}easuring the Effectiveness of General Warnings and Fact-Check Tags in Reducing Belief in False Stories on Social Media},
	Ty = {JOUR},
	Url = {https://doi.org/10.1007/s11109-019-09533-0},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.1007/s11109-019-09533-0}
}


