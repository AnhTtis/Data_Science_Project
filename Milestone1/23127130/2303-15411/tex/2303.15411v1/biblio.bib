issn = {13652966},
journal = {MNRAS},
keywords = {Methods: statistical,PASTIS,Planetary systems,Techniques: photometric,techniques: radial velocities},
mendeley-groups = {Papers/2019{\_}TOI{\_}263},
mendeley-tags = {PASTIS},
