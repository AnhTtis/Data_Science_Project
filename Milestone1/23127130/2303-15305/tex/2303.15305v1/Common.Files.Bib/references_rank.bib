	journal = {Statistical Modelling},
	number = {5},
	pages = {449-470},
	title = {Predicting match outcomes in association football using team ratings and player ratings},
	url = {https://doi.org/10.1177/1471082X20929881},
	volume = {21},
	year = {2021},
