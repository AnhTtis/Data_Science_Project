@article{popescu2006entanglement,
abstract = {Statistical mechanics is one of the most successful areas of physics. Yet, almost 150 years since its inception, its foundations and basic postulates are still the subject of debate. Here we suggest that the main postulate of statistical mechanics, the equal a priori probability postulate, should be abandoned as misleading and unnecessary. We argue that it should be replaced by a general canonical principle, whose physical content is fundamentally different from the postulate it replaces: it refers to individual states, rather than to ensemble or time averages. Furthermore, whereas the original postulate is an unprovable assumption, the principle we propose is mathematically proven. The key element in this proof is the quantum entanglement between the system and its environment. Our approach separates the issue of finding the canonical state from finding out how close a system is to it, allowing us to go even beyond the usual boltzmannian situation. {\textcopyright}2006 Nature Publishing Group.},
author = {Popescu, Sandu and Short, Anthony J. and Winter, Andreas},
doi = {10.1038/nphys444},
file = {:Users/shoki/GitHub/Mendeley/2006_Popescu, Short, Winter_Nature Physics_Entanglement and the foundations of statistical mechanics(2).pdf:pdf},
