    eprint = {1509.06411},
    file = {:Users/shoki/GitHub/Mendeley/2016_D'Alessio et al._Advances in Physics_From quantum chaos and eigenstate thermalization to statistical mechanics and thermodynamics.pdf:pdf},
    issn = {0001-8732},
    journal = {Advances in Physics},
    keywords = {Eigenstate thermalization,Review,eigenstate thermalization,few-body ETH,generalized Gibbs ensemble,quantum chaos,quantum quench,quantum statistical mechanics,quantum thermodynamics,random matrix theory},
    mendeley-tags = {Eigenstate thermalization,Review,few-body ETH},
    month = {may},
