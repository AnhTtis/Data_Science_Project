issn = {2160-3308},
journal = {Physical Review X},
keywords = {Eigenstate thermalization,class of operators,condensed matter physics,doi:10.1103/PhysRevX.8.021026 url:https://doi.org/,nonlocal ETH,quantum physics,statistical physics,strong ETH},
mendeley-tags = {Eigenstate thermalization,class of operators,nonlocal ETH,strong ETH},
