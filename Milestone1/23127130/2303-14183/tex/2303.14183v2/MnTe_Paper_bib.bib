@article{Planken01,
author = {Paul C. M. Planken and Han-Kwang Nienhuys and Huib J. Bakker and Tom Wenckebach},
journal = {J. Opt. Soc. Am. B},
keywords = {Harmonic generation and mixing ; Ultrafast nonlinear optics; Spectroscopy, far infrared; Ultrafast processes in condensed matter, including semiconductors; Electric fields; Femtosecond pulses; Polarization; Refractive index; Terahertz detection; Ultrashort pulses},
number = {3},
pages = {313--317},
publisher = {Optica Publishing Group},
title = {Measurement and calculation of the orientation dependence of terahertz pulse detection in ZnTe},
volume = {18},
month = {Mar},
year = {2001},
url = {http://opg.optica.org/josab/abstract.cfm?URI=josab-18-3-313},
doi = {10.1364/JOSAB.18.000313},
abstract = {We have performed measurements and calculations of the efficiency of terahertz (THz) detection in ZnTe as functions of the angle between the THz polarization and the crystal (001) axis of the angle between the THz polarization and the probe-beam polarization. We find that, for angles of 0{\textdegree} and 90{\textdegree} between the probe and THz polarization, the THz detection process is most efficient, whereas the commonly used angle of 45{\textdegree} gives less than optimal results. In addition, we show how the polarization direction of a THz beam can be found if the direction of the crystal (001) axis is known. Our results are valid for ZnTe and other zinc-blende crystals such as GaP.},
}



