@article{eggert_1989,
title = {The trapezoidal rule for analytic functions of rapid decrease},
journal = {Journal of Computational and Applied Mathematics},
volume = {27},
number = {3},
pages = {389-406},
year = {1989},
issn = {0377-0427},
doi = {https://doi.org/10.1016/0377-0427(89)90024-1},
url = {https://www.sciencedirect.com/science/article/pii/0377042789900241},
author = {Norm Eggert and John Lund},
keywords = {Quadrature, trapezoidal rule},
abstract = {The evaluation of the integral of an analytic function f over the entire real line may be accurately approximated by the trapezoidal method of integration or by “mapped” trapezoidal rules. Typically the latter is employed to accelerate the convergence rate of the rule at the expense of a bit more complicated formula. In the case that the integrand f decreases rapidly and has its singularities sufficiently removed from the real line, the trapezoidal rule provides not only a simpler method of evaluation but also a more accurate method of approximation.}
}


@article{poetz_2020,
title = {Perfectly matched layers for {S}chrödinger-type equations with nontrivial energy–momentum dispersion},
journal = {Computer Physics Communications},
volume = {257},
pages = {107503},
year = {2020},
issn = {0010-4655},
doi = {https://doi.org/10.1016/j.cpc.2020.107503},
url = {https://www.sciencedirect.com/science/article/pii/S0010465520302344},
author = {Walter Pötz},
keywords = {Quantum wave equations, Quantum transport, Perfectly matched layers, Group velocity, 
Finite-difference approximation},
abstract = {The construction of perfectly-matched-layer (PML) boundary conditions 
is extended to wave equations with non-trivial energy–momentum dispersion relation. 
Complex coordinate extension is based upon the relative sign between the surface-normal 
component of group velocity and the k-vector. 
This ensures selective damping of propagating and counter-propagating modes at the simulation boundaries. 
Moreover, the technical complexity of the PML method can be inferred 
from the energy–momentum dispersion relation of the underlying wave equation, 
respectively, the finite-difference scheme employed. 
The PML transform is derived for selected Schrödinger-type (i.e. first-order-in-time) 
kinetic transport equations. 
Spectral analysis demonstrates proper damping behaviour of out-propagating modes. 
Results are presented for the Schrödinger equation for particles and holes, 
the s- and p-wave Bogoliubov–de Gennes equation, 
the Dirac equation, and the Dohnal k⋅p model. 
Where available, a comparison to previous PML formulations is made.}
}


