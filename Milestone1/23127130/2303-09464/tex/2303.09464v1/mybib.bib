@article{scrinzi_2014,
title = {On the non-equivalence of {P}erfectly {M}atched {L}ayers and exterior complex scaling},
journal = {Journal of Computational Physics},
volume = {269},
pages = {98-107},
year = {2014},
issn = {0021-9991},
doi = {https://doi.org/10.1016/j.jcp.2014.03.007},
url = {https://www.sciencedirect.com/science/article/pii/S0021999114001806},
author = {A. Scrinzi and H.P. Stimming and N.J. Mauser},
keywords = {Absorbing boundary conditions, Exterior complex scaling, Perfectly matched layers, Schrödinger equation},
abstract = {The perfectly matched layers (PML) and exterior complex scaling (ECS) methods for absorbing boundary conditions 
are analyzed using spectral decomposition. Both methods are derived through analytical continuations from unitary to contractive transformations. 
We find that the methods are mathematically and numerically distinct: ECS is complex stretching that rotates the operator's spectrum into the complex plane, 
whereas PML is a complex gauge transform which shifts the spectrum. Consequently, the schemes differ in their time-stability. Numerical examples are given.}
}


