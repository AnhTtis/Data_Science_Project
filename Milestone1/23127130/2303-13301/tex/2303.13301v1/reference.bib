@article{Yu:2010,
title = {Entanglement evolution in a non-Markovian environment},
journal = {Optics Communications},
volume = {283},
number = {5},
pages = {676-680},
year = {2010},
note = {Quo vadis Quantum Optics?},
issn = {0030-4018},
