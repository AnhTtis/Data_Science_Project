@Article{Ta-Paper,
author ="Wauschkuhn, Nils and Frenzel, Katja and Beckhoff, Burkhard and Hönicke, Philipp",
title  ="Experimental determination of tantalum L-shell fluorescence yields and Coster–Kronig transition probabilities",
journal  ="J. Anal. At. Spectrom.",
