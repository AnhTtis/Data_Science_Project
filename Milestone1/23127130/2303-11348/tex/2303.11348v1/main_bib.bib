	date-added = {2013-10-18 00:16:23 +0000},
	date-modified = {2013-10-18 00:16:52 +0000},
	doi = {10.1088/0004-637X/714/1/663},
	eprint = {0911.3900},
	journal = {\apj},
	keywords = {Galaxy: disk, Galaxy: halo, Galaxy: structure, stars: statistics},
	month = may,
	pages = {663-674},
	primaryclass = {astro-ph.GA},
	title = {{Mapping the Stellar Structure of the Milky Way Thick Disk and Halo Using SEGUE Photometry}},
	volume = 714,
