@article{mandelker2018,
	abstract = {We propose that cold filamentary accretion in massive galaxies at high redshifts can lead to the formation of star-forming clumps in the halos of these galaxies without dark matter substructure. In certain cases, these clumps can be the birthplaces of metal-poor globular clusters (MP GCs). Using cosmological simulations, we show that narrow streams of dense gas feeding massive galaxies from the cosmic web can fragment, producing star-forming clumps. We then derive an analytical model for the properties of streams as a function of halo mass and redshift, and assess when these are gravitationally unstable, when this can lead to collapse and star formation in the halo, and when it may result in the formation of MP GCs. For stream metallicities ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn1.gif] {$\gtrsim 0.01{Z}_{\odot }$} , this is likely to occur at ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn2.gif] {$z\gt 4.5$} . At ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn3.gif] {$z\sim 6$} , the collapsing clouds have masses of ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn4.gif] {$\sim (5\mbox{--}10)\times {10}^{7}\,{M}_{\odot }$} , and the average stream pressure is ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn5.gif] {$\sim {10}^{6}\,{\mathrm{cm}}^{-3}\,{\rm{K}}$} . The conditions for GC formation are met in the extremely turbulent ``eyewall'' at ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn6.gif] {$\sim 0.3{R}_{{\rm{v}}}$} , where counter-rotating streams can collide, driving very large densities. Our scenario can account for the observed kinematics and spatial distribution of MP GCs, the correlation between their mass and metallicity, and the mass ratio between the GC system and the host halo. For MW-mass halos, we infer that ##IMG## [http://ej.iop.org/images/0004-637X/861/2/148/apjaaca98ieqn7.gif] {$\sim 30 \% $} of MP GCs could have formed in this way, with the remainder likely accreted in mergers. Our predictions for GC formation along circumgalactic filaments at high redshift are testable with JWST .},
	author = {Nir Mandelker and Pieter G. van Dokkum and Jean P. Brodie and Frank C. van den Bosch and Daniel Ceverino},
	date-added = {2018-07-17 21:18:58 +0000},
	date-modified = {2018-07-17 21:19:18 +0000},
	journal = {\apj},
	number = {2},
	pages = {148},
	title = {Cold Filamentary Accretion and the Formation of Metal-poor Globular Clusters and Halo Stars},
	url = {http://stacks.iop.org/0004-637X/861/i=2/a=148},
	volume = {861},
	year = {2018},
	bdsk-url-1 = {http://stacks.iop.org/0004-637X/861/i=2/a=148}}

