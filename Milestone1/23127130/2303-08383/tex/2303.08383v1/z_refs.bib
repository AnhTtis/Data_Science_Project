@article {Simon2010,
	author = {Simon, John and Protasenko, Vladimir and Lian, Chuanxin and Xing, Huili and Jena, Debdeep},
	title = {Polarization-Induced Hole Doping in Wide{\textendash}Band-Gap Uniaxial Semiconductor Heterostructures},
	volume = {327},
	number = {5961},
	pages = {60--64},
	year = {2010},
	doi = {10.1126/science.1183226},
	publisher = {American Association for the Advancement of Science},
	abstract = {Impurity-based p-type doping in wide{\textendash}band-gap semiconductors is inefficient at room temperature for applications such as lasers because the positive-charge carriers (holes) have a large thermal activation energy. We demonstrate high-efficiency p-type doping by ionizing acceptor dopants using the built-in electronic polarization in bulk uniaxial semiconductor crystals. Because the mobile hole gases are field-ionized, they are robust to thermal freezeout effects and lead to major improvements in p-type electrical conductivity. The new doping technique results in improved optical emission efficiency in prototype ultraviolet light-emitting{\textendash}diode structures. Polarization-induced doping provides an attractive solution to both p- and n-type doping problems in wide{\textendash}band-gap semiconductors and offers an unconventional path for the development of solid-state deep-ultraviolet optoelectronic devices and wide{\textendash}band-gap bipolar electronic devices of the future.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/327/5961/60},
	journal = {Science}}

