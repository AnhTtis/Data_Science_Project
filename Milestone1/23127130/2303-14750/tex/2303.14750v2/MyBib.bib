@Article{Higham2001,
  author    = {Higham, Desmond J.},
  journal   = {SIAM Review},
  title     = {An Algorithmic Introduction to Numerical Simulation of Stochastic Differential Equations},
  year      = {2001},
  issn      = {1095-7200},
  month     = {Jan},
  number    = {3},
  pages     = {525--546},
  volume    = {43},
  comment   = {This paper gives a basic overview of numerically computing solutions to stochastic differential equations (SDE) using Monte-Carlo simulations and the Euler-Maruyama (EM) integrator. It is a basic introduction to the topic using programs in Matlab to convey concepts of numerical convergence and (linear) stability. The paper discusses these concepts using the EM integrator as its main example.

For linear SDEs, a solution is stable if $\lim \limits_{t \rightarrow \infty} X(t) = 0$. There are two definitions for measuring the linear stability of a system when it comes to the long-term behavior of the integrator as $t \rightarrow \infty$: mean-square and asymptotic stability.

Strong convergence concerns itself with the expected absolute error between a numerical approximation and the exact solution to an SDE at a fixed point in time. An integrator has a strong order of convergence equal to $\gamma$ if there exists a constant $C$ such that $\mathbb{E}|X_n - X(\tau)| \leq C \Delta t^\gamma$ for any fixed $\tau = n \Delta t \in [0, T]$. The EM integrator has strong order of convergence equal to $\frac{1}{2}$.

Weak convergence measures the absolute error between the means of numerical and actual distributions at a fixed point in time. An algorithm has a weak order of convergence equal to $\gamma$ if there exists a constant $C$ such that for all functions $p$, in some class, $\mathbb{E} p(X_n) - \mathbb{E} p(X(\tau)) \leq C \Delta t^\gamma$ at any fixed $\tau = n \Delta t \in [0, T]$. In the paper, $p$ is taken to be the identity function. For this class of functions $p$, EM has a weak order of convergence equal to 1.

In both of these definitions, $\Delta t$ needs to be sufficiently small for the inequality to hold.},
  doi       = {10.1137/s0036144500378302},
  publisher = {Society for Industrial \& Applied Mathematics (SIAM)},
  url       = {http://dx.doi.org/10.1137/S0036144500378302},
}

