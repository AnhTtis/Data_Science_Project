@InProceedings{Denk2001,
  author    = {Denk, Joachim and Schmidt, Gunther},
  booktitle = {Proceedings of IEEE-RAS International Conference on Humanoid Robots},
  title     = {Synthesis of a Walking Primitive Database for a Humanoid Robot using Optimal Control Techniques},
  year      = {2001},
  month     = {November},
  pages     = {319-326},
  comment   = {This paper describes a standard direct collocation method for generating trajectories for bipedal robots assuming full dynamics subject to heel/toe impacts and the stance foot always flat on the ground. In addition to the dynamics, the other constraints applied are no-slip, ZMP always satisfied, impactless collisions, and joint/torque limits. The initial trajectory used to find solutions is given from a statically balanced walking gait with constraints initially relaxed. The constraints are then tightened as solutions are found. Once a solution that satisfies the original constraints are found, a natural parameter continuation is used to find new gaits over a range of step lengths. An unsubstantiated observation made is that ``convergence is greatly enhanced in the vicinity of existing solutions.''},
}

