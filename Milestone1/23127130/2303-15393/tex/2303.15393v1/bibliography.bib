@article{Cugliandolo_2017,
	doi = {10.1088/1742-5468/aa7dfb},
	url = {https://doi.org/10.1088/1742-5468/aa7dfb},
	year = 2017,
	month = {aug},
	publisher = {{IOP} Publishing},
	volume = {2017},
	number = {8},
	pages = {083301},
	author = {Leticia F Cugliandolo and Gustavo S Lozano and Emilio N Nessi},
	title = {Non equilibrium dynamics of isolated disordered systems: the classical Hamiltonian p-spin model},
	journal = {Journal of Statistical Mechanics: Theory and Experiment},
	abstract = {We study the dynamics of a classical disordered macroscopic model completely isolated from the environment reproducing, in a classical setting, the ‘quantum quench’ protocol. We show that, depending on the pre and post quench parameters, the system approaches equilibrium, succeeding to act as a bath on itself, or remains out of equilibrium, in two different ways. In the latter one, the system stays confined in a metastable state in which it undergoes stationary dynamics characterised by a single temperature. In the other, the system ages and its dynamics are characterised by two temperatures associated with observations made at short and long time differences (high and low frequencies). The parameter dependence of the asymptotic states is rationalised in terms of a dynamic phase diagram with one equilibrium and two out of equilibrium phases. Aspects of pre-thermalisation are observed and discussed. Similarities and differences with the dynamics of the dissipative model are also explained.}
}
