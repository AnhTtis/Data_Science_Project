journal = {Epidemiology (Cambridge, Mass.)},
month = {sep},
number = {5},
pages = {751--61},
pmid = {22828661},
publisher = {NIH Public Access},
title = {{Components of the indirect effect in vaccine trials: identification of contagion and infectiousness effects.}},
volume = {23},
year = {2012}
}
