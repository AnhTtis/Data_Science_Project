file = {:Users/georgiapapadogeorgou/Library/Application Support/Mendeley Desktop/Downloaded/Hirano, Imbens, Ridder ' - 2003 - Efficient Estimation of Average Treatment Effect Using the Estimated Propensity Score(2).pdf:pdf;:Users/georgiapapadogeorgou/Documents/Papers/hirano{\_}imbens{\_}ridder{\_}2003.pdf:pdf},
