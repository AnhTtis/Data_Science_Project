@article{Athey2018exact,
abstract = {We study the calculation of exact p-values for a large class of nonsharp null hypotheses about treatment effects in a setting with data from experiments involving members of a single connected network. The class includes null hypotheses that limit the effect of one unit's treatment status on another according to the distance between units, for example, the hypothesis might specify that the treatment status of immediate neighbors has no effect, or that units more than two edges away have no effect. We also consider hypotheses concerning the validity of sparsification of a network (e.g., based on the strength of ties) and hypotheses restricting heterogeneity in peer effects (so that, e.g., only the number or fraction treated among neighboring units matters). Our general approach is to define an artificial experiment, such that the null hypothesis that was not sharp for the original experiment is sharp for the artificial experiment, and such that the ran-domization analysis for the artificial experiment is validated by the design of the original experiment.},
annote = {Randomization tests for testing hypotheses:
1) nodes at distance greater than k do not affect each other
2) nodes with connection having a specific characteristic do not affect each other
3) heterogeneity in the impact of treatment across different neighbors

Quote: "Most of the null hypotheses we consider in this article imply that for a given unit i only a limited set of changes in the treatment actually change the outcome for this unit."
This is related to what I want to do, where I want to check if there is a distance at which treatment does not matter.

What might be useful
- they have a test of whether network G2 is necessary to represent causal directions versus simply a network G1.},
author = {Athey, Susan and Eckles, Dean and Imbens, Guido W},
file = {:Users/georgiapapadogeorgou/Library/Application Support/Mendeley Desktop/Downloaded/Athey, Eckles, Imbens - 2018 - Exact p-Values for Network Interference.pdf:pdf},
journal = {Journal of the American Statistical Association},
number = {521},
pages = {230--240},
title = {{Exact p-Values for Network Interference}},
volume = {113},
year = {2018}
}
@article{Arpino2011,
abstract = {The use of multilevel models for the estimation of the propensity score for data with a hierarchical structure and unobserved cluster-level variables is proposed. This approach is compared with models that ignore the hierarchy, and models in which the hierarchy is represented by a fixed parameter for each cluster. It is shown, by simulation, that simple models with dummy variables outperform both random effect models and models ignoring the hierarchy in terms of balance of cluster-level unobserved covariates and omitted variable bias. The representation of the clusters by fixed or random effects defines a model more general than would be ideal if the relevant cluster-level variables were available. The general conclusion confirms that when conducting propensity score analysis it is safer to specify a more general model than pursuing model parsimony.},
annote = {fixed or random effect propensity score model to adjust for unmeasured cluster-level covariates},
author = {Arpino, Bruno and Mealli, Fabrizia},
file = {:Users/georgiapapadogeorgou/Library/Application Support/Mendeley Desktop/Downloaded/Arpino, Mealli - 2011 - The specification of the propensity score in multilevel observational studies(2).pdf:pdf},
journal = {Computational Statistics and Data Analysis},
pages = {1770--1780},
title = {{The specification of the propensity score in multilevel observational studies}},
url = {www.elsevier.com/locate/csda},
volume = {55},
year = {2011}
}
