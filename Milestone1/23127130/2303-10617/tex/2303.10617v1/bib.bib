	issn = {0033-5606},
	journal = {The Quarterly Journal of Mathematics},
	month = {08},
	number = {1-2},
	pages = {71-114},
	title = {{On the Voevodsky Motive of the Moduli Stack of Vector Bundles on a Curve}},
