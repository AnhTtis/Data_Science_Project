@article{parkinMagneticDomainWallRacetrack2008,
  title = {Magnetic {{Domain-Wall Racetrack Memory}}},
  author = {Parkin, Stuart S. P. and Hayashi, Masamitsu and Thomas, Luc},
  year = {2008},
  month = apr,
  journal = {Science},
  volume = {320},
  number = {5873},
