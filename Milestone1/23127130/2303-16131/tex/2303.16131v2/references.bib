@article{unikandanunniAnisotropicUltrafastSpin2021,
  title = {Anisotropic Ultrafast Spin Dynamics in Epitaxial Cobalt},
  author = {Unikandanunni, Vivek and Medapalli, Rajasekhar and Fullerton, Eric E},
  year = {2021},
  journal = {Applied Physics Letters},
  abstract = {We investigate the ultrafast spin dynamics in an epitaxial hcp(1100) cobalt thin film. By performing pump\textendash probe magneto-optical measurements with the magnetization along either the easy or hard magnetic axis, we determine the demagnetization and recovery time for the two axes. We observe an average of 33\% slower dynamics along the easy magnetization axis, which we attribute to magneto-crystalline anisotropy of the electron\textendash phonon coupling, supported by our ab initio calculations. This points toward an unambiguous and previously undisclosed role of anisotropic electron\textendash lattice coupling in ultrafast magnetism.}
}

