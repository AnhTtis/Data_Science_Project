@article{langAstrometryNetBlind2010,
  title = {Astrometry.{{Net}}: {{Blind Astrometric Calibration}} of {{Arbitrary Astronomical Images}}},
  author = {Lang, D. and Hogg, D. W. and Mierle, K. and Blanton, M. and Roweis, S.},
  year = {2010},
