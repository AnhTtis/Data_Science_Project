@article{fu_josephson_2009,
	title = {Josephson current and noise at a superconductor/quantum-spin-{Hall}-insulator/superconductor junction},
	volume = {79},

	
	abstract = {We study junctions between superconductors mediated by the edge states of a quantum-spin-Hall insulator. We show that such junctions exhibit a fractional Josephson effect, in which the current phase relation has a 4$\pi$ rather than a 2$\pi$ periodicity. This effect is a consequence of the conservation of fermion parity—the number of electron mod 2—in a superconducting junction and is closely related to the Z2 topological structure of the quantum-spin-Hall insulator. Inelastic processes, which violate the conservation of fermion parity, lead to telegraph noise in the equilibrium supercurrent. We predict that the low-frequency noise due these processes diverges exponentially with temperature T as T→0. Possible experiments on HgCdTe quantum wells will be discussed.},
	number = {16},
	
	journal = {Phys. Rev. B},
	author = {Fu, Liang and Kane, C. L.},
	month = apr,
	year = {2009},

	pages = {161408},
	
}

@article{english_observation_2016,
	title = {Observation of nonsinusoidal current-phase relation in graphene {Josephson} junctions},
	volume = {94},
	
	abstract = {The current-phase relation of a Josephson junction can reveal valuable information about the processes influencing the supercurrent. In this paper we present direct measurements of the current-phase relation for Josephson junctions having a graphene barrier, obtained by a phase-sensitive SQUID interferometry technique. We find that the current-phase relation is forward skewed with respect to the commonly observed sinusoidal behavior for short junctions in the quasiballistic transport regime, consistent with predictions for the behavior of Dirac fermions in a Josephson junction. The skewness increases with critical current and decreases sharply with increasing temperature.},
	number = {11},
	
	journal = {Phys. Rev. B},
	author = {English, C. D. and Hamilton, D. R. and Chialvo, C. and Moraru, I. C. and Mason, N. and Van Harlingen, D. J.},
	month = sep,
	year = {2016},
	pages = {115435},
}

@article{wickramasinghe_transport_2018,
