@article{Yuan2022,
author = {Noah F. Q. Yuan  and Liang Fu },
title = {Supercurrent diode effect and finite-momentum superconductors},
journal = {Proceedings of the National Academy of Sciences},
volume = {119},
number = {15},
pages = {e2119548119},
year = {2022},
doi = {10.1073/pnas.2119548119},
URL = {https://www.pnas.org/doi/abs/10.1073/pnas.2119548119},
eprint = {https://www.pnas.org/doi/pdf/10.1073/pnas.2119548119},
abstract = {When both inversion and time-reversal symmetries are broken, the critical current of a superconductor can be nonreciprocal. In this work, we show that, in certain classes of two-dimensional superconductors with antisymmetric spin–orbit coupling, Cooper pairs acquire a finite momentum upon the application of an in-plane magnetic field, and, as a result, critical currents in the direction parallel and antiparallel to the Cooper pair momentum become unequal. This supercurrent diode effect is also manifested in the polarity dependence of in-plane critical fields induced by a supercurrent. These nonreciprocal effects may be found in polar SrTiO3 film, few-layer MoTe2 in the Td phase, and twisted bilayer graphene in which the valley degree of freedom plays a role analogous to spin.}}



@article{Silaev2014,
