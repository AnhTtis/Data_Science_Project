@article{Amet2016,
abstract = {A promising route for creating topological states and excitations is to combine superconductivity and the quantum Hall (QH) effect. Despite this potential, signatures of supercon ductivity in the QH regime remain scarce, and a superconducting current through a QH weak link has been challenging to observe. We demonstrate the existence of a distinct supercurrent mechanism in encapsulated graphene samples contacted by superconducting electrodes, in magnetic fields as high as 2 tesla. The observation of a supercurrent in the QH regime marks an important step in the quest for exotic topological excitations, such as Majorana fermions and parafermions, which may find applications in fault-tolerant quantum computing.},
archivePrefix = {arXiv},
arxivId = {1512.09083},
author = {Amet, F. and Ke, C. T. and Borzenets, I. V. and Wang, J. and Watanabe, K. and Taniguchi, T. and Deacon, R. S. and Yamamoto, M. and Bomze, Y. and Tarucha, S. and Finkelstein, G.},
doi = {10.1126/science.aad6203},
eprint = {1512.09083},
issn = {10959203},
journal = {Science},
title = {{Supercurrent in the quantum Hall regime}},
year = {2016}
}


@article{Dartiailh2021,
abstract = {Josephson junctions hosting Majorana fermions have been predicted to exhibit a 4$\pi$ periodic current phase relation. One experimental consequence of this periodicity is the disappearance of odd steps in Shapiro steps experiments. Experimentally, missing odd Shapiro steps have been observed in a number of materials systems with strong spin-orbit coupling and have been interpreted in the context of topological superconductivity. Here we report on missing odd steps in topologically trivial Josephson junctions fabricated on InAs quantum wells. We ascribe our observations to the high transparency of our junctions allowing Landau-Zener transitions. The probability of these processes is shown to be independent of the drive frequency. We analyze our results using a bi-modal transparency distribution which demonstrates that only few modes carrying 4$\pi$ periodic current are sufficient to describe the disappearance of odd steps. Our findings highlight the elaborate circumstances that have to be considered in the investigation of the 4$\pi$ Josephson junctions in relationship to topological superconductivity.},
archivePrefix = {arXiv},
arxivId = {2005.00077},
author = {Dartiailh, Matthieu C. and Cuozzo, Joseph J. and Elfeky, Bassel H. and Mayer, William and Yuan, Joseph and Wickramasinghe, Kaushini S. and Rossi, Enrico and Shabani, Javad},
doi = {10.1038/s41467-020-20382-y},
eprint = {2005.00077},
issn = {20411723},
journal = {Nature Communications},
mendeley-groups = {InAs-NbTiN{\_}QH},
pmid = {33397966},
title = {{Missing Shapiro steps in topologically trivial Josephson junction on InAs quantum well}},
year = {2021}
}

