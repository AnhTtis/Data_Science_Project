@article{Gamayun2017,
abstract = {We study electric transport along an integer quantum Hall edge where the proximity effect is induced due to a coupling to a superconductor. Such an edge exhibits two Majorana-Weyl fermions with different group velocities set by the induced superconducting pairing. We show that this structure of the spectrum results in interference fringes that can be observed in both the two-terminal conductance and shot noise. We develop a complete analytical theory of such fringes for an arbitrary smooth profile of the induced pairing.},
