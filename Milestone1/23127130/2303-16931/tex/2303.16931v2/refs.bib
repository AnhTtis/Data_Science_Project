author = {Sun, Y. and Kyrychenko, F. V. and Sanders, G. D. and Stanton, C. J. and Khodaparast, G. A. and Kono, J. and Matsuda, Y. H. and Munekata, H.},
title = {Probe of the Band Structure of MBE Grown p-Type InMnAs at Ultrahigh Magnetic Fields},
