@article{Wiedenmann2016a,
  author =        {Wiedenmann, Jonas and Bocquillon, Erwann and
                   Deacon, Russell S. and Hartinger, Simon and
                   Herrmann, Oliver and Klapwijk, Teun M. and
                   Maier, Luis and Ames, Christopher and
                   Br{\"{u}}ne, Christoph and Gould, Charles and
