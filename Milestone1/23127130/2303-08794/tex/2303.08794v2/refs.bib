@article{KIRK1999663,
title = {Twisted knot polynomials: inversion, mutation and concordance},
journal = {Topology},
volume = {38},
number = {3},
pages = {663-671},
year = {1999},
issn = {0040-9383},
doi = {https://doi.org/10.1016/S0040-9383(98)00040-8},
url = {https://www.sciencedirect.com/science/article/pii/S0040938398000408},
author = {Paul Kirk and Charles Livingston},
abstract = {The twisted Alexander polynomial of a knot is applied in three areas of knot theory: invertibility of knots, mutation, and concordance. Three examples are used to illustrate the utility of this invariant. First, a simple proof that the knot 817 is non-invertible is given. It is then proved that 817 is not even concordant to its inverse. Finally, the twisted polynomial is shown to distinguish the concordance class of the pretzel knot P(−3,5,7,2) from that of its positive mutant, P(5,−3,7,2). This last example completes the solution to problem 1.53 of Kirby (1984, 1997) asking for a relation between mutation and concordance.}
}

