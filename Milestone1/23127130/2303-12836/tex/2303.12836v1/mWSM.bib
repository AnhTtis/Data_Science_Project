	Id = {Nuckolls2020},
	Isbn = {1476-4687},
	Journal = {Nature},
	Number = {7839},
	Pages = {610--615},
	Title = {Strongly correlated Chern insulators in magic-angle twisted bilayer graphene},
	Ty = {JOUR},
