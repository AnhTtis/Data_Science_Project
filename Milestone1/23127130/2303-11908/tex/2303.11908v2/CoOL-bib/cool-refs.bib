  issn		= {00729752},
  journal	= {Handbook of Clinical Neurology},
  keywords	= {Deep brain stimulation,Motor symptoms,Nonmotor
		  symptoms,Parkinson,Selection criteria,Side-effects,Targets
		  for DBS},
  mendeley-groups={Research/RefLearn/Manuscript Citations,Research/RefLearn},
  pages		= {107--128},
  pmid		= {24112889},
  title		= {{Clinical outcome of deep brain stimulation for
		  Parkinson's disease}},
  volume	= {116},
  year		= {2013}
}

