	number = {6},
	pages = {1407-1456},
	title = {Non-stationary flow of an ideal incompressible liquid},
	url = {https://www.sciencedirect.com/science/article/pii/0041555363902477},
	volume = {3},
	year = {1963},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/0041555363902477},
