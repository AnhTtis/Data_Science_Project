
@misc{NP_footnote,
    title = {Non-deterministic polynomial (\mbox{NP}) time is a formal com-
plexity class of decision (yes or no) problems defined by having solutions whose validity can be checked in poly- nomial time, while finding a solution might take exponential time for some problem instances. \mbox{NP}-complete problems are a subset of \mbox{NP} problems for which, if a general-purpose polynomial-time solving algorithm could be found, it could also be used to solve all other problems in \mbox{NP}.}
}


@misc{SAT_footnote,
    title = {The acronym \mbox{SAT} is shorthand for ``satisfiability problem,''
a type of decision problem over binary strings. \mbox{Circuit SAT} is a problem about deciding if an input exists to a given \mbox{Boolean} circuit for which the output is True. \mbox{Another} prototypical \mbox{SAT} problem class is \mbox{3-SAT}, involving constraints on groups of three \mbox{Boolean} variables.}
}

