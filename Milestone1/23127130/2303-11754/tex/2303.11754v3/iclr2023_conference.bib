@inproceedings{reco_graph,
author = {Fan, Wenqi and Ma, Yao and Li, Qing and He, Yuan and Zhao, Eric and Tang, Jiliang and Yin, Dawei},
title = {Graph Neural Networks for Social Recommendation},
year = {2019},
