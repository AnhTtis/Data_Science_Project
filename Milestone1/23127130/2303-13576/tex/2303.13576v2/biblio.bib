@article{ZIRAKASHVILI201628,
title = {Type IIn supernovae as sources of high energy astrophysical neutrinos},
journal = {Astroparticle Physics},
volume = {78},
pages = {28-34},
year = {2016},
issn = {0927-6505},
doi = {https://doi.org/10.1016/j.astropartphys.2016.02.004},
url = {https://www.sciencedirect.com/science/article/pii/S0927650516300160},
author = {V.N. Zirakashvili and V.S. Ptuskin},
keywords = {Cosmic rays, Supernova, Neutrino},
abstract = {It is shown that high-energy astrophysical neutrinos observed in the IceCube experiment can be produced by protons accelerated in extragalactic Type IIn supernova remnants by shocks propagating in the dense circumstellar medium. The nonlinear diffusive shock acceleration model is used for description of particle acceleration. We calculate the neutrino spectrum produced by an individual Type IIn supernova and the spectrum of neutrino background produced by IIn supernovae in the expanding Universe. We also found that the arrival direction of one Icecube neutrino candidate (track event 47) is at 1.35° from Type IIn supernova 2005bx.}
}

