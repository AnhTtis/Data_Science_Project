title = {{Elementary Andreev processes in a driven superconductor–normal metal contact}}, 
author = {Belzig, Wolfgang and Vanevic, Mihajlo}, 
journal = {Physica E-Low-Dimensional Systems \& Nanostructures}, 
issn = {1386-9477}, 
