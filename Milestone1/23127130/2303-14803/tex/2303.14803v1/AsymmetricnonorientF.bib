  citeulike-linkout-3  = {http://link.aps.org/abstract/PRA/v54/i2/p1098},
  citeulike-linkout-4  = {http://link.aps.org/pdf/PRA/v54/i2/p1098},
  day                  = {16},
  doi                  = {10.1103/physreva.54.1098},
  eprint               = {quant-ph/9512032},
  posted-at            = {2017-01-16 13:59:37},
