@Article{rejutfmask10,
  author    = {Reju, V.G. and Soo Nqee Koh and Ing Yann Soon},
  title     = {Underdetermined Convolutive Blind Source Separation via Time-Frequency Masking},
  doi       = {10.1109/TASL.2009.2024380},
  issn      = {1558-7916},
  number    = {1},
  pages     = {101-116},
  volume    = {18},
  abstract  = {In this paper, we consider the problem of separation of unknown number of sources from their underdetermined convolutive mixtures via time-frequency (TF) masking. We propose two algorithms, one for the estimation of the masks which are to be applied to the mixture in the TF domain for the separation of signals in the frequency domain, and the other for solving the permutation problem. The algorithm for mask estimation is based on the concept of angles in complex vector space. Unlike the previously reported methods, the algorithm does not require any estimation of the mixing matrix or the source positions for mask estimation. The algorithm clusters the mixture samples in the TF domain based on the Hermitian angle between the sample vector and a reference vector using the well known k -means or fuzzy c -means clustering algorithms. The membership functions so obtained from the clustering algorithms are directly used as the masks. The algorithm for solving the permutation problem clusters the estimated masks by using k-means clustering of small groups of nearby masks with overlap. The effectiveness of the algorithm in separating the sources, including collinear sources, from their underdetermined convolutive mixtures obtained in a real room environment, is demonstrated.},
  journal   = {Audio, Speech, and Language Processing, IEEE Transactions on},
  keywords  = {blind source separation, convolution, fuzzy set theory, pattern clustering, signal sampling, speech intelligibility, speech processing, time-frequency analysisHermitian angle, collinear sources, frequency domain, fuzzy c -means clustering, k -means clustering, mask estimation, membership function, permutation problem, reference vector, sample vector, signal separation, time-frequency masking, underdetermined convolutive blind source separation},
  month     = {Jan.},
  owner     = {xxl},
  timestamp = {2010.01.18},
  year      = {2010},
}

