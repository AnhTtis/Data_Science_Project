@Article{Christensen2014198,
  author    = {Ole Christensen and Say Song Goh},
  title     = {From dual pairs of {G}abor frames to dual pairs of wavelet frames and vice versa},
  doi       = {http://dx.doi.org/10.1016/j.acha.2013.03.005},
  issn      = {1063-5203},
  number    = {2},
  pages     = {198 - 214},
  url       = {http://www.sciencedirect.com/science/article/pii/S1063520313000262},
  volume    = {36},
  abstract  = {Abstract We discuss an elementary procedure that allows us to construct dual pairs of wavelet frames based on certain dual pairs of Gabor frames and vice versa. The construction preserves tightness of the involved frames. Starting with Gabor frames generated by characteristic functions the construction leads to a class of tight wavelet frames that include the Shannon (orthonormal) wavelet, and applying the construction to Gabor frames generated by certain exponential B-splines yields wavelet frames generated by functions whose Fourier transforms are compactly supported splines with geometrically distributed knot sequences. On the other hand, the pendant of the Meyer wavelet turns out to be a tight Gabor frame generated by a C a ( R ) function with compact support. As an application of our results we show that for each given pair of bandlimited dual wavelet frames it is possible to construct dual wavelet frames for any desired scaling and translation parameters.},
  journal   = {Applied and Computational Harmonic Analysis},
  keywords  = {Gabor frames},
  owner     = {xxl},
  timestamp = {2014.07.15},
  year      = {2014},
}

