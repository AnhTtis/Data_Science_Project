@misc{CalcProc,
note = "For both,  type--I and type--II  structural models,  we have
used an energy cutoff of 12~Ry for the plane wave expansion, and a 
4$\times$6 surface unit cell, with  8 ML of Si plus 5/6 ML of
(topmost) Bi, and 1 ML of H at the botton layer of the slab (260 atoms
per  supercell).   For this  supercell  we  used  a vacuum  region  of
11.74~\AA. Again,  we have  used the same  supercell, for  type--I and
type--II models, in order to keep the same convergence criterion."}

@misc{BiBulk, 
note = "$\mu^{\rm  bulk}_{Bi}$  was   calculated  by
considering a rhombohedral structure, where the atomic positions of Bi
atoms and the unit cell volume were minimized with respect to the total
energy. The Brillouin zone sampling was performed using up to 
28 special {\bf k}-points, and energy cutoff of 8 and 12 Ry for the
plane wave expansion"}

@misc{InBulk,
note = "$\mu^{\rm  Bulk}_{\rm In}$ was calculated by considering a
tetragonal unit cell. The atomic positions and the unit cell volume were
fully relaxed. The valence electrons were described using the same
DZP basis set as used for InP$NW$ system, and the Brillouin zone sampling 
was performed by using 24 special {\bf k} points."}

@misc{EShiftSiesta,
note = "Within the SIESTA code, the cutoff radius of the basis set
                  (pseudoatomic orbitals) can be tuned by a single
                  parameter, {\it energy shift}. For lower {\it energy
                  shift} we have larger cutoff radii for the atomic
                  orbitals, that is, the basis set has been
                  improved. In the present work we have considered an
                  energy shift of 0.10~eV to determine the radius
                  cutoff of the pseudoatomic orbitals. Here we
                  verify the convergence of our total energy results
                  for an  {\it energy shift} of 0.05~eV."}

@misc{convergencia,
note = "We verify the convergence of our results with respect to
the size of the basis set and the number of special k-points to the
sampling of the Brillouin zone. We find that  our total energy  
results converged by  5~meV/unit cell."}


@misc{MagModel,
                  note = "Here we are considering only the orbital
                  couplings between the Mn$_{\rm Zn}$ 3d states, since
                  there is no net energy gain upon the interaction
                  between the Mn 3d and the host $e^\prime$ states,
                  composed by O 2p orbitals, of the valence band
                  maximum."}


@misc{lcaoXpw,
note = "Using plane wave basis set (energy cutoff of 25~Ry) to
                  describe the Kohn-Sham wave functions, ultra-soft
                  Vanderbilt pseudopotentials to describe the
                  electron-ion interactions, and the GGA-PBE
                  functional to calculate the exchange-correlation
                  energy, we find that the adsorption energy of the
                  $array$ structure is higher by 44~emeV, in
                  comparison with the $E1$-F configuration."}

@misc{StrainEnergy,
  note =	 "The surface strain energy ($E^s$) is defined as,
                  $E^s=E^{strain}- E^{relax}$.  The latter represents
                  the total energy of the relaxed system, pristine
                  GNR, and $E^{strain}$ represents the total energy of
                  the strained system.  This term was calculated
                  keeping the C atoms at the equilibrium geometry as
                  that obtained for the Au adsorbed system. The
                  adsorbate (Au adatom) should be excluded in
                  $E^{strain}$."}

@misc{corrug,
note = "The a-SiO$_2$ surface  corrugations was inferred by the calculation 
of the variance of vertical positions of the topmost  O atoms, while
for the adsorbed graphene sheet, we considered the variance of vertical 
positions of the graphene C atoms."} 

@misc{corrug2,
note = "The HfO$_2$ surface  corrugations was inferred by the calculation 
of the root mean square deviation of vertical positions of the topmost  O
atoms."} 

@misc{stretch-pwscf,
note = "In order to verify the accuracy of our results with respect to the basis
set,  we calculated the total energy difference, $\rm E_{AFM} - E_{FM}$, by
using plane waves (energy cutoff of 30~Ry) to expand the KS orbitals. In this
case, we find $\rm E_{AFM} - E_{FM}$ = $-$0.02 and 0.21~eV for $l/l_0$=1.025 and
1.15, respectively"}

@misc{IntPdos,
note = "The net electronic charge transfers were calculated by comparing the
integrated (occupied) density of states, projected onto the carbon atoms of the
isolated and adsorbed graphene sheet or graphene nanoribbons."}


@misc{MD,
note = "  Initially we
obtained the  a-SiO$_2$ bulk phase. We  have used a  supercell with 96
atoms and  we made a suitable  choice of the lattice  constants of the
supercell,  12.47  and  12.96~\AA\  along  the  x  and  y  directions,
respectively,  in  order  to  match with  the  calculated  equilibrium
lattice     constant     of     an     isolated     graphene     sheet
(Fig.~\ref{structure}). While the supercell volume was minimized along
the z direction, thus, avoiding any artificial strain in the a-SiO$_2$
bulk.  We  next include a  vacuum region of $\sim$12~\AA\  parallel to
the z  direction, within the  slab method, and  then we repeat  the MD
simulations  in   order  to  get  an  a-SiO$_2$   clean  surface.   In
Ref.~\cite{scopelPRB2008}  we   present  details  on   the  generation
procedure of amorphous SiO$_2$ bulk structure. Once we obtained a well
described a-SiO$_2$  surface, we start to  investigate the equilibrium
geometry,  energetic stability,  and  the electronic  properties of  a
graphene sheet adsorbed onto  a-SiO$_2$ surface.  The  electron-ion  interactions
were         calculated        by         using        norm-conserving
pseudopotentials~\cite{pseudoTM}.   All the  atomic  positions of  the
G/a-SiO$_2$  system  were relaxed  by  using  the conjugated  gradient
scheme, within a force convergence criterion of 20~meV/\AA."}

@misc{ldau-1,
note = "For the ZnO bulk phase, the energy bandgap increases by 0.55~eV, 
0.66  $\rightarrow$1.17~eV, within the DFT+$U$ calculation."}

@misc{ldau-2,
note = "Within the LSDA approach, we find metallic
spin-down channels for the FM Co doped ZnO nanoribbon, however,  
by inclusion of Hubbard $U$ for the Zn and Co 3d orbitals, DFT+$U$ 
calculations, we verify that those  partially occupied metallic 
states are supressed."}

@misc{FrozQL,
note = "The calculations were performed by considering the total energy
as a function of the distamiscnce between two isolated QLs. The atomic positions
were not allowed to relax."}

@misc{FormEnergy,
note = "Hafnium and oxygen chemical potentials,  $\rm\mu_{Hf}$ and $\rm\mu_{O}$ , 
respectively, present an energy range, 
 where the upper limit are given by 
$\rm\mu_{[Hf]}$ and $\rm\mu_{[O_2]}$, that is, the total energies of Hf bulk 
phase and isolated O$_2$ molecule.  The lower limit for the oxygen chemical potential,
$\mu_{\rm O}^{\rm min}$, is determined  by the enthalpy of formation of HfO$_2$ 
($\Delta H_{\rm [HfO_2]}$),  $\mu_{\rm O}^{\rm min} = \Delta H_{[\rm HfO_2]}/2 + \mu_{[\rm O_2]}$." }

@misc{cohesive-energy,
note = "For the cohesive energy we calculated the total energy of the
  bulk most common phase per atom and subtracted from the total energy of single
  atom, all with the inclusion of spin-orbit interaction. The Kohn Sham
  orbitals in all calculation were expanded in a plane wave basis set with an
  energy cutoff of 270 eV. We obtained the following cohesive energies (in eV/atom), 
Cr:4.03; Mn:3.73; Fe:4.87; Co:5.27;
  Ni:4.46; Bi:2.16; Se:2.58. All in excellent agreement with both experiment
  and theory."}

@misc{vdW,
note = "The vdW interaction was described  following  the semiempirical Grimme formula 
[S. Grimme, J. Comput. Chem. {\bf 27}, 1787 (2006)], and a plane wave basis set
to expand the Kohn-Sham orbitals. "}

@misc{vdW2,
note = "In order to preserve the vertical distance of 3.40~\AA,  
only the coordinates of the Li  adatom, the first (S), and the second (Mo) atomic 
layers of the MoS$_2$/Li/G  interface were allowed to relax. The atomic 
positions of the (graphene) C atoms, and the third (S) layer were not 
allowed to relax. "}

@misc{vdW-spin,
note = "The PDOS and the spin-polarized calculations were performed within the 
GGA approach, keeping the equilibrium geometries obtained from the vdW-DF
calculations."}

@misc{spadafora,
note = "E. J. Spadafora and J. Berger and P. Mutombo and M. Telychko and 
M. Svec and Z. Majzik and A. B. McLean and P. Jel\'inek, to be published."}

@misc{boltzmann,
note = "The relative distribution of defects ($c_i$) can be written as, 
$c_i = e^{\rm E[B_{\it i}]}/\sum_j e^{E[B_{\it j}]}$, where 
$\rm E_{\it i}$ and $\rm E_{\it j}$ represents the total energies 
of the defects {\it i} and {\it j}."}"}

@misc{stm-dft,
note = "It is worth noting that, within our theoretical approach (GGA-DFT), the
energy bandgap is underestimated, and thus, experimentally	 it is
expected larger values of bias voltage in order to compare with the theoretical
STM simulations."}

@misc{wangRCS2013Comment,
note = "We believe that such disagreement can be attributed to the different
approach used to describe the vdW interactio. In Ref.\,\cite{wangRCS2013} the
author used the (semiempirical) Grimme method\,\cite{grimmeJCompChem2006},
while in the present work we have used a self-consistent approach as described
in Refs.\,\cite{dionPRL2004,thonhauserPRB2007,perezPRL2009}. Indeed, by
comparing the binding energy of an isolated graphene bilayer (AB stacking), 
they find 120\,meV/C-atom, whereas we obtained 46.3\,meV/C-atom."}

@misc{MnGB,
note = "We have used a energy cutoff of 35~Ry for the plane wave basis set, and 
280~Ry to describe the total charge density. The Brillouin zone was sampled by 
using a grid  of 1$\times$10$\times$1."}

@misc{comment1,
note = "We are aware that, due to the periodic boundary condition, the TMs 
adsorbed on graphene present a ferromagnetic phase."}

@misc{beyond,
note = "We are aware that, in order to present a complete picture of the 
molecular diffusion on the surface,  it is necessary to calculate the transition 
states of CuPc/SiB; which beyond the scope of the present study."}

@misc{supercell,
note= "In order to maintain the periodic boundary condition, within our 
supercell approach, we have considered a surface unit cell with ortogonal 
vectors ($\beta = 90^\circ$) {\bf a} = 6.70\,\AA\ and {\bf b} = 
11.71\,\AA, where  {\bf b} is rotated by 30$^\circ$ with respect to the [$11\bar 
2$] direction. In the experimental work\,\cite{wagnerPRL2013}, the authors 
measured lattice vectors of 6.7 and 12.3\,\AA\ ({\bf a} and {\bf b}), with 
$\beta = 92^\circ$ and  {\bf b} rotated by 27$^\circ$ with respect to the 
[$11\bar 2$] direction."}

@misc{fe-o,
note = " As shown in Fig. 4(f), Fe[O]/GB(5-8) presents metallic bands 
(composed by Fe-4s and -4d$_{\rm xy}$ orbitals) for both 
spin-channels. The net magnetic moment in Ru[O]/GB(5-8) is (mostly)  due to the 
unoccupied spin-down 4d$_{\rm xy}$ and 4d$_{\rm xz}$ orbitals at $\rm E_F  + 
1$\,eV, Fig. 4(g)."}

@misc{neb2,
note = "In order to verify the accuracy of our energy barrier results, we have 
calculated the Fe[P] $\rightarrow$ Fe[O] energy barrier by 
increasing the number of images, namely 5 and 7 images; where we find 
$\rm E^{barr}$ of 
0.37\,eV for both cases."}

@misc{lattice,
note = "At the equilibrium geometry, the MoS$_2$ and MoSe$_2$ MLs present  
lattice constants of 3.19, and 3.32\,\AA;  less than 1\% larger than the 
experimental measurements."}

@misc{dimers,
note = "The TM[]"}

@misc{EnergyLevelsTMDC,
note = "For single layers of MoS$_2$ (MoSe$_2$) we find  ionization-potential 
(IP) and electron-affinity (EA)  of 5.94 (4.26)\,eV and  5.33 (3.89)\,eV, 
respectively, calculated  within our DFT-PBE approach. For "},

@misc{g-mos2,
note = "Graphene on pristine MoS$_2$ presents practically the 
same value of binding energy, 0.05\,eV/C-atom, and graphene--MoS$_2$ 
equilibrium vertical distance, 3.65\,\AA."}

@misc{hole,
note = "Hole density ($\eta$) is defined as the ratio between the number of 
hexagon holes and the total number of atoms in the original (pristine) 
triangular lattice\,\cite{tangPRL2007}."}

@misc{tilted,
note = "The lateral distance $d$ in (CuPc)$_t$/Au is constrained  by the 
periodic boundary condition, within our supercell approach, in order to describe 
the Au(111) surface. Here, the lateral distance $d$=5.97\,\AA\, is larger 
compared with  the experimental findings.\cite{bergerJMatSci2000}  Meanwhile, 
by removing such a constraint, we find $d$=4.80, and $h$=3.16\,\AA, giving rise 
to a tilt angle of 49$^\circ$, which are in good agreement with the 
experimental measurements of   the $\beta$ phase of CuPc 
crystals.\cite{bergerJMatSci2000}"}

@misc{CohesionEnergy,
note = "$E^c$ is defined as the 
total energy difference between the borophene sheet or superlattice and the 
isolated B 
atoms."}


