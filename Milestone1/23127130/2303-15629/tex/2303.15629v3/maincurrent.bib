@article{Balmaceda2005,
abstract = {This article analyzes firm and worker?s incentives to invest in general and specific training when these are separable in the production technology and wages are determined by the outside?option principle. It is shown that firms pay for general training, while workers receive the full return on it, and firms and workers share both the costs and benefits of specific training. The case of delayed general training is also studied. When general training is delayed, it is shown that the strategic complementarity between specific and general training increases the worker?s incentives to invest in specific training.},
annote = {studies firms' incentives to invest in general and specific human capital, and workers' incentives to invest in general human capital when these are technologically independent and wages are determined by Binmore, Rubinstein, & Wolinsky (1986) bargaining game with outside instead of inside options.

He shows that firms invest in general skills despite the fact that the market fully rewards general skills since bargaining with outside options makes general and specific skills strategic complements. Furthermore, workers also invest in human capital but at the sub-optimal level since with positive probability they are held-up by the employer. Firms and workers share the return and cost of specific training, while workers receive the full return to general training due to Bertrand-like labor market competition},
author = {Balmaceda, Felipe},
doi = {10.1086/425435},
issn = {0734-306X},
journal = {Journal of Labor Economics},
mendeley-groups = {Labor and Productivity/Hold up Problem,Labor and Productivity/General vs. Specific},
month = {jan},
number = {1},
pages = {115--133},
publisher = {The University of Chicago Press},
title = {{Firm‐Sponsored General Training}},
url = {https://doi.org/10.1086/425435},
volume = {23},
year = {2005}
}


