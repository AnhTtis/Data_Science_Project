@article{Borge-Holthoefer2017,
abstract = {Online social networks have transformed the way in which humans communicate and interact, leading to a new information ecosystem where people send and receive information through multiple channels, including traditional communication media. Despite many attempts to characterize the structure and dynamics of these techno-social systems, little is known about fundamental aspects such as how collective attention arises and what determines the information life-cycle. Current approaches to these problems either focus on human temporal dynamics or on semiotic dynamics. In addition, as recently shown, information ecosystems are highly competitive, with humans and memes striving for scarce resources –visibility and attention, respectively. Inspired by similar problems in ecology, here we develop a methodology that allows to cast all the previous aspects into a compact framework and to characterize, using microblogging data, information-driven systems as mutualistic networks. Our results show that collective attention around a topic is reached when the user-meme network self-adapts from a modular to a nested structure, which ultimately allows minimizing competition and attaining consensus. Beyond a sociological interpretation, we explore such resemblance to natural mutualistic communities via well-known dynamics of ecological systems.},
author = {Borge-Holthoefer, Javier and Ba{\~{n}}os, Raquel A and Gracia-L{\'{a}}zaro, Carlos and Moreno, Yamir},
doi = {10.1038/srep41673},
issn = {2045-2322},
journal = {Scientific Reports},
mendeley-groups = {Complex Systems/Nestedness,Complex Systems/Nestedness/Nestedness vs. Modularity},
number = {1},
pages = {41673},
title = {{Emergence of consensus as a modular-to-nested transition in communication dynamics}},
url = {https://doi.org/10.1038/srep41673},
volume = {7},
year = {2017}
}


