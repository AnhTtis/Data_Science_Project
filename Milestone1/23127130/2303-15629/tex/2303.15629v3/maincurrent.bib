@article{Stantcheva2017,
abstract = {This paper derives optimal income tax and human capital policies in a life cycle model with risky human capital. The government faces asymmetric information regarding agents? ability, its evolution, and labor supply. When the wage elasticity with respect to ability is increasing in human capital, the optimal subsidy involves less than full deductibility of human capital expenses on the tax base and falls with age. Income-contingent loans or a deferred deductibility scheme can implement the optimum. Numerical results suggest that full deductibility of expenses is close to optimal and that simple linear age-dependent policies perform very well.},
annote = {studies a dynamic perfectly-competitive labor market model, in which labor market earnings depend on the workers' human capital and a stochastic, yet persistent, ability term that is not observed by the government. She shows that it is optimal for the government to subsidize human capital expenses, thus counterbalancing distortions on wage and capital income taxation, encouraging labor supply, and providing insurance against adverse draws of productivity in the labor market. Her numerical simulations suggest that simple linear age-dependent policies can come fairly close to the second best, and that full expense deductibility might be close to optimal.},
author = {Stantcheva, Stefanie},
doi = {10.1086/694291},
file = {:Users/mohhoss/Google Drive/Mendeley Articles/2017/Journal of Political Economy/Stantcheva - 2017 - Optimal Taxation and Human Capital Policies over the Life Cycle.pdf:pdf},
