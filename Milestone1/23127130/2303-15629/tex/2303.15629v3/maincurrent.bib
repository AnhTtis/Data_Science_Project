number = {4},
pages = {811--817},
title = {{Nestedness versus modularity in ecological networks: two sides of the same coin?}},
url = {https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-2656.2010.01688.x},
volume = {79},
