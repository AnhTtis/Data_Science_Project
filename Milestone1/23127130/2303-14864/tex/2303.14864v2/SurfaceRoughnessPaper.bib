	journal = {Nature Communications 2021 12:1},
	author = {Leon, Ross C. C. and Yang, Chih Hwan and Hwang, Jason C. C. and Lemyre, Julien Camirand and Tanttu, Tuomo and Huang, Wei and Huang, Jonathan Y. and Hudson, Fay E. and Itoh, Kohei M. and Laucht, Arne and Pioro-Ladrière, Michel and Saraiva, Andre and Dzurak, Andrew S.},
	month = aug,
