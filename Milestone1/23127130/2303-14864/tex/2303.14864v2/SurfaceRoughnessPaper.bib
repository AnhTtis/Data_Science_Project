	author = {Shehata, M. Mohamed El Kordy and Simion, George and Li, Ruoyu and Mohiyaddin, Fahd A. and Wan, Danny and Mongillo, Massimo and Govoreanu, Bogdan and Radu, Iuliana and De Greve, Kristiaan and Van Dorpe, Pol},
