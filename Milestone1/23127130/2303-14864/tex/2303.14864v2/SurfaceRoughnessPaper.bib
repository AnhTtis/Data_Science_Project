@article{Leon2020,
	title = {Coherent spin control of s-, p-, d- and f-electrons in a silicon quantum dot},
	volume = {11},
	issn = {20411723},
	doi = {10.1038/s41467-019-14053-w},
	abstract = {Once the periodic properties of elements were unveiled, chemical behaviour could be understood in terms of the valence of atoms. Ideally, this rationale would extend to quantum dots, and quantum computation could be performed by merely controlling the outer-shell electrons of dot-based qubits. Imperfections in semiconductor materials disrupt this analogy, so real devices seldom display a systematic many-electron arrangement. We demonstrate here an electrostatically confined quantum dot that reveals a well defined shell structure. We observe four shells (31 electrons) with multiplicities given by spin and valley degrees of freedom. Various fillings containing a single valence electron—namely 1, 5, 13 and 25 electrons—are found to be potential qubits. An integrated micromagnet allows us to perform electrically-driven spin resonance (EDSR), leading to faster Rabi rotations and higher fidelity single qubit gates at higher shell states. We investigate the impact of orbital excitations on single qubits as a function of the dot deformation and exploit it for faster qubit control.},
	number = {1},
	urldate = {2020-03-26},
	journal = {Nature Communications},
	author = {Leon, R. C.C. and Yang, C. H. and Hwang, J. C.C. and Lemyre, J. Camirand and Tanttu, T. and Huang, W. and Chan, K. W. and Tan, K. Y. and Hudson, F. E. and Itoh, K. M. and Morello, A. and Laucht, A. and Pioro-Ladrière, M. and Saraiva, A. and Dzurak, A. S.},
	month = dec,
	year = {2020},
	note = {arXiv: 1902.01550
Publisher: Nature Research},
	keywords = {Qubits, Quantum dots},
	pages = {1--7},
	file = {PDF:/Users/jesuscifuentes/Zotero/storage/2RPTXBSA/full-text.pdf:application/pdf},
}

@article{Tagliaferri2018,
	title = {Impact of valley phase and splitting on readout of silicon spin qubits},
	volume = {97},
	issn = {24699969},
	doi = {10.1103/PhysRevB.97.245412},
	abstract = {We investigate the effect of the valley degree of freedom on Pauli-spin blockade readout of spin qubits in silicon. The valley splitting energy sets the singlet-triplet splitting and thereby constrains the detuning range. The valley phase difference controls the relative strength of the intra- and intervalley tunnel couplings, which, in the proposed Pauli-spin blockade readout scheme, couple singlets and polarized triplets, respectively. We find that high conversion fidelity is possible for a wide range of phase differences, while taking into account experimentally observed valley splittings and tunnel couplings. We also show that the control of the valley splitting together with the optimization of the readout detuning can compensate the effect of the valley phase difference. To increase the measurement fidelity and extend the relaxation time we propose a latching protocol that requires a triple quantum dot and exploits weak long-range tunnel coupling. These opportunities are promising for scaling spin qubit systems and improving qubit readout fidelity.},
	number = {24},
	urldate = {2020-03-26},
	journal = {Physical Review B},
	author = {Tagliaferri, M. L.V. and Bavdaz, P. L. and Huang, W. and Dzurak, A. S. and Culcer, D. and Veldhorst, M.},
	month = jun,
	year = {2018},
	note = {arXiv: 1803.01811
Publisher: American Physical Society},
	pages = {245412},
	file = {PDF:/Users/jesuscifuentes/Zotero/storage/FM96Y49N/full-text.pdf:application/pdf},
}

@article{Ferdous2018,
	title = {Interface-induced spin-orbit interaction in silicon quantum dots and prospects for scalability},
	volume = {97},
	issn = {24699969},
	doi = {10.1103/PhysRevB.97.241401},
	abstract = {We identify the presence of monatomic steps at the Si/SiGe or Si/SiO2 interface as a dominant source of variations in the dephasing time of silicon (Si) quantum dot (QD) spin qubits. First, using atomistic tight-binding calculations we show that the g-factors and their Stark shifts undergo variations due to these steps. We compare our theoretical predictions with experiments on QDs at a Si/SiO2 interface, in which we observe significant differences in Stark shifts between QDs in two different samples. We also experimentally observe variations in the g-factors of one-electron and three-electron spin qubits realized in three neighboring QDs on the same sample, at a level consistent with our calculations. The dephasing times of these qubits also vary, most likely due to their varying sensitivity to charge noise, resulting from different interface conditions. More importantly, from our calculations we show that by employing the anisotropic nature of the spin-orbit interaction (SOI) in a Si QD, we can minimize and control these variations. Ultimately, we predict that the dephasing times of the Si QD spin qubits will be anisotropic and can be improved by at least an order of magnitude, by aligning the external dc magnetic field towards specific crystal directions, given other decoherence mechanisms do not dominate over charge noise.},
	number = {24},
	urldate = {2020-03-26},
	journal = {Physical Review B},
	author = {Ferdous, Rifat and Chan, Kok W. and Veldhorst, Menno and Hwang, J. C.C. and Yang, C. H. and Sahasrabudhe, Harshad and Klimeck, Gerhard and Morello, Andrea and Dzurak, Andrew S. and Rahman, Rajib},
	month = jun,
	year = {2018},
	note = {arXiv: 1703.03840
Publisher: American Physical Society},
	pages = {241401},
	file = {PDF:/Users/jesuscifuentes/Zotero/storage/9S2VR6RS/full-text.pdf:application/pdf},
}

@article{Zhao2009,
	title = {On surface roughness scattering-limited mobilities of electrons and holes in biaxially tensile-strained {Si} {MOSFETs}},
	volume = {30},
	issn = {07413106},
	doi = {10.1109/LED.2009.2026661},
	abstract = {In this letter, we examine the impact of biaxial tensile strain on the surface roughness scattering-limited mobility (μsr) of electrons and holes, experimentally. It is found that tensile strain monotonically enhances electron μsr with the increase in the amount of strain but does not enhance hole μsr even when the strain is quite high (in the S1-xGev relaxed layers with 40\% Ge content). This different strain dependence of electron and hole μsr can be explained by a combination of a slight reduction in the roughness height (Δ) of the Si/SiO2 interfaces and a decrease in the correlation length (Λ), induced by tensile strain. The strain-induced Δ reduction is confirmed by direct transmission electron microscopy measurements for the first time. © 2009 IEEE.},
	number = {9},
	urldate = {2020-04-13},
	journal = {IEEE Electron Device Letters},
	author = {Zhao, Yi and Takenaka, Mitsuru and Takagi, Shinichi},
	year = {2009},
	keywords = {Mobility, MOSFETs, Roughness height, Si/SiO2 interface, Strained-Si (s-Si), Surface roughness (SR) scattering},
	pages = {987--989},
	file = {PDF:/Users/jesuscifuentes/Zotero/storage/GK9JGDCZ/full-text.pdf:application/pdf},
}

