	note = {Conference Name: IEEE Transactions on Robotics},
	keywords = {6-D object pose tracking, Computer vision, Pose estimation, Solid modeling, state estimation, Target tracking, Task analysis, Tracking, Training, Uncertainty},
	pages = {1328--1342},
	file = {IEEE Xplore Full Text PDF:C\:\\Users\\25691\\Zotero\\storage\\MG7XEQF3\\Deng et al. - 2021 - PoseRBPF A Rao–Blackwellized Particle Filter for .pdf:application/pdf},
}

@inproceedings{wen_bundletrack_2021,
