@article{hopkinsDustWindResonant2021,
  title = {Dust in the {{Wind}} with {{Resonant Drag Instabilities}}: {{I}}. {{The Dynamics}} of {{Dust-Driven Outflows}} in {{GMCs}} and {{HII Regions}}},
  shorttitle = {Dust in the {{Wind}} with {{Resonant Drag Instabilities}}},
  author = {Hopkins, Philip F. and Rosen, Anna L. and Squire, Jonathan and Panopoulou, Georgia V. and Soliman, Nadine H. and Seligman, Darryl and Steinwandel, Ulrich P.},
