@article{chenL2SobolevSpaceBijectivity2023,
  title = {L2-{{Sobolev}} Space Bijectivity of the Scattering-Inverse Scattering Transforms Related to Defocusing {{Ablowitz}}\textendash{{Ladik}} Systems},
  author = {Chen, Meisen and Fan, Engui and He, Jingsong},
  year = {2023},
  month = jan,
  journal = {Physica D: Nonlinear Phenomena},
  volume = {443},
  pages = {133565},
  issn = {0167-2789},
  doi = {10.1016/j.physd.2022.133565},
  abstract = {In this paper, we establish the l2-Sobolev space bijectivity of the inverse scattering transform related to the defocusing Ablowitz\textendash Ladik system. On the one hand, based on the spectral problem, we establish the reflection coefficient and the corresponding Riemann\textendash Hilbert problem for the direct scattering problem. And we prove that if the potential belongs to l2,k space, then the reflection coefficient belongs to H\texttheta k({$\Sigma$}). On the other hand, based on the Riemann\textendash Hilbert problem, we obtain the corresponding reconstruction formula and recover the potential from the reflection coefficient. Furthermore, we confirm that if the reflection coefficient is in H\texttheta k({$\Sigma$}), then we show that the corresponding potential also belongs to l2,k for the inverse scattering problem. This study also confirms that for the initial-value problem of the defocusing Ablowitz\textendash Ladik system, if the initial potential qn(0) belongs to l2,k and satisfies {$\parallel$}qn{$\parallel\infty<$}1, then the solution qn(t) for t{$\neq$}0 also belongs to l2,k.},
  langid = {english},
  keywords = {-Sobolev space bijectivity,Defocusing Ablowitz–Ladik system,Inverse scattering transform,Riemann–Hilbert problem},
  file = {C\:\\Users\\gavin\\Zotero\\storage\\UBZ5YWPZ\\Chen et al. - 2023 - l2-Sobolev space bijectivity of the scattering-inv.pdf}
}

