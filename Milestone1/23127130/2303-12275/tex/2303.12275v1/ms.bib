@article{Zhang2014,
	Abstract = {Massive stars ( M > 8 M ??? ) typically form in parsec-scale molecular clumps that collapse and fragment, leading to the birth of a cluster of stellar objects. We investigate the role of magnetic fields in this process through dust polarization at 870 ??m obtained with the Submillimeter Array (SMA). The SMA observations reveal polarization at scales of ##IMG## [http://ej.iop.org/icons/Entities/lsim.gif] {lsim} 0.1 pc. The polarization pattern in these objects ranges from ordered hour-glass configurations to more chaotic distributions. By comparing the SMA data with the single dish data at parsec scales, we found that magnetic fields at dense core scales are either aligned within 40$\,^{\circ}$ of or perpendicular to the parsec-scale magnetic fields. This finding indicates that magnetic fields play an important role during the collapse and fragmentation of massive molecular clumps and the formation of dense cores. We further compare magnetic fields in dense cores with the major axis of molecular outflows. Despite a limited number of outflows, we found that the outflow axis appears to be randomly oriented with respect to the magnetic field in the core. This result suggests that at the scale of accretion disks ( ##IMG## [http://ej.iop.org/icons/Entities/lsim.gif] {lsim} 10 3 AU), angular momentum and dynamic interactions possibly due to close binary or multiple systems dominate over magnetic fields. With this unprecedentedly large sample of massive clumps, we argue on a statistical basis that magnetic fields play an important role during the formation of dense cores at spatial scales of 0.01-0.1 pc in the context of massive star and cluster star formation.},
	Author = {Qizhou Zhang and Keping Qiu and Josep M. Girart and Hauyu (Baobab) Liu and Ya-Wen Tang and Patrick M. Koch and Zhi-Yun Li and Eric Keto and Paul T. P. Ho and Ramprasad Rao and Shih-Ping Lai and Tao-Chung Ching and Pau Frau and How-Huan Chen and Hua-Bai Li and Marco Padovani and Sylvain Bontemps and Timea Csengeri and Carmen Ju{\'a}rez},
	Date-Added = {2018-12-28 16:25:30 -0300},
	Date-Modified = {2018-12-28 16:25:30 -0300},
	Journal = {The Astrophysical Journal},
	Number = {2},
	Pages = {116},
	Title = {Magnetic Fields and Massive Star Formation},
	Url = {http://stacks.iop.org/0004-637X/792/i=2/a=116},
	Volume = {792},
	Year = {2014},
	Bdsk-Url-1 = {http://stacks.iop.org/0004-637X/792/i=2/a=116}}

