	Journal = {The Astrophysical Journal},
	Number = {2},
	Pages = {1792},
	Title = {The Allen Telescope Array Pi GHz Sky Survey. I. Survey Description and Static Catalog Results for the Bo{\"o}tes Field},
	Url = {http://stacks.iop.org/0004-637X/725/i=2/a=1792},
