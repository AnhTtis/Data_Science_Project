@incollection{angristhullwalters2023,
title = {Chapter 1 - Methods for measuring school effectiveness},
editor = {Eric A. Hanushek and Stephen Machin and Ludger Woessmann},
series = {Handbook of the Economics of Education},
publisher = {Elsevier},
volume = {7},
pages = {1-60},
