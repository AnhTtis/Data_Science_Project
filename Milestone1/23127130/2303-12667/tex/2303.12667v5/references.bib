@article{das2005,
title = {Instrumental variables estimators of nonparametric models with discrete endogenous regressors},
journal = {Journal of Econometrics},
volume = {124},
number = {2},
pages = {335-361},
year = {2005},
issn = {0304-4076},
doi = {https://doi.org/10.1016/j.jeconom.2004.02.001},
url = {https://www.sciencedirect.com/science/article/pii/S0304407604000363},
author = {M. Das},
keywords = {Nonparametric estimation, Instrumental variables, Series estimation, Conditional local average treatment effects, Binary endogenous, Discrete endogenous regressors},
abstract = {This paper discusses estimation of nonparametric models whose regressor vectors consist of a vector of exogenous variables and a univariate discrete endogenous regressor with finite support. Both identification and estimators are derived from a transform of the model that evaluates the nonparametric structural function via indicator functions in the support of the discrete regressor. A two-step estimator is proposed where the first step constitutes nonparametric estimation of the instrument and the second step is a nonparametric version of two-stage least squares. Linear functionals of the model are shown to be asymptotically normal, and a consistent estimator of the asymptotic covariance matrix is described. For the binary endogenous regressor case, it is shown that one functional of the model is a conditional (on covariates) local average treatment effect, that permits both unobservable and observable heterogeneity in treatments. Finite sample properties of the estimators from a Monte Carlo simulation study illustrate the practicability of the proposed estimators.}
}
