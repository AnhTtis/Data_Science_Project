year = "2015",
note = "Heterogeneity in Panel Data and in Nonparametric Analysis in honor of Professor Cheng Hsiao",
issn = "0304-4076",
doi = "https://doi.org/10.1016/j.jeconom.2015.03.006",
url = "https://www-sciencedirect-com.biblio-dist.ut-capitole.fr/science/article/pii/S0304407615000755",
author = "Victor Chernozhukov and Iván Fernández-Val and Stefan Hoderlein and Hajo Holzmann and Whitney Newey",
keywords = "Panel data, Nonseparable model, Average effect, Quantile effect, Engel curve",
abstract = "This paper considers identification and estimation of ceteris paribus effects of continuous regressors in nonseparable panel models with time homogeneity. The effects of interest are derivatives of the average and quantile structural functions of the model. We find that these derivatives are identified with two time periods for “stayers”, i.e. for individuals with the same regressor values in two time periods. We show that the identification results carry over to models that allow location and scale time effects. We propose nonparametric series methods and a weighted bootstrap scheme to estimate and make inference on the identified effects. The bootstrap proposed allows inference for function-valued parameters such as quantile effects uniformly over a region of quantile indices and/or regressor values. An empirical application to Engel curve estimation with panel data illustrates the results."
}
@article{aj2003, abstract = {A popular way to account for unobserved heterogeneity is to assume that the data are drawn from a finite mixture distribution. A barrier to using finite mixture models is that parameters that could previously be estimated in stages must now be estimated jointly: using mixture distributions destroys any additive separability of the log-likelihood function. We show, however, that an extension of the EM algorithm reintroduces additive separability, thus allowing one to estimate parameters sequentially during each maximization step. In establishing this result, we develop a broad class of estimators for mixture models. Returning to the likelihood problem, we show that, relative to full information maximum likelihood, our sequential estimator can generate large computational savings with little loss of efficiency.}, author = {Arcidiacono, Peter and Jones, John Bailey}, doi = {https://doi.org/10.1111/1468-0262.00431}, eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/1468-0262.00431}, journal = {Econometrica}, keywords = {Unobserved heterogeneity, mixture distributions, EM algorithm, dynamic discrete choice}, number = {3}, pages = {933-946}, title = {Finite Mixture Distributions, Sequential Likelihood and the EM Algorithm}, url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/1468-0262.00431}, volume = {71}, year = {2003}, Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/1468-0262.00431}, Bdsk-Url-2 = {https://doi.org/10.1111/1468-0262.00431}}
