@article{2015PhRvD..91j2001B,
	title = {Search for {Gamma}-ray {Emission} from {Dark} {Matter} {Annihilation} in the {Large} {Magellanic} {Cloud} with the {Fermi} {Large} {Area} {Telescope}},
