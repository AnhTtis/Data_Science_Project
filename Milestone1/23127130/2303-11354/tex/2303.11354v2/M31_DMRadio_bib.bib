	file = {Sofue and Nakanishi - 2016 - Three-Dimensional Distribution of the ISM in the M.pdf:/Users/mitchellweikert/Zotero/storage/GIKBFA2Z/Sofue and Nakanishi - 2016 - Three-Dimensional Distribution of the ISM in the M.pdf:application/pdf},
