	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	pages = {083009},
	annote = {Comment: 13 pages, 11 figures},
	file = {Hooper et al. - 2013 - Pulsars Cannot Account for the Inner Galaxy's GeV .pdf:/Users/mitchellweikert/Zotero/storage/FXJNWTLX/Hooper et al. - 2013 - Pulsars Cannot Account for the Inner Galaxy's GeV .pdf:application/pdf},
}

