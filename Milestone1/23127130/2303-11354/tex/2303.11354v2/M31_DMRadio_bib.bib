	note = {arXiv: 1306.5725},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	pages = {083521},
	annote = {Comment: 20 pages, 13 figures, v2: typos corrected and references added; v3 several clarification comments added, v4 typos corrected, matches version to appear in PRD, v5: erratum included, with a correction to the explanation of the estimate of the number of millisecond pulsars required, conclusions unchanged},
	file = {Gordon and Macias - 2013 - Dark Matter and Pulsar Model Constraints from Gala.pdf:/Users/mitchellweikert/Zotero/storage/WGDWZ9SK/Gordon and Macias - 2013 - Dark Matter and Pulsar Model Constraints from Gala.pdf:application/pdf},
