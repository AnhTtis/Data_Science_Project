	author = {Yanasak, N. E. and Wiedenbeck, M. E. and Mewaldt, R. A. and Davis, A. J. and Cummings, A. C. and George, J. S. and Leske, R. A. and Stone, E. C. and Christian, E. R. and von Rosenvinge, T. T. and Binns, W. R. and Hink, P. L. and Israel, M. H.},
