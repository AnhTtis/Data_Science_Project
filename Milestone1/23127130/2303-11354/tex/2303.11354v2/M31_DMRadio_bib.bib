}

@article{2016PASJ...68...63S,
	title = {Three-{Dimensional} {Distribution} of the {ISM} in the {Milky} {Way} {Galaxy}: {IV}. {3D} {Molecular} {Fraction} and {Galactic}-{Scale} {HI}-to-{H}\$\_2\$ {Transition}},
