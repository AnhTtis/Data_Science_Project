	doi = {10.1103/PhysRevD.88.023504},
	abstract = {We used radio observations of the neighbour galaxy M31 in order to put constraints on dark matter particle mass and annihilation cross section. Dark matter annihilation in M31 halo produces highly energetic leptons, which emit synchrotron radiation on radio frequencies in the galactic magnetic field. We predicted expected radio fluxes for the two annihilation channels: {\textbackslash}chi{\textbackslash}chi -{\textgreater} bb* and {\textbackslash}chi{\textbackslash}chi -{\textgreater} {\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}-. We then compared them with available data on the central radio emission of M31 as observed by four radio surveys: VLSS (74 MHz), WENSS (325 MHz), NVSS (1400 MHz) and GB6 (4850 MHz). Assuming a standard NFW dark matter density profile and a conservative magnetic field distribution inside the Andromeda galaxy, we find that the thermal relic annihilation cross section {\textless}{\textbackslash}sigma v{\textgreater} = 3*10{\textasciicircum}\{-26\} cm{\textasciicircum}3/s or higher are only allowed for WIMP masses greater than 100 GeV and 55 GeV for annihilation into bb* and {\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}- respectively. Taking into account potential uncertainties in the distributions of DM density and magnetic field, the mentioned WIMP limiting masses can be as low as 23 GeV for both channels, and as high as 280 and 130 GeV for annihilation into bb* and {\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}- respectively. These mass values exceed the best up-to-day known constraints from Fermi gamma observations: 40 GeV and 19 GeV respectively [A.Geringer-Sameth and S.M.Koushiappas, Phys. Rev. Lett. 107, 241303 (2011)]. Precise measurements of the magnetic field in the relevant region and better reconstruction of the DM density profile of M31 will be able to reduce the uncertainties of our exclusion limits.},
	language = {en},
	number = {2},
	urldate = {2020-11-11},
	journal = {Physical Review D},
	author = {Egorov, A. E. and Pierpaoli, E.},
	month = jul,
	year = {2013},
	note = {arXiv: 1304.0517},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {023504},
	annote = {Comment: Submitted into Phys. Rev. D},
	file = {Egorov and Pierpaoli - 2013 - Constraints on dark matter annihilation by radio o.pdf:/Users/mitchellweikert/Zotero/storage/FEWZ6HIG/Egorov and Pierpaoli - 2013 - Constraints on dark matter annihilation by radio o.pdf:application/pdf},
}

@article{2018PhRvD..97j3021M,
	title = {A {Multi}-{Wavelength} {Analysis} of {Annihilating} {Dark} {Matter} as the {Origin} of the {Gamma}-{Ray} {Emission} from {M31}},
	volume = {97},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/1802.05258},
	doi = {10.1103/PhysRevD.97.103021},
	abstract = {[Abridged] Indirect detection of dark matter (DM) by multi-wavelength astronomical observations provides a promising avenue for probing the particle nature of DM. In the case of DM consisting of Weakly-Interacting Massive Particles (WIMPs), self-annihilation ultimately produces observable products including \$e{\textasciicircum}\{{\textbackslash}pm\}\$ pairs and gamma rays. The gamma rays can be detected directly, while the \$e{\textasciicircum}\{{\textbackslash}pm\}\$ pairs can be detected by radio emission from synchrotron radiation or X-rays and soft gamma rays from inverse Compton scattering. An intriguing region to search for astrophysical signs of DM is the Galactic center (GC) of the Milky Way, due in part to an observed excess of gamma-rays that could be DM. A recent observation by the Fermi-LAT collaboration of a similar excess in the central region of the Andromeda galaxy (M31) leads us to explore the possibility of a DM-induced signal there as well. We use the RX-DMFIT tool to perform a multi-frequency analysis of potential DM annihilation emissions in M31. We consider WIMP models consistent with the GC excess and calculate the expected emission across the electromagnetic spectrum in comparison with available observational data from M31. We find that the particle models that best fit the M31 excess favor lower masses than the GC excess. The best fitting models are for a \$b{\textbackslash}bar\{b\}\$ final state with \$M\_\{{\textbackslash}chi\}=11\$ GeV and \${\textbackslash}left{\textless}{\textbackslash}sigma v{\textbackslash}right{\textgreater}=2.6{\textbackslash}times 10{\textasciicircum}\{-26\}\$ cm\${\textasciicircum}3\$s\${\textasciicircum}\{-1\}\$, as well as an evenly mixed \$b{\textbackslash}bar\{b\}/{\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}-\$ final state with \$M\_\{{\textbackslash}chi\}=5.8\$ GeV and \${\textbackslash}left{\textless}{\textbackslash}sigma v{\textbackslash}right{\textgreater}=2.03{\textbackslash}times 10{\textasciicircum}\{-26\}\$ cm\${\textasciicircum}3\$s\${\textasciicircum}\{-1\}\$. For conservative estimates of the diffusion and magnetic field models the expected radio emissions appear to be in tension with currently available data in the central region of M31, although this constraint has a fairly strong dependence on the values chosen for parameters describing the magnetic field strength and geometry.},
	language = {en},
	number = {10},
	urldate = {2020-11-11},
	journal = {Physical Review D},
	author = {McDaniel, Alex and Jeltema, Tesla and Profumo, Stefano},
	month = may,
