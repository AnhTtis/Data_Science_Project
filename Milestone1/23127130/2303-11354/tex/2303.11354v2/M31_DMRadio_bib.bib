@article{2016PDU....12....1D,
	title = {The {Characterization} of the {Gamma}-{Ray} {Signal} from the {Central} {Milky} {Way}: {A} {Compelling} {Case} for {Annihilating} {Dark} {Matter}},
	volume = {12},
	issn = {22126864},
	shorttitle = {The {Characterization} of the {Gamma}-{Ray} {Signal} from the {Central} {Milky} {Way}},
	url = {http://arxiv.org/abs/1402.6703},
	doi = {10.1016/j.dark.2015.12.005},
	abstract = {Past studies have identified a spatially extended excess of \${\textbackslash}sim\$1-3 GeV gamma rays from the region surrounding the Galactic Center, consistent with the emission expected from annihilating dark matter. We revisit and scrutinize this signal with the intention of further constraining its characteristics and origin. By applying cuts to the {\textbackslash}textit\{Fermi\} event parameter CTBCORE, we suppress the tails of the point spread function and generate high resolution gamma-ray maps, enabling us to more easily separate the various gamma-ray components. Within these maps, we find the GeV excess to be robust and highly statistically significant, with a spectrum, angular distribution, and overall normalization that is in good agreement with that predicted by simple annihilating dark matter models. For example, the signal is very well fit by a 36-51 GeV dark matter particle annihilating to \$b{\textbackslash}bar\{b\}\$ with an annihilation cross section of \${\textbackslash}sigma v = (1-3){\textbackslash}times 10{\textasciicircum}\{-26\}\$ cm\${\textasciicircum}3\$/s (normalized to a local dark matter density of 0.4 GeV/cm\${\textasciicircum}3\$). Furthermore, we confirm that the angular distribution of the excess is approximately spherically symmetric and centered around the dynamical center of the Milky Way (within \${\textbackslash}sim\$\$0.05{\textasciicircum}\{{\textbackslash}circ\}\$ of Sgr A\${\textasciicircum}*\$), showing no sign of elongation along the Galactic Plane. The signal is observed to extend to at least \${\textbackslash}simeq10{\textasciicircum}\{{\textbackslash}circ\}\$ from the Galactic Center, disfavoring the possibility that this emission originates from millisecond pulsars.},
	language = {en},
	urldate = {2020-11-11},
	journal = {Physics of the Dark Universe},
	author = {Daylan, Tansu and Finkbeiner, Douglas P. and Hooper, Dan and Linden, Tim and Portillo, Stephen K. N. and Rodd, Nicholas L. and Slatyer, Tracy R.},
	month = jun,
	year = {2016},
	note = {arXiv: 1402.6703},
	keywords = {High Energy Physics - Theory, Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	pages = {1--23},
	annote = {Comment: 30 pages, 34 figures},
	file = {Daylan et al. - 2016 - The Characterization of the Gamma-Ray Signal from .pdf:/Users/mitchellweikert/Zotero/storage/CCPZ4QLR/Daylan et al. - 2016 - The Characterization of the Gamma-Ray Signal from .pdf:application/pdf},
