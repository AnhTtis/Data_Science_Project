	pages = {102001},
	annote = {Comment: 33 pages, 22 figures Version 2: minor corrections and clarifications after journal peer review process},
	file = {Buckley et al. - 2015 - Search for Gamma-ray Emission from Dark Matter Ann.pdf:/Users/mitchellweikert/Zotero/storage/L98FH2I7/Buckley et al. - 2015 - Search for Gamma-ray Emission from Dark Matter Ann.pdf:application/pdf},
