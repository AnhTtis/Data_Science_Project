	url = {https://ui.adsabs.harvard.edu/abs/1972Phy....60..145G},
	doi = {10.1016/0031-8914(72)90227-3},
	abstract = {The stopping power of a plasma for fast electrons and positrons is calculated. First the classical limit is considered where β = ν/ c ≪ α is the fine structure constant. Then the nonrelativistic Born-approximation formulas are derived; this domain corresponds to α ≪ β ≪ 1. Finally the general case of relativistic electrons and positrons is treated; in the relativistic case the scattering cross sections of Møller (electron-electron) and Bhabha (positron-electron) are used in the calculation. In all three energy domains the problem is broken up into cases of small and large momentum transfers ( ℏq). For large q, scattering off individual plasma electrons is considered, while in the limit of very small q for the quantum-mechanical domain, excitation of quantized plasma oscillations contributes to d E/d x; in the classical limit for small q (or large impact parameter), the polarizability of the plasma provides the effective cutoff. The formulas for the stopping power differ slightly from those for a heavy ion going through a plasma because (i) there are exchange effects and (ii) the fast electrons and positrons can lose a large fraction of their energy in one scattering off a plasma electron.},
	urldate = {2022-01-10},
	journal = {Physica},
	author = {Gould, R. J.},
	month = jul,
	year = {1972},
	pages = {145--154},
	file = {Gould - 1972 - Energy loss of fast electrons and positrons in a p.pdf:/Users/mitchellweikert/Zotero/storage/6WE9QKTT/Gould - 1972 - Energy loss of fast electrons and positrons in a p.pdf:application/pdf},
}

