	pages = {68},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/G7TLKJQ7/Vladimirov et al. - 2012 - Testing the Origin of High-energy Cosmic Rays.pdf:application/pdf},
}

@article{1995ApJ...441..209H,
	title = {Propagation of {Galactic} {Cosmic} {Rays} under {Diffusive} {Reacceleration}},
