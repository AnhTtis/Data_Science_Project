@article{boveiaDarkMatterSearches2018,
	title = {Dark {Matter} {Searches} at {Colliders}},
	volume = {68},
	issn = {0163-8998, 1545-4134},
	url = {http://arxiv.org/abs/1810.12238},
	doi = {10.1146/annurev-nucl-101917-021008},
	abstract = {Colliders, among the most successful tools of particle physics, have revealed much about matter. This review describes how colliders contribute to the search for particle dark matter, focusing on the highestenergy collider currently in operation, the Large Hadron Collider (LHC) at CERN. In the absence of hints about the character of interactions between dark matter and standard matter, this review emphasizes what could be observed in the near future, presents the main experimental challenges, and discusses how collider searches ﬁt into the broader ﬁeld of dark matter searches. Finally, it highlights a few areas to watch for the future LHC program.},
	language = {en},
