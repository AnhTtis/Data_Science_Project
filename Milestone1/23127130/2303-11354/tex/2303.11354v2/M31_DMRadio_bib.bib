	keywords = {Astrophysics, Galactic Structure, Interstellar Gas, GALAXIES: INDIVIDUAL MESSIER NUMBER: M31, GALAXIES: STRUCTURE, Spiral Galaxies, Andromeda Galaxy, GALAXIES: INTERNAL MOTIONS, GALAXIES: INTERSTELLAR MATTER, Gas Dynamics, Mass Distribution, Neutral Gases, RADIO SOURCES: 21 CM RADIATION, Spatial Distribution},
	pages = {54},
