	keywords = {High Energy Physics - Phenomenology, Astrophysics - High Energy Astrophysical Phenomena},
	pages = {083005},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/5KQ6JGMH/Génolini et al. - 2021 - New minimal, median, and maximal propagation model.pdf:application/pdf},
}

