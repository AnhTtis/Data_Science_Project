	file = {McDaniel et al. - 2018 - A Multi-Wavelength Analysis of Annihilating Dark M.pdf:/Users/mitchellweikert/Zotero/storage/LQVVRD5S/McDaniel et al. - 2018 - A Multi-Wavelength Analysis of Annihilating Dark M.pdf:application/pdf},
