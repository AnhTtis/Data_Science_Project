	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/68DB3WKU/Groves and Krause - 2012 - Hot & cold dust in M31 the resolved SED of Androm.pdf:application/pdf},
}

@article{2016MNRAS.456.4128R,
	title = {Star formation laws in the {Andromeda} galaxy: gas, stars, metals and the surface density of star formation},
	volume = {456},
	issn = {0035-8711},
	shorttitle = {Star formation laws in the {Andromeda} galaxy},
	url = {https://ui.adsabs.harvard.edu/abs/2016MNRAS.456.4128R},
	doi = {10.1093/mnras/stv2951},
	abstract = {We use hierarchical Bayesian regression analysis to investigate star formation laws in the Andromeda galaxy (M31) in both local (30, 155 and 750 pc) and global cases. We study and compare the well-known Kennicutt-Schmidt law, the extended Schmidt law and the metallicity/star formation correlation. Using a combination of Hα and 24 μm emission, a combination of far-ultraviolet and 24 μm, and the total infrared emission, we estimate the total star formation rate (SFR) in M31 to be between 0.35 ± 0.04 and 0.4 ± 0.04 M⊙ yr-1. We produce a stellar mass surface density map using IRAC 3.6 μm emission and measured the total stellar mass to be 6.9 × 1010 M⊙. For the Kennicutt-Schmidt law in M31, we find the power-law index N to be between 0.49 and 1.18; for all the laws, the power-law index varies more with changing gas tracer than with SFR tracer. The power-law index also changes with distance from the centre of the galaxy. We also applied the commonly used ordinary least-squares fitting method and showed that using different fitting methods leads to different power-law indices. There is a correlation between the surface density of SFR and the stellar mass surface density, which confirms that the Kennicutt-Schmidt law needs to be extended to consider the other physical properties of galaxies. We found a weak correlation between metallicity, the SFR and the stellar mass surface density.},
	urldate = {2022-10-25},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Rahmani, S. and Lianou, S. and Barmby, P.},
	month = mar,
	year = {2016},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: stellar content, galaxies: ISM, galaxies: spiral, galaxies: star formation, methods: observational, methods: statistical},
	pages = {4128--4144},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/K6LX6JKM/Rahmani et al. - 2016 - Star formation laws in the Andromeda galaxy gas, .pdf:application/pdf},
}

@article{2010A&A...517A..77T,
	title = {Relating dust, gas, and the rate of star formation in {M} 31},
	volume = {517},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2010A&A...517A..77T/abstract},
	doi = {10.1051/0004-6361/200913593},
	abstract = {{\textless}BR /{\textgreater} Aims: We investigate the relationships between dust and gas, and study the star formation law in M 31. {\textless}BR /{\textgreater} Methods: We have derived distributions of dust temperature and dust opacity across M 31 at 45'' arcsec resolution using the Spitzer data. With the opacity map and a standard dust model we de-reddened the Hα emission yielding the first Hα map of M 31 corrected for extinction. We compared the emissions from dust, Hα, HI, and H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} by means of radial distributions, pixel-to-pixel correlations, and wavelet cross-correlations. We calculated the star formation rate and star formation efficiency from the de-reddened Hα emission. {\textless}BR /{\textgreater} Results: The dust temperature steeply decreases from 30 K near the center to 15 K at large radii. The mean dust optical depth at the Hα wavelength along the line of sight is about 0.7. The radial decrease in the dust-to-gas ratio is similar to that of the oxygen abundance. Extinction is nearly linearly correlated with the total gas surface density within limited radial intervals. On scales \&lt;2 kpc, cold dust emission is best correlated with that of neutral gas, and warm dust emission with that of ionized gas. The Hα emission is slightly better correlated with emission at 70 μm than at 24 μm. The star formation rate in M 31 is low. In the area 6 kpc \&lt; R \&lt; 17 kpc, the total SFR is ≃0.3 \{M\}{\textless}SUB{\textgreater}⊙{\textless}/SUB{\textgreater} yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}. A linear relationship exists between surface densities of SFR and H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater}. The Kennicutt-Schmidt law between SFR and total gas has a power-law index of 1.30 ± 0.05 in the radial range of R = 7-11 kpc increasing by about 0.3 for R = 11-13 kpc. {\textless}BR /{\textgreater} Conclusions: The better 70 μm-Hα than 24 μm-Hα correlation plus an excess in the 24 μm/70 μm intensity ratio indicates that other sources than dust grains, e.g. those of stellar origin, contribute to the 24 μm emission. The lack of H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} in the central region could be related to the lack of HI and the low opacity/high temperature of the dust. Since neither SFR nor SFE is well correlated with the surface density of H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} or total gas, other factors than gas density must play an important role in the formation of massive stars in M 31. The molecular depletion time scale of 1.1 Gyr indicates that M 31 is about three times less efficient in forming young massive stars than M 33.},
	language = {en},
	urldate = {2022-10-25},
	journal = {Astronomy and Astrophysics},
	author = {Tabatabaei, F. S. and Berkhuijsen, E. M.},
	month = jul,
	year = {2010},
	pages = {A77},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/FKVZAJZX/Tabatabaei and Berkhuijsen - 2010 - Relating dust, gas, and the rate of star formation.pdf:application/pdf;Snapshot:/Users/mitchellweikert/Zotero/storage/V3Q4W24R/abstract.html:text/html},
}

@article{2007ARNPS..57..285S,
