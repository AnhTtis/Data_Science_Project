	author = {Mao, S. A. and McClure-Griffiths, N. M. and Gaensler, B. M. and Haverkorn, M. and Beck, R. and McConnell, D. and Wolleben, M. and Stanimirovic, S. and Dickey, J. M. and Staveley-Smith, L.},
	month = nov,
