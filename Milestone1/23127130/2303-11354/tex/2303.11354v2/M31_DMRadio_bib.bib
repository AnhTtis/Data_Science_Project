}

@article{porterHighEnergyGammaRays2017,
	title = {High-{Energy} {Gamma} {Rays} from the {Milky} {Way}: {Three}-{Dimensional} {Spatial} {Models} for the {Cosmic}-{Ray} and {Radiation} {Field} {Densities} in the {Interstellar} {Medium}},
