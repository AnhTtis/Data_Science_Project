@article{2017A&A...607A.106M,
	abstract = {The detailed distribution and kinematics of the atomic and the CO-bright molecular hydrogen in the disc of the Milky Way inside the solar circle are derived under the assumptions of axisymmetry and pure circular motions. We divide the Galactic disc into a series of rings, and assume that the gas in each ring is described by four parameters: its rotation velocity, velocity dispersion, midplane density and its scale height. We fit these parameters to the Galactic H I and {\textless}SUP{\textgreater}12{\textless}/SUP{\textgreater}CO (J = 1-0) data by producing artificial H I and CO line-profiles and comparing them with the observations. Our approach allows us to fit all parameters to the data simultaneously without assuming a-priori a radial profile for one of the parameters. We present the distribution and kinematics of the H I and H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} in both the approaching (QIV) and the receding (QI) regions of the Galaxy. Our best-fit models reproduces remarkably well the observed H I and CO longitude-velocity diagrams up to a few degrees of distance from the midplane. With the exception of the innermost 2.5 kpc, QI and QIV show very similar kinematics. The rotation curves traced by the H I and H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} follow each other closely, flattening beyond R = 6.5 kpc. Both the H I and the H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} surface densities show a) a deep depression at 0.5 \&lt; R \&lt; 2.5 kpc, analogous to that shown by some nearby barred galaxies, b) local overdensities that can be interpreted in terms of spiral arms or ring-like features in the disc. The H I (H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater}) properties are fairly constant in the region outside the depression, with typical velocity dispersion of 8.9 $\pm$ 1.1 (4.4 $\pm$ 1.2) km s{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}, density of 0.43 $\pm$ 0.11 (0.42 $\pm$ 0.22) cm{\textless}SUP{\textgreater}-3{\textless}/SUP{\textgreater} and HWHM scale height of 202 $\pm$ 28 (64 $\pm$ 12) pc. We also show that the H I opacity in the LAB data can be accounted for by using an "effective" spin temperature of 150 K: assuming an optically thin regime leads to an underestimate of the H I mass by about 30\%.},
	author = {Marasco, A. and Fraternali, F. and van der Hulst, J. M. and Oosterloo, T.},
	doi = {10.1051/0004-6361/201731054},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/VPG6XRC2/Marasco et al. - 2017 - Distribution and kinematics of atomic and molecula.pdf:application/pdf;Snapshot:/Users/mitchellweikert/Zotero/storage/4IZDPJBY/abstract.html:text/html},
	issn = {0004-6361},
	journal = {Astronomy and Astrophysics},
	language = {en},
	month = nov,
	pages = {A106},
	title = {Distribution and kinematics of atomic and molecular gas inside the solar circle},
	url = {https://ui.adsabs.harvard.edu/abs/2017A&A...607A.106M/abstract},
	urldate = {2022-10-21},
	volume = {607},
	year = {2017},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2017A&A...607A.106M/abstract},
	bdsk-url-2 = {https://doi.org/10.1051/0004-6361/201731054}}

@article{2012MNRAS.426..892G,
	abstract = {We use new Herschel multiband imaging of the Andromeda galaxy to analyse how dust heating occurs in the central regions of galaxy spheroids that are essentially devoid of young stars. We construct a dust temperature map of M31 through fitting modified blackbodies to the Herschel data, and find that the temperature within 2 kpc rises strongly from the mean value in the disc of 17 $\pm$ 1 to ∼35 K at the centre. Ultraviolet (UV) to near-infrared (IR) imaging of the central few kpc shows directly the absence of young stellar populations, delineates the radial profile of the stellar density and demonstrates that even the near-UV dust extinction is optically thin in M31's bulge. This allows the direct calculation of the stellar radiation heating in the bulge, U*(r), as a function of radius. The increasing temperature profile in the centre matches that expected from the stellar heating, i.e. that the dust heating and cooling rates track each other over nearly two orders of magnitude in U*. The modelled dust heating is in excess of the observed dust temperatures, suggesting that it is more than sufficient to explain the observed IR emission. Together with the wavelength-dependent absorption cross-section of the dust, this demonstrates directly that it is the optical, not UV, radiation that sets the heating rate. This analysis shows that neither young stellar populations nor stellar near-UV radiation is necessary to heat dust to warm temperatures in galaxy spheroids. Rather, it is the high densities of Gyr-old stellar populations that provide a sufficiently strong diffuse radiation field to heat the dust. To the extent which these results pertain to the tenuous dust found in the centres of early-type galaxies remains yet to be explored.},
	author = {Groves, Brent and Krause, Oliver and Sandstrom, Karin and Schmiedeke, Anika and Leroy, Adam and Linz, Hendrik and Kapala, Maria and Rix, Hans-Walter and Schinnerer, Eva and Tabatabaei, Fatemeh and Walter, Fabian and da Cunha, Elisabete},
	doi = {10.1111/j.1365-2966.2012.21696.x},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/CGZ8AEMR/Groves et al. - 2012 - The heating of dust by old stellar populations in .pdf:application/pdf},
	issn = {0035-8711},
	journal = {Monthly Notices of the Royal Astronomical Society},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, galaxies: bulges, galaxies: individual: M31, galaxies: ISM, infrared: galaxies},
	month = oct,
	pages = {892--902},
	title = {The heating of dust by old stellar populations in the bulge of {M31}},
	url = {https://ui.adsabs.harvard.edu/abs/2012MNRAS.426..892G},
	urldate = {2022-10-24},
	volume = {426},
	year = {2012},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2012MNRAS.426..892G},
	bdsk-url-2 = {https://doi.org/10.1111/j.1365-2966.2012.21696.x}}

@article{2012IAUS..284..112G,
	abstract = {Due to its proximity, the Andromeda galaxy (M31, NGC 224) offers a unique insight into how the spectra of stars, dust, and gas combine to form the integrated Spectral Energy Distribution (SED) of galaxies. We introduce here Herschel Space Observatory PACS and SPIRE photometric observations of M31 which cover the far-infrared to sub-mm wavelengths (70-500 μm). These new observations reveal that the total IR luminosity of M31 is relatively weak, with L IR=109.65 L ⊙, only 10\% of the total luminosity of M31. However, as seen in the previous studies of M31, the IR luminosity is dominated by a 10 kpc ring in all Herschel bands. This is distinct from the optical, where the bulge in the central 2kpc, dominates the luminosity, clearly demonstrating how different components at distinct positions in a galaxy contribute to make the integrated SED.},
	author = {Groves, Brent and Krause, Oliver},
	doi = {10.1017/S174392131200885X},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/68DB3WKU/Groves and Krause - 2012 - Hot & cold dust in M31 the resolved SED of Androm.pdf:application/pdf},
	keywords = {Galaxies:individual:M31, ISM:dust},
	month = aug,
