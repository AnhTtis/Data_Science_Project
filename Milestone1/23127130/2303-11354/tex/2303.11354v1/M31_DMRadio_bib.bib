	issn = {2470-0010, 2470-0029},
	journal = {Physical Review D},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies},
