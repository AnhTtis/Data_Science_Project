@misc{2022PhRvD.106b3023E,
	abstract = {The present work derived the robust constraints on annihilating WIMP parameters utilizing new radio observations of M31, as well as new studies of its dark matter distribution and other properties. The characteristics of emission due to DM annihilation were computed in the frame of 2D galactic model employing GALPROP code adapted specifically for M31. This enabled to refine various inaccuracies of previous studies on the subject. DM constraints were obtained for two representative annihilation channels: \${\textbackslash}chi{\textbackslash}chi {\textbackslash}rightarrow b{\textbackslash}bar\{b\}\$ and \${\textbackslash}chi{\textbackslash}chi {\textbackslash}rightarrow {\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}-\$. A wide variety of radio data was utilized in the frequency range \${\textbackslash}approx\$(0.1-10) GHz. As the result the thermal WIMP lighter than \${\textbackslash}approx\$ 72 GeV for \$b{\textbackslash}bar\{b\}\$ channel and \${\textbackslash}approx\$ 39 GeV for \${\textbackslash}tau{\textasciicircum}+{\textbackslash}tau{\textasciicircum}-\$ was excluded. The corresponding mass threshold uncertainty ranges were estimated to be 20-210 GeV and 18-89 GeV. The obtained exclusions are competitive to those from Fermi-LAT observations of dwarfs and AMS-02 measurements of antiprotons. Our constraints does not exclude the explanation of the gamma-ray outer halo of M31 and Galactic center excess by annihilating DM. The thermal WIMP with \$m\_x {\textbackslash}approx 70\$ GeV, which explains the outer halo, would make a significant contribution into the non-thermal radio flux in M31 nucleus fitting well both the spectrum and morphology. And finally we questioned the possibility to robustly constrain heavy thermal WIMP with \$m\_x {\textgreater} 100\$ GeV by radio data on M31 claimed in other studies.},
	annote = {Comment: 24 pages, 12 figures, 6 tables, 86 references; submitted to Phys. Rev. D; our DM addition for GALPROP is available at https://github.com/a-e-egorov/GALPROP\_DM},
	author = {Egorov, A. E.},
	file = {Egorov - 2022 - Updated constraints on dark matter (WIMP) annihila.pdf:/Users/mitchellweikert/Zotero/storage/KY7AT2NH/Egorov - 2022 - Updated constraints on dark matter (WIMP) annihila.pdf:application/pdf},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	language = {en},
	month = may,
	note = {Number: arXiv:2205.01033 arXiv:2205.01033 [astro-ph]},
