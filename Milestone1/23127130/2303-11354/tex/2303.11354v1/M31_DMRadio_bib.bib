	issn = {0004-637X, 1538-4357},
	journal = {The Astrophysical Journal},
	keywords = {High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	language = {en},
