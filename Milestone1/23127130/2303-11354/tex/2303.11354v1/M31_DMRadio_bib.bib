@article{2003astro.ph.10287B,
	abstract = {The average strength of the total magnetic field in the Milky Way, derived from radio synchrotron data under the energy equipartition assumption, is 6{\textbackslash}muG locally and {\textbackslash}simeq 10{\textbackslash}muG at 3 kpc Galactic radius. Optical and synchrotron polarization data yield a strength of the local regular field of {\textbackslash}simeq 4{\textbackslash}muG (an upper limit if anisotropic fields are present), while pulsar rotation measures give {\textbackslash}simeq 1.5{\textbackslash}muG (a lower limit if small-scale fluctuations in regular field strength and in thermal electron density are anticorrelated). In spiral arms of external galaxies, the total [regular] field strength is up to {\textbackslash}simeq 35{\textbackslash}muG [{\textbackslash}simeq 15{\textbackslash}muG]. In nuclear starburst regions the total field reaches {\textbackslash}simeq 50{\textbackslash}muG. Little is known about the global field structure in the Milky Way. The local regular field may be part of a ``magnetic arm'' between the optical arms, a feature that is known from other spiral galaxies. Unlike external galaxies, rotation measure data indicate several global field reversals in the Milky Way, but some of these could be due to field distortions. The Galaxy is surrounded by a thick radio disk of similar extent as around many edge-on spiral galaxies. While the regular field of the local disk is of even symmetry with respect to the plane (quadrupole), the regular field in the inner Galaxy or in the halo may be of dipole type. The Galactic center region hosts highly regular fields of up to milligauss strength which are oriented perpendicular to the plane.},
	author = {Beck, Rainer},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/N32IKUS7/Beck - 2003 - Magnetic fields in the Milky Way and other spiral .pdf:application/pdf},
	journal = {arXiv e-prints},
	keywords = {Astrophysics},
	month = oct,
	pages = {astro--ph/0310287},
	title = {Magnetic fields in the {Milky} {Way} and other spiral galaxies},
	url = {https://ui.adsabs.harvard.edu/abs/2003astro.ph.10287B},
	urldate = {2022-01-11},
	year = {2003},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2003astro.ph.10287B}}

@article{2012SSRv..166..215B,
	abstract = {Radio synchrotron emission, its polarization and its Faraday rotation are powerful tools to study the strength and structure of magnetic fields in galaxies. Unpolarized emission traces turbulent fields which are strongest in spiral arms and bars (20-30 μG) and in central starburst regions (50-100 μG). Such fields are dynamically important, e.g. they can drive gas inflows in central regions. Polarized emission traces ordered fields which can be regular or anisotropic random, generated from isotropic random fields by compression or shear. The strongest ordered fields of 10-15 μG strength are generally found in interarm regions and follow the orientation of adjacent gas spiral arms. Ordered fields with spiral patterns exist in grand-design, barred and flocculent galaxies, and in central regions of starburst galaxies. Faraday rotation measures (RM) of the diffuse polarized radio emission from the disks of several spiral galaxies reveal large-scale patterns, which are signatures of regular fields generated by a mean-field dynamo. However, in most spiral galaxies observed so far the field structure is more complicated. Ordered fields in interacting galaxies have asymmetric distributions and are an excellent tracer of past interactions between galaxies or with the intergalactic medium. Ordered magnetic fields are also observed in radio halos around edge-on galaxies, out to large distances from the plane, with X-shaped patterns. Future observations of polarized emission at high frequencies, with the EVLA, the SKA and its precursors, will trace galactic magnetic fields in unprecedented detail. Low-frequency telescopes (e.g. LOFAR and MWA) are ideal to search for diffuse emission and small RMs from weak interstellar and intergalactic fields.},
	author = {Beck, Rainer},
	doi = {10.1007/s11214-011-9782-z},
	file = {Beck - 2012 - Magnetic Fields in Galaxies.pdf:/Users/mitchellweikert/Zotero/storage/MX8AN5ZF/Beck - 2012 - Magnetic Fields in Galaxies.pdf:application/pdf},
	issn = {0038-6308},
	journal = {Space Science Reviews},
	keywords = {Dynamo action, Faraday rotation, Galaxies: magnetic fields, Galaxies: spiral structure, Radio telescopes, Synchrotron emission},
	month = may,
	pages = {215--230},
	title = {Magnetic {Fields} in {Galaxies}},
	url = {https://ui.adsabs.harvard.edu/abs/2012SSRv..166..215B},
	urldate = {2022-01-11},
	volume = {166},
	year = {2012},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2012SSRv..166..215B},
	bdsk-url-2 = {https://doi.org/10.1007/s11214-011-9782-z}}

@article{2010Natur.463...65C,
	abstract = {The amplitude of the magnetic field near the Galactic Centre has been uncertain by two orders of magnitude for several decades. On a scale of {\textasciitilde}100 parsecs (pc), fields of {\textasciitilde}1,000 microgauss (μG refs 1-3) have been reported, implying a magnetic energy density more than 10,000 times stronger than typical for the Galaxy. Alternatively, the assumption of pressure equilibrium between the various phases of the Galactic Centre interstellar medium (including turbulent molecular gas, the contested `very hot' plasma, and the magnetic field) suggests fields of {\textasciitilde}100 μG over {\textasciitilde}400 pc size scales. Finally, assuming equipartition, fields of only {\textasciitilde}6 μG have been inferred from radio observations for 400 pc scales. Here we report a compilation of previous data that reveals a downward break in the region's non-thermal radio spectrum (attributable to a transition from bremsstrahlung to synchrotron cooling of the in situ cosmic-ray electron population). We show that the spectral break requires that the Galactic Centre field be at least {\textasciitilde}50 μG on 400 pc scales, lest the synchrotron-emitting electrons produce too much γ-ray emission, given other existing constraints. Other considerations support a field of 100 μG, implying that over 10\% of the Galaxy's magnetic energy is contained in only {\textless}{\textasciitilde}0.05\% of its volume.},
	author = {Crocker, Roland M. and Jones, David I. and Melia, Fulvio and Ott, J{\"u}rgen and Protheroe, Raymond J.},
	doi = {10.1038/nature08635},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/6IQ5XFDP/Crocker et al. - 2010 - A lower limit of 50 microgauss for the magnetic fi.pdf:application/pdf},
	issn = {0028-0836},
	journal = {Nature},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Galaxy Astrophysics},
	month = jan,
	pages = {65--67},
	title = {A lower limit of 50 microgauss for the magnetic field near the {Galactic} {Centre}},
	url = {https://ui.adsabs.harvard.edu/abs/2010Natur.463...65C},
	urldate = {2022-01-11},
	volume = {463},
	year = {2010},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2010Natur.463...65C},
