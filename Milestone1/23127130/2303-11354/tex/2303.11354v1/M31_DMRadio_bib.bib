	issn = {0004-6361},
	journal = {Astronomy and Astrophysics},
	language = {en},
	month = oct,
	number = {2},
	pages = {497--508},
	shorttitle = {Milky {Way} versus {Andromeda}},
	title = {Milky {Way} versus {Andromeda}: a tale of two disks},
	url = {https://ui.adsabs.harvard.edu/abs/2009A&A...505..497Y/abstract},
	urldate = {2022-07-26},
