@article{vielWarmDarkMatter2013,
	abstract = {We present updated constraints on the free-streaming of warm dark matter (WDM) particles derived from an analysis of the Lya flux power spectrum measured from high-resolution spectra of 25 z {\textgreater} 4 quasars obtained with the Keck High Resolution Echelle Spectrometer (HIRES) and the Magellan Inamori Kyocera Echelle (MIKE) spectrograph. We utilize a new suite of high-resolution hydrodynamical simulations that explore WDM masses of 1, 2 and 4 keV (assuming the WDM consists of thermal relics), along with different physically motivated thermal histories. We carefully address different sources of systematic error that may affect our final results and perform an analysis of the Lya flux power with conservative error estimates. By using a method that samples the multi-dimensional astrophysical and cosmological parameter space, we obtain a lower limit mwdm {\textgreater} 3.3 keV (2sigma) for warm dark matter particles in the form of early decoupled thermal relics. Adding the Sloan Digital Sky Survey (SDSS) Lya flux power spectrum does not improve this limit. Thermal relics of masses 1 keV, 2 keV and 2.5 keV are disfavoured by the data at about the 9sigma, 4sigma and 3sigma C.L., respectively. Our analysis disfavours WDM models where there is a suppression in the linear matter power spectrum at (non-linear) scales corresponding to k=10h/Mpc which deviates more than 10\% from a LCDM model. Given this limit, the corresponding "free-streaming mass" below which the mass function may be suppressed is 2x10{\textasciicircum}8 Msun/h. There is thus very little room for a contribution of the free-streaming of WDM to the solution of what has been termed the small scale crisis of cold dark matter.},
	annote = {Comment: 20 pages, 17 figures. Accepted to PRD},
	author = {Viel, M. and Becker, G. D. and Bolton, J. S. and Haehnelt, M. G.},
	doi = {10.1103/PhysRevD.88.043502},
	file = {Viel et al. - 2013 - Warm Dark Matter as a solution to the small scale .pdf:/Users/mitchellweikert/Zotero/storage/TFXVU7C8/Viel et al. - 2013 - Warm Dark Matter as a solution to the small scale .pdf:application/pdf},
	issn = {1550-7998, 1550-2368},
	journal = {Physical Review D},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	language = {en},
	month = aug,
	note = {arXiv: 1306.2314},
	number = {4},
	pages = {043502},
	shorttitle = {Warm {Dark} {Matter} as a solution to the small scale crisis},
	title = {Warm {Dark} {Matter} as a solution to the small scale crisis: new constraints from high redshift {Lyman}-alpha forest data},
	url = {http://arxiv.org/abs/1306.2314},
	urldate = {2020-11-11},
	volume = {88},
	year = {2013},
	bdsk-url-1 = {http://arxiv.org/abs/1306.2314},
	bdsk-url-2 = {https://doi.org/10.1103/PhysRevD.88.043502}}

