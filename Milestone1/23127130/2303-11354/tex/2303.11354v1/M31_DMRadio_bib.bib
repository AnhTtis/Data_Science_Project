@article{aziziScalarHexaquarkUuddss2020,
	abstract = {It is conventionally argued that Dark Matter (DM) has a non-baryonic nature, but if we assume that DM was frozen out before primordial nucleosynthesis and could not significantly impact primordial abundances this argument may be evaded. Then a hypothetical \$SU(3)\$ flavor-singlet, highly symmetric, deeply bound neutral scalar hexaquark \${\textbackslash}mathrm\{S\}=uuddss\$, which due to its features has escaped from experimental detection so far, may be considered as a candidate for a baryonic DM. In the present work we calculate the mass and coupling constant of the scalar six-quark particle \${\textbackslash}mathrm\{S\}\$ by means of the QCD sum rule method. Our predictions for its mass are \$ m\_\{S\}=1180\_\{-26\}{\textasciicircum}\{+40\}{\textasciitilde}{\textbackslash}mathrm\{MeV\}\$ (\$m\_s=95{\textasciitilde}{\textbackslash}mathrm\{MeV\}\$) and \$ {\textbackslash}widetilde \{m\}\_\{S\}=1239\_\{-28\}{\textasciicircum}\{+42\}{\textasciitilde}{\textbackslash}mathrm\{MeV\}\$ (\$m\_s=128{\textasciitilde}{\textbackslash}mathrm\{MeV\}\$). Although these values of mass would produce thermally the cosmological DM abundance, existence of this state may contradict to stability of the oxygen nuclei, which requires further thorough analysis.},
	annote = {Comment: 10 Pages and 2 Figures},
	author = {Azizi, K. and Agaev, S. S. and Sundu, H.},
	doi = {10.1088/1361-6471/ab9a0e},
	file = {Azizi et al. - 2020 - The Scalar Hexaquark \$uuddss\$ a Candidate to Dark.pdf:/Users/mitchellweikert/Zotero/storage/2GBUD5KH/Azizi et al. - 2020 - The Scalar Hexaquark \$uuddss\$ a Candidate to Dark.pdf:application/pdf},
	issn = {0954-3899, 1361-6471},
	journal = {Journal of Physics G: Nuclear and Particle Physics},
	keywords = {High Energy Physics - Phenomenology, Astrophysics - Astrophysics of Galaxies, High Energy Physics - Experiment, High Energy Physics - Lattice},
	language = {en},
	month = sep,
	note = {arXiv: 1904.09913},
	number = {9},
	pages = {095001},
	shorttitle = {The {Scalar} {Hexaquark} \$uuddss\$},
	title = {The {Scalar} {Hexaquark} \$uuddss\$: a {Candidate} to {Dark} {Matter}?},
	url = {http://arxiv.org/abs/1904.09913},
	urldate = {2020-11-11},
	volume = {47},
	year = {2020},
	bdsk-url-1 = {http://arxiv.org/abs/1904.09913},
	bdsk-url-2 = {https://doi.org/10.1088/1361-6471/ab9a0e}}

@article{aprileExcessElectronicRecoil2020,
	abstract = {We report results from searches for new physics with low-energy electronic recoil data recorded with the XENON1T detector. With an exposure of 0.65 t-y and an unprecedentedly low background rate of \$76{\textbackslash}pm2\$ events/(t y keV) between 1 and 30 keV, the data enables sensitive searches for solar axions, an enhanced neutrino magnetic moment, and bosonic dark matter. An excess over known backgrounds is observed at low energies and most prominent between 2 and 3 keV. The solar axion model has a 3.4\${\textbackslash}sigma\$ significance, and a 3D 90\% confidence surface is reported for axion couplings to electrons, photons, and nucleons. This surface is inscribed in the cuboid defined by \$g\_\{ae\}{\textless}3.8 {\textbackslash}times 10{\textasciicircum}\{-12\}\$, \$g\_\{ae\}g\_\{an\}{\textasciicircum}\{eff\}{\textless}4.8{\textbackslash}times 10{\textasciicircum}\{-18\}\$, and \$g\_\{ae\}g\_\{a{\textbackslash}gamma\}{\textless}7.7{\textbackslash}times10{\textasciicircum}\{-22\} GeV{\textasciicircum}\{-1\}\$, and excludes either \$g\_\{ae\}=0\$ or \$g\_\{ae\}g\_\{a{\textbackslash}gamma\}=g\_\{ae\}g\_\{an\}{\textasciicircum}\{eff\}=0\$. The neutrino magnetic moment signal is similarly favored over background at 3.2\${\textbackslash}sigma\$ and a confidence interval of \${\textbackslash}mu\_\{{\textbackslash}nu\} {\textbackslash}in (1.4,2.9){\textbackslash}times10{\textasciicircum}\{-11\}{\textbackslash}mu\_B\$ (90\% C.L.) is reported. Both results are in strong tension with stellar constraints. The excess can also be explained by \${\textbackslash}beta\$ decays of tritium at 3.2\${\textbackslash}sigma\$ with a trace amount that can neither be confirmed nor excluded with current knowledge of its production and reduction mechanisms. The significances of the solar axion and neutrino magnetic moment hypotheses are reduced to 2.0\${\textbackslash}sigma\$ and 0.9\${\textbackslash}sigma\$, respectively, if an unconstrained tritium component is included in the fitting. With respect to bosonic dark matter, the excess favors a monoenergetic peak at (\$2.3{\textbackslash}pm0.2\$) keV (68\% C.L.) with a 3.0\${\textbackslash}sigma\$ global (4.0\${\textbackslash}sigma\$ local) significance. We also consider the possibility that \${\textasciicircum}\{37\}\$Ar may be present in the detector and yield a 2.82 keV peak. Contrary to tritium, the \${\textasciicircum}\{37\}\$Ar concentration can be tightly constrained and is found to be negligible.},
	annote = {Comment: 26 pages, 15 figures. v2 added Ar37 background discussion and best-fit mass of bosonic dark matter, v3 updated Ar37 discussion, tritium estimation, and solar axion energy spectrum. Data in Fig. 2, 4, and 15, including unbinned energy points in Fig. 4, are available in 10.5281/zenodo.4088778},
