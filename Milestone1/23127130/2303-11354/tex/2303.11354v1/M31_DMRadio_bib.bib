	journal = {Physical Review Letters},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, 95.35.+d, 95.85.Pw, 98.52.Wz, Astrophysics - Cosmology and Extragalactic Astrophysics, Dark matter, Dwarf galaxies, gamma-ray},
