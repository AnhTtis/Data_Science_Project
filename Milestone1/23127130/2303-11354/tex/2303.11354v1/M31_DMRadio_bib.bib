	doi = {10.1088/1475-7516/2015/03/038},
	file = {Calore et al. - 2015 - Background model systematics for the Fermi GeV exc.pdf:/Users/mitchellweikert/Zotero/storage/7WUS8EUC/Calore et al. - 2015 - Background model systematics for the Fermi GeV exc.pdf:application/pdf},
	issn = {1475-7516},
	journal = {Journal of Cosmology and Astroparticle Physics},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Astrophysics - High Energy Astrophysical Phenomena},
	language = {en},
	month = mar,
	note = {arXiv: 1409.0042},
	number = {03},
