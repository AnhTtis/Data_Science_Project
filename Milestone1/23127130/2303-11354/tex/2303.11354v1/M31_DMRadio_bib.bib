@article{1994ApJ...431..156W,
	abstract = {We present a study of diffuse ionized gas (DIG, or WIM for warm ionized medium) in the nearby spiral M31, based on CCD imagery in Hα and [S II] emission lines. We find that the DIG in M31 contributes 40\% of the observed total Hα luminosity, and at least 20\% of the total after correction for extinction. This fraction is constant across the disk of M31. The total Hα luminosity of M31, corrected for extinction, is about 4.1 x 10{\textasciicircum}40{\textasciicircum} ergs s{\textasciicircum}-1{\textasciicircum}. This implies a low star formation rate of about 0.35 M\_sun\_ yr{\textasciicircum}-1{\textasciicircum}, in agreement with earlier estimates. DIG is concentrated near regions of star formation. The average emission measure (EM) perpendicular to the disk ranges from about 6 pc cm{\textasciicircum}-6{\textasciicircum} in weaker spiral arms to 15 pc cm{\textasciicircum}-6{\textasciicircum} in the main spiral arms, with (rare) peak values reaching 50 pc cm{\textasciicircum}-6{\textasciicircum}. Overall, the EM in the main arms is substantially higher than that of DIG in the solar neighborhood, implying higher power requirements per unit area in these regions of M31. However, the strong concentration of DIG near regions of star formation keeps the total area covered by DIG in M31 relatively small, which makes it possible to sustain this gas in spite of the low star formation rate. We argue that the DIG extent in vertical direction in M31 may well be lower than in our Galaxy, because of its low star formation rate. A simple parameterization of the electron distribution then shows that the DIG in the spiral arms may contribute between 15\% and 40\% of the total atomic gas column. The average electron densities implied by the high EMs reached in some regions seem to indicate a higher thermal pressure in the DIG than in the diffuse H I gas in M31. This pressure in-equilibrium can be avoided if part of the diffuse emission is due to a denser component, as proposed most recently by Miller \& Cox, because this would reduce the EM and implied pressures for the actual widespread DIG. Column densities of the ionized gas might then be lower as well. We also show, however, that it is not possible to attribute all of the diffuse emission to a relatively dense component, such as earlier proposed extended low-density H II regions. The DIG in M31 shows an overall [S II]/Hα intensity ratio of 0.5, significantly higher than in discrete H II regions, and similar to that in other galaxies. This number does not vary with radius in M31. There is a suggestion that [S II]/Hα intensity ratios may be higher at the lowest detectable emission measures, up to 0.7 or even 1. The discrete source population in M31 also shows a trend toward increasing [S II]/Hα flux ratios as the sources become more diffuse. Few sources (apart from supernova remnants), however, reach the value of 0.5 found in the DIG as a whole. The distinct spectral signature of the DIG in M31, and its energy requirements favor photoionization of the gas, although it is likely that shock ionization occurs in selected regions.},
	author = {Walterbos, Rene A. M. and Braun, Robert},
	doi = {10.1086/174475},
	file = {Full Text PDF:/Users/mitchellweikert/Zotero/storage/NLM3U3KA/Walterbos and Braun - 1994 - Diffuse Ionized Gas in the Spiral Galaxy M31.pdf:application/pdf},
	issn = {0004-637X},
	journal = {The Astrophysical Journal},
	keywords = {Astrophysics, Galactic Structure, Interstellar Gas, Interstellar Matter, Electron Distribution, Emission Spectra, GALAXIES: INDIVIDUAL MESSIER NUMBER: M31, GALAXIES: ISM, GALAXIES: PHOTOMETRY, GALAXIES: SPIRAL, GALAXIES: STRUCTURE, Gaseous Diffusion, H Alpha Line, H Ii Regions, Ionized Gases, ISM: H II REGIONS, Luminosity, Photoionization, Spiral Galaxies},
	month = aug,
	pages = {156},
	title = {Diffuse {Ionized} {Gas} in the {Spiral} {Galaxy} {M31}},
	url = {https://ui.adsabs.harvard.edu/abs/1994ApJ...431..156W},
	urldate = {2022-01-07},
	volume = {431},
	year = {1994},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/1994ApJ...431..156W},
	bdsk-url-2 = {https://doi.org/10.1086/174475}}

@article{1972Phy....60..145G,
	abstract = {The stopping power of a plasma for fast electrons and positrons is calculated. First the classical limit is considered where β = ν/ c ≪ α is the fine structure constant. Then the nonrelativistic Born-approximation formulas are derived; this domain corresponds to α ≪ β ≪ 1. Finally the general case of relativistic electrons and positrons is treated; in the relativistic case the scattering cross sections of M{\o}ller (electron-electron) and Bhabha (positron-electron) are used in the calculation. In all three energy domains the problem is broken up into cases of small and large momentum transfers ( ℏq). For large q, scattering off individual plasma electrons is considered, while in the limit of very small q for the quantum-mechanical domain, excitation of quantized plasma oscillations contributes to d E/d x; in the classical limit for small q (or large impact parameter), the polarizability of the plasma provides the effective cutoff. The formulas for the stopping power differ slightly from those for a heavy ion going through a plasma because (i) there are exchange effects and (ii) the fast electrons and positrons can lose a large fraction of their energy in one scattering off a plasma electron.},
	author = {Gould, R. J.},
	doi = {10.1016/0031-8914(72)90227-3},
	file = {Gould - 1972 - Energy loss of fast electrons and positrons in a p.pdf:/Users/mitchellweikert/Zotero/storage/6WE9QKTT/Gould - 1972 - Energy loss of fast electrons and positrons in a p.pdf:application/pdf},
	journal = {Physica},
	month = jul,
	pages = {145--154},
	title = {Energy loss of fast electrons and positrons in a plasma},
	url = {https://ui.adsabs.harvard.edu/abs/1972Phy....60..145G},
	urldate = {2022-01-10},
	volume = {60},
	year = {1972},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/1972Phy....60..145G},
