	file = {Collaboration - 2017 - Observations of M31 and M33 with the Fermi Large A.pdf:/Users/mitchellweikert/Zotero/storage/P96KYHCZ/Collaboration - 2017 - Observations of M31 and M33 with the Fermi Large A.pdf:application/pdf},
	issn = {1538-4357},
	journal = {The Astrophysical Journal},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	language = {en},
