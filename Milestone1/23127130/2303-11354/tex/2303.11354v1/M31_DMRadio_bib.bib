	journal = {The Astrophysical Journal},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, astroparticle physics, cosmic rays, dark matter, diffuse radiation, diffusion, elementary particles, gamma rays: ISM, infrared: ISM, ISM: general, radio continuum: ISM},
	month = jun,
