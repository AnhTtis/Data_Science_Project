@article{carrConstraintsPrimordialBlack2020,
	abstract = {We update the constraints on the fraction of the Universe going into primordial black holes (PBHs) over the mass range \$10{\textasciicircum}\{-5\}\$--\$10{\textasciicircum}\{50\}\$ g. Those smaller than \${\textbackslash}sim 10{\textasciicircum}\{15\}\$ g would have evaporated by now due to Hawking radiation, so their abundance at formation is constrained by the effects of evaporated particles on big bang nucleosynthesis, the cosmic microwave background (CMB), the Galactic and extragalactic \${\textbackslash}gamma\$-ray and cosmic ray backgrounds and the possible generation of stable Planck mass relics. PBHs larger than \${\textbackslash}sim 10{\textasciicircum}\{15\}\$ g are subject to a variety of constraints associated with gravitational lensing, dynamical effects, influence on large-scale structure, accretion and gravitational waves. We discuss the constraints on both the initial collapse fraction and the current fraction of the cold dark matter in PBHs at each mass scale but stress that many of the constraints are associated with observational or theoretical uncertainties and some are no longer applicable. We also consider indirect constraints associated with the amplitude of the primordial density fluctuations, such as second-order tensor perturbations and \${\textbackslash}mu\$-distortions arising from the effect of acoustic reheating on the CMB, but these only apply if PBHs are created from the high-\${\textbackslash}sigma\$ peaks of nearly Gaussian fluctuations. Finally we discuss how the constraints are modified if the PBHs have an extended mass function, this being relevant if PBHs provide some combination of the dark matter, the LIGO/Virgo coalescences and the seeds for cosmic structure.},
	annote = {Comment: 59 pages, 21 figures, REVTeX 4.1. arXiv admin note: text overlap with arXiv:0912.5297},
	author = {Carr, Bernard and Kohri, Kazunori and Sendouda, Yuuiti and Yokoyama, Jun'ichi},
	file = {Carr et al. - 2020 - Constraints on Primordial Black Holes.pdf:/Users/mitchellweikert/Zotero/storage/L53GPKSG/Carr et al. - 2020 - Constraints on Primordial Black Holes.pdf:application/pdf},
	journal = {arXiv:2002.12778 [astro-ph, physics:gr-qc, physics:hep-ph, physics:hep-th]},
	keywords = {General Relativity and Quantum Cosmology, High Energy Physics - Theory, Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology},
	language = {en},
	month = feb,
	note = {arXiv: 2002.12778},
	title = {Constraints on {Primordial} {Black} {Holes}},
	url = {http://arxiv.org/abs/2002.12778},
	urldate = {2020-11-11},
	year = {2020},
	bdsk-url-1 = {http://arxiv.org/abs/2002.12778}}

@article{jaffeMultiquarkHadronsII1977,
	abstract = {Techniques for estimating the masses and decay couplings of multiquark hadrons (Qm¯Qn, n+m{\textgreater}3) are developed with specific reference to the Q2¯Q2 sector. The dynamics is based on a quark-bag model of light, colored, and permanently confined quarks gauge-coupled to non-Abelian colored gluons. The SU (6) of "color-spin" generated by color SU (3) and the SU (2) of relativistic j=12 quarks dominates the spectrum. Color-spin rules analogous to Hund's rules of atomic spectroscopy dictate that the lightest multiquark handrons are not exotic, are of low spin, and are coupled predominantly to strange Q3 and Q¯Q decay channels, Multiquark hadrons are consequently elusive and may be misclassified as conventional Q¯Q mesons or Q3 baryons.},
	author = {Jaffe, R. L.},
	doi = {10.1103/PhysRevD.15.281},
	file = {APS Snapshot:/Users/mitchellweikert/Zotero/storage/LR7M9FPE/PhysRevD.15.html:text/html;Submitted Version:/Users/mitchellweikert/Zotero/storage/M728B83V/Jaffe - 1977 - Multiquark hadrons. II. Methods.pdf:application/pdf},
	journal = {Physical Review D},
	month = jan,
	note = {Publisher: American Physical Society},
	number = {1},
	pages = {281--289},
	title = {Multiquark hadrons. {II}. {Methods}},
	url = {https://link.aps.org/doi/10.1103/PhysRevD.15.281},
	urldate = {2020-11-11},
	volume = {15},
	year = {1977},
	bdsk-url-1 = {https://link.aps.org/doi/10.1103/PhysRevD.15.281},
	bdsk-url-2 = {https://doi.org/10.1103/PhysRevD.15.281}}

@article{wittenCosmicSeparationPhases1984,
	abstract = {A first-order QCD phase transition that occurred reversibly in the early universe would lead to a surprisingly rich cosmological scenario. Although observable consequences would not necessarily survive, it is at least conceivable that the phase transition would concentrate most of the quark excess in dense, invisible quark nuggets, providing an explanation for the dark matter in terms of QCD effects only. This possibility is viable only if quark matter has energy per baryon less than 938 MeV. Two related issues are considered in appendices: the possibility that neutron stars generate a quark-matter component of cosmic rays, and the possibility that the QCD phase transition may have produced a detectable gravitational signal.},
	author = {Witten, Edward},
	doi = {10.1103/PhysRevD.30.272},
	file = {APS Snapshot:/Users/mitchellweikert/Zotero/storage/J63FK56P/PhysRevD.30.html:text/html},
	journal = {Physical Review D},
	month = jul,
	note = {Publisher: American Physical Society},
	number = {2},
	pages = {272--285},
	title = {Cosmic separation of phases},
	url = {https://link.aps.org/doi/10.1103/PhysRevD.30.272},
	urldate = {2020-11-11},
	volume = {30},
	year = {1984},
	bdsk-url-1 = {https://link.aps.org/doi/10.1103/PhysRevD.30.272},
