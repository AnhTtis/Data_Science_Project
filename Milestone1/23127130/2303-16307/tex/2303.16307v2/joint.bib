@article{bozdal2018hardware,
    title        = {Hardware Trojan enabled denial of service attack on {CAN} bus},
    author       = {Bozdal, M. and Randa, M. and Samie, M. and Jennions, I.},
    year         = 2018,
    journal      = {Procedia Manufacturing},
    publisher    = {Elsevier},
    volume       = 16,
    pages        = {47--52},
    annote       = {\textbf{Abstract:} The trend of technological advances in the vehicle industry illustrates that future cars would have added functionalities with smart features, better connectivity and autonomous behavior. These naturally involve a higher number of Electronic Control Units (ECUs) being connected using existing conventional in-network protocols such as Controller Area Network (CAN). In this context, security of systems in now becoming a major concern while industry's primary interest in the manufacturing of cars is reliability and safety. It is now in daily new that smart cars are being hacked due to weaknesses in their embedded electronics that provides ways of hardware attacks [1][2]. Hardware Trojan (HT) is the threat that has been recently recognized as one of the primary sources of backdoor access that enables hackers to attack systems. As trouble, HT remains silent until a rare function/event triggers it for activation. This paper contributes to the challenges of demonstration of distrution in CAN buses raised from hidden Hardware Trojan. In this regard, it is presented how just a small size Hardware Trojan dirupts the CAN bus communication without an adversary having physical access to the bus. The attack is neither detectable via frame analysis, nor can be prevented via network segmentation; additionally, a rare triggering mechanism activates HT to process untracable faults.\\ \newline \textbf{Notes:} This paper reiterates much of the information from the bozdal2018survey paper, then describes their implementation and testing of an HT.}
}
@article{bozdal2020evaluation,
    title        = {Evaluation of {CAN} bus security challenges},
    author       = {Bozdal, M. and Samie, M. and Aslam, S. and Jennions, I.},
    year         = 2020,
    journal      = {Sensors},
    publisher    = {Multidisciplinary Digital Publishing Institute},
    volume       = 20,
    number       = 8,
    pages        = 2364,
    annote       = {\textbf{Abstract:} The automobile industry no longer relies on pure mechanical systems; instead, it benefits from many smart features based on advanced embedded electronics. Although the rise in electronics and connectivity has improved comfort, functionality, and safe driving, it has also created new attack surfaces to penetrate the in-vehicle communication network, which was initially designed as a close loop system. For such applications, the Controller Area Network (CAN) is the most-widely used communication protocol, which still suffers from various security issues because of the lack of encryption and authentication. As a result, any malicious/hijacked node can cause catastrophic accidents and financial loss. This paper analyses the CAN bus comprehensively to provide an outlook on security concerns. It also presents the security vulnerabilities of the CAN and a state-of-the-art attack surface with cases of implemented attack scenarios and goes through different solutions that assist in attack prevention, mainly based on an intrusion detection system (IDS).}
}
@inproceedings{bozdal2018survey,
    title        = {A Survey on {CAN} Bus Protocol: Attacks, Challenges, and Potential Solutions},
    author       = {Bozdal, M. and Samie, M. and Jennions, I.},
    year         = 2018,
    booktitle    = {2018 International Conference on Computing, Electronics   Communications Engineering (iCCECE)},
    volume       = {},
    number       = {},
    pages        = {201--205},
    doi          = {10.1109/iCCECOME.2018.8658720},
    annote       = {\textbf{Abstract:} The vehicles are equipped with electronic control units that control their functions. These units communicate with each other via in-vehicle communication protocols like CAN bus. Although CAN is the most common in-vehicle communication protocol, its lack of encryption and authentication can cause serious security shortcommings. In the literature, many attacks are report related to CAN bus and the number increases with rising connectivity in the cars. In this paper, we present CAN protocol and analyze its security vulnerabilities. Then we survey the implemented attacks and proposed solutions in the literature.\\ \newline \textbf{Notes:} Their conclusion is the CAN is insecure and that the electronic control units lack the processing power to make secure it by provided authentication and encryption. They recommand intrusion detection and intrusion prevention systems. They do provide a laundry list of vulnerabilities and reference several works that describe them.}
