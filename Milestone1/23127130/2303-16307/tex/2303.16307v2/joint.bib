@article{linkov2013resilience,
    title        = {Resilience metrics for cyber systems},
    author       = {Linkov, I. and Eisenberg, D.A. and Plourde, K. and Seager, T.P. and Allen, J. and Kott, A.},
    year         = 2013,
    journal      = {Environment Systems and Decisions},
    publisher    = {Springer},
    volume       = 33,
    number       = 4,
    pages        = {471--476},
    annote       = {\begin{description} \item[Abstract:] As federal agencies and businesses rely more on cyber infrastructure, they are increasingly vulnerable to cyber attacks that can cause damages disproportionate to the sophistication and cost to launch the attack. In response, regulatory authorities call for focusing attention on enhancing infrastructure resilience. For example, in the USA, President Obama issued an Executive Order and policy directives focusing on improving the resilience and security of cyber infrastructure to a wide range of cyber threats. Despite the national and international importance, resilience metrics to inform management decisions are still in the early stages of development. We apply the resilience matrix framework developed by Linkov et al. (Environ Sci Technol 47:10108-10110, 2013) to develop and organize effective resilience metrics for cyber systems. These metrics link national policy goals to specific system measures, such that resource allocation decisions can be translated into actionable interventions and investments. In this paper, a number of metrics have been identified and assessed using quantitative and qualitative measures found in the literature. We have proposed a generic approach and could integrate actual data, technical judgment, and literature-based measures to assess system resilience across physical, information, cognitive, and social domains. \item[Quotes:] \begin{itemize} \item However, current concepts and methods discussed as ways to improve resilience often conflate resilience (i.e., ability to withstand and recover quickly from unknown and known threats) and risk (i.e., product of likelihood of an adverse event and the magnitude of the resulting damage (Kaplan and Garrick 1981)). \end{itemize} \end{description}}
}
@incollection{linkov2019fundamental,
    title        = {Fundamental concepts of cyber resilience: Introduction and overview},
    author       = {Linkov, I. and Kott, A.},
    year         = 2019,
    booktitle    = {Cyber resilience of systems and networks},
    publisher    = {Springer},
    pages        = {1--25},
    abstract     = {Society is increasingly reliant upon complex and interconnected cyber systems to conduct daily life activities. From personal finance to managing defense capabilities to controlling a vast web of aircraft traffic, digitized information systems and software packages have become integrated at virtually all levels of individual and collective activity. While such integration has been met with immense increases in efficiency of service delivery, it has also been subject to a diverse body of threats from nefarious hackers, groups, and even state government bodies. Such cyber threats have shifted over time to affect various cyber functionalities, such as with direct denial of service (DDoS), data theft, changes to data code, infection via computer virus, and many others.},
    annote       = {\begin{description} \item{Abstract:} Society is increasingly reliant upon complex and interconnected cyber systems to conduct daily life activities. From personal finance to managing defense capabilities to controlling a vast web of aircraft traffic, digitized information systems and software packages have become integrated at virtually all levels of individual and collective activity. While such integration has been met with immense increases in efficiency of service delivery, it has also been subject to a diverse body of threats from nefarious hackers, groups, and even state government bodies. Such cyber threats have shifted over time to affect various cyber functionalities, such as with direct denial of service (DDoS), data theft, changes to data code, infection via computer virus, and many others. \item{Notes:} \begin{itemize} \item This chapter serves as a overview of the book \textit{Cyber Resilience of Systems and Networks.} \item ``Resilience engineering, for example, has been defined as ``the ability of systems to anticipate and adapt to the potential for surprise and failure,'' and has been associated with a shift in safety paradigm acknowledging that system coping is important when prevention is impossible (Hollnagel, Woods, \& Leveson, 2006).'' \item {``}`the ability to prepare and plan for, absorb, recover from, and more successfully adapt to adverse events' is emerging as one of the most widely used by various organizations and governance agencies (Larkin et al., 2015).'' \end{itemize} \item{Defintions:} \begin{itemize} \item the ability of the system to prepare, absorb, recover and adapt to adverse effects, especially those associated with cyber-attacks. \item the ability of systems to anticipate and adapt to the potential for surprise and failure, \item the ability of the system to absorb and withstand shocks, with an emphasis on persistence (Holling, 1996). \item cyber resiliency refers to the system's ability to recover or regenerate its performance after a cyber attacks produces a degradation of its performance. \end{itemize} \end{description}}
}
