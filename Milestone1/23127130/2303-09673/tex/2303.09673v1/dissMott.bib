	Doi = {10.1038/nphys1073},
	Id = {Diehl2008},
	Isbn = {1745-2481},
	Journal = {Nature Physics},
	Number = {11},
	Pages = {878--883},
	Title = {Quantum states and phases in driven open quantum systems with cold atoms},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/nphys1073},
	Volume = {4},
	Year = {2008},
	Bdsk-Url-1 = {https://doi.org/10.1038/nphys1073}}


