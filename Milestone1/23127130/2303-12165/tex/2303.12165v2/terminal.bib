file = {:lhome/calum/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bouret, Lanz, Hillier - 2004 - Low mass loss rates in O-type stars Spectral signatures of dense clumps in the wind of two Galactic O4(2).pdf:pdf},
issn = {00046361},
journal = {Astronomy and Astrophysics},
keywords = {HD 96715,Stars: atmospheres,Stars: early-type,Stars: fundamental parameters,Stars: individual: HD 190429A,Stars: winds,outflows},
month = {dec},
number = {1},
