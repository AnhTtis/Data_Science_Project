eprint = {2009.10341},
issn = {1538-4357},
journal = {The Astrophysical Journal},
keywords = {1335,1636,1806,301,Astrophysics,Radiative transfer,Rayet stars,Solar and Stellar Astrophysics,Stellar convective zones,Stellar winds,Wolf},
month = {nov},
number = {2},
pages = {113},
title = {{Clumping in the Winds of Wolf-Rayet Stars}},
url = {https://iopscience.iop.org/article/10.3847/1538-4357/abba24 http://arxiv.org/abs/2009.10341},
volume = {903},
year = {2020}
}
@article{Bianchi2002,
abstract = {We derived photospheric parameters, mass-loss rates, and wind velocities$\backslash$nof Galactic O6-O7 stars by analyzing high-resolution spectra in the$\backslash$nfar-UV and UV ranges with line-blanketed, hydrodynamic, non-LTE$\backslash$nspherical models. We combined spectra from the Far Ultraviolet$\backslash$nSpectroscopic Explorer (FUSE) in the range 905-1187 {\{}{\AA}{\}} and$\backslash$nInternational Ultraviolet Explorer (IUE) archival spectra (1150-3250$\backslash$n{\{}{\AA}{\}}) and used the WM-BASIC code of Pauldrach et al. to compute model$\backslash$nspectra. Lines in the FUSE range include high ionization stages (e.g., O$\backslash$nVI) and lower abundance non-CNO elements (e.g., P V). Analyzed in$\backslash$naddition to the N IV, N V, Si IV, and C IV lines in the IUE range, these$\backslash$nfeatures play a crucial role in uniquely constraining the stellar$\backslash$nparameters, assessing the presence of shocks in the wind, and$\backslash$nquantifying the effects of the resulting soft X-rays on the wind$\backslash$nionization. The effective temperatures derived from the consistent$\backslash$nanalysis of the far-UV and UV spectra are significantly ({\~{}}6000 K or 15{\%}$\backslash$non average, or between 4000 and 8000 K) lower than most values$\backslash$npreviously derived for some of our targets and lower than typical values$\backslash$nassigned to their spectral types from different compilations. This$\backslash$nresult has great implications for our understanding of the evolution of$\backslash$nmassive stars and the characterization of young stellar populations, as$\backslash$nwell as for energy balance calculations of H II regions. Based on$\backslash$nobservations with the NASA-CNES-CSA Far Ultraviolet Spectroscopic$\backslash$nExplorer, which is operated by The Johns Hopkins University under NASA$\backslash$ncontract NAS5-32985, and on IUE observations from the MAST and INES$\backslash$narchives.$\backslash$n},
