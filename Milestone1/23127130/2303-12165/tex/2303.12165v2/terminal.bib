@article{Chiosi1986,
abstract = {The characteristics of mass-loss in massive stars (at least 9 solar$\backslash$nmasses) are described. An evolutionary model for a constant mass star is$\backslash$ndefined for reference purposes, along with diagnostic methods for$\backslash$nstudying stellar winds and parameterizing mass-loss rates in early,$\backslash$nlate-type and WR stars. Modifications in the internal structure and$\backslash$nevolution of massive stars due to mass loss are explored in terms of$\backslash$ncore and shell H-burning and core He-burning phases. The influence of$\backslash$nmass-loss on the location of stellar objects on the HR diagram and on$\backslash$nnucleosynthesis processes is considered, as is the possibility that WR$\backslash$nstars are the result of massive star mass-loss. Finally, the galactic$\backslash$ndistribution of O, blue and red supergiant and WR stars is discussed.$\backslash$n},
