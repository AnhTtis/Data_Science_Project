@Article{Kim2022a,
  author        = {Sun-Woo Kim and Hanbit Oh and Eun-Gook Moon and Youngkuk Kim},
  journal       = {arXiv},
  title         = {Monolayer Kagome Metals AV3Sb5},
  year          = {2022},
  month         = feb,
  abstract      = {Recently, layered kagome metals AV3Sb5 (A = K, Rb, and Cs) have emerged as a fertile platform for exploring frustrated geometry, correlations, and topology. Here we demonstrate that AV3Sb5 can crystallize in a mono-layered form, revealing a range of properties that render the system unique by using first-principles and mean-field calculations. Most importantly, the two-dimensional monolayer preserves intrinsically different symmetries from the three-dimensional layered bulk, enforced by stoichiometry. Enrichment of the van Hove singularities, a logarithmic divergence of electronic density of states, consequently appears, leading to a variety of competing instabilities such as doublets of charge density waves and s-and d-wave superconductivity. We show that the competition between orders can be fine-tuned in the monolayer via electron-filling of the van Hove singularities. Thus, our results suggest the monolayer kagome metal AV3Sb5 as a promising platform for designer metallic quantum phases.},
  archiveprefix = {arXiv},
  eprint        = {2202.11521},
  file          = {:2202.11521.pdf:PDF},
  keywords      = {cond-mat.str-el, cond-mat.mtrl-sci, cond-mat.supr-con},
  primaryclass  = {cond-mat.str-el},
  priority      = {prio1},
}

