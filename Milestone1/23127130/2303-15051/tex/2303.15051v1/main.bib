@Article{BOSCH201876,
  author    = {Bsch,P.M. and Becker,F. and Becker,H. and Axhausen, K.W.},
  title     = {Cost-based analysis of autonomous mobility services},
  journal   = {Transport Policy},
  volume    = {64},
  pages     = {76-91},
  year      = {2018},
  abstract  = {Fast advances in autonomous driving technology trigger the question of suitable operational models for future autonomous vehicles. A key determinant of such operational models viability is the competitiveness of their cost structures. Using a comprehensive analysis of the respective cost structures, this research shows that public transportation (in its current form) will only remain economically competitive where demand can be bundled to larger units. In particular, this applies to dense urban areas, where public transportation can be offered at lower prices than autonomous taxis (even if pooled) and private cars. Wherever substantial bundling is not possible, shared and pooled vehicles serve travel demand more efficiently. Yet, in contrast to current wisdom, shared fleets may not be the most efficient alternative. Higher costs and more effort for vehicle cleaning could change the equation. Moreover, the results suggest that a substantial share of vehicles may remain in private possession and use due to their low variable costs. Even more than today, high fixed costs of private vehicles will continue to be accepted, given the various benefits of a private mobility robot.},
  doi       = {https://doi.org/10.1016/j.tranpol.2017.09.005},
  issn      = {0967-070X},
  owner     = {fpaparella},
