@inproceedings{Holleis2008a,
	author = {Holleis, Paul and Schmidt, Albrecht and Paasovaara, Susanna and Puikkonen, Arto and H\"{a}kkil\"{a}, Jonna},
	title = {Evaluating Capacitive Touch Input on Clothes},
