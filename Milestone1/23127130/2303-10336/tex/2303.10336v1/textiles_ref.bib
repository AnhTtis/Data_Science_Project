@inproceedings{Ho2011a, 
	author = {Ho, Van Anh and Kondo, Daisuke and Okada, Shima and Araki, Takahiho and Fujita, Emi and Makikawa, Masaaki and Hirai, Shinichi}, 
	booktitle = {2011 IEEE/RSJ International Conference on Intelligent Robots and Systems}, 
	title = {Development of a low-profile sensor using electro-conductive yarns in recognition of slippage}, 
	year = {2011}, 
	volume = {}, 
