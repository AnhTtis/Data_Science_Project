@inproceedings{Agcayazi2016a,
	author = {Agcayazi, Talha and McKnight, Michael and Kausche, Hannah and Ghosh, Tushar and Bozkurt, Alper},
	booktitle = {2016 IEEE SENSORS},
	title = {A finger touch force detection method for textile based capacitive tactile sensor arrays},
	year = {2016},
	volume = {},
	number = {},
	pages = {1--3},
	keywords = {capacitive sensors, force sensors, sensor arrays, tactile sensors, textiles, finger touch force detection method, textile based capacitive tactile sensor arrays, insulated object touch event, custom textile, capacitance change, neighboring cross-over points, Fingers, Force, Tactile sensors, Capacitance, Fabrics, Wearable Sensors, Capacitive Tactile Sensors, Finger Detection in Tactile Sensors},
	doi = {10.1109/ICSENS.2016.7808528},
	issn = {},
	month=  {October},
	abstract = {The use of touch-based technology to interact with electronic devices pre-dates modern day multi-touch technology and even the personal computer. It has recently been growing in popularity in wearable computing devices especially in the form of textile based tactile sensor. These sensors often target the detection of not only touch but also force applied. A significant problem arises here in differentiating inputs from an intended finger touch and just a bend of the sensor or other objects touching the sensor. In this work, we present our initial efforts to differentiate between a finger and an insulated object touch event on a custom textile based tactile sensor we developed before. Our experiments show that the two cases could be differentiated using the capacitance change of the neighboring cross-over points.}
}

@inproceedings{Alagi2016a,
	author = {Alagi, H. and Navarro, S. E. and Mende, M. and Hein, B.},
	booktitle = {2016 IEEE Haptics Symposium (HAPTICS)},
	title = {A versatile and modular capacitive tactile proximity sensor},
	year = {2016},
	volume = {},
	number = {},
	pages = {290--296},
	keywords = {capacitive sensors, consumer electronics, human-robot interaction, tactile sensors, versatile capacitive tactile proximity sensor, modular capacitive tactile proximity sensor, robotics, consumer-electronics, HRI, self-capacitive proximity mode, mutual-capacitive proximity mode, mutual-capacitive tactile mode, capacitive measurement principle, proximity measurements, spatial resolution, Robot sensing systems, Electrodes, Current measurement, Capacitance, Voltage measurement, Wires}, 
	doi = {10.1109/HAPTICS.2016.7463192},
	ISSN = {2324-7355},
	month = {April},
	abstract = {In this work we discuss the design and realization of a novel capacitive tactile proximity sensor (CTPS) with applications in robotics and consumer-electronics. The current concept is an advancement with respect to a previous design developed at our lab. Experience in the development of applications based on the old sensor, such as manipulation and safe HRI, helped determining the requirements. The new sensor is capable of operating in a self- and mutual-capacitive proximity mode and in a mutual-capacitive tactile mode. The issue of having a trade-off between spatial resolution and sensing range in proximity mode due to the capacitive measurement principle is addressed. A circuit is designed to dynamically select or join electrodes involved in the proximity measurements, a technique which at the same time allows the implementation of the spatial resolution in the tactile mode. Also, all signal processing is done on board of the prototype sensor module, thus complete modularity is achieved.}
}

@inproceedings{Apriyanto2016a,
	author = {Apriyanto, H. and Ravet, G. and Bernal, O. D. and Cattoen, M. and Lizion, F. and Seat, H. C. and Chavagnac, V.}, 
	booktitle = {2016 IEEE SENSORS},
	title = {A multimode fiber refractive index sensor},
	year = {2016},
	volume = {},
	number = {},
	pages = {1--3},
	keywords = {fibre optic sensors, refractive index measurement, multimode fiber refractive index sensor, refractive index variation, fiber core, stripping, cladding, wavelength 1550 nm, Optical fiber sensors, Optical fibers, Refractive index, Sensitivity, Reflection, Refractive index sensor, Refractometer, Multimode fiber sensor, Evanescent wave, Stripped cladding},
	doi = {10.1109/ICSENS.2016.7808490},
	issn = {},
	month = {October},
	abstract = {We present a simple refractive index sensor based on multimode fibers realized by stripping three different lengths of cladding. We theoretically explain and experimentally validate three mechanisms occurring in the multimode fiber for sensing surrounding refractive index. This sensor has been demonstrated at 1550 nm for a wide range of refractive index variation from 1.3164 to 1.608. Our sensor is very sensitive for measuring refractive index values lower than but close to the refractive index value of the fiber core.}
}

@inproceedings{Eom2016a,
	author = {Eom, Jimi and Lee, Woobin and Kim, Yong-Hoon and Kim, Yong-Hoon},
	booktitle = {2016 IEEE SENSORS},
	title = {Textile-based wearable sensors using metal-nanowire embedded conductive fibers},
	year = {2016},
	volume = {},
	number = {},
	pages = {1--3},
	keywords = {coatings, conducting materials, intelligent sensors, nanofabrication, nanofibres, nanosensors, nanowires, polymers, pressure measurement, pressure sensors, reliability, strain measurement, strain sensors, reliability, sewing machine, nylon fiber, NW, C-PEDOT:PSS, glycerol-doped poly(3,4-ethylene-dioxythiophene):poly(styrene sulfonate), coating, textile-based pressure sensor, textile-based strain sensor, metal-nanowire embedded conductive fiber, textile-based wearable sensor, Optical fiber sensors, Capacitive sensors, Pressure sensors, Strain, Nanowires, Fabrics, Resistance, textile-based sensor, strain sensor, pressure sensor, conductive fiber},
	doi = {10.1109/ICSENS.2016.7808601},
	issn = {},
	month = {October},
	abstract = {We demonstrate textile-based strain and pressure sensors using metal-nanowire embedded conductive fibers. The conductive fibers were fabricated by coating glycerol-doped poly(3,4-ethylene-dioxythiophene):poly(styrene sulfonate) (G-PEDOT:PSS) and silver nanowires (Ag NWs) on nylon fibers. By using a commercially available sewing machine, textile-based strain and pressure sensors were fabricated on fabrics. The textile-based strain sensor showed high sensitivity (maximum gauge factor of ~4) and reliability under applied mechanical strain of 5\%, and also the pressure sensor showed reasonable pressure-responses to different levels of loading pressure.}
}

@inproceedings{Hamdan2016a,
    author = {Hamdan, Nur Al-huda and Blum, Jeffrey R. and Heller, Florian and Kosuru, Ravi Kanth and Borchers, Jan},
