@article{Lemey2014a, 
	author = {Lemey, Sam and Declercq, Frederick and Rogier, Hendrik},
	journal = {Proceedings of the IEEE},
	title = {Textile Antennas as Hybrid Energy-Harvesting Platforms},
	year = {2014},
	volume = {102},
	number = {11},
	pages = {1833--1857},
	keywords = {antenna arrays, biomedical communication, clothing, energy harvesting, wearable antennas, hybrid energy harvesting platforms, smart-fabric interactive-textile systems, textile multiantenna systems, professional garment, energy-efficient stable wireless body-centric communication links, compact integration, power management system, multiple diverse scavenging transducers, storage module, textile antenna topology, wearable system, Antennas, Energy management, Energy harvesting, Photovoltaic cells, Radio frequency, Batteries, Sensors, Textiles, Wearable computers, Renewable energy sources, Energy harvesting, energy management, energy storage, interactive textiles, smart fabrics, textile antenna, wearable antenna, Energy harvesting, energy management, energy storage, interactive textiles, smart fabrics, textile antenna, wearable antenna},
	doi = {10.1109/JPROC.2014.2355872},
	ISSN = {0018-9219},
	month = {November},
	abstract = {Smart-fabric interactive-textile systems offer exciting new possibilities, provided that they exhibit sufficient robustness and autonomy to be reliably deployed in critical applications. Textile multiantenna systems, unobtrusively integrated in a professional garment, are key components of such systems, as they set up energy-efficient and stable wireless body-centric communication links. Yet, their functionality may be further extended by exploiting their surface as energy-harvesting platform. Different state-of-the-art energy harvesters are suitable for compact integration onto a textile antenna. We demonstrate this by integrating a power management system, together with multiple diverse scavenging transducers and a storage module, on a well-chosen textile antenna topology. We provide guidelines to ensure that the additional hardware does not affect the textile antenna's performance. Simultaneous scavenging from different energy sources significantly increases the autonomy of a wearable system, in the meanwhile reducing battery size.}
}

