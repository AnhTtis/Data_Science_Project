	title = {Whole-body covering tactile interface for human robot coordination},
	year = {2002},
	volume = {4},
	number = {},
	pages = {3818-3824 vol.4},
	keywords = {haptic interfaces, robots, man-machine systems, whole-body covering tactile interface, human robot coordination, design method, surface cover sensor, 3D force vector detection, human engineering, humanoid robot, WENDY, locus tracking, force-following, Robot kinematics, Tactile sensors, Robot sensing systems, Design methodology, Force sensors, Human robot interaction, Information analysis, Ergonomics, Sensor phenomena and characterization, Humanoid robots},
	doi = {10.1109/ROBOT.2002.1014315},
	issn = {},
	month = {May},
	abstract = {In this paper, we propose a design method of wholebody tactile interface for elevating human-robot coordination. The proposed surface cover sensor as the tactile interface enables robots to detect accurate 3D force vector applied on various parts of the body during physical interaction with humans. First, after analysis of a variety of contact situations between humans and robots, tactile and force information needed are specified, and the specifications are decided based on knowledge of human engineering. Next, a design method of a cover sensor that can detect accurate force vector and contact position is described. The cover sensor utilizes a force-torque sensor and is surrounded with several touch sensors. The mechanism was implemented on a humanoid robot, WENDY. Finally, evaluation experiments of locus tracking on the surface and force-following to humans were carried out. The experimental results demonstrate the high capabilities of the proposed cover sensor as human-robot interface, and indicate that the proposed design method of whole-body tactile interface is capable of enhancing human-robot coordination.}
}

@inproceedings{Sergio2002a,
	author = {Sergio, Maximilian and Manaresi, Nicol\`{o} and Tartagni, Marco and Guerrieri, Roberto and Canegallo, Roberto},
	booktitle = {SENSORS, 2002 IEEE},
	title = {A textile based capacitive pressure sensor},
	year = {2002},
	volume = {2},
	number = {},
	pages = {1625--1630},
	keywords = {pressure sensors, capacitive sensors, distributed sensors, readout electronics, decoding, arrays, pressure information decoding, capacitive sensing, distributed passive capacitor array, pressure-sensitive fabric, textile based capacitive pressure sensor, array read-out, run-time data post-processing, smart textile approach, wearable computing, Capacitive sensors, Sensor arrays, Capacitors, Capacitance measurement, Pressure measurement, Decoding, Fabrics, Sensor systems, Textile fibers, Performance evaluation},
	doi={10.1109/ICSENS.2002.1037367},
	issn = {},
	month = {June},
	abstract = {This paper introduces an approach for decoding the pressure information exerted over a broad piece of fabric by means of capacitive sensing. The proposed sensor includes a distributed passive array of capacitors (i.e. an array where no active elements are involved), whose capacitance depends on the pressure exerted on the textile surface, and an electronic system that acquire and process the subsequent capacitance variations. Capacitors can be made in different ways, though, in our demonstrator they have been implemented between rows and columns of conductive fibers patterned on the two opposite sides of an elastic synthetic foam. Measures performed over a prototype has been demonstrated the reliability of the approach by detecting pressure images at 3 F/s and by measuring capacitances as low as hundreds of fF spaced apart at meters of distance.}
}

