@Article{SCC.Chien.Peng.ea2002,
  author                     = {Chien, I. L. and Peng, S. C. and Liu, J. H.},
  title                      = {Simple control method for integrating processes with long deadtime},
  journal                    = {J. Process Control},
  year                       = {2002},
  volume                     = {12},
  number                     = {{3}},
  pages                      = {391--404},
  issn                       = {0959-1524},
  abstract                   = {{Control of integrating processes with long deadtime is a challenging problem. Using original Smith predictor control structure will result in an offset problem during load disturbance. Using the internal model control (IMC) principle to design the control structure will result in large overshoot during servo response. In this work, a simple modified Smith predictor controller design is proposed for this important type of system. The overall control structure has only two physically meaningful tuning parameters. One is used to set the speed of the closed-loop servo response and the other tuning parameter is used to set the speed of the closed-loop load response. Two examples are used to demonstrate the improved closed-loop performance of the proposed controller design. (C) 2002 Elsevier Science Ltd. All rights reserved.}},
