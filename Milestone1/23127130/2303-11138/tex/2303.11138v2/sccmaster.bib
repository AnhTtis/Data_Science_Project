@Article{SCC.Wise.Morgan.ea2001,
  author   = {Wise, A. K. and Morgan, D. L. and Gregory, J. E. and Proske, U.},
  title    = {Fatigue in mammalian skeletal muscle stimulated under computer control},
  journal  = {J. Appl. Physiol.},
  year     = {2001},
  volume   = {90},
  number   = {1},
  pages    = {189--197},
  abstract = {Functional electrical stimulation (FES) is used to provide paralyzed human subjects with postural support and a limited range of movements. Problems encountered with FES include jerky movements from tension oscillations during stimulation and rapid muscle fatigue. In this paper, we report experiments on anesthetized cats that test a new, computer-controlled method of stimulation of the muscle nerve supply, distributed across several inputs, which reduces these problems. After 5 min of continuous, distributed stimulation of the medial gastrocnemius muscle at 6 pulses per second (pps) across 6 channels, tension fell to 55.9 ?3.9% (SE) of its original value. In comparison, after 5 min of synchronous stimulation of one muscle portion at 36 pps, tension fell to 11 ?3.7%. At higher stimulation rates, 10 pps per channel (distributed) and 60 pps (synchronous), the differences in fatigue were even greater. Similar results were obtained when an intermittent, rather than a continuous, stimulation protocol was used. These findings indicate that distributed stimulation has important advantages over other methods for applications such as FES.},
  url      = {http://jap.physiology.org/content/90/1/189.abstract},
}

