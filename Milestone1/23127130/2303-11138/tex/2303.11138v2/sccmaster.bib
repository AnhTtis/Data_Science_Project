@Article{SCC.Willems.Rapisarda.ea2005,
  author           = {Willems, Jan C. and Rapisarda, Paolo and Markovsky, Ivan and De Moor, Bart L. M.},
  journal          = {Systems Control Lett.},
  title            = {A note on persistency of excitation},
  year             = {2005},
