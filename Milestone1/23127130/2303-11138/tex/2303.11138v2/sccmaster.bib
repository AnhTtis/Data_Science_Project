@Article{SCC.Walt2014,
  author   = {van der Walt, Jan Harm},
  title    = {Linear and quasi-linear spaces of set-valued maps},
  journal  = {Comput. Math. Appl.},
  year     = {2014},
  volume   = {68},
  number   = {9},
  pages    = {1006--1015},
  month    = nov,
  issn     = {0898-1221},
  abstract = {In this paper we investigate the algebraic structure of certain spaces of set-valued maps. Among other results, we show that for an arbitrary topological space X and a metrisable topological vector space Z, the space M(X,Z) of minimal upper semi-continuous compact valued (musco) maps from X into Z is a linear space. This result extends a previously known result on the linear structure of spaces of musco maps. Previously, this result was known only in the case when X is a Baire space. We also study topologies of uniform convergence on compact sets on M(X,Z).},
