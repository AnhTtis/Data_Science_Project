@Article{SCC.PietLahanier.Siguerdidjane2012,
  author   = {Piet-Lahanier, H\'{e}l\`{e}ne and Siguerdidjane, Houria},
  title    = {Wind detection and reconfiguration of guidance and control law for a small-scaled helicopter},
