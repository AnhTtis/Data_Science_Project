@Article{SCC.Thrasher.Graham.ea2005,
  author      = {Thrasher, A. and Graham, G. M. and Popovic, M. R.},
  title       = {Reducing muscle fatigue due to functional electrical stimulation using random modulation of stimulation parameters},
  journal     = {Artif. Organs},
  year        = {2005},
  volume      = {29},
  number      = {6},
  pages       = {453--458},
  month       = jun,
  abstract    = {A major limitation of many functional electrical stimulation (FES) applications is that muscles tend to fatigue very rapidly. It was hypothesized that FES-induced muscle fatigue could be reduced by randomly modulating the pulse frequency, amplitude, and pulse width in a range of +/-15\%. Seven subjects with spinal-cord injuries participated in this study. FES was applied to quadriceps and tibialis anterior muscles using surface electrodes. Isometric force was measured, and the time for the force to drop by 3 dB (fatigue time) was compared between trials. Four different modes of FES were applied in random order: constant stimulation, randomized frequency, randomized amplitude, and randomized pulse width. There was no significant difference between the fatigue-time measurements for the four modes of stimulation (P=0.329). Therefore, random modulation appeared to have no effect. Based on an observed correlation between maximum force measurements and trial order, we concluded that having 10-min rest periods between trials was insufficient.},
  doi         = {10.1111/j.1525-1594.2005.29076.x},
  medline-pst = {ppublish},
  pii         = {AOR29076},
  pmid        = {15926981},
  school      = {Institute of Biomaterials and Biomedical Engineering, University of Toronto, Canada. adam.thrasher@utoronto.ca},
}

