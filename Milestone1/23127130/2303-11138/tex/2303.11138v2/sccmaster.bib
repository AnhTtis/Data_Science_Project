@Article{SCC.Karafyllis.Malisoff.ea2015,
  author   = {Karafyllis, Iasson and Malisoff, Michael and de Queiroz, Marcio and Krstic, Miroslav and Yang, Ruzhou},
  title    = {Predictor-based tracking for neuromuscular electrical stimulation},
  journal  = {Int. J. Robust Nonlinear Control},
  year     = {2015},
  volume   = {25},
  number   = {14},
  pages    = {2391--2419},
  issn     = {1099-1239},
  abstract = {We present a new tracking controller for neuromuscular electrical stimulation (NMES), which is an emerging technology that artificially stimulates skeletal muscles to help restore functionality to human limbs. The novelty of our work is that we prove that the tracking error globally asymptotically and locally exponentially converges to zero for any positive input delay, coupled with our ability to satisfy a state constraint imposed by the physical system. Also, our controller only requires sampled measurements of the states instead of continuous measurements and allows perturbed sampling schedules, which can be important for practical purposes. Our work is based on a new method for constructing predictor maps for a large class of time-varying systems, which is of independent interest.},
  doi      = {10.1002/rnc.3211},
}

