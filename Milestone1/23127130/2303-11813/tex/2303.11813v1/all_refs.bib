  title         = {{Electrically tunable, room-temperature quantum-cascade lasers}},
  journal       = {Appl. Phys. Lett.},
  year          = {1999},
  volume        = {75},
  number        = {11},
  pages         = {1509--1511},
  issn          = {00036951},
  abstract      = {Electrical tuning of two-section quantum-cascade lasers is systematically investigated as a function of temperature and optical power. In pulsed operation, the active region design exhibits a low threshold current density (5.2 kA/cm2), a high peak 100 mW, and average 3mW powers at 300 K. The strong linear Stark tuning of the laser transition allows large tuning ranges of 40cm1, corresponding to a wavelength tune from 9.75 to 10.15 m at 260 K for a peak optical output power of 10 mW. The tuning range is still 20cm1 at the same optical output power at T300 K},
  bdsk-url-1    = {https://doi.org/10.1063/1.124738},
  date-added    = {2018-12-11 11:07:06 +0100},
  date-modified = {2018-12-11 11:09:46 +0100},
  doi           = {10.1063/1.124738},
}

@Article{monz16a,
  author        = {Monz, Thomas and Nigg, Daniel and Martinez, Esteban A. and Brandl, Matthias F. and Schindler, Philipp and Rines, Richard and Wang, Shannon X. and Chuang, Isaac L. and Blatt, Rainer},
  title         = {{Realization of a scalable Shor algorithm}},
  journal       = {Science},
  year          = {2016},
  volume        = {351},
  number        = {6277},
  pages         = {1068--1070},
  issn          = {0036-8075},
  abstract      = {Quantum computers are able to outperform classical algorithms. This was long recognized by the visionary Richard Feynman who pointed out in the 1980s that quantum mechanical problems were better solved with quantum machines. It was only in 1994 that Peter Shor came up with an algorithm that is able to calculate the prime factors of a large number vastly more efficiently than known possible with a classical computer. This paradigmatic algorithm stimulated the flourishing research in quantum information processing and the quest for an actual implementation of a quantum computer. Over the last fifteen years, using skillful optimizations, several instances of a Shor algorithm have been implemented on various platforms and clearly proved the feasibility of quantum factoring. For general scalability, though, a different approach has to be pursued. Here, we report the realization of a fully scalable Shor algorithm as proposed by Kitaev. For this, we demonstrate factoring the number fifteen by effectively employing and controlling seven qubits and four "cache-qubits", together with the implementation of generalized arithmetic operations, known as modular multipliers. The scalable algorithm has been realized with an ion-trap quantum computer exhibiting success probabilities in excess of 90{\%}.},
  archiveprefix = {arXiv},
  arxivid       = {1507.08852},
  bdsk-url-1    = {http://www.sciencemag.org/cgi/doi/10.1126/science.aad9480},
  bdsk-url-2    = {https://doi.org/10.1126/science.aad9480},
  date-added    = {2018-12-11 11:06:54 +0100},
  date-modified = {2018-12-11 11:06:54 +0100},
  doi           = {10.1126/science.aad9480},
  eprint        = {1507.08852},
  pmid          = {26941315},
  url           = {http://www.sciencemag.org/cgi/doi/10.1126/science.aad9480},
}

