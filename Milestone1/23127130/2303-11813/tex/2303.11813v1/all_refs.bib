@Article{Filsinger:PRL100:133003,
  author        = {Frank Filsinger and Undine Erlekam and Gert von Helden and Jochen K{\"u}pper and Gerard Meijer},
  title         = {Selector for structural isomers of neutral molecules},
  journal       = prl,
  year          = {2008},
  volume        = {100},
  pages         = {133003},
  __markedentry = {[ardita:]},
  abstract      = {We have selected and spatially separated the two conformers of 3-aminophenol
                  (C$_6$H$_7$NO) present in a molecular beam. Analogous to the separation of ions
                  based on their mass-to-charge ratios in a quadrupole mass filter, the neutral
                  conformers are separated based on their different mass-to-dipole-moment ratios in
                  an ac electric quadrupole selector. For a given ac frequency, the individual
                  conformers experience different focusing forces, resulting in different
                  transmissions through the selector. These experiments demonstrate that
                  conformer-selected samples of large molecules can be prepared, offering new
                  possibilities for the study of gas-phase biomolecules.},
  archiveprefix = {arXiv},
  doi           = {10.1103/PhysRevLett.100.133003},
  eprint        = {0802.2795},
  issue         = {13},
  primaryclass  = {physics},
  publisherurl  = {http://link.aps.org/abstract/PRL/v100/e133003},
