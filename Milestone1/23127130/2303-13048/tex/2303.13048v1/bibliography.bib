@Article{Souquet2014,
author={Souquet, J.-R.
and Woolley, M. J.
and Gabelli, J.
and Simon, P.
and Clerk, A. A.},
title={Photon-assisted tunnelling with nonclassical light},
journal={Nature Communications},
year={2014},
month={Nov},
day={26},
volume={5},
number={1},
pages={5562},
abstract={Among the most exciting recent advances in the field of superconducting quantum circuits is the ability to coherently couple microwave photons in low-loss cavities to quantum electronic conductors. These hybrid quantum systems hold great promise for quantum information-processing applications; even more strikingly, they enable exploration of new physical regimes. Here we study theoretically the new physics emerging when a quantum electronic conductor is exposed to nonclassical microwaves (for example, squeezed states, Fock states). We study this interplay in the experimentally relevant situation where a superconducting microwave cavity is coupled to a conductor in the tunnelling regime. We find that the conductor acts as a nontrivial probe of the microwave state: the emission and absorption of photons by the conductor is characterized by a nonpositive definite quasi-probability distribution, which is related to the Glauber--Sudarshan P-function of quantum optics. These negative quasi-probabilities have a direct influence on the conductance of the conductor.},
issn={2041-1723},
doi={10.1038/ncomms6562},
url={https://doi.org/10.1038/ncomms6562}
}

