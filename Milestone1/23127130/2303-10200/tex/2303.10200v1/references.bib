	author = {Belokurov, Vasily and Penoyre, Zephyr and Oh, Semyeong and Iorio, Giuliano and Hodgkin, Simon and Evans, N. Wyn and Everall, Andrew and Koposov, Sergey E. and Tout, Christopher A. and Izzard, Robert and Clarke, Cathie J. and Brown, Anthony G. A.},
	month = jun,
