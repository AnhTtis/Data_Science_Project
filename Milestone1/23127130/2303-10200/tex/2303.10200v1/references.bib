	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Deacon, N. R. and Kraus, A. L. and Mann, A. W. and Magnier, E. A. and Chambers, K. C. and Wainscoat, R. J. and Tonry, J. L. and Kaiser, N. and Waters, C. and Flewelling, H. and Hodapp, K. W. and Burgett, W. S.},
