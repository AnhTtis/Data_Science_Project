	pages = {57},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/YTX3FTM8/Dupuy et al. - 2018 - The Hawaii Infrared Parallax Program. III. 2MASS J.pdf:application/pdf},
}

@article{dupuy_model-independent_2019,
	title = {A {Model}-independent {Mass} and {Moderate} {Eccentricity} for β {Pic} b},
