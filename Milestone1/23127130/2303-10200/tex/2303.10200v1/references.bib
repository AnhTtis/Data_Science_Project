	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Forbes, Duncan A. and Alabi, Adebusola and Romanowsky, Aaron J. and Brodie, Jean P. and Strader, Jay and Usher, Christopher and Pota, Vincenzo},
