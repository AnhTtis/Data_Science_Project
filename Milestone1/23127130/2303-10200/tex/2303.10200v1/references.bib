@article{kerr_stars_2021,
	title = {Stars with {Photometrically} {Young} {Gaia} {Luminosities} {Around} the {Solar} {System} ({SPYGLASS}) {I}: {Mapping} {Young} {Stellar} {Structures} and their {Star} {Formation} {Histories}},
	shorttitle = {Stars with {Photometrically} {Young} {Gaia} {Luminosities} {Around} the {Solar} {System} ({SPYGLASS}) {I}},
	url = {http://arxiv.org/abs/2105.09338},
	abstract = {Young stellar associations hold a star formation record that can persist for millions of years, revealing the progression of star formation long after the dispersal of the natal cloud. To identify nearby young stellar populations that trace this progression, we have designed a comprehensive framework for the identification of young stars, and use it to identify \${\textbackslash}sim\$3\${\textbackslash}times 10{\textasciicircum}4\$ candidate young stars within a distance of 333 pc using Gaia DR2. Applying the HDBSCAN clustering algorithm to this sample, we identify 27 top-level groups, nearly half of which have little to no presence in previous literature. Ten of these groups have visible substructure, including notable young associations such as Orion, Perseus, Taurus, and Sco-Cen. We provide a complete subclustering analysis on all groups with substructure, using age estimates to reveal each region's star formation history. The patterns we reveal include an apparent star formation origin for Sco-Cen along a semicircular arc, as well as clear evidence for sequential star formation moving away from that arc with a propagation speed of \${\textbackslash}sim\$4 km s\${\textasciicircum}\{-1\}\$ (\${\textbackslash}sim\$4 pc Myr\${\textasciicircum}\{-1\}\$). We also identify earlier bursts of star formation in Perseus and Taurus that predate current, kinematically identical active star-forming events, suggesting that the mechanisms that collect gas can spark multiple generations of star formation, punctuated by gas dispersal and cloud regrowth. The large spatial scales and long temporal scales on which we observe star formation offer a bridge between the processes within individual molecular clouds and the broad forces guiding star formation at galactic scales.},
	urldate = {2021-05-21},
	journal = {arXiv:2105.09338 [astro-ph]},
	author = {Kerr, Ronan and Rizzuto, Aaron C. and Kraus, Adam L. and Offner, Stella S. R.},
	month = may,
	year = {2021},
	note = {arXiv: 2105.09338},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies},
	file = {arXiv Fulltext PDF:/Users/loganpearce/Zotero/storage/5Y7P6R33/Kerr et al. - 2021 - Stars with Photometrically Young Gaia Luminosities.pdf:application/pdf;arXiv.org Snapshot:/Users/loganpearce/Zotero/storage/LDKDHZA5/2105.html:text/html},
}

