@article{galli_kinematic_2013,
	title = {A kinematic study and membership analysis of the {Lupus} star-forming region},
	volume = {558},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2013A%26A...558A..77G/abstract},
	doi = {10.1051/0004-6361/201220704},
	abstract = {{\textless}BR /{\textgreater} Aims: A precise determination of the distance to individual stars is required to reliably determine the fundamental parameters (mass and age) of young stellar objects. This paper is dedicated to investigating the kinematic properties of the Lupus moving group of young stars with the primary objective of deriving individual parallaxes for each group member. {\textless}BR /{\textgreater} Methods: We identify those stars in the Lupus star-forming region that define the comoving association of young stars by utilizing our new and improved convergent point search method that allows us to derive the precise position of the convergent point of the comoving association from the stars' proper motions. We used published proper motion catalogs and searched the literature for radial velocities, which are needed to compute individual parallaxes. We supplemented the radial velocity data with new measurements from spectroscopic observations performed with the FEROS spectrograph mounted on the MPG/ESO 2.2 m telescope at La Silla. {\textless}BR /{\textgreater} Results: We identify a comoving group with 109 pre-main sequence stars and candidates that define the kinematic properties of the Lupus low-mass star-forming region. We derive individual parallaxes for stars with known radial velocity and tentative parallaxes for the remaining group members by assuming that all stars share the same space motion. The convergent point method, combined with the k-NN algorithm, makes it possible to distinguish the Lupus and Upper Centaurus Lupus stars from the adjacent Scorpius-Centaurus association. We find significant depth effects in this region and show that the classical T Tauri stars, located in the close vicinity of the Lupus molecular clouds, form a background population, while the weak-emission line T Tauri stars are dispersed not only in angular extent but also in depth. {\textless}BR /{\textgreater} Conclusions: The newly derived individual parallaxes will be used in a forthcoming paper to refine the masses and ages of Lupus T Tauri stars, with the aim of better constraining the lifetimes of their circumstellar, protoplanetary disks. {\textless}P /{\textgreater}Based partly on observations collected at the European Southern Observatory, Chile (ESO Programme 087.C-0315).Tables 4, 7, and Appendices A, B are available in electronic form at {\textless}A href="http://www.aanda.org"{\textgreater}http://www.aanda.org{\textless}/A{\textgreater}},
	language = {en},
	urldate = {2021-08-09},
	journal = {Astronomy \&amp; Astrophysics, Volume 558, id.A77, {\textless}NUMPAGES{\textgreater}21{\textless}/NUMPAGES{\textgreater} pp.},
	author = {Galli, P. a. B. and Bertout, C. and Teixeira, R. and Ducourant, C.},
	month = oct,
	year = {2013},
	pages = {A77},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/S5JLZNI2/Galli et al. - 2013 - A kinematic study and membership analysis of the L.pdf:application/pdf;Snapshot:/Users/loganpearce/Zotero/storage/JJNJ7JUM/abstract.html:text/html},
}

