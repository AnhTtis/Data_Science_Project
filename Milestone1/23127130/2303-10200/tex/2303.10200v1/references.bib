@article{kraus_coevality_2009,
	title = {The {Coevality} of {Young} {Binary} {Systems}},
	volume = {704},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/2009ApJ...704..531K},
	doi = {10.1088/0004-637X/704/1/531},
	abstract = {Multiple star systems are commonly assumed to form coevally; they thus provide the anchor for most calibrations of stellar evolutionary models. In this paper, we study the binary population of the Taurus-Auriga association, using the component positions in an HR diagram in order to quantify the frequency and degree of coevality in young binary systems. After identifying and rejecting the systems that are known to be affected by systematic errors (due to further multiplicity or
obscuration by circumstellar material), we find that the relative binary ages, {\textbar}Δlog τ{\textbar}, have an overall dispersion
σ{\textbar}Δlog τ{\textbar}{\textasciitilde} 0.40 dex. Random pairs of Taurus
members are coeval only to within σ{\textbar}Δlog τ{\textbar}{\textasciitilde}
0.58 dex, indicating that Taurus binaries are indeed more coeval than the association as a whole. However, the distribution of {\textbar}Δlog
τ{\textbar} suggests two populations, with {\textasciitilde}2/3 of the sample appearing
coeval to within the errors (σ{\textbar}Δlog τ{\textbar}{\textasciitilde} 0.16
dex) and the other {\textasciitilde}1/3 distributed in an extended tail reaching {\textbar}Δlog τ{\textbar}{\textasciitilde} 0.4-0.9 dex. To explain the finding of a multipeaked
distribution, we suggest that the tail of the differential age
distribution includes unrecognized hierarchical multiples, stars seen in scattered light, or stars with disk contamination; additional follow-up is required to rule out or correct for these explanations. The relative coevality of binary systems does not depend significantly on the system mass, mass ratio, or separation. Indeed, any pair of Taurus members wider than {\textasciitilde}10' ({\textasciitilde}0.7 pc) shows the full age spread of the association.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Hillenbrand, Lynne A.},
	month = oct,
	year = {2009},
	keywords = {stars: fundamental parameters, stars: pre-main sequence, binaries: general, stars: evolution, Hertzsprung-Russell (HR) diagram, open clusters and associations: individual: Taurus-Auriga},
	pages = {531--547},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/8GVME6DX/Kraus and Hillenbrand - 2009 - The Coevality of Young Binary Systems.pdf:application/pdf},
}

@article{kraus_mass-radius-rotation_2011,
	title = {The {Mass}-{Radius}(-{Rotation}?) {Relation} for {Low}-mass {Stars}},
	volume = {728},
	issn = {0004-637X},
	shorttitle = {The {Mass}-{Radius}(-{Rotation}?},
	url = {http://adsabs.harvard.edu/abs/2011ApJ...728...48K},
	doi = {10.1088/0004-637X/728/1/48},
	abstract = {The fundamental properties of low-mass stars are not as well understood as those of their more massive counterparts. The best method for constraining these properties, especially masses and radii, is to study eclipsing binary systems, but only a small number of late-type ({\textgreater}=M0) systems have been identified and well characterized to date. We present the discovery and characterization of six new M dwarf eclipsing binary systems. The 12 stars in these eclipsing systems have masses spanning 0.38-0.59 M sun and orbital periods of 0.6-1.7 days, with typical uncertainties of {\textasciitilde}0.3\% in mass and {\textasciitilde}0.5\%-2.0\% in radius. Combined with six known systems with high-precision measurements, our results reveal an intriguing trend in the low-mass regime. For stars with M= 0.35-0.80 M sun, components in short-period binary systems (Plsim1 day; 12 stars) have radii which are inflated by up to 10\% (μ = 4.8\% ± 1.0\%) with respect to evolutionary models for
low-mass main-sequence stars, whereas components in longer-period systems ({\textgreater}1.5 days; 12 stars) tend to have smaller radii (μ = 1.7\% ± 0.7\%). This trend supports the hypothesis that short-period
systems are inflated by the influence of the close companion, most likely because they are tidally locked into very high rotation speeds that enhance activity and inhibit convection. In summary, very close binary systems are not representative of typical M dwarfs, but our results for longer-period systems indicate that the evolutionary models are broadly valid in the M{\textasciitilde} 0.35-0.80 M sun regime.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Tucker, Roy A. and Thompson, Michael I. and Craine, Eric R. and Hillenbrand, Lynne A.},
	month = feb,
	year = {2011},
	keywords = {stars: fundamental parameters, binaries: eclipsing, stars: low-mass, stars: late-type, stars: rotation},
	pages = {48},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/DA77JPS5/Kraus et al. - 2011 - The Mass-Radius(-Rotation) Relation for Low-mass .pdf:application/pdf},
}

@article{kraus_mapping_2011,
	title = {Mapping the {Shores} of the {Brown} {Dwarf} {Desert}. {II}. {Multiple} {Star} {Formation} in {Taurus}-{Auriga}},
	volume = {731},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/2011ApJ...731....8K},
	doi = {10.1088/0004-637X/731/1/8},
	abstract = {We have conducted a high-resolution imaging study of the Taurus-Auriga 
star-forming region in order to characterize the primordial outcome of
multiple star formation and the extent of the brown dwarf desert. Our
survey identified 16 new binary companions to primary stars with masses
of 0.25-2.5 M sun, raising the total number of binary pairs
(including components of high-order multiples) with separations of
3-5000 AU to 90. We find that {\textasciitilde}2/3-3/4 of all Taurus members are
multiple systems of two or more stars, while the other {\textasciitilde}1/4-1/3 appear
to have formed as single stars; the distribution of high-order
multiplicity suggests that fragmentation into a wide binary has no
impact on the subsequent probability that either component will fragment
again. The separation distribution for solar-type stars (0.7-2.5 M
sun) is nearly log-flat over separations of 3-5000 AU, but
lower-mass stars (0.25-0.7 M sun) show a paucity of binary
companions with separations of gsim200 AU. Across this full mass range,
companion masses are well described with a linear-flat function; all
system mass ratios (q = MB /MA ) are equally
probable, apparently including substellar companions. Our results are
broadly consistent with the two expected modes of binary formation
(free-fall fragmentation on large scales and disk fragmentation on small
scales), but the distributions provide some clues as to the epochs at
which the companions are likely to form.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Ireland, Michael J. and Martinache, Frantz and Hillenbrand, Lynne A.},
	month = apr,
	year = {2011},
	keywords = {stars: pre-main sequence, binaries: general, brown dwarfs, stars: low-mass},
	pages = {8},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/YJ5GGBXT/Kraus et al. - 2011 - Mapping the Shores of the Brown Dwarf Desert. II. .pdf:application/pdf},
}

@article{kraus_lkca_2012,
	title = {{LkCa} 15: {A} {Young} {Exoplanet} {Caught} at {Formation}?},
	volume = {745},
	issn = {0004-637X},
	shorttitle = {{LkCa} 15},
	url = {http://adsabs.harvard.edu/abs/2012ApJ...745....5K},
	doi = {10.1088/0004-637X/745/1/5},
	abstract = {Young and directly imaged exoplanets offer critical tests of planet-formation models that are not matched by radial velocity surveys of mature stars. These targets have been extremely elusive to date, with no exoplanets younger than 10-20 Myr and only a handful of direct-imaged exoplanets at all ages. We report the direct-imaging discovery of a likely (proto)planet around the young ({\textasciitilde}2 Myr) solar analog LkCa 15, located inside a known gap in the protoplanetary disk (a "transitional disk"). Our observations use non-redundant aperture masking
interferometry at three epochs to reveal a faint and relatively blue point source (M\_\{K{\textasciicircum}\{{\textbackslash}prime \}\}=9.1 +/- 0.2, K' - L' = 0.98 ±
0.22), flanked by approximately co-orbital emission that is red and resolved into at least two sources (M\_\{L{\textasciicircum}\{{\textbackslash}prime \}\}=7.5+/- 0.2, K' - L' = 2.7 ± 0.3; M\_\{L{\textasciicircum}\{{\textbackslash}prime \}\}=7.4+/- 0.2, K' - L' = 1.94 ±
0.16). We propose that the most likely geometry consists of a newly formed (proto)planet that is surrounded by dusty material. The nominal estimated mass is {\textasciitilde}6 M Jup according to the 1 Myr hot-start models. However, we argue based on its luminosity, color, and the presence of circumplanetary material that the planet has likely been caught at its epoch of assembly, and hence this mass is an upper limit due to its extreme youth and flux contributed by accretion. The
projected separations (71.9 ± 1.6 mas, 100.7 ± 1.9 mas,
and 88.2 ± 1.8 mas) and deprojected orbital radii (16, 21, and 19 AU) correspond to the center of the disk gap, but are too close to the primary star for a circular orbit to account for the observed inner edge of the outer disk, so an alternative explanation (i.e., additional planets or an eccentric orbit) is likely required. This discovery is the first direct evidence that at least some transitional disks do indeed host newly formed (or forming) exoplanetary systems, and the observed properties provide crucial insight into the gas giant formation process.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Ireland, Michael J.},
	month = jan,
	year = {2012},
	keywords = {planets and satellites: detection, planets and satellites: formation, protoplanetary disks, stars: pre-main sequence, stars: individual: LkCa 15},
	pages = {5},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/BJ752MLN/Kraus and Ireland - 2012 - LkCa 15 A Young Exoplanet Caught at Formation.pdf:application/pdf},
}

@article{kraus_role_2012,
	title = {The {Role} of {Multiplicity} in {Disk} {Evolution} and {Planet} {Formation}},
	volume = {745},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/2012ApJ...745...19K},
	doi = {10.1088/0004-637X/745/1/19},
	abstract = {The past decade has seen a revolution in our understanding of protoplanetary disk evolution and planet formation in single-star systems. However, the majority of solar-type stars form in binary systems, so the impact of binary companions on protoplanetary disks is an important element in our understanding of planet formation. We have compiled a combined multiplicity/disk census of Taurus-Auriga, plus a restricted sample of close binaries in other regions, in order to explore the role of multiplicity in disk evolution. Our results imply that the tidal influence of a close (lsim40 AU) binary companion significantly hastens the process of protoplanetary disk dispersal, as {\textasciitilde}2/3 of all close binaries promptly disperse their disks within lsim1 Myr after formation. However, prompt disk dispersal only occurs for a small fraction of wide binaries and single stars, with {\textasciitilde}80\%-90\%
retaining their disks for at least {\textasciitilde}2-3 Myr (but rarely for more than {\textasciitilde}5 Myr). Our new constraints on the disk clearing timescale have
significant implications for giant planet formation; most single stars have 3-5 Myr within which to form giant planets, whereas most close binary systems would have to form giant planets within lsim1 Myr. If core accretion is the primary mode for giant planet formation, then gas giants in close binaries should be rare. Conversely, since almost all single stars have a similar period of time within which to form gas giants, their relative rarity in radial velocity (RV) surveys indicates either that the giant planet formation timescale is very well matched to the disk dispersal timescale or that features beyond the disk lifetime set the likelihood of giant planet formation.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Ireland, Michael J. and Hillenbrand, Lynne A. and Martinache, Frantz},
	month = jan,
	year = {2012},
	keywords = {planets and satellites: formation, protoplanetary disks, stars: pre-main sequence, stars: formation, binaries: visual, binaries: close},
	pages = {19},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/E4WUM3L6/Kraus et al. - 2012 - The Role of Multiplicity in Disk Evolution and Pla.pdf:application/pdf},
}

