@article{close_diffraction-limited_2013,
	title = {Diffraction-limited {Visible} {Light} {Images} of {Orion} {Trapezium} {Cluster} with the {Magellan} {Adaptive} {Secondary} {Adaptive} {Optics} {System} ({MagAO})},
