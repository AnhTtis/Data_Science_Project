	pages = {200},
	file = {arXiv Fulltext PDF:/Users/loganpearce/Zotero/storage/A8HE7UIW/Ruffio et al. - 2019 - Radial Velocity Measurements of HR 8799 b and c wi.pdf:application/pdf;arXiv.org Snapshot:/Users/loganpearce/Zotero/storage/ET84DXAB/1909.html:text/html},
