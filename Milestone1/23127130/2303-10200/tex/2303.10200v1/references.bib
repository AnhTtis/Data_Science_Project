	pages = {195},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/GQGRJEYB/Rizzuto et al. - 2018 - Zodiacal Exoplanets in Time (ZEIT). VIII. A Two-pl.pdf:application/pdf},
}

