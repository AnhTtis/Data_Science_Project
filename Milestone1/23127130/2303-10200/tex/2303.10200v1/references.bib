	pages = {3152--3160},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/F7WAM6DC/Malhotra and Zhang - 2020 - On the divergence of first-order resonance widths .pdf:application/pdf},
}

@article{duncan_long-term_1989,
