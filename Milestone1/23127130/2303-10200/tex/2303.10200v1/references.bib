@article{kraus_usco_2007,
	title = {{USco} {J1606}-1935: {An} {Unusually} {Wide} {Low}-{Mass} {Triple} {System}?},
	volume = {664},
	issn = {0004-637X},
	shorttitle = {{USco} {J1606}-1935},
	url = {http://adsabs.harvard.edu/abs/2007ApJ...664.1167K},
	doi = {10.1086/518826},
	abstract = {We present photometric, astrometric, and spectroscopic observations of USco J160611.9-193532 AB, a candidate ultrawide ({\textasciitilde}1600 AU), low-mass (Mtot{\textasciitilde}0.4 Msolar) multiple system in the nearby OB association Upper Scorpius. We conclude that both components are young, comoving members of the association; we also present high-resolution observations that show that the primary is itself a close binary system. If the Aab and B components are gravitationally bound, the system would fall into the small class of young multiple systems that have unusually wide separations as compared to field systems of similar mass. However, we demonstrate that physical association cannot be assumed purely on probabilistic grounds for any individual candidate system in this separation range. Analysis of the association's two-point correlation function shows that there is a significant probability (25\%) that at least one pair of low-mass association members will be separated in projection by {\textless}{\textasciitilde}15", so analysis of the wide binary population in Upper Sco will require a systematic search for all wide systems; the detection of another such pair would represent an excess at the 98\% confidence level.},
	urldate = {2020-05-18},
	journal = {The Astrophysical Journal},
	author = {Kraus, Adam L. and Hillenbrand, Lynne A.},
	month = aug,
	year = {2007},
	keywords = {Stars: Pre-Main-Sequence, Stars: Binaries: General, Brown Dwarfs, Stars: Low-Mass, stars: individual ([PBB2002] USco J160611.9-193532)},
	pages = {1167--1175},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/ME2KHXI7/Kraus and Hillenbrand - 2007 - USco J1606-1935 An Unusually Wide Low-Mass Triple.pdf:application/pdf},
