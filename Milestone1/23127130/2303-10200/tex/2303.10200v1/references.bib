@article{artymowicz_dynamics_1992,
	title = {Dynamics of binary and planetary-system interaction with disks - {Eccentricity} changes},
	volume = {104},
	issn = {0004-6280},
	url = {http://adsabs.harvard.edu/abs/1992PASP..104..769A},
	doi = {10.1086/133051},
	abstract = {Protostellar and protoplanetary systems, as well as merging galactic 
nuclei, often interact tidally and resonantly with the astrophysical
disks via gravity. Underlying our understanding of the formation
processes of stars, planets, and some galaxies is a dynamical theory of
such interactions. Its main goals are to determine the geometry of the
binary-disk system and, through the torque calculations, the rate of
change of orbital elements of the components. We present some recent
developments in this field concentrating on eccentricity driving
mechanisms in protoplanetary and protobinary systems. In those two types
of systems the result of the interaction is opposite. A small body
embedded in a disk suffers a decrease of orbital eccentricity, whereas
newly formed binary stars surrounded by protostellar disks may undergo a
significant orbital evolution increasing their eccentricities.},
	urldate = {2020-05-18},
	journal = {Publications of the Astronomical Society of the Pacific},
	author = {Artymowicz, Pawel},
	month = sep,
	year = {1992},
	keywords = {Binary Stars, Celestial Mechanics, Protostars, Planetary Evolution, Planetary Systems, Gravitational Effects, Interacting Galaxies, Protoplanets, Quasars},
	pages = {769--774},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/I5UJGZPV/Artymowicz - 1992 - Dynamics of binary and planetary-system interactio.pdf:application/pdf},
}

