	keywords = {planets and satellites: detection, planets and satellites: formation, planets and satellites: dynamical evolution and stability, binaries: general, binaries: visual, binaries: close},
	pages = {8},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/4EQLIBPB/Kraus et al. - 2016 - The Impact of Stellar Multiplicity on Planetary Sy.pdf:application/pdf},
}

