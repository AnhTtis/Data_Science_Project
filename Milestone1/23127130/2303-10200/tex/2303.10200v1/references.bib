@article{bell_self-consistent_2015,
	title = {A self-consistent, absolute isochronal age scale for young moving groups in the solar neighbourhood},
	volume = {454},
	issn = {0035-8711},
	url = {http://adsabs.harvard.edu/abs/2015MNRAS.454..593B},
	doi = {10.1093/mnras/stv1981},
	abstract = {We present a self-consistent, absolute isochronal age scale for young ( 
≲ 200 Myr), nearby ( ≲ 100 pc) moving groups in the solar
neighbourhood based on homogeneous fitting of semi-empirical
pre-main-sequence model isochrones using the τ2
maximum-likelihood fitting statistic of Naylor \& Jeffries in the
MV, V - J colour-magnitude diagram. The final adopted ages
for the groups are as follows: 149{\textasciicircum}\{+51\}\_\{-19\} \{Myr\} for the AB Dor
moving group, 24 ± 3 Myr for the β Pic moving group (BPMG),
45{\textasciicircum}\{+11\}\_\{-7\} \{Myr\} for the Carina association, 42{\textasciicircum}\{+6\}\_\{-4\} \{Myr\} for
the Columba association, 11 ± 3 Myr for the η Cha cluster, 45
± 4 Myr for the Tucana-Horologium moving group (Tuc-Hor), 10
± 3 Myr for the TW Hya association and 22{\textasciicircum}\{+4\}\_\{-3\} \{Myr\} for the
32 Ori group. At this stage we are uncomfortable assigning a final,
unambiguous age to the Argus association as our membership list for the
association appears to suffer from a high level of contamination, and
therefore it remains unclear whether these stars represent a single
population of coeval stars. Our isochronal ages for both the BPMG and
Tuc-Hor are consistent with recent lithium depletion boundary (LDB)
ages, which unlike isochronal ages, are relatively insensitive to the
choice of low-mass evolutionary models. This consistency between the
isochronal and LDB ages instils confidence that our self-consistent,
absolute age scale for young, nearby moving groups is robust, and hence
we suggest that these ages be adopted for future studies of these
groups. Software implementing the methods described in this study is
available from http://www.astro.ex.ac.uk/people/timn/tau-squared/.},
	urldate = {2021-04-28},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Bell, Cameron P. M. and Mamajek, Eric E. and Naylor, Tim},
	month = nov,
	year = {2015},
	keywords = {Hertzsprung-Russell and colour-magnitude diagrams, solar neighbourhood, stars: evolution, stars: pre-main-sequence, open clusters and associations: general, techniques: photometric},
	pages = {593--614},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/TGQADVF2/Bell et al. - 2015 - A self-consistent, absolute isochronal age scale f.pdf:application/pdf},
}

@article{david_ages_2015,
	title = {The {Ages} of {Early}-type {Stars}: {Strömgren} {Photometric} {Methods} {Calibrated}, {Validated}, {Tested}, and {Applied} to {Hosts} and {Prospective} {Hosts} of {Directly} {Imaged} {Exoplanets}},
	volume = {804},
	issn = {0004-637X},
	shorttitle = {The {Ages} of {Early}-type {Stars}},
	url = {http://adsabs.harvard.edu/abs/2015ApJ...804..146D},
	doi = {10.1088/0004-637X/804/2/146},
	abstract = {Age determination is undertaken for nearby early type (BAF) stars, which 
constitute attractive targets for high-contrast debris disk and planet
imaging surveys. Our analysis sequence consists of acquisition of
uvbyβ photometry from catalogs, correction for the effects of
extinction, interpolation of the photometry onto model atmosphere grids
from which atmospheric parameters are determined, and finally,
comparison to the theoretical isochrones from pre-main sequence through
post-main sequence stellar evolution models, accounting for the effects
of stellar rotation. We calibrate and validate our methods at the
atmospheric parameter stage by comparing our results to fundamentally
determined Teff and log g values. We validate and test
our methods at the evolutionary model stage by comparing our results on
ages to the accepted ages of several benchmark open clusters (IC 2602,
α Persei, Pleiades, Hyades). Finally, we apply our methods to
estimate stellar ages for 3493 field stars, including several with
directly imaged exoplanet candidates.},
	urldate = {2021-04-27},
	journal = {The Astrophysical Journal},
	author = {David, Trevor J. and Hillenbrand, Lynne A.},
	month = may,
	year = {2015},
	keywords = {stars: early-type, stars: fundamental parameters, planetary systems, stars: evolution, catalogs, Hertzsprung–Russell and C–M diagrams},
	pages = {146},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/F8WRD9LU/David and Hillenbrand - 2015 - The Ages of Early-type Stars Strömgren Photometri.pdf:application/pdf},
}

