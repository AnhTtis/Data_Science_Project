	keywords = {stars: activity, stars: variables: general, stars: individual: Boyajian’s Star, stars: peculiar: except chemically peculiar},
	pages = {77},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/7N3I9KX6/Simon et al. - 2018 - Where Is the Flux Going The Long-term Photometric.pdf:application/pdf},
}

@article{hitchcock_non-gray_2019,
	title = {Non-gray, {Month}-long {Brightening} of {KIC} 8462852 in the {Immediate} {Aftermath} of a {Deep} {Dip}},
	volume = {131},
	issn = {0004-6280},
	url = {http://adsabs.harvard.edu/abs/2019PASP..131h4204H},
	doi = {10.1088/1538-3873/ab254f},
	abstract = {We present an analysis of the results of long-term, multi-band 
photometric monitoring of the enigmatic star, KIC 8462852. Observations
in the B, g‧, V, r‧, and I C passbands have
been acquired at University College London Observatory between 2017 May
and 2018 September. We interrogate the wavelength dependence of the
∼month-long dimming and brightening exhibited by the target star
over an 85 day interval, immediately following a days-long ∼5\% drop
in brightness on Julian Date (JD) 2,458,203. Between JD 2,458,215-300 we
measure brightness variations which correspond to relative extinctions
of \{A\}B/\{A\}V=1.39+/- 0.27, A 
g‧/A V = 1.16 ± 0.11, A 
r‧/A V = 0.80 ± 0.25 and A Ic
/A V = 0.49 ± 0.19, from which we infer an
Ångström absorption coefficient of 1.33 ± 0.43
(\{R\}V≃ \{3.2\}-1.0+2.6). As with the
days-long “dips”, the wavelength dependence of the
longer-term brightness variations must also be associated with
extinction arising from a dust distribution containing a substantial
fraction of submicron-sized grains. This implies some common mechanism
is responsible for the star’s variability over both these short
and longer timescales.},
	urldate = {2020-06-10},
