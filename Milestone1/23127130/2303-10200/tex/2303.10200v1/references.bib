	title = {The {Complete} {Light}-curve {Sample} of {Spectroscopically} {Confirmed} {Type} {Ia} {Supernovae} from {Pan}-{STARRS1} and {Cosmological} {Constraints} from {The} {Combined} {Pantheon} {Sample}},
