	keywords = {circumstellar matter, stars: pre-main sequence, planetary systems: formation, open clusters and associations: individual: Upper Scorpius, planetary systems: protoplanetary disks},
	pages = {1646--1671},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/W6WEVXMU/Carpenter et al. - 2009 - Debris Disks in the Upper Scorpius OB Association.pdf:application/pdf},
