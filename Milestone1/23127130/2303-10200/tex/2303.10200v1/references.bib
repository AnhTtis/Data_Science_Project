	keywords = {planets and satellites: formation, stars: pre-main sequence, stars: individual: GSC 06214-00210},
	pages = {148},
	file = {Full Text PDF:/Users/loganpearce/Zotero/storage/HQNQTFHI/Bowler et al. - 2011 - A Disk around the Planetary-mass Companion GSC 062.pdf:application/pdf},
}

