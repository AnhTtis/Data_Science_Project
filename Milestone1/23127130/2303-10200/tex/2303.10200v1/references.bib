	author = {Winn, Joshua N. and Johnson, John Asher and Howard, Andrew W. and Marcy, Geoffrey W. and Bakos, Gáspár Á. and Hartman, Joel and Torres, Guillermo and Albrecht, Simon and Narita, Norio},
