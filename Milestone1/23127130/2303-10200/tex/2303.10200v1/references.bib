	keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	file = {arXiv Fulltext PDF:/Users/loganpearce/Zotero/storage/M8C6UUAT/Tofflemire et al. - 2021 - TESS Hunt for Young and Maturing Exoplanets (THYME.pdf:application/pdf;arXiv.org Snapshot:/Users/loganpearce/Zotero/storage/KATILLS9/2102.html:text/html},
}

