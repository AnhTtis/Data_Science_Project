@article{batalha_color_2018,
	title = {Color {Classification} of {Extrasolar} {Giant} {Planets}: {Prospects} and {Cautions}},
	volume = {156},
	issn = {0004-6256},
	shorttitle = {Color {Classification} of {Extrasolar} {Giant} {Planets}},
