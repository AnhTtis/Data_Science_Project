@article{kervella_stellar_2019,
	title = {Stellar and substellar companions of nearby stars from \textit{{Gaia}} {DR2}: {Binarity} from proper motion anomaly},
	volume = {623},
	issn = {0004-6361, 1432-0746},
	shorttitle = {Stellar and substellar companions of nearby stars from \textit{{Gaia}} {DR2}},
	url = {https://www.aanda.org/10.1051/0004-6361/201834371},
	doi = {10.1051/0004-6361/201834371},
	abstract = {Methods. Orbiting secondary bodies inﬂuence the proper motion of their parent star through their gravitational reﬂex motion. Using the Hipparcos and Gaia’s second data release (GDR2) catalogs, we determined the long-term proper motion of the stars common to these two catalogs. We then searched for a proper motion anomaly (PMa) between the long-term proper motion vector and the GDR2 (or Hipparcos) measurements, indicative of the presence of a perturbing secondary object. We focussed our analysis on the 6741 nearby stars located within 50 pc, and we also present a catalog of the PMa for 99\% of the Hipparcos catalog (≈117 000 stars).
Results. 30\% of the stars studied present a PMa greater than 3σ. The PMa allows us to detect orbiting companions, or set stringent limits on their presence. We present a few illustrations of the PMa analysis to interesting targets. We set upper limits of 0.1−0.3 MJ to potential planets orbiting Proxima between 1 and 10 au (Porb = 3 to 100 years). We conﬁrm that Proxima is gravitationally bound to α Cen. We recover the masses of the known companions of Eri, Ind, Ross 614 and β Pic. We also detect the signature of a possible planet of a few Jovian masses orbiting τ Ceti.
Conclusions. Based on only 22 months of data, the GDR2 has limitations. But its combination with the Hipparcos catalog results in very high accuracy PMa vectors, that already enable us to set valuable constraints on the binarity of nearby objects. The detection of tangential velocity anomalies at a median accuracy of σ(∆vT) = 1.0 m s−1 per parsec of distance is already possible with the GDR2. This type of analysis opens the possibility to identify long period orbital companions otherwise inaccessible. For long orbital periods, Gaia’s complementarity to radial velocity and transit techniques (that are more sensitive to short orbital periods) already appears to be remarkably powerful.},
	language = {en},
	urldate = {2021-01-13},
	journal = {A\&A},
	author = {Kervella, Pierre and Arenou, Frédéric and Mignard, François and Thévenin, Frédéric},
	month = mar,
	year = {2019},
	pages = {A72},
	file = {Kervella et al. - 2019 - Stellar and substellar companions of nearby stars .pdf:/Users/loganpearce/Zotero/storage/4WDNKGPS/Kervella et al. - 2019 - Stellar and substellar companions of nearby stars .pdf:application/pdf},
}

