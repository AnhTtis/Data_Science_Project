@article{Jülicher_2018,
	doi = {10.1088/1361-6633/aab6bb},
	url = {https://dx.doi.org/10.1088/1361-6633/aab6bb},
	year = {2018},
	month = {jun},
	publisher = {IOP Publishing},
	volume = {81},
	number = {7},
	pages = {076601},
	author = {Frank Jülicher and Stephan W Grill and Guillaume Salbreux},
	title = {Hydrodynamic theory of active matter},
	journal = {Rep. Prog. Phys.},
	abstract = {We review the general hydrodynamic theory of active soft materials that is motivated in particular by biological matter. We present basic concepts of irreversible thermodynamics of spatially extended multicomponent active systems. Starting from the rate of entropy production, we identify conjugate thermodynamic fluxes and forces and present generic constitutive equations of polar active fluids and active gels. We also discuss angular momentum conservation which plays a role in the the physics of active chiral gels. The irreversible thermodynamics of active gels provides a general framework to discuss the physics that underlies a wide variety of biological processes in cells and in multicellular tissues.}
}

