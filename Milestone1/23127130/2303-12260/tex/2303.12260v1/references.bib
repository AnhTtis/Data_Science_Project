	journal = {Experiments in Fluids},
	keywords = {Impingement ; Fluid Dynamics ; Tanks ; Reynolds Number ; Water Tanks ; Hydrodynamics ; Reynolds Number ; Velocity ; Network Design ; Physics and Chemistry;},
