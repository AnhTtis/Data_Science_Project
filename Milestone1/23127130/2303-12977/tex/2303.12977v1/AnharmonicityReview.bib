@article{anderson2000,
    author = {Anderson, Orson L.},
    title = "{The Grüneisen ratio for the last 30 years}",
    journal = {Geophysical Journal International},
    volume = {143},
    number = {2},
    pages = {279-294},
    year = {2000},
    month = {11},
    abstract = "{Knopoff \\&amp; Shapiro (1969) reviewed the status of the Grüneisen ratio, γ=(αKTV/CV), in particular with regard to its use in geophysics. They presented equations for γ that were commonly used at that time and pointed out that these equations were marred because they apparently did not account for the effect of two important properties, shear displacement and dispersion (frequency versus wavenumber). I review the progress in research on γ for the past 30 years and show what has been done to overcome or bypass these two defects. I also show that the experimental database has increased sufficiently to test a basic assumption in the Mie–Grüneisen equation of state (EoS) derivation, namely that γ is independent of T. The basic assumption should be that γ is independent of T at constant V, and many solids fail this test in that (∂γ/∂T)V is not zero. This weakens the assumption that the Mie–Grüneisen EoS can be used reliably at high T without a careful check. The main progress, made for closely packed solids, is in the determination of γ for the lower mantle and the core. Little progress has been made for loosely packed solids, so the value of γ for the upper mantle and the lithosphere is still an open question.}",
    issn = {0956-540X},
    doi = {10.1046/j.1365-246X.2000.01266.x},
}


