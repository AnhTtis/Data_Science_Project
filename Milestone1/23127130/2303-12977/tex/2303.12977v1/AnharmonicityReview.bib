@Article{data2,
author={Bergmann, G.
and Rainer, D.},
title={The sensitivity of the transition temperature to changes in a2F(o)},
journal={Zeitschrift f{\"u}r Physik},
year={1973},
volume={263},
number={1},
pages={59-68},
abstract={The transition temperature of a superconductor depends on a2F(o), the spectral function of the effective interaction due to phonon exchange. We discuss how strongly the transition temperature is influenced by different frequency parts of a2F(o). For this purpose the functional derivative dTc/da2F(o) is calculated. It is shown that all frequency regions of a2F(o) yield a positive contribution toTcand that the most effective range covers frequencies, slightly above 2pTc. The functional derivative is calculated numerically for several superconductors from their measured a2F(o)-spectra. Finally, we discuss the change in transition temperature due to the softening of a2F(o) which has been observed in amorphous superconductors.},
issn={0044-3328},
doi={10.1007/BF02351862},
url={https://doi.org/10.1007/BF02351862}
}

@article{data1,
title = "McMillan's equation and the Tc of superconductors",
journal = "Solid State Communications",
volume = "10",
number = "7",
pages = "615 - 618",
year = "1972",
issn = "0038-1098",
doi = "https://doi.org/10.1016/0038-1098(72)90603-5",
url = "http://www.sciencedirect.com/science/article/pii/0038109872906035",
author = "R.C. Dynes",
abstract = "Compiling available superconducting tunneling data, we compare experimentally measured critical temperatures with those calculated using the McMillan equation. It is found that this equation gives Tc quite accurately for all these materials when the average phonon energy <ω> is defined in one particular way.
Résumé
En compliant des données existantes sur l'effet tunnel des supraconducteurs, on compare les valeurs de la température critique, mesurees expérimentalement, aux valeurs calculées a l'aide de l'équation de McMillan. On constate que cette équation donne une Tc très precise pour tous ces matériaux lorsque l'énergie moyenne <ω> des phonons est définie d'une manière particulière."
}







@Article{Tanaka2008,
author={Shintani, Hiroshi
and Tanaka, Hajime},
title={Universal link between the boson peak and transverse phonons in glass},
journal={Nature Materials},
year={2008},
volume={7},
number={11},
pages={870-877},
abstract={The physical properties of a topologically disordered amorphous material (glass), such as heat capacity and thermal conductivity, are markedly different from those of its ordered crystalline counterpart. The understanding of these phenomena is a notoriously complex problem. One of the universal features of disordered glasses is the 'boson peak', which is observed in neutron and Raman scattering experiments. The boson peak is typically ascribed to an excess density of vibrational states. Here, we study the nature of the boson peak, using numerical simulations of several glass-forming systems. We discovered evidence suggestive of the equality of the boson peak frequency to the Ioffe-Regel limit for 'transverse' phonons, above which transverse phonons no longer propagate. Our results indicate a possibility that the origin of the boson peak is transverse vibrational modes associated with defective soft structures in the disordered state. Furthermore, we suggest a possible link between slow structural relaxation and fast boson peak dynamics in glass-forming systems.},
issn={1476-4660},
doi={10.1038/nmat2293},
url={https://doi.org/10.1038/nmat2293}
}









