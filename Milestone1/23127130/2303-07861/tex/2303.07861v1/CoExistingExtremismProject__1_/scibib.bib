mendeley-groups = {_icwsm.22.bitchute},
number = {9},
pages = {1--36},
title = {{Dehumanisation of Outgroups' on Facebook and Twitter: towards a framework for assessing online hate organisations and actors}},
volume = {1},
year = {2021}
}
