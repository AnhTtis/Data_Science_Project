file = {:Users/cbuntain/Documents/Mendeley/Qazi, Imran, Ofli/SIGSPATIAL Special/Qazi, Imran, Ofli - 2020 - GeoCoV19 A Dataset of Hundreds of Millions of Multilingual COVID-19 Tweets with Location Information.pdf:pdf},
journal = {SIGSPATIAL Special},
mendeley-groups = {iscram2021,_icwsm.22.bitchute},
month = {jun},
number = {1},
pages = {6--15},
publisher = {Association for Computing Machinery},
title = {{GeoCoV19: A Dataset of Hundreds of Millions of Multilingual COVID-19 Tweets with Location Information}},
