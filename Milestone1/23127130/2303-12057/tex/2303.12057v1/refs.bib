@article{russell_2017,
author = {Annelise Russell},
title ={{U.S.} Senators on {Twitter}: Asymmetric Party Rhetoric in 140 Characters},
journal = {American Politics Research},
volume = {46},
number = {4},
pages = {695-723},
year = {2018},
doi = {10.1177/1532673X17715619},

URL = { 
        https://doi.org/10.1177/1532673X17715619
    
},
eprint = { 
        https://doi.org/10.1177/1532673X17715619
    
}
,
    abstract = { The U.S. Senate is a party-polarized institution where divisive political rhetoric stems from the partisan divide. Senators regularly chastise political opponents, but not all senators are equally critical. Research finds that elite party polarization is asymmetrical with greater divergence by Republicans, so I expect Republican senators to mimic that trend with higher levels of partisan rhetoric. To assess the variance in partisan rhetoric, I catalogue senators’ Twitter activity during the first 6 months of the 113th and 114th Congresses, and find that Republicans are more likely to name-call their Democratic opponents and to make expressions of intraparty loyalty, particularly when they are the minority party. }
}

@article{johnson_seconddim,
author = {Lauren R. Johnson and Deon McCray and Jordan M. Ragusa},
title ={\#NeverTrump: Why Republican members of Congress refused to support their party’s nominee in the 2016 presidential election},
journal = {Research \& Politics},
volume = {5},
number = {1},
pages = {2053168017749383},
year = {2018},
doi = {10.1177/2053168017749383},

URL = { 
        https://doi.org/10.1177/2053168017749383
    
},
eprint = { 
        https://doi.org/10.1177/2053168017749383
    
}
,
    abstract = { In an election characterized by countless headlines, the refusal of Republicans to support their party’s nominee was a constant topic of discussion in 2016. Our paper looks to explain why Republican members of Congress joined the so-called \#NeverTrump movement. In the first part, we document the varied—and often contradictory—explanations of the \#NeverTrump movement offered by journalists, pundits, and politicians during the campaign. We then categorize these popular explanations into four theoretical categories: policy preferences, identity, electoral motivations, and establishment dynamics. In the second part, we test the varied claims. We believe two findings stand out and have broader implications for American politics. First, despite the popular belief that members of Congress are single-minded in their pursuit of reelection, we found that a lawmaker’s religion and sex—both in the identity category—had the largest effects on the decision to join the \#NeverTrump movement. Second, the results show that establishment Republicans were more likely to support Donald Trump’s candidacy. Notably, the direction of this effect is inconsistent with popular explanations of the \#NeverTrump movement but consistent with a range of academic studies. }
}

