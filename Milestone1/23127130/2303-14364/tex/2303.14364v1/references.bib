@article{ARULSELVAN2014214,
    title = {A note on the set union knapsack problem},
    journal = {Discrete Applied Mathematics},
    volume = {169},
    pages = {214-218},
    year = {2014},
    issn = {0166-218X},
    doi = {https://doi.org/10.1016/j.dam.2013.12.015},
    url = {https://www.sciencedirect.com/science/article/pii/S0166218X1300588X},
    author = {Ashwin Arulselvan},
    keywords = {Approximation, Densest -subgraph, Set-union knapsack, Greedy},
    abstract = {Recently, Khuller, Moss and Naor presented a greedy algorithm for the budgeted maximum coverage problem. In this note, we observe that this algorithm also approximates a special case of a set-union knapsack problem within a constant factor. In the special case, an element is a member of less than a constant number of subsets. This guarantee naturally extends to densest k-subgraph problem on graphs of bounded degree.}
