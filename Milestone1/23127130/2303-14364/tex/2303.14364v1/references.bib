@article{franke-arnold_magneto-optical_2001,
	title = {Magneto-optical effects with cold lithium atoms},
	volume = {34},
	url = {http://stacks.iop.org/0953-4075/34/i=12/a=316},
	abstract = {We report measurements of magneto-rotational effects in the coherent forward scattering of light on an ensemble of cold 7 Li atoms. The effect of the cold atomic cloud on the polarization state of a probe beam is studied for magnetic fields in the Faraday and Voigt configurations. Magneto-optical effects with cold atoms have the potential to be highly sensitive tools for the spatially and temporally resolved detection of either small atomic ensembles or small magnetic fields.},
