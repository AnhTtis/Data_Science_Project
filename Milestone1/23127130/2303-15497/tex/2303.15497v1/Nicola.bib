@ARTICLE{Dudarev_et_al:1998,
   author = {S.~L.~Dudarev and G.~A.~Botton and S.~Y.~Savrasov and C.~J.~Humphreys and A.~P.~Sutton},
   title = "Electron-energy-loss spectra and the structural stability of nickel
  oxide: An {LSDA+U} study",
   journal = PRB,
   volume = 57,
   pages = {1505-1509},
   year = 1998,
   summary = {
An alternative approach of the LSDA+U method is presented. The usual
Hartree-Fock electron-electron Hamiltonian with Hartree and exchange term in
terms of the occupation number operators n_{m\sigma} with m-independent U anf
J is taken as a starting point. This Hamiltonian gives an energy difference
for states with integer/noninteger occupation numbers with the same total
occupation number N_sigma = sum_m n_m\sigma. LSDA doesn't give such an energy
difference. Therfore the LSDA+I total energy is obtained by adding the
expression for this enery difference to the LSDA total energy. For integer
occupation number this difference vanishes. The resulting expressions depend
only on U_eff = U - J. This method is then applied to investigate the
electronic structure of NiO.
},
}

