@Article{Junquera03,
  author = 	 {Javier Junquera and Magali Zimmer and Pablo Ordej\'on and Philippe Ghosez},
  title = 	 {First-principles calculation of the band offset at BaO/BaTiO3 and SrO/SrTiO3 interfaces},
  journal = 	 {Phys. Rev. B },
  year = 	 {2003},
  OPTkey = 	 {},
  OPTvolume = 	 {},
  number = 	 {67},
  pages = 	 {155327},
  OPTmonth = 	 {},
  OPTnote = 	 {},
  OPTannote = 	 {We report first-principles density-functional pseudopotential calculations on the atomic structures, electronic properties, and band offsets of BaO/BaTiO3 and SrO/SrTiO3 nanosized heterojunctions grown on top of a silicon substrate. The density of states at the junction does not reveal any electronic induced interface states. A dominant perovskite character is found at the interface layer. The tunability of the band offset with the strain conditions imposed by the substrate is studied. Using previously reported theoretical data available for Si/SrO, Si/BaO, SrTiO3/Pt, and BaTiO3/SrRuO3 interfaces we extrapolate the band alignments along two gate stacks of technological interest: Si/SrO/SrTiO3/Pt and Si/BaO/BaTiO3/SrRuO3 heterostructures. }
}


