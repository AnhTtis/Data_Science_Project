@inproceedings{RN1717,
   author = {Achsan, Harry and Wibowo, Wahyu C. and Achsan, Muhammad M. Baswara and Purnama, Diki G. and Lubis, Kenny B.},
   title = {The Quality of Indonesian Scientific Articles and Its Neighboring Countries},
   booktitle = {2019 IEEE Conference of Russian Young Researchers in Electrical and Electronic Engineering (EIConRus)},
   pages = {174-178},
   abstract = {The Indonesian Government has announced several policies to mandate lecturers and students, the vast majority of Indonesian researchers, to publish scientific articles. The policies are considered very effective; it increased the quantity of Indonesian scientific articles tremendously. Unfortunately, we did not know the quality of the articles. The objective of this research is to assess the quality of Indonesian scientific articles. For comparison purpose, we also assess the quality of Indonesia neighboring countries scientific articles i.e. Malaysia, Singapore, Thailand, and Vietnam. This research used three methods to assess the quality of scientific papers, they were data mining, bibliometrics, and scientometrics. The dataset used was mined from Scopus web database. The results of this research are: a) the quality of Indonesian scientific articles is the lowest, b) efficiency and effectivity of Indonesian scientific articles is low, and c) research fund is directly proportional to research quality.},
