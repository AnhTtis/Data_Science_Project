@article{Banach1924,
abstract = {Nous \'{e}tudions dans cette note les notions de l'\'{e}quivalence des ensembles de points par d\'{e}composition finie, resp. d\'{e}nombrable. Les principaux r\'{e}sultats contenus dans le pr\'{e}sent article sont les suivants: Th\'{e}or\`{e}me: Dans un espace euclidien \`{a} n &#8805; 3 dimensions deux ensembles arbitraires, bornes et contenant des points int\'{e}rieurs (par exemple deux sph\`{e}res a rayons diff\'{e}rentes), sont \'{e}quivalents par d\'{e}composition finie. Un th\'{e}or\`{e}me analogue subsiste pour les ensembles situes sur la surface d'une sph\`{e}re, mais le th\'{e}or\`{e}me correspondant concernant l'espace euclidien \`{a} 1 ou 2 dimensions est faux. D'autre part: Th\'{e}or\`{e}me: Dans un espace euclidien \`{a} $n \geq 1$ dimensions deux ensembles arbitraires (bornes ou non), contenant des points int\'{e}rieures, sont \'{e}quivalents par d\'{e}composition d\'{e}nombrable.},
author = {Banach, Stefan and Tarski, Alfred},
journal = {Fundamenta Mathematicae},
keywords = {podzia\l zbioru; twierdzenie Banacha-Tarskiego; przestrze\'{n} euklidesowa; rozk\lad zbioru punkt\'{o}w na cz&#281;&#347;ci odpowiednio przystaj&#261;ce},
language = {fre},
number = {1},
pages = {244-277},
title = {Sur la d\'{e}composition des ensembles de points en parties respectivement congruentes},
url = {http://eudml.org/doc/214280},
volume = {6},
year = {1924}
}

