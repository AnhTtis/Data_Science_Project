@article{MYROSHNYCHENKO20182516,
title = {Grünbaum's inequality for sections},
journal = {Journal of Functional Analysis},
volume = {275},
number = {9},
pages = {2516-2537},
year = {2018},
issn = {0022-1236},
doi = {https://doi.org/10.1016/j.jfa.2018.04.001},
url = {https://www.sciencedirect.com/science/article/pii/S0022123618301393},
author = {S. Myroshnychenko and M. Stephen and N. Zhang},
keywords = {Centroid, Convex Bodies, Grünbaum's inequality, Sections},
abstract = {We show∫E∩θ+f(x)dx∫Ef(x)dx≥(kγ+1(n+1)γ+1)kγ+1γ for all k-dimensional subspaces E⊂Rn, θ∈E∩Sn−1, and all γ-concave functions f:Rn→[0,∞) with γ>0, 0<∫Rnf(x)dx<∞, and ∫Rnxf(x)dx at the origin o∈Rn. Here, θ+:={x:〈x,θ〉≥0}. As a consequence of this result, we get the following generalization of Grünbaum's inequality:volk(K∩E∩θ+)volk(K∩E)≥(kn+1)k for all convex bodies K⊂Rn with centroid at the origin, k-dimensional subspaces E⊂Rn, and θ∈E∩Sn−1. The lower bounds in both of our inequalities are the best possible, and we discuss the equality conditions.}
}

