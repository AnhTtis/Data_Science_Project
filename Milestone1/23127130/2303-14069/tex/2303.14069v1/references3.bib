	pages = {34},
	file = {arXiv Fulltext PDF:/Users/andrewlitteken/Zotero/storage/VWUK6DGS/Hadfield et al. - 2019 - From the Quantum Approximate Optimization Algorith.pdf:application/pdf;arXiv.org Snapshot:/Users/andrewlitteken/Zotero/storage/24G9USCK/1709.html:text/html},
}

