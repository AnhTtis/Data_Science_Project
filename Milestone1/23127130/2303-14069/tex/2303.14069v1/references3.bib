}

@article{propson_robust_2021,
	title = {Robust {Quantum} {Optimal} {Control} with {Trajectory} {Optimization}},
	url = {http://arxiv.org/abs/2103.15716},
	abstract = {The ability to engineer high-fidelity gates on quantum processors in the presence of systematic errors remains the primary barrier to achieving quantum advantage. Quantum optimal control methods have proven effective in experimentally realizing high-fidelity gates, but they require exquisite calibration to be performant. We apply robust trajectory optimization techniques to suppress gate errors arising from system parameter uncertainty. We propose a derivative-based approach that maintains computational efficiency by using forward-mode differentiation. Additionally, the effect of depolarization on a gate is typically modeled by integrating the Lindblad master equation, which is computationally expensive. We employ a computationally efficient model and utilize time-optimal control to achieve high-fidelity gates in the presence of depolarization. We apply these techniques to a fluxonium qubit and suppress simulated gate errors due to parameter uncertainty below \$10{\textasciicircum}\{-7\}\$ for static parameter deviations on the order of \$1{\textbackslash}\%\$.},
	language = {en},
	urldate = {2022-04-14},
	journal = {arXiv:2103.15716 [quant-ph]},
	author = {Propson, Thomas and Jackson, Brian E. and Koch, Jens and Manchester, Zachary and Schuster, David I.},
	month = mar,
	year = {2021},
	note = {arXiv: 2103.15716},
	keywords = {Quantum Physics, Quantum Optimal Control},
	file = {Propson et al. - 2021 - Robust Quantum Optimal Control with Trajectory Opt.pdf:/Users/andrewlitteken/Zotero/storage/PDMSDEZ5/Propson et al. - 2021 - Robust Quantum Optimal Control with Trajectory Opt.pdf:application/pdf},
}

