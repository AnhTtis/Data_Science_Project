@article{bocharov_factoring_2017,
	title = {Factoring with qutrits: {Shor}'s algorithm on ternary and metaplectic quantum architectures},
	volume = {96},
	shorttitle = {Factoring with qutrits},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.96.012306},
	doi = {10.1103/PhysRevA.96.012306},
	abstract = {We determine the cost of performing Shor's algorithm for integer factorization on a ternary quantum computer, using two natural models of universal fault-tolerant computing: (i) a model based on magic state distillation that assumes the availability of the ternary Clifford gates, projective measurements, classical control as its natural instrumentation set; (ii) a model based on a metaplectic topological quantum computer (MTQC). A natural choice to implement Shor's algorithm on a ternary quantum computer is to translate the entire arithmetic into a ternary form. However, it is also possible to emulate the standard binary version of the algorithm by encoding each qubit in a three-level system. We compare the two approaches and analyze the complexity of implementing Shor's period-finding function in the two models. We also highlight the fact that the cost of achieving universality through magic states in MTQC architecture is asymptotically lower than in generic ternary case.},
	number = {1},
	urldate = {2022-07-06},
	journal = {Physical Review A},
	author = {Bocharov, Alex and Roetteler, Martin and Svore, Krysta M.},
	month = jul,
	year = {2017},
	note = {Publisher: American Physical Society},
	pages = {012306},
	file = {APS Snapshot:/Users/andrewlitteken/Zotero/storage/RTZJJG8C/PhysRevA.96.html:text/html;Full Text PDF:/Users/andrewlitteken/Zotero/storage/FXRZQLCS/Bocharov et al. - 2017 - Factoring with qutrits Shor's algorithm on ternar.pdf:application/pdf},
}

@article{bocharov_improved_2016,
	title = {Improved {Quantum} {Ternary} {Arithmetics}},
	url = {http://arxiv.org/abs/1512.03824},
	doi = {10.48550/arXiv.1512.03824},
	abstract = {Qutrit (or ternary) structures arise naturally in many quantum systems, particularly in certain non-abelian anyon systems. We present efficient circuits for ternary reversible and quantum arithmetics. Our main result is the derivation of circuits for two families of ternary quantum adders, namely ripple carry adders and carry look-ahead adders. The main difference to the binary case is the more complicated form of the ternary carry, which leads to higher resource counts for implementations over a universal ternary gate set. Our ternary ripple adder circuit has a circuit depth of \$O(n)\$ and uses only \$1\$ ancilla, making it more efficient in both, circuit depth and width than previous constructions. Our ternary carry lookahead circuit has a circuit depth of only \$O({\textbackslash}log{\textbackslash},n)\$, while using with \$O(n)\$ ancillas. Our approach works on two levels of abstraction: at the first level, descriptions of arithmetic circuits are given in terms of gates sequences that use various types of non-Clifford reflections. At the second level, we break down these reflections further by deriving them either from the two-qutrit Clifford gates and the non-Clifford gate \$C(X): {\textbar}i,j{\textbackslash}rangle {\textbackslash}mapsto {\textbar}i, j + {\textbackslash}delta\_\{i,2\} {\textbackslash}mod 3{\textbackslash}rangle\$ or from the two-qutrit Clifford gates and the non-Clifford gate \$P\_9={\textbackslash}mbox\{diag\}(e{\textasciicircum}\{-2 {\textbackslash}pi {\textbackslash}, i/9\},1,e{\textasciicircum}\{2 {\textbackslash}pi {\textbackslash}, i/9\})\$. The two choices of elementary gate sets correspond to two possible mappings onto two different prospective quantum computing architectures which we call the metaplectic and the supermetaplectic basis, respectively. Finally, we develop a method to factor diagonal unitaries using multi-variate polynomial over the ternary finite field which allows to characterize classes of gates that can be implemented exactly over the supermetaplectic basis.},
	urldate = {2022-07-06},
	author = {Bocharov, Alex and Cui, Shawn X. and Roetteler, Martin and Svore, Krysta M.},
	month = jun,
	year = {2016},
	note = {arXiv:1512.03824 [quant-ph]
type: article},
	keywords = {Quantum Physics, Computer Science - Emerging Technologies},
	file = {arXiv Fulltext PDF:/Users/andrewlitteken/Zotero/storage/RXB5XIL8/Bocharov et al. - 2016 - Improved Quantum Ternary Arithmetics.pdf:application/pdf;arXiv.org Snapshot:/Users/andrewlitteken/Zotero/storage/V8H2C6XR/1512.html:text/html},
}

