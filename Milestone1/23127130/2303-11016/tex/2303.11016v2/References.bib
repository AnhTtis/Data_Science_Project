@article{adam,
title = {{Massive Schwinger Model within Mass Perturbation Theory}},
journal = {Ann. Phys.},
volume = {259},
number = {1},
pages = {1-63},
year = {1997},
issn = {0003-4916},
doi = {https://doi.org/10.1006/aphy.1997.5697},
url = {https://www.sciencedirect.com/science/article/pii/S0003491697956979},
author = {Christoph Adam},
abstract = {In this article we give a detailed discussion of the mass perturbation theory of the massive Schwinger model. After discussing some general features and briefly reviewing the exact solution of the massless case, we compute the vacuum energy density of the massive model and some related quantities. We derive the Feynman rules of mass perturbation theory and discuss the exactn-point functions with the help of the Dyson–Schwinger equations. Further, we identify the stable and unstable bound states of the theory and compute some bound-state masses and decay widths. Finally we discuss scattering processes, where the resonances and particle production thresholds of the model are properly taken into account by our methods}
