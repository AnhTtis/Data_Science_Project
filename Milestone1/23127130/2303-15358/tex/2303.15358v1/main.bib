@article{Matsuyama2022,
title = {{Multiplicity, localization, and domains in the Hartree–Fock ground state of the two-dimensional Hubbard model}},
journal = {Annals of Physics},
volume = {442},
pages = {168922},
year = {2022},
issn = {0003-4916},
doi = {https://doi.org/10.1016/j.aop.2022.168922},
url = {https://www.sciencedirect.com/science/article/pii/S0003491622001014},
author = {Kazue Matsuyama and Jeff Greensite},
keywords = {Hubbard model, Hartree–Fock, Localization, Strongly correlated systems},
abstract = {We explore certain properties of the Hartree–Fock approximation to the ground state of the two-dimensional Hubbard model, emphasizing the fact that in the Hartree approach there is an enormous multiplicity of self-consistent solutions which are nearly degenerate in energy, reminiscent of a spin glass, but which may differ substantially in other bulk properties. It is argued that this multiplicity is physically relevant at low temperatures. We study the localization properties of the one-particle wavefunctions comprising the Hartree–Fock states, and find that these are unlocalized at small and moderate values of U/t, in particular in the stripe region, but become highly localized at values corresponding to strong repulsion. We also find rectangular domains as well as stripes in the stripe region of the phase diagram, and study pair correlations in the neighborhood of half-filling.}
}


