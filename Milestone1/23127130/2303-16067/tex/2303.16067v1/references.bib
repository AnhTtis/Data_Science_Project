{10.1007/s11689-009-9023-x}, institution = {Cellular Biology, Harvard 
University, 52 Oxford St., Cambridge, MA 02138.}, language    = {eng}, 
medline-pst = {ppublish}, owner       = {vross}, pmid        = {20664807}, 
timestamp   = {2011.09.22}, }

@Article{Gold2007, author      = {Gold, Joshua I. and Shadlen, Michael N.}, 
title       = {The neural basis of decision making.}, journal     = {Annu Rev 
Neurosci}, year        = {2007}, volume      = {30}, pages       = {535--574}, 
abstract    = {The study of decision making spans such varied fields as 
neuroscience, psychology, economics, statistics, political science, and computer 
science. Despite this diversity of applications, most decisions share common 
