@Article{Chklovskii2004b,
  author        = {Chklovskii, D B and Mel, B W and Svoboda, K},
  journal       = {Nature},
  title         = {Cortical rewiring and information storage},
  year          = {2004},
  issn          = {1476-4687 (Electronic)},
  month         = oct,
  number        = {7010},
  pages         = {782--788},
  volume        = {431},
  abstract      = {Current thinking about long-term 
memory in the cortex is focused on changes in the strengths of connections 
between neurons. But ongoing structural plasticity in the adult brain, including 
synapse formation/elimination and remodelling of axons and dendrites, suggests 
that memory could also depend on learning-induced changes in the cortical 
'wiring diagram'. Given that the cortex is sparsely connected, wiring plasticity 
could provide a substantial boost in storage capacity, although at a cost of 
more elaborate biological machinery and slower learning.},
