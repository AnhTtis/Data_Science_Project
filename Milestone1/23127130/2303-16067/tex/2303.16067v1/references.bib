@Article{Crick1995, author      = {Crick, F. and Mitchison, G.}, title       = 
{REM sleep and neural nets.}, journal     = {Behav Brain Res}, year        = 
{1995}, volume      = {69}, number      = {1-2}, pages       = {147--155}, 
abstract    = {The broad features of rapid eye movement (REM) sleep are 
reviewed. Memory storage in the brain is probably quite unlike that in a digital 
computer, being distributed, superimposed and robust. Such memory systems are 
easily overloaded. If the stored memories share common features, random 
stimulation often produces mixed outputs. Simulations show that such overloading 
can be reduced by a process we call 'reverse learning'. We propose that this 
