@Article{Squire2004e, author      = {Squire, Larry R.}, title       = {Memory 
systems of the brain: a brief history and current perspective.}, journal     = 
{Neurobiol Learn Mem}, year        = {2004}, volume      = {82}, number      = 
{3}, pages       = {171--177}, abstract    = {The idea that memory is composed 
of distinct systems has a long history but became a topic of experimental 
inquiry only after the middle of the 20th century. Beginning about 1980, 
evidence from normal subjects, amnesic patients, and experimental animals 
converged on the view that a fundamental distinction could be drawn between a 
kind of memory that is accessible to conscious recollection and another kind 
that is not. Subsequent work shifted thinking beyond dichotomies to a view, 
grounded in biology, that memory is composed of multiple separate systems 
