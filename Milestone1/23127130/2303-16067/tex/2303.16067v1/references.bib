@Article{Yang2009, author      = {Guang Yang and Feng Pan and Wen-Biao Gan}, 
title       = {{S}tably maintained dendritic spines are associated with lifelong 
memories.}, journal     = {Nature}, year        = {2009}, volume      = {462}, 
number      = {7275}, pages       = {920--924}, abstract    = {Changes in 
synaptic connections are considered essential for learning and memory formation. 
However, it is unknown how neural circuits undergo continuous synaptic changes 
during learning while maintaining lifelong memories. Here we show, by following 
postsynaptic dendritic spines over time in the mouse cortex, that learning and 
novel sensory experience lead to spine formation and elimination by a protracted 
