@Article{Schulz2006, author      = {Schulz, David J.}, title       = {Plasticity 
and stability in neuronal output via changes in intrinsic excitability: it's 
what's inside that counts.}, journal     = {J Exp Biol}, year        = {2006}, 
volume      = {209}, number      = {Pt 24}, pages       = {4821--4827}, abstract 
   = {The nervous system faces an extremely difficult task. It must be flexible, 
both during development and in adult life, so that it can respond to a variety 
of environmental demands and produce adaptive behavior. At the same time the 
nervous system must be stable, so that the neural circuits that produce behavior 
function throughout the lifetime of the animal and that changes produced by 
learning endure. We are only beginning to understand how neural networks strike 
a balance between altering individual neurons in the name of plasticity, while 
maintaining long-term stability in neural system function. The balance of this 
plasticity and stability in neural networks undoubtedly plays a critical role in 
the normal functioning of the nervous system. While mechanisms of synaptic 
plasticity have garnered extensive study over the past three decades, it is only 
recently that more attention has been turned to plasticity of intrinsic 
excitability as a key player in neural network function. This review will focus 
on this emerging area of research that undoubtedly will contribute a great deal 
to our understanding of the functionality of the nervous system.}, bdsk-url-1  = 
{http://dx.doi.org/10.1242/jeb.02567}, doi         = {10.1242/jeb.02567}, 
institution = {Biological Sciences, University of Missouri-Columbia, Columbia, 
MO 65211, USA. SchulzD@missouri.edu}, keywords    = {Animals; Homeostasis; 
Long-Term Potentiation, physiology; Membrane Potentials; Nerve Net, 
injuries/physiology; Neuronal Plasticity, physiology}, language    = {eng}, 
medline-pst = {ppublish}, owner       = {vross}, pii         = {209/24/4821}, 
pmid        = {17142671}, timestamp   = {2013.10.01}, }

