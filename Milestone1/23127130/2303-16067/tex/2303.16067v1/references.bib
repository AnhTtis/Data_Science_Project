@Article{Miller1993, Title                    = {{S}uppression of visual 
responses of neurons in inferior temporal cortex of the awake macaque by 
addition of a second stimulus.}, Author                   = {E. K. Miller and P. 
M. Gochin and C. G. Gross}, journal = {Brain Res}, Year         
