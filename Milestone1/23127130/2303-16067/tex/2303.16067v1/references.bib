@Article{Groc2006, Title                    = {{AMPA} signalling in nascent 
glutamatergic synapses: there and not there!}, Author                   = 
{Laurent Groc and Bengt Gustafsson and Eric Hanse}, journal = 
{Trends Neurosci}, Year                     = {2006}, Number                   = 
{3}, Pages                    = {132--139}, Volume                   = {29},

  Abstract                 = {Nascent glutamatergic synapses are thought to be 
equipped with only NMDA receptors and to mature in a stepwise fashion when AMPA 
receptors are acquired later, through specific patterns of activity. We review 
recent data suggesting that AMPA receptors are in fact present in the nascent 
synapse but in a labile state. The nascent synapse can easily switch between 
AMPA-signalling and AMPA-silent states in a manner not requiring activation of 
NMDA receptors or metabotropic glutamate receptors. NMDA receptor activation by 
correlated presynaptic and postsynaptic activity can switch the nascent synapse 
to a mature, more stable state, in which AMPA receptor signalling is modified 
only through conventional plasticity processes. Thus, the AMPA receptor silence 
of nascent glutamatergic synapses depends on the synaptic activation history 
rather than on the nascent state itself.}, Bdsk-url-1               = 
{http://dx.doi.org/10.1016/j.tins.2006.01.005}, Doi                      = 
{10.1016/j.tins.2006.01.005}, Institution              = {Physiologie Cellulaire 
de la Synapse, CNRS-UMR 5091, Universit? Bordeaux 2, 33077 Bordeaux, France. 
laurent.groc@u-bordeaux2.fr}, Keywords                 = {Animals; Brain, 
