@Article{VanRullen2005, author      = {R. VanRullen and L. Reddy and L. 
Fei-Fei}, title       = {{B}inding is a local problem for natural objects and 
scenes.}, journal     = {Vision Res}, year        = {2005}, volume      = {45}, 
pages       = {3133--3144}, abstract    = {Current theories hold that attention 
is necessary for binding the features of a visual object into a coherent 
representation, implying that interference should be observed when two objects 
must be recognized simultaneously: this is the well-known binding problem. 
Recent studies have suggested, however, that discriminating isolated natural 
scenes, objects or faces might be possible in the near absence of attention. It 
is still unclear what mechanisms underlie this remarkable ability. Here, we 
investigate whether the binding problem affects natural objects in the same way 
as other stimuli: is interference observed when two natural objects or scenes 
must be simultaneously processed? We show that in the presence of competing 
objects, performance in the near absence of attention depends on the relative 
distance between stimuli: discrimination is good for stimuli far enough apart, 
and poor for close enough stimuli. In contrast, seemingly simpler but unfamiliar 
synthetic objects could not be bound in the near absence of attention, 
independent of the distance between them. Thus, natural objects are special in 
that they suffer from the binding problem, but only locally. We surmise that 
this particular type of local binding for natural objects and scenes could be 
"hardwired" by dedicated neuronal populations.}, bdsk-url-1  = 
{http://dx.doi.org/10.1016/j.visres.2005.05.012}, doi         = 
{10.1016/j.visres.2005.05.012}, institution = {Centre de Recherche Cerveau et 
Cognition, CNRS-UPS, 133 Rte de Narbonne, 31062 Toulouse Cedex, France. 
rufin@klab.caltech.edu}, keywords    = {Adult; Attention; Discrimination 
(Psychology); Face; Humans; Orientation; Pattern Recognition, Visual; Photic 
Stimulation; Recogni; tion (Psychology)}, owner       = {vross}, pii         = 
{S0042-6989(05)00259-2}, pmid        = {16023696}, timestamp   = {2007.07.27}, }

