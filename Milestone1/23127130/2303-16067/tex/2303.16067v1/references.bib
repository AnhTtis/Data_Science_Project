@Article{Reich2001, author      = {D. S. Reich and F. Mechler and J. D. Victor}, 
title       = {{I}ndependent and redundant information in nearby cortical 
neurons.}, journal     = {Science}, year        = {2001}, volume      = {294}, 
number      = {5551}, pages       = {2566--2568}, abstract    = {In the primary 
visual cortex (V1), nearby neurons are tuned to similar stimulus features, and, 
depending on the manner and time scale over which neuronal signals are analyzed, 
the resulting redundancy may mitigate deleterious effects of response 
variability. We estimated information rates in the short-time scale responses of 
