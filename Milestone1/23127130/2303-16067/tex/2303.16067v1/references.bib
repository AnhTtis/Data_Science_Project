@Article{Segal1989, author      = {Segal, M. and Greenberger, V. and Markram, 
H.}, title       = {Presynaptic cholinergic action in the hippocampus.}, journal 
    = {EXS}, year        = {1989}, volume      = {57}, pages       = {88--96}, 
abstract    = {The hippocampus is among the regions in the brain richest in M1 
cholinergic receptors. Topical application of acetylcholine (ACh) onto 
hippocampal slices produces a characteristic complex response consisting of a 
depolarization, an increase in input resistance especially upon depolarization 
and a blockade of a slow afterhyperpolarization (AHP). The first two of these 
responses can be recorded also in non-cholinergic septal neurons in an area 
which contains about 8\% of the M1 muscarinic receptors found in the 
hippocampus. The responses of hippocampal but not septal neurons to ACh involve 
an increase in the spontaneous synaptic activity and a decrease in evoked 
responses to afferent stimulation. The dissociated hippocampal culture was used 
to study these presynaptic effects. The neurons in culture possess muscarinic 
receptors which develop gradually over a period of several weeks after plating. 
ACh rarely depolarizes hippocampal neurons in culture. Instead, it causes an 
increase in spontaneous discharge of small postsynaptic currents (PSC's) and a 
marked decrease of large, evoked PSC's. In some cultured hippocampal cells ACh 
reduced ICa without affecting any of several outward K currents studied. It is 
suggested that ACh reduces evoked activity by reducing Ca currents at 
presynaptic terminals.}, institution = {Center for Neurosciences, Weizmann 
Institute of Science, Rehovot, Israel.}, keywords    = {Acetylcholine, 
pharmacology; Animals; Brain, drug effects/physiology; Cells, Cultured; 
Hippocampus, drug effects/physiology; In Vitro Techniques; Neurons, drug 
effects/physiology; Rats; Synapses, drug effects/physiology; Trimethyltin 
Compounds, pharmacology}, language    = {eng}, medline-pst = {ppublish}, owner   
    = {vross}, pmid        = {2533107}, timestamp   = {2015.10.20}, }

@Article{Segal1991,
  author      = {Segal, M. and Markram, H. and Richter-Levin, G.},
  journal     = {Prog Brain Res},
  title       = {Actions of norepinephrine in the rat hippocampus.},
  year        = {1991},
  pages       = {323--330},
  volume      = {88},
  abstract    = {Acting at postsynaptic alpha 1- and beta 1-receptors, 
norepinephrine (NE) exerts a complex action in rat hippocampus. It is currently 
believed that beta 1-receptor activation enhances excitability of recorded 
neurons, whereas alpha 1 activation suppresses reactivity to afferent 
stimulation. These reported effects of alpha-agonists are not consistent with 
alpha 1 effects found elsewhere in the brain. We have conducted experiments in 
the anesthetized rat and found that an amphetamine-induced increase in the 
dentate gyrus population spike can be blocked by a beta-antagonist but also by 
an alpha 1-antagonist. We have conducted experiments in the brain slide 
preparation and found that an alpha-agonist, phenylephrine (PHE), selectively 
enhances responses to N-methyl-D-aspartate (NMDA) but not to quisqualate. We 
propose that the product of activation of both alpha- and beta-receptor types 
will enhance reactivity of hippocampal cells to afferent stimulation.},
  institution = {Center for Neurosciences, Weizmann Institute, Rehovot, Israel.},
  keywords    = {Afferent Pathways, physiology; Animals; Drug Synergism; Hippocampus, drug effects/physiology; Locus Coeruleus, physiology; N-Methylaspartate, pharmacology; Norepinephrine, physiology; Phenylephrine, pharmacology; Quisqualic Acid, pharmacology; Rats; Receptors, Adrenergic, alpha, physiology; Receptors, beta, physiology},
  language    = {eng},
