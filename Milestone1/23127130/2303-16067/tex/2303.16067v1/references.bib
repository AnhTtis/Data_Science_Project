@Article{Opper2001, author      = {M. Opper and R. Urbanczik}, title       = 
{{U}niversal learning curves of support vector machines.}, journal     = {Phys 
Rev Lett}, year        = {2001}, volume      = {86}, number      = {19}, pages   
    = {4410--4413}, abstract    = {Using methods of statistical physics, we 
investigate the role of model complexity in learning with support vector 
