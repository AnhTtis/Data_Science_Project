@Article{Borghuis2008a, author      = {Borghuis, Bart G. and Ratliff, Charles P. 
and Smith, Robert G. and Sterling, Peter and Balasubramanian, Vijay}, title      
 = {Design of a neuronal array.}, journal     = {J Neurosci}, year        = 
{2008}, volume      = {28}, number      = {12}, pages       = {3178--3189}, 
abstract    = {Retinal ganglion cells of a given type overlap their dendritic 
fields such that every point in space is covered by three to four cells. We 
investigated what function is served by such extensive overlap. Recording from 
pairs of ON or OFF brisk-transient ganglion cells at photopic intensities, we 
confirmed that this overlap causes the Gaussian receptive field centers to be 
spaced at approximately 2 SDs (sigma). This, together with response 
