@Article{Happel2014, Title                    = {Enhanced cognitive flexibility 
in reversal learning induced by removal of the extracellular matrix in auditory 
cortex.}, Author                   = {Happel, Max F K and Niekisch, Hartmut and 
Castiblanco Rivera, Laura L and Ohl, Frank W and Deliano, Matthias and 
Frischknecht, Renato}, journal = {Proc Natl Acad Sci U S A}, 
