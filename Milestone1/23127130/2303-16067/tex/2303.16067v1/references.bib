  keywords    = {Blood Glucose, metabolism; Diabetes Mellitus, Type 1, Type 2, metabolism; Glucagon, biosynthesis; Homeostasis, physiology; Humans; Insulin, biosynthesis; Islets of Langerhans, metabolism; Models, Biological},
  language    = {eng},
