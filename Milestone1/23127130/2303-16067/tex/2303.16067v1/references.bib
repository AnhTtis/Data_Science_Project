@Article{Konnerth1983, author      = {Konnerth, A. and Heinemann, U.}, title     
  = {{P}resynaptic involvement in frequency facilitation in the hippocampal 
slice.}, journal     = {Neurosci Lett}, year        = {1983}, volume      = 
{42}, number      = {3}, pages       = {255--260}, abstract    = {Orthodromic 
stimulation with frequencies from 1 to 30 Hz causes facilitation of 
