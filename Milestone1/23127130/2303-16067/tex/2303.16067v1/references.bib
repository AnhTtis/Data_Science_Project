@Article{Tamas2000, author      = {G. Tam?s and E. H. Buhl and A. L?rincz and P. 
Somogyi}, title       = {{P}roximally targeted {GABA}ergic synapses and gap 
junctions synchronize cortical interneurons.}, journal     = {Nat Neurosci}, 
year        = {2000}, volume      = {3}, number      = {4}, pages       = 
{366--371}, abstract    = {Networks of GABAergic interneurons are implicated in 
synchronizing cortical activity at gamma frequencies (30-70 Hz). Here we 
demonstrate that the combined electrical and GABAergic synaptic coupling of 
basket cells instantaneously entrained gamma-frequency postsynaptic firing in 
layers 2/3 of rat somatosensory cortex. This entrainment was mediated by rapid 
