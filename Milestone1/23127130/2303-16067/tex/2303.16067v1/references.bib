@Article{Brincat2004b, author     = {S. L. Brincat and C. E. Connor}, title      
= {{U}nderlying principles of visual shape selectivity in posterior 
inferotemporal cortex.}, journal    = {Nat Neurosci}, year       = {2004}, 
volume     = {7}, pages      = {880--886}, abstract   = {Object perception 
depends on shape processing in the ventral visual pathway, which in monkeys 
culminates in inferotemporal cortex (IT). Here we provide a description of 
fundamental quantitative principles governing neural selectivity for complex 
shape in IT. By measuring responses to large, parametric sets of two-dimensional 
(2D) silhouette shapes, we found that neurons in posterior IT (Brodmann's areas 
TEO and posterior TE) integrate information about multiple contour elements 
(straight and curved edge fragments of the type represented in lower-level 
areas) using both linear and nonlinear mechanisms. This results in complex, 
distributed response patterns that cannot be characterized solely in terms of 
example stimuli. We explained these response patterns with tuning functions in 
multidimensional shape space and accurately predicted neural responses to the 
widely varying shapes in our stimulus set. Integration of contour element 
information in earlier stages of IT represents an important step in the 
transformation from low-level shape signals to complex object representation.}, 
bdsk-url-1 = {http://dx.doi.org/10.1038/nn1278}, doi        = {10.1038/nn1278}, 
keywords   = {Action Potentials; Animals; Form Perception; Macaca mulatta; 
Models, Neurological; Neurons; Research Support, Non-U.S. Gov't; Research 
Support, U.S. Gov't, P.H.S.; Temporal Lobe; Visual Pathways; Visual Perception}, 
owner      = {vross}, pii        = {nn1278}, pmid       = {15235606}, timestamp  
= {2006.12.18}, }

