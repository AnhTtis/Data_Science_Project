@Article{Fischer2007, author      = {B. J. Fischer and J. L. {Pe\~a} and M. 
Konishi}, title       = {{E}mergence of multiplicative auditory responses in the 
midbrain of the barn owl.}, journal     = {J Neurophysiol}, year        = 
{2007}, volume      = {98}, number      = {3}, pages       = {1181--1193}, 
abstract    = {Space-specific neurons in the barn owl's auditory space map gain 
spatial selectivity through tuning to combinations of the interaural time 
