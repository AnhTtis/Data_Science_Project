@Article{Roelfsema2018a, author       = {Roelfsema, Pieter R and Holtmaat, 
Anthony}, title        = {Control of synaptic plasticity in deep cortical 
networks.}, journal      = {Nature reviews. Neuroscience}, year         = 
{2018}, volume       = {19}, pages        = {166--180}, month        = feb, issn 
        = {1471-0048}, abstract     = {Humans and many other animals have an 
enormous capacity to learn about sensory stimuli and to master new skills. 
However, many of the mechanisms that enable us to learn remain to be understood. 
One of the greatest challenges of systems neuroscience is to explain how 
synaptic connections change to support maximally adaptive behaviour. Here, we 
provide an overview of factors that determine the change in the strength of 
synapses, with a focus on synaptic plasticity in sensory cortices. We review the 
influence of neuromodulators and feedback connections in synaptic plasticity and 
suggest a specific framework in which these factors can interact to improve the 
functioning of the entire network.}, country      = {England}, doi          = 
{10.1038/nrn.2018.6}, issn-linking = {1471-003X}, issue        = {3}, nlm-id     
  = {100962781}, owner        = {NLM}, pii          = {nrn.2018.6}, pmid         
= {29449713}, pubmodel     = {Print}, pubstatus    = {ppublish}, revised      = 
{2018-11-13}, }

@Article{Lisman2011, author          = {Lisman, John and Grace, Anthony A and 
Duzel, Emrah}, title           = {A neoHebbian framework for episodic memory; 
role of dopamine-dependent late LTP.}, journal         = {Trends in 
neurosciences}, year            = {2011}, volume          = {34}, pages          
 = {536--547}, month           = oct, issn            = {1878-108X}, abstract    
    = {According to the Hebb rule, the change in the strength of a synapse 
depends only on the local interaction of presynaptic and postsynaptic events. 
Studies at many types of synapses indicate that the early phase of long-term 
potentiation (LTP) has Hebbian properties. However, it is now clear that the 
Hebb rule does not account for late LTP; this requires an additional signal that 
is non-local. For novel information and motivational events such as rewards this 
signal at hippocampal CA1 synapses is mediated by the neuromodulator, dopamine. 
In this Review we discuss recent experimental findings that support the view 
that this 'neoHebbian' framework can account for memory behavior in a variety of 
learning situations.}, chemicals       = {Dopamine}, citation-subset = {IM}, 
completed       = {2012-01-27}, country         = {England}, doi             = 
{10.1016/j.tins.2011.07.006}, issn-linking    = {0166-2236}, issue           = 
{10}, keywords        = {Animals; Dopamine, metabolism; Long-Term Potentiation, 
physiology; Memory, Episodic; Neuronal Plasticity, physiology; Synapses, 
