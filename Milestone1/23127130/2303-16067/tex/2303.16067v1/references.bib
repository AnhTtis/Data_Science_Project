@Article{Harvey2007, author      = {Christopher D Harvey and Karel Svoboda}, 
title       = {{L}ocally dynamic synaptic learning rules in pyramidal neuron 
dendrites.}, journal     = {Nature}, year        = {2007}, volume      = {450}, 
number      = {7173}, pages       = {1195--1200}, abstract    = {Long-term 
potentiation (LTP) of synaptic transmission underlies aspects of learning and 
memory. LTP is input-specific at the level of individual synapses, but neural 
network models predict interactions between plasticity at nearby synapses. Here 
we show in mouse hippocampal pyramidal cells that LTP at individual synapses 
