@Article{Markram2015, author      = {Markram, Henry and Muller, Eilif and 
Ramaswamy, Srikanth and Reimann, Michael W. and Abdellah, Marwan and Sanchez, 
Carlos Aguado and Ailamaki, Anastasia and Alonso-Nanclares, Lidia and Antille, 
Nicolas and Arsever, Selim and Kahou, Guy Antoine Atenekeng and Berger, Thomas 
