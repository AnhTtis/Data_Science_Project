@Article{Mareschal2001, author      = {I. Mareschal and M. P. Sceniak and R. M. 
Shapley}, title       = {{C}ontextual influences on orientation discrimination: 
binding local and global cues.}, journal     = {Vision Res}, year        = 
{2001}, volume      = {41}, number      = {15}, pages       = {1915--1930}, 
abstract    = {We sought to determine how local and global features within an 
image interact by examining whether orientation discrimination thresholds could 
be modified by contextual information. In particular, we investigated how local 
orientation signals within an image are pooled together, and whether this 
pooling process is dependent on the global orientation content present in the 
image. We find that observers' orientation judgments depend on surround 
contextual information, with performance being optimal when the center and 
surround stimuli are clearly distinct. In cases where the center and surround 
were not clearly segregated, we report two sets of results. If there was an 
ambiguity regarding the perception of a global structure (i.e. a small mismatch 
between local cues), observers' performance was impaired. If there was no 
mismatch and local and global cues were consistent with the perception of a 
single surface, observers performed as well as in the distinct surfaces case. 
Although some of our results can be largely accounted for by interactions 
between differently oriented filters, other aspects are more difficult to 
reconcile with this explanation. We suggest that low level filtering constrains 
observers' performance, and that influences arising from image segmentation 
modify how local orientation signals are pooled together.}, institution = 
{Center for Neural Science, New York University, 4 Washington Place, 10003, New 
York, NY, USA. isabelle@cns.nyu.edu}, keywords    = {Contrast Sensitivity; Cues; 
Discrimination (Psychology); Humans; Normal Distribution; Orientation; Sensory 
Thresholds}, owner       = {vross}, pii         = {S0042-6989(01)00082-7}, pmid  
      = {11412884}, timestamp   = {2007.07.26}, }

