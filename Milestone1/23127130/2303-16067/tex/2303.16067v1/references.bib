@Article{Soden2010, author      = {Soden, Marta E. and Chen, Lu}, title       = 
{{F}ragile {X} protein {FMRP} is required for homeostatic plasticity and 
regulation of synaptic strength by retinoic acid.}, journal     = {J Neurosci}, 
year        = {2010}, volume      = {30}, number      = {50}, pages       = 
