@Article{fusi05,
  author     = {S. Fusi and P. J. Drew and L.F. Abbott},
  title      = {{C}ascade {M}odels of {S}ynaptically {S}tored {M}emoriest},
  journal    = {Neuron},
  year       = {2005},
  volume     = {45},
  number     = {4},
  pages      = {599--611},
  abstract   = {Storing memories of ongoing, everyday experiences 
requires a high degree of plasticity, but retaining these memories demands 
protection against changes induced by further activity and experience. Models in 
which memories are stored through switch-like transitions in synaptic efficacy 
are good at storing but bad at retaining memories if these transitions are 
likely, and they are poor at storage but good at retention if they are unlikely. 
We construct and study a model in which each synapse has a cascade of states 
with different levels of plasticity, connected by metaplastic transitions. This 
cascade model combines high levels of memory storage with long retention times 
and significantly outperforms alternative models. As a result, we suggest that 
memory storage requires synapses with multiple states exhibiting dynamics over a 
wide range of timescales, and we suggest experimental tests of this 
hypothesis.},
  bdsk-url-1 = {http://dx.doi.org/ron.2005.02.001},
  bdsk-url-2 = {http://dx.doi.org/2005.02.001},
  doi        = {ron.2005.02.001},
  keywords   = {Animals; Humans; Memory; Models, Neurological; Neuronal Plasticity; Synapses; Time Factors},
  owner      = {vross},
  pii        = {S0896-6273(05)00117-0},
  pmid       = {15721245},
  timestamp  = {2007.05.18},
  url        = {http://dx.doi.org/ron.2005.02.001},
}

@Article{Gardner1988,
  author        = {E. Gardner},
  journal       = {J. of Physics A: Mathematical and General},
  title         = {The space of interactions in neural network models},
  year          = {1988},
  number        = {1},
  pages         = {257--270},
  volume        = {21},
  abstract      = {The typical fraction of the space of interactions 
between each pair of N Ising spins which solve the problem of storing a given 
set of p random patterns as N-bit spin configurations is considered. The volume 
is calculated explicitly as a function of the storage ratio, alpha =p/N, of the 
value kappa (>0) of the product of the spin and the magnetic field at each site 
and of the magnetisation, m. Here m may vary between 0 (no correlation) and 1 
(completely correlated). The capacity increases with the correlation between 
patterns from alpha =2 for correlated patterns with kappa =0 and tends to 
infinity as m tends to 1. The calculations use a saddle-point method and the 
order parameters at the saddle point are assumed to be replica symmetric. This 
solution is shown to be locally stable. A local iterative learning algorithm for 
