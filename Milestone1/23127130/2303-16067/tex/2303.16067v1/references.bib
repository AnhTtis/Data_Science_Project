@Article{Clark2006, author     = {Paul T Clark and Mark C W van Rossum}, title   
   = {{T}he optimal synapse for sparse, binary signals in the rod pathway.}, 
journal    = {Neural Comput}, year       = {2006}, volume     = {18}, number     
= {1}, pages      = {26--44}, abstract   = {The sparsity of photons at very low 
light levels necessitates a nonlinear synaptic transfer function between the rod 
photoreceptors and the rod-bipolar cells. We examine different ways to 
characterize the performance of the pathway: the error rate, two variants of the 
