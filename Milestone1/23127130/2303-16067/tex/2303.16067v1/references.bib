@Article{Alle2009,
  author      = {Alle, Henrik and Roth, Arnd and Geiger, J{\"{o}}rg R P.},
  title       = {Energy-efficient action potentials in hippocampal mossy fibers.},
  journal     = {Science},
  year        = {2009},
  volume      = {325},
  number      = {5946},
  pages       = {1405--1408},
  abstract    = {Action potentials in nonmyelinated axons are 
considered to contribute substantially to activity-dependent brain metabolism. 
Here we show that fast Na+ current decay and delayed K+ current onset during 
action potentials in nonmyelinated mossy fibers of the rat hippocampus minimize 
the overlap of their respective ion fluxes. This results in total Na+ influx and 
associated energy demand per action potential of only 1.3 times the theoretical 
minimum, in contrast to the factor of 4 used in previous energy budget 
calculations for neural activity. Analysis of ionic conductance parameters 
revealed that the properties of Na+ and K+ channels are matched to make axonal 
action potentials energy-efficient, minimizing their contribution to 
activity-dependent metabolism.},
  bdsk-url-1  = {http://dx.doi.org/10.1126/science.1174331},
  doi         = {10.1126/science.1174331},
  institution = {Independent Hertie Research Group, Max-Planck-Institute for Brain Research, 60528 Frankfurt, Germany. henrik.alle@charite.de},
  keywords    = {Action Potentials; Animals; Axons, physiology; Energy Metabolism; Mossy Fibers, Hippocampal, physiology; Patch-Clamp Techniques; Potassium Channels, metabolism; Potassium, metabolism; Presynaptic Terminals, physiology; Rats; Rats, Wistar; Sodium Channels, metabolism; Sodium, metabolism; Sodium-Potassium-Exchanging ATPase, metabolism; Synaptic Transmission},
  language    = {eng},
  medline-pst = {ppublish},
  owner       = {vrossum},
  pii         = {325/5946/1405},
  pmid        = {19745156},
  publisher   = {American Association for the Advancement of Science},
  timestamp   = {2015.10.05},
}

