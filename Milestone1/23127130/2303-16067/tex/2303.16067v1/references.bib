@Article{Smith2004, author      = {Philip L Smith and Roger Ratcliff}, title     
  = {{P}sychology and neurobiology of simple decisions.}, journal     = {Trends 
Neurosci}, year        = {2004}, volume      = {27}, number      = {3}, pages    
   = {161--168}, abstract    = {Patterns of neural firing linked to eye movement 
decisions show that behavioral decisions are predicted by the differential 
firing rates of cells coding selected and nonselected stimulus alternatives. 
