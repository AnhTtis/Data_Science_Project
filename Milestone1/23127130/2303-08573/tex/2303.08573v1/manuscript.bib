@article{chenMeasurementLinearBirefringence2009,
  title = {Measurement of Linear Birefringence and Diattenuation Properties of Optical Samples Using Polarimeter and {{Stokes}} Parameters},
  author = {Chen, Po-Chun and Lo, Yu-Lung and Yu, Tsung-Chih and Lin, Jing-Fung and Yang, Tsung-Tse},
  year = {2009},
  month = aug,
  journal = {Optics Express},
  volume = {17},
  number = {18},
  pages = {15860--15884},
  publisher = {{Optica Publishing Group}},
  issn = {1094-4087},
  doi = {10.1364/OE.17.015860},
  abstract = {A technique is proposed for measuring the linear birefringence and linear diattenuation of an optical sample using a polarimeter. In the proposed approach, the principal axis angle ({$\alpha$}), phase retardance ({$\beta$}), diattenuation axis angle (\texttheta d), and diattenuation (D) are derived using an analytical model based on the Mueller matrix formulation and the Stokes parameters. The dynamic measurement ranges of the four parameters are shown to be {$\alpha$} = 0\textasciitilde 180\textdegree, {$\beta$} = 0\textasciitilde 180\textdegree, \texttheta d = 0\textasciitilde 180\textdegree, and D = 0\textasciitilde 1, respectively. Thus, full-range measurements are possible for all parameters other than {$\beta$}. In this study, the proposed methodology does not require the principal birefringence axes and diattenuation axes to be aligned. In addition, the linear birefringence and linear diattenuation properties are decoupled within the analytical model, and thus the birefringence properties of the sample can be solved directly without any prior knowledge of the diattenuation parameters. Also, the characteristic parameters in the baked polarizer with linear birefringence are successfully extracted from an optically equivalent model and proved by the respective simulation and experiment introduced in this study.},
  copyright = {\textcopyright{} 2009 OSA},
  langid = {english}
}

@article{chenRheologyConcentratedCellulose2009a,
  title = {Rheology of Concentrated Cellulose Solutions in 1-Butyl-3-Methylimidazolium Chloride},
  author = {Chen, Xun and Zhang, Yumei and Cheng, Lingyan and Wang, Huaping},
  year = {2009},
  journal = {Journal of Polymers and the Environment},
  volume = {17},
  number = {4},
  pages = {273--279},
  issn = {15662543},
  doi = {10.1007/s10924-009-0149-4},
  abstract = {Rheological behavior of the concentrated cellulose/1-butyl-3- methylimidazolium chloride ([BMIM]Cl) solutions was investigated. As polymeric fluid, solutions of cellulose in [BMIM]Cl display a marked elastic behavior under shear flow. The dependence of the shear viscosity {$\eta$}, and of the dynamic modulus, on concentration, average degree of polymerization (DP) and temperature is discussed. At lower concentrations and degrees of polymerization (DP), cellulose solutions show viscous, inelastic behavior at low frequencies and low shear rate. At higher concentration and DP, cellulose solutions are more elastic at higher frequencies and shear rate. Such solutions also have some usual rheological properties. The dynamic rheological responses revealed that the Cox-Merz rule did not hold for these cellulose solutions at high deformation rate. Plotting storage modulus G{${'}$} against loss modulus G" gave almost a master curve which is independent of temperature and concentration, with the slope of about 1.651 for 10 wt\% cellulose solutions. This value indicates the existence of microheterogeneity in the solution system. \textcopyright{} 2009 Springer Science+Business Media, LLC.},
  keywords = {Cellulose,Ionic liquid,Rheology,Solution}
}

@article{chenSolutionRheologyCellulose2011a,
  title = {Solution Rheology of Cellulose in 1-Butyl-3-Methyl Imidazolium Chloride},
  author = {Chen, Xun and Zhang, Yumei and Wang, Huaping and Wang, Shih-Wa and Liang, Siwei and Colby, Ralph H.},
  year = {2011},
  journal = {Journal of Rheology},
  volume = {55},
  number = {3},
  pages = {485--494},
  issn = {0148-6055},
  doi = {10.1122/1.3553032},
  abstract = {Solution rheology of cellulose in 1-butyl-3-methyl imidazolium chloride\textbackslash n([BMIm]Cl) is reported using oscillatory and steady shear for cellulose\textbackslash nconcentrations from 0.1 to 10 wt \%, spanning the dilute, semidilute\textbackslash nunentangled, and entangled regimes. Although pure [BMIm]Cl is a crystalline\textbackslash nsolid at room temperature with a melting temperature of 65 degrees\textbackslash nC, all solutions prepared at 75 degrees C are transparent and visually\textbackslash nhomogenous at 25 degrees C, and these supercooled solutions, with\textbackslash nof order 0.1 wt \% water, show no sign of crystallizing for months\textbackslash nin either calorimetry or rheology measurements, allowing the potential\textbackslash nfor room temperature solution processing of native cellulose, such\textbackslash nas fiber spinning. The overlap concentration of our cellulose in\textbackslash n([BMIm]Cl) is 0.5 wt \% and the entanglement concentration is a factor\textbackslash nof 4 larger (2 wt\%). For semidilute unentangled solutions (between\textbackslash n0.5 and 2 wt \%), the specific viscosity, relaxation time, and terminal\textbackslash nmodulus exhibit concentration dependences eta(sp) similar to c(2),\textbackslash ntau similar to c, and G similar to c, respectively, while for entangled\textbackslash nsolutions (between 2 and 10 wt \%) we find eta(sp) similar to c(14/\textbackslash n3), tau similar to c(2.3), and G similar to c(2.3), consistent with\textbackslash nscaling predictions for neutral polymers in a theta solvent. However,\textbackslash nfailure of the Cox-Merz rule with steady shear viscosity larger than\textbackslash ncomplex viscosity and the observed internal mode structure of dilute\textbackslash nand semidilute unentangled solutions suggest that cellulose in ([BMIm]Cl)\textbackslash nis not simply a flexible polymer in a theta solvent. (C) 2011 The\textbackslash nSociety of Rheology. [DOI: 10.1122/1.3553032]}
}

@article{clasenHowDiluteAre2006a,
  title = {How Dilute Are Dilute Solutions in Extensional Flows?},
  author = {Clasen, C. and Plog, J. P. and Kulicke, W.-M. and Owens, M. and Macosko, C. and Scriven, L. E. and Verani, M. and McKinley, G. H.},
  year = {2006},
  month = nov,
