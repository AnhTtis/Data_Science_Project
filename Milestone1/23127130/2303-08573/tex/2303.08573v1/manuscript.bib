@article{colbyStructureLinearViscoelasticity2010a,
  title = {Structure and Linear Viscoelasticity of Flexible Polymer Solutions: {{Comparison}} of Polyelectrolyte and Neutral Polymer Solutions},
  author = {Colby, Ralph H.},
  year = {2010},
  journal = {Rheologica Acta},
  volume = {49},
  number = {5},
  pages = {425--442},
  issn = {00354511},
  doi = {10.1007/s00397-009-0413-5},
  abstract = {The current state of understanding for solution conformations of flexible polymers and their linear viscoelastic response is reviewed. Correlation length, tube diameter, and chain size of neutral polymers in good solvent, neutral polymers in \texttheta -solvent, and polyelectrolyte solutions with no added salt are compared as these are the three universality classes for flexible polymers in solution. The 1956 Zimm model is used to describe the linear viscoelasticity of dilute solutions and of semidilute solutions inside their correlation volumes. The 1953 Rouse model is used for linear viscoelasticity of semidilute unentangled solutions and for entangled solutions on the scale of the entanglement strand. The 1971 de Gennes reptation model is used to describe linear viscoelastic response of entangled solutions. In each type of solution, the terminal dynamics, reflected in the terminal modulus, chain relaxation time, specific viscosity, and diffusion coefficient are reviewed with experiment and theory compared. Overall, the agreement between theory and experiment is remarkable, with a few unsettled issues remaining. \textcopyright{} 2009 Springer-Verlag.},
  keywords = {Dynamic moduli,Polymer solution,Relaxation time,Reptation,Semidilute polymer solution,Viscoelasticity}
}

@article{collierRheology1butyl3methylimidazoliumChloride2009a,
