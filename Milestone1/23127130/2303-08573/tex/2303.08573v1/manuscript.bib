  abstract = {Lives and livelihoods in the Rural South are becoming increasingly divorced from farming and, therefore, from the land. Patterns and associations of wealth and poverty have become more diffuse and diverse as non-farm opportunities have expanded and heightened levels of mobility have led to the delocalization of livelihoods. This, in turn, has had ramifications for the production and reproduction of poverty in the countryside, which is becoming progressively de-linked from agricultural resources. This requires a reconsideration of some old questions regarding how best to achieve pro-poor development in the Rural South.},
  keywords = {acsmacro-2019,apprheol-2021}
}

@article{roddCapillaryBreakupRheometry2005c,
  title = {Capillary Break-up Rheometry of Low-Viscosity Elastic Fluids},
  author = {Rodd, Lucy E. and Scott, Timothy P. and {Cooper-White}, Justin J. and McKinley, Gareth H.},
  year = {2005},
  journal = {Applied Rheology},
  volume = {15},
