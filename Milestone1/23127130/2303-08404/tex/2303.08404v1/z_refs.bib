@article{Suzuki1997, title={{Feasibility Study on Ultrafast Nonlinear Optical Properties of 1.55-µm Intersubband Transition in AlGaN/GaN Quantum Wells}}, volume={36}, ISBN={0021-4922}, ISSN={00214922}, DOI={10.1143/JJAP.36.L1006}, abstractNote={The feasibility of the intersubband transition (ISBT) in Al(Ga)N/GaN quantum wells (QWs) as a device mechanism for ultrafast optical switches is theoretically investigated. The 1.55-mu m ISBT is shown to Lie feasible because of its large conduction band discontinuity. The intersubband relaxation time at 1.55-mu m is estimated to be 80 fs, which is about 30 times shorter than that in InGaAs QWs, The third order nonlinear susceptibility is estimated to be 1.6 x 10(-15) ma V-2 for N = 1 x 10(18) cm(-3). These characteristics suggest that the ISBT in nitride QWs is a promising mechanism for multi-terabit/s optical switches.}, number={Part 2, No. 8A}, journal={Japanese Journal of Applied Physics}, author={Suzuki, Nobuo and Iizuka, Norio}, year={1997}, month={Aug}, pages={L1006–L1008},
