@article{Iizuka2000, title={{Ultrafast intersubband relaxation ($\leq 150$~fs) in AlGaN/GaN multiple quantum wells}}, volume={77}, ISBN={0003-6951}, ISSN={00036951}, DOI={10.1063/1.127073}, abstractNote={The ultrafast intersubband relaxation in GaN quantum wells has been verified. Al0.65Ga0.35N/GaN multiple quantum wells, with as many as 200 wells, were grown by optimizing the barrier thickness and introducing GaN intermediate layers. The intersubband absorption is sufficiently strong for the relaxation time to be measured. A pump–probe measurement is performed to investigate the relaxation. An ultrashort relaxation time of less than 150 fs is obtained at a wavelength of 4.5 μm. The transient time is shorter than that of InGaAs quantum wells by approximately an order of magnitude. This result is promising for realizing ultrafast optical switches. © 2000 American Institute of Physics.}, journal={Applied Physics Letters}, author={Iizuka, Norio and Kaneko, Kei and Suzuki, Nobuo and Asano, Takashi and Noda, Susumu and Wada, Osamu}, year={2000}, pages={648},
