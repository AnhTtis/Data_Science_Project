	Keywords = {Display calculi,Fey,distributive lattice expansions,properly displayable logics,unified correspondence},
	Number = {7},
	Pages = {1367--1442},
	Title = {{Unified correspondence as a proof-theoretic tool}},
	Volume = {28},
