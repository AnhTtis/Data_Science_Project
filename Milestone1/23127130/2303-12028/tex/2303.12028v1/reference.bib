title = {Chapter Five - Nanocrystalline Diamond: A High-Impact Carbon Nanomaterial for Multifunctional Applications Including as Nanofiller in Biopolymeric Matrices},
editor = {Srinivasarao Yaragalla and Raghvendra Mishra and Sabu Thomas and Nandakumar Kalarikkal and Hanna J. Maria},
booktitle = {Carbon-Based Nanofillers and Their Rubber Nanocomposites},
publisher = {Elsevier},
