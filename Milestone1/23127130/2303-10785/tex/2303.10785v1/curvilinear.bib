keywords = {Artificial dissipation,Finite difference,Higher order,Localized artificial diffusivity,Shock capturing,WENO},
pages = {184--208},
publisher = {Elsevier Ltd.},
title = {{A comparison of higher-order finite-difference shock capturing schemes}},
url = {http://dx.doi.org/10.1016/j.compfluid.2015.08.023},
volume = {122},
year = {2015}
}
