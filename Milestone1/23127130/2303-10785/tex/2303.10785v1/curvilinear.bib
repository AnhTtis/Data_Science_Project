pages = {181--193},
publisher = {Elsevier Ltd},
title = {{Implementing high-order weighted compact nonlinear scheme on patched grids with a nonlinear interpolation}},
url = {http://dx.doi.org/10.1016/j.compfluid.2013.02.015},
volume = {77},
year = {2013}
}



