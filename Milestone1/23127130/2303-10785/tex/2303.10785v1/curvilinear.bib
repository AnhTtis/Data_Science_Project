@article{Nishikawa2011a,
abstract = {In this paper, we extend the diffusion schemes derived in the AIAA paper 2010-5093 to the Navier-Stokes schemes. There are two ways to do it. One is a widely-used approach: apply the gradient formula identified in the diffusion scheme to the gradients in the physical viscous ux. The other is a new way. It is a direct extension of the hyperbolic approach to the Navier-Stokes equations: a viscous scheme is derived from an upwind scheme applied to a hyperbolic model for the viscous term. Viscous discretizations derived from the two approaches are presented for the finite-volume method. A particular emphasis is given on the damping term which is essential to robust and accurate viscous computations. It is demonstrated that the hyperbolic approach is a general approach by which the damping term is automatically introduced into the viscous discretization. Preliminary results show that both approaches yield Navier-Stokes schemes of comparable accuracy and the lack of damping leads to inaccurate solutions. {\textcopyright} 2011 by Hiroaki Nishikawa. Published by the American Institute of Aeronautics and Astronautics, Inc.},
author = {Nishikawa, H.},
