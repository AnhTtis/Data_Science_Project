@article{CHAZELLE2017365,
title = {Well-posedness of the limiting equation of a noisy consensus model in opinion dynamics},
journal = {Journal of Differential Equations},
volume = {263},
number = {1},
pages = {365-397},
year = {2017},
issn = {0022-0396},
doi = {https://doi.org/10.1016/j.jde.2017.02.036},
url = {https://www.sciencedirect.com/science/article/pii/S0022039617301055},
author = {B. Chazelle and Q. Jiu and Q. Li and C. Wang},
keywords = {Hegselmann–Krause model, Nonlinear Fokker–Planck equation, Well-posedness, Global stability},
abstract = {This paper establishes the global well-posedness of the nonlinear Fokker–Planck equation for a noisy version of the Hegselmann–Krause model. The equation captures the mean-field behavior of a classic multiagent system for opinion dynamics. We prove the global existence, uniqueness, nonnegativity and regularity of the weak solution. We also exhibit a global stability condition, which delineates a forbidden region for consensus formation. This is the first nonlinear stability result derived for the Hegselmann–Krause model.}
}

 @article{Vera_2017, title={A convergent iterative method for a logistic chemotactic system}, abstractNote={In this paper we study a nonlinear system of diﬀerential equations arising in chemotaxis. The system consists of a PDE that describes the evolution of a population and another which models the concentration of a chemical substance. In particular, we prove the existence and uniqueness of nonnegative solutions via an iterative method. First, we generate a Cauchy sequence of approximate solutions from a linear modiﬁcation of the original system. Next, some uniform bounds on the solutions are used to ﬁnd a subsequence that converges weakly to the solution of the original system.}, author={Vera, J. M. R.}, journal ={Revista Colombiana de Matemáticas}, volume = {51}, number = {1}, year={2017}, pages={103--117}, language={en} }

