@article{mpgdrd4,
author = "Chen, Shi and Liu, Hongbang and Liu, Qian and Zheng, Yangheng and Wang, Binglong and Huang, Wenqian and Dong, Yang and Rong, Yu and Jiao, Xinda and Guan, Yu and Wang, Jing and Li, Min and Liu, Jizhou and Zhang, Mengmeng",
title = "{One-dimensional parallax-free position-sensitive detector for diffraction measurements based on a home-made thin THGEM}",
journal = "Journal of Synchrotron Radiation",
year = "2019",
volume = "26",
number = "1",
pages = "83--88",
month = "Jan",
doi = {10.1107/S160057751801086X},
url = {https://doi.org/10.1107/S160057751801086X},
abstract = {A large parallax-free gas diffraction meter based on a thinner-THGEM (thick gaseous electron multiplier) has been developed at the Beijing Synchrotron Radiation Facility (BSRF). A thinner-THGEM of thickness 200{$\mu$}m is adopted, which can be shaped into a curve to eliminate parallax-error effects. The detector is designed to have a 48{$^\circ$} open angle positioned 20cm from the powder samples. A front-end electronics board with 128channels direct-current mode was adapted for the 8keV BSRF beamline with 0.2ns/100ns stable duty cycle. Two powder samples, TiO${\sb 2}$ and SnO${\sb 2}$, were tested separately. The measured spectra with an angular resolution of 0.148 {$\pm$} 0.081{$^\circ$} are consistent with the data from the powder diffraction file. Combining the gas gain of the thinner-THGEM with the electronic circuit dynamic range, a very broad dynamic range of about 10${\sp 7}$ could be obtained.},
keywords = {micropattern gaseous detectors, thinner-THGEM, X-ray diffraction, BSRF},
}

