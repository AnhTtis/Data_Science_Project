@article{osti_4351657,
title = {Europium-doped, calcium-fluoride scintillator system for low-level tritium detection},
author = {Colmenares, C and Shapiro, E G and Barry, P E and Prevo, C T},
abstractNote = {An in-line detector and electronic system for low-level tritium detection have been built and tested. Tritium betas are detected with a europium- doped calcium-fluoride CaF2 (Eu) scintillatorphotomultiplier combination. An inexpensive, fully integrated circuit, amplifying pulse shape analyzer, was designed and built for detecting low-level signals in the presence of phototube noise and cosmic ray overloads. The amplifying pulse shape analyzer performs signal post-amplification and -shaping, pulse shape discrimination, and single channel energy analysis. The system is ideally suited for computer monitoring and controlling of tritium-contaminated gas streamns. A detailed description is presented of the detector and accessory electronics as well as the system calibration using tritiated gas standards for a range of tritium concertration from 0.35 nCi/cm3 (STP) to 0.68 mu Ci/cm3 (STP). The minimum detectable concentration was calculated to be 48 pCi/cm3 (STP).},
doi = {10.1016/0029-554X(74)90545-X},
url = {https://www.osti.gov/biblio/4351657}, journal = {Nucl. Instrum. Methods},
number = {2},
volume = {114},
pages = {277-289},
year = {1974},
month = {1}
}

