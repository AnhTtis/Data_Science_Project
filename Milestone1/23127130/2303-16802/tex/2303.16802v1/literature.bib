@inproceedings{repe2001,
 abstract = {The determination of the strength characteristics of rotating machinery is an important

part of modern design process and is essential to avoid undesirable stress levels and resonant

operating conditions and to ensure reliable, long life structures. The general tasks in this area

for turbines include the calculation of steady state stresses, the estimation of natural frequencies

and blade shapes and the calculation of the dynamic stress response due to stationary and

non-stationary force excitations. This formulation is based on a 2-D and 3-D finite element

analysis and the technique of cyclic symmetry. The stresses in different zones of the {\textless}bladedisk-

shaft{\textgreater} are calculated due to influence of centrifugal and temperature forces. The analysis

of the influence of blade geometric parameters on the disk-shaft steady stresses is presented.

The forced vibrations of the bladed disk on the stationary and non stationary regimes carried

out the influence of rotation speed, damping, number of nozzles at the forced vibration are

presented.},
 author = {Repetski, O.},
 title = {Investigation of Steady State Stresses, Free and Transient Vibrations of Bladed Systems},
 pages = {307--313},
 publisher = {Vieweg},
 isbn = {978-3528039387},
