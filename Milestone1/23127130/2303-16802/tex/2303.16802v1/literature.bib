@misc{flee2008,
 abstract = {A model is developed to evaluate rotor blade flow

induced vibrations due to aerodynamic mistuning, the

small differences in unsteady aerodynamic forces acting

on turbomachinery blades in a nominally symmetric airfoil

row. The difference in the airfoil-to-airfoil unsteady forces

is expressed as spinning acoustic and vorticity waves, with

the effect of these spinning modes quantified by a linear

unsteady aerodynamic model that accounts for inter-row

coupling, i.e. the reflection of spinning modes between

airfoil rows. An aerodynamic mistuning metric is defined

for each airfoil on the row that quantifies the difference

between the airfoil unsteady pressure differential and the

average unsteady pressure differential. The effect of

aerodynamic mistuning on resonant response and

aerodynamic damping is demonstrated by applying this

model to a transonic rotor. It is found that inter-row

coupling can magnify the unsteady forces on airfoil rows

above that expected for an aerodynamically mistuned row

in isolation. Similarly, inter-row coupling can change the

aerodynamic damping beyond that expected for an

aerodynamically mistuned row in isolation.},
 author = {Fleeter, S. and Gottfried, D.},
 year = {2008},
 title = {Aerodynamic mistuning effects on blade row unsteady aerodynamics},
 howpublished = {Paper ISROMAC12-2008-20007, Proc. of The 12th International Symposium on Transport Phenomena and Dynamics of Rotating Machinery, Honolulu, Hawaii, February 17-22},
 file = {Fleeter_2008:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Fleeter_2008.pdf:pdf}
}


