@article{genz2005a,
 abstract = {The present paper deals with the forced vibration analysis of a test structure. The test structure consists of two parts, an upper

and a lower half pipe, joint in two bolted flanges which represent the extended friction contacts. Depending on the clamping

loads different normal pressure distributions can be established in the contact interfaces. Since the test structure is loaded with

a harmonic external force relative displacements occur in the contact interface. This leads to microslip effects affecting the

dynamic behaviour. The experimental validation of the calculation method accounting for these effects is shown by comparing

measured and calculated frequency response functions (FRF).},
 author = {Genzo, Alexander and Sextro, Walter and Popp, Karl},
 year = {2005},
 title = {Analysis of the Forced Vibration of Two Bolted Half-Pipes with Extended Friction Contacts},
 pages = {91--92},
 volume = {5},
 number = {1},
 journal = {PAMM Proceedings in Applied Mathematics and Mechanics},
 doi = {10.1002/pamm.200510026}
}


@misc{genz2006a,
 abstract = {In many technical applications elastic bodies are coupled through extended contact interfaces by means

of bolted joints forming an elastic structure. The housing of an internal combustion engine built up of the

crankcase, the oil pan, the cylinder head and various covers can be regarded as a practical example for

an elastic mechanical structure. To understand the influence of extended friction contacts on the dynamic

behaviour of elastic structures, the forced vibrations of a test structure consisting of two elastic half tubes

joint together in two bolted flanges will be analysed. The contact interfaces in the flanges can be considered as

dry and extended friction contacts. Since the test structure is loaded with a harmonic external force, relative

displacements occur in the contact interfaces. Depending on the normal pressure distribution and magnitude

as well as on the contact surface characteristics these relative displacements lead to microslip effects affecting

the dynamic behaviour of the test structure.A calculation method accounting for these effects is presented

and experimentally validated by comparing measured and calculated frequency response functions (FRF) of

the test structure.},
 author = {Genzo, A. and Sextro, W. and Panning, L.},
 year = {2006},
 title = {Dynamic Behaviour of Elastic Bodies Coupled by Extended Friction Contacts},
 pages = {1303--1318},
 howpublished = {Proc. of ISMA~2006, International Conference on Noise and Vibration Engineering, Leuven, from September 18th to 20th},
 file = {Genzo_2006a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Genzo_2006a.pdf:pdf}
}


@incollection{genz2007a,
 abstract = {Ein schwingungsf{\"a}higes mechanisches Gesamtsystem, wie die Geh{\"a}usestruktur eines

Verbrennungsmotors, besteht aus einzelnen Bauteilen, wie zum Beispiel dem Zylinderkurbelgeh{\"a}use

und der {\"O}lwanne, die als elastische K{\"o}rper angesehen werden k{\"o}nnen. Die

elastischen K{\"o}rper treten in den Trennfugen an trockenen und rauhen Oberfl{\"a}chen miteinander

in Kontakt und werden durch Schraubenverbindungen gekoppelt. Durch die Verschraubung

entsteht in den Trennfugen eine inhomogene statische Normalpressungsverteilung.

Bei der Analyse der erzwungenen Schwingungen derartiger mechanischer Systeme

werden h{\"a}ufig Nichtlinearit{\"a}ten beobachtet, die durch die nichtlinearen Kopplungen zwischen

den elastischen K{\"o}rpern in den ausgedehnten Reibkontakten hervorgerufen werden oder als

solche beschrieben werden k{\"o}nnen. In diesem Beitrag wird ein Kontaktmodell vorgestellt,

das es erm{\"o}glicht, das nichtlineare Kontaktverhalten in einem ausgedehnten Reibkontakt wie

einer Schraubenverbindung n{\"a}herungsweise abzubilden und bei der Analyse der erzwungenen

Schwingungen eines Gesamtsystems zu ber{\"u}cksichtigen.



An assembled mechanical system, for example the housing structure of an internal combustion

engine, consists of single parts like the crankcase and the oil-sump which are treated as

elastic bodies. These contacts are coupled by bolt fastenings. The fastenings yield to a nonconstant,

static normal pressure distribution in the contact interfaces. Analysing the dynamic

behaviour of this kind of mechanical systems, nonlinearities can be observed which occur

due to non-linear couplings in the extended friction contacts between elastic bodies. This

contribution deals with a contact model which allows the approximation of the non-linear contact

behaviour in extended friction contacts such as a bolt fastening and which accounts for

the contact behaviour in the analysis of the forced vibrations of an assembled system.},
 author = {Genzo, A. and Sextro, W. and Panning, L.},
 title = {Schwingungsverhalten elastischer K{\"o}rper mit nichtlinearen Kopplungen in ausgedehnten Reibkontakten},
 pages = {147--161},
 editor = {{Verband Deutscher Ingenieure (VDI)}},
 booktitle = {VDI-Berichte 2002},
 year = {2007},
 file = {Genzo_2007a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Genzo_2007a.pdf:pdf}
}


