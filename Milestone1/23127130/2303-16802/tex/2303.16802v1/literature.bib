@misc{phad2008,
 abstract = {High cycle fatigue (HCF) is one of the primary causes of

turbine blade failure. Resonant vibratory stresses contribute

strongly to blade life reduction. Over the past 25 years, a large

amount of research has examined using friction damping to reduce

these stresses, most of which has focused on developing

experimental and low-order computational modeling tools for

blade stress predictions. However, the highly nonlinear and nonsmooth

nature of friction limits the ability of many methods

to produce accurate and robust predictions. Higher-order approaches

such as the finite element method (FEM) can provide

improved predictions at the cost of increased computational time.

Commercial FEM codes use a penalty formulation at the contact

interface and often the user is required to tune the penalty stiffness

to produce meaningful results that compare favorably with

analytical or experimental benchmark data. Our research effort

focuses on implementing a Lagrange-multiplier-based interface

formulation in a commercial FEM code (ANSYS). The approach

removes the penalty stiffness formulation and has been implemented

using ANSYS APDL scripting. Our approach produces

favorable comparisons with experimental data without the need

for parameter tuning, and at the same time, there is sufficient

scope for reducing computational costs. We present comparisons

with experimental data as well as parameter studies on friction

coefficient, platform angle and damper normal load. We expect

this research effort to develop into an accurate passive damping

prediction tool that can be easily integrated with industry-based

tools.},
 author = {Phadke, R. and Berger, E. J.},
 year = {2008},
 title = {Friction damping analysis in turbine blades using a user-programmed function in Ansys},
 howpublished = {Paper ISROMAC12-2008-20176, Proc. of The 12th International Symposium on Transport Phenomena and Dynamics of Rotating Machinery, Honolulu Hawaii, February 17-22},
 file = {Phadke_2008:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Phadke_2008.pdf:pdf}
}


