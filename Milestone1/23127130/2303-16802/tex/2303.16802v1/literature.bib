@misc{jasc1987,
 abstract = {Die Rollreibungstheorie nach Kalker wird soweit wie m{\"o}glich in einer Kurzdarstellung 

pr{\"a}sentiert, wobei insbesondere die Anwendungen f{\"u}r Simulationsberechnungen in der

Rad-Schiene-Technik im Vordergrund stehen. Dem auf diesem Gebiet t{\"a}tigen Ingenieur

soll damit Gelegenheit gegeben werden, die Rollreibungstheorie in ihren Grundlagen

zu verstehen. Hinsichtlich der praktischen Anwendung werden N{\"a}herungsans{\"a}tze 

behandelt. Die Grenzen der Rollreibungstheorie f{\"u}r die Simulation der Fahrdynamik

werden aufgezeigt. 



Kalker's Theory of Rolling Contact is presented in a brief description as far as possible,

with emphasis to the applications on wheel-rail simulations. The aim is to provide

fundamental knowledge about the theory of rolling contact to engineers which are engaged

in the simulation area. Approximations are treated with respect to practical applications. 

Also the limitations to the theory are indicated with regard to dynamic simulations

of the vehicle lateral behaviour.},
 author = {Jaschinski, A.},
 date = {1987},
 title = {Anwendung der Kalkerschen Rollreibungstheorie zur dynamischen Simulation von Schienenfahrzeugen},
 address = {Oberpfaffenhofen},
 institution = {{Deutsche Forschungs- und Versuchsanstalt f{\"u}r Luft- und Raumfahrt / Forschungsbereich Flugmechanik/Flugf{\"u}hrung / Institut f{\"u}r Dynamik der Flugsysteme / Abteilung Mehrk{\"o}rperdynamik}},
 file = {Jaschinski_1987:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_reibung und kontakt\\Jaschinski_1987.pdf:pdf}
}


@article{jaum2010,
 abstract = {Aeronautical structures are commonly assembled with bolted joints in which friction phenomena, in combination with slapping in the joint, provide damping on the dynamic behavior. Some models, mostly nonlinear, have consequently been developed and the harmonic balance method (HBM) is adapted to compute nonlinear response functions in the frequency domain. The basic idea is to develop the response as Fourier series and to solve equations linking Fourier coefficients. One specific HBM feature is that response accuracy improves as the number of harmonics increases, at the expense of larger computational time. Thus this paper presents an original adaptive HBM which adjusts the number of retained harmonics for a given precision and for each frequency value. The new proposed algorithm is based on the observation of the relative variation of an approximate strain energy for two consecutive numbers of harmonics. The developed criterion takes the advantage of being calculated from Fourier coefficients avoiding time integration and is also expressed in a condensation case. However, the convergence of the strain energy has to be smooth on tested harmonics and this constitutes a limitation of the method. Condensation and continuation methods are used to accelerate calculation. An application case is selected to illustrate the efficiency of the method and is composed of an asymmetrical two cantilever beam system linked by a bolted joint represented by a nonlinear LuGre model. The practice of adaptive HBM shows that, for a given value of the criterion, the number of harmonics increases on resonances indicating that nonlinear effects are predominant. For each frequency value, convergence of approximate strain energy is observed. Emergence of third and fifth harmonics is noticed near resonances both on vibratory responses and on approximate strain energy. Parametric studies are carried out by varying the excitation force amplitude and the threshold value of the adaptive algorithm. Maximal amplitudes of vibration and frequency response functions are plotted for three different points of the structure. Nonlinear effects become more predominant for higher force amplitudes and consequently the number of retained harmonics is increased.},
 author = {Jaumouill{\'e}, V. and Sinou, J. -J. and Petitjean, B.},
 year = {2010},
 title = {An Adaptive Harmonic Balance Method for Predicting the Nonlinear Dynamic Responses of Mechanical Systems - Application to Bolted Structures},
 pages = {4048--4067},
 volume = {329},
 number = {19},
 journal = {Journal of Sound and Vibration},
 file = {Jaumouille_2010:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Jaumouille_2010.pdf:pdf}
}


