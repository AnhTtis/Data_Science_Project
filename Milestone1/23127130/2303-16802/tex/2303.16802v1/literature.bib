@misc{brak2014,
 abstract = {In order to assess the predicted performance of a manufactured system, analysts must typically consider random variations (both geometric and material) in the development of a finite element model, instead of a single deterministic model of an idealized geometry. The incorporation of random variations, however, could potentially require the development of thousands of nearly identical solid geometries that must be meshed and separately analyzed, which would require an impractical number of man-hours to complete. This paper proposes a new approach to uncertainty quantification by developing parameterized reduced order models. These parameterizations are based upon Taylor series expansions of the systems matrices about the ideal geometry, and a component mode synthesis representation for each linear substructure is used to form an efficient basis with which to study the system. The numerical derivatives required for the Taylor series expansions are obtained efficiently using hyper dual numbers, which enable the derivatives to be calculated precisely to within machine precision. The theory is applied to a stepped beam system in order to demonstrate proof of concept. The accuracy of the methodology, level at which the parameterization is introduced, and efficiency of the methodology are discussed. Hyper dual numbers can be used to construct parameterized models both efficiently and accurately and constitute an appropriate methodology to account for perturbations in a structural system.},
 author = {Brake, M. R. and Fike, J.A and Topping, S.D and Schultz, R. and Field, R.V and McPeek-Bechtold, N.M and Dingreville, R.},
 year = {2014},
 title = {Model Order Reduction for Geometric Nonlinear Structures with Variable State-dependent Basis},
 publisher = {{The Society for Experimental Mechanics}},
 howpublished = {Nonlinear Dynamics, Volume 2: Proceedings of the 32nd IMAC, A Conference and Exposition on Structural Dynamics}
}


