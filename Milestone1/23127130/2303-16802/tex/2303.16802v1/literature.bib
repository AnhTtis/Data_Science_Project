@misc{petr2000b,
 abstract = {The effects of blade mistuning have been studied

extensively in the past although most of the

research efforts have been directed towards

understanding its qualitative aspects. Addressing

the quantitative aspects of the mistuning problem,

especially for industrial designs, has been

somewhat limited in the past, mainly due to the

need for using reduced-order and thus simplified

models in such analyses. In other words, it has been

difficult to translate previous mistuning results

directly for high-cycle fatigue predictions for a

given bladed-disc.



A new method for the dynamic analysis of

mistuned bladed discs is presented. The method is

based on exact calculation of the response of the

mistuned system using response levels of the tuned

assembly and a modification matrix constructed

from the Frequency Response Function (FRF)

matrix of the tuned system and a matrix describing

the mistuning. The main advantages of the method

are its efficiency and accuracy, which allows the

use of large finite element models of practical

bladed disc assemblies in parametric studies of

mistuning effects on vibration amplitudes. A new

method of calculating the FRF matrix of the tuned

system using a sector model is also developed so as

to improve the efficiency of the method further,

making the proposed method a very attractive tool

for mistuning studies of practical design.},
 author = {Petrov, E. and Sanliturk, K. and Ewins, D. and Elliott, R.},
 year = {2000},
 title = {Quantitative Prediction of the Effects of Mistuning Arrangement on Resonant Response of a Practical Turbine Bladed Disc},
 file = {Petrov_2000:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Petrov_2000.pdf:pdf}
}


@misc{petr2001,
 abstract = {In the paper, the problem of determining of the worst

mistuning patterns is formulated and solved as an optimization

problem. Maximum resonant amplitudes searched across the

many nodes of a large-scale finite element model of a mistuned

bladed disc and across all the excitation frequencies in a given

range are combined into an objective function. Individual blade

mistuning is controlled by varying design parameters, whose

variation range is constrained by manufacture tolerances.

Detailed realistic finite element models, which have so far

only been used for analysing tuned bladed discs, are used for

calculation of the forced resonant response of mistuned

assemblies and for determination of its sensitivity coefficients

with respect to mistuning variation.

Results of the optimum search of mistuning patterns for

some practical bladed discs are analysed and reveal higher

worst cases than those found in previous studies.},
 author = {Petrov, E. P. and Ewins, D. J.},
 year = {2001},
 title = {Analysis of the Worst Mistuning Patterns in Bladed Disc Assemblies},
 howpublished = {Paper 2001-GT-0292, Proc. of ASME TURBO EXPO 2001, June 4--7, New Orleans, Louisiana, USA},
 file = {Petrov_2001:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Petrov_2001.pdf:pdf}
}


@misc{petr2002a,
 abstract = {An analytical formulation for the vectors of contact forces

and the stiffness matrix of the non-linear friction contact

interface is developed for the analysis of multi-harmonic

vibrations in the frequency domain.

The contact interface elements provided here an exact

description of friction and unilateral contact forces at the

interacting surfaces, taking into account the influence of the

variable normal load on the friction forces, including the

extreme cases of separation of the two surfaces. Initial gaps and

interferences at the contact nodes, which affect the normal

force, as well as the unilateral action of the normal force at the

contact surface, are all included in the model.

The accurate calculation of the force vector and the tangent

stiffness matrix provides a very reliable and fast convergence of

the iteration process used in the search for the amplitudes of

nonlinear vibrations of bladed discs.

Numerical investigations demonstrate excellent

performance with respect to speed, accuracy and stability of

computation.},
 author = {Petrov, E. P. and Ewins, D. J.},
 year = {2002},
 title = {Analytical Formulation of Friction Interface Elements for Analysis of Nonlinear Multi-Harmonic Vibrations of Bladed Discs},
 howpublished = {Paper GT-2002-30325, Proc. of ASME Turbo Expo 2002, June 3-6, Amsterdam, The Netherlands},
 file = {Petrov, Ewins 2002 - Analytical Formulation of Friction Interface:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Petrov, Ewins 2002 - Analytical Formulation of Friction Interface.pdf:pdf}
}


@misc{petr2002b,
 abstract = {New models of friction have been developed for consistent and effective determining the friction forces for

time-domain analysis of vibrations of mechanical systems. The models allow arbitrary variation of the

normal load, including the case of partial separation of the contacting surfaces, and arbitrary variation of the

characteristics of the friction interface (e.g. friction coefficient, contact stiffness, etc.) in time. Relative

motion of the contacting surfaces along a line and a general case of the relative motion in a plane of the

friction contact interface are examined. Anisotropy and variation of the characteristics of the friction

interface as a function of co-ordinates of the contact surface is accounted for. Examples of friction forces

modelling and analysis of forced response using the developed models are presented.},
 author = {Petrov, E. P. and Ewins, D. J.},
 year = {2002},
 title = {Models of friction damping with variable normal load for time-domain analysis of vibrations},
 pages = {421--430},
 howpublished = {Proc. of ISMA2002, International Conference on Noise and Vibration Engineering, Leuven, September 16th to 18th},
 file = {Petrov, Ewins 2002 - Models of friction damping:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Petrov, Ewins 2002 - Models of friction damping.pdf:pdf}
}


@article{petr2002c,
 abstract = {A new method for the dynamic analysis of mistuned bladed disks is presented. The method

is based on exact calculation of the response of a mistuned system using response levels

for the tuned assembly togeher with a modification matrix constructed from the frequency

response function (FRF) matrix of the tuned system and a matrix describing the mistuning.

The main advantages of the method are its efficiency and accuracy, which allow the

use of large finite element models of practical bladed disk assemblies in parametric

studies of mistuning effects on vibration amplitudes. A new method of calculating the FRF

matrix of the tuned system using a sector model is also developed so as to improve the

efficiency of the method even further, making the proposed method a very attractive tool

for mistuning studies. Various numerical aspects of the proposed method are addressed

and its accuracy and efficiency are demonstrated using representative test cases.},
 author = {Petrov, E. P. and Sanliturk, K. Y. and Ewins, D. J.},
 year = {2002},
 title = {A New Method for Dynamic Analysis of Mistuned Bladed Disks Based on the Exact Relationship Between Tuned and Mistuned Systems},
 keywords = {engineering computing;finite element analysis;frequency response;modelling;rotation;turbines;vibrations},
 pages = {586--597},
 volume = {124},
 number = {3},
 journal = {Journal of Engineering for Gas Turbines and Power},
 doi = {10.1115/1.1451753},
 file = {Petrov, Sanliturk et al 2002 - A New Method for Dynamic:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Petrov, Sanliturk et al 2002 - A New Method for Dynamic.pdf:pdf}
}


@misc{petr2002d,
 abstract = {An accurate and robust approach has been developed for multiharmonic analysis of forced periodic

vibrations of bladed discs subjected to abrupt changes of elastic and damping properties at contact

nodes located on interfaces of the components. The approach is based on an analytical formulation of

friction and gap interface elements which provides exact expressions for forces and stiffness matrices

of the elements. The analytical formulation overcomes the numerical difficulties in searching for a

solution of the nonlinear equations, which have been inherent so far for the considered systems, and

provides a breakthrough in analysis of nonlinear forced response of bladed discs and other structures

with gap and friction interfaces.

A new model for friction under conditions of variable normal load is developed. The model

provides a consistent description of friction forces under an arbitrary level of normal load variation,

including the case of separation of the contacting nodes in some intervals of the vibration period.

Numerical investigations demonstrate outstanding properties of the proposed approach with respect

to computational speed, accuracy and stability of calculations for practical bladed discs.},
 author = {Petrov, E. and Ewins, D.},
 year = {2002},
 title = {Analysis of nonlinear multiharmonic vibrations of bladed discs with friction and impact dampers},
 file = {Petrov, Ewins 2002 - Analysis of nonlinear multiharmonic vibrations:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Petrov, Ewins 2002 - Analysis of nonlinear multiharmonic vibrations.pdf:pdf}
}


@misc{petr2002e,
 abstract = {The problem of determining of the best arrangements of pre-manufactured sets of mistuned blades in

bladed-disc assemblies is formulated and solved as a combinatorial optimization problem. Two

algorithms for the optimization search for the mistuned blade arrangement providing the lowest level of

forced response is developed and explored: (i) combinatorial random search, when blade arrangements

are generated with a generator of integer numbers; (ii) combinatorial search using sensitivity

coefficients of the maximum response with respect to individual blade mistuning.

Detailed realistic finite element models are used to calculate the maximum forced response of the

mistuned assemblies and to determine the sensitivity coefficients with respect to mistuning variation.

Results of the combinatorial search for the best mistuned blade arrangement for practical bladed discs

are analysed and shown to reveal the possibility of significant reductions in response levels by simple

rearrangement of the existing mistuned blades in a bladed disc.},
 author = {Petrov, E. and Ewins, D.},
 year = {2002},
 title = {Search for the best blade arrangement in a mistuned bladed disc assembly},
 file = {Petrov_2002e:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Petrov_2002e.pdf:pdf}
}


@misc{petr2002f,
 abstract = {An accurate and robust approach has been developed for analysis of forced periodic vibrations of

mechanical structures subjected to abrupt changes of elastic and damping properties at contact nodes

located on interfaces of the components. The approach is based on an analytical formulation of the

friction and gap interface elements, which provides exact expressions for matrices of the elements. The

analytical formulation allows overcoming numerical difficulties in searching for a solution of the

nonlinear equations, which have been inherent so far for the considered systems and could lead to loss

of convergence and failure to determine solutions. Exact expressions for the tangent stiffness matrix

and for the sensitivity coefficients of the interaction forces with respect to element parameters (e.g.

friction coefficient, contact stiffness coefficient, static normal load, etc.) are derived. The

multiharmonic balance method is used for transformation of the solutions and excitation forces into the

frequency domain.

A new model for friction under condition of variable normal load is developed. The model

provides a consistent description of friction forces under an arbitrary level of the normal load variation

including the case of separation of the contacting nodes in some intervals of the vibration period.

Numerical investigations demonstrate excellent properties of the proposed approach with respect

to computational speed, accuracy and stability of calculations. Examples of studies of influence of

different interface and excitation parameters on periodic regimes of vibrations are presented.},
 author = {Petrov, E. P. and Ewins, D. J.},
 year = {2002},
 title = {Robust analysis of periodic vibration of structures with friction and gaps based on analytical derivation of nonlinear interface elements},
 keywords = {friction;gap;interface elements;multiharmonic;nonlinear vibration},
 howpublished = {Proc. of WCCM V, Fifth World Congress on Computational Mechanics, July 7-12,Vienna, Austria},
 file = {Petrov_2002f:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Petrov_2002f.pdf:pdf}
}


@article{petr2003b,
 abstract = {An analytical formulation for the vectors of contact forces and the stiffness matrix of the nonlinear friction contact interface is developed for the analysis of multi-harmonic vibrations in the frequency domain. The contact interface elements provided here an exact description of friction and unilateral contact forces at the interacting surfaces, taking into account the influence of the variable normal load on the friction forces, including the extreme cases of separation of the two surfaces. Initial gaps and interferences at the contact nodes, which affect the normal force, as well as the unilateral action of the normal force at the contact surface, are all included in the model. The accurate calculation of the force vector and the tangent stiffness matrix provides a very reliable and fast convergence of the iteration process used in the search for the amplitudes of nonlinear vibrations of bladed disks. Numerical investigations demonstrate excellent performance with respect to speed, accuracy and stability of computation.},
 author = {Petrov, E. P. and Ewins, D. J.},
 year = {2003},
 title = {Analytical Formulation of Friction Interface Elements for Analysis of Nonlinear Multi-Harmonic Vibrations of Bladed Disks},
 keywords = {elastic constants;friction;interface phenomena;iterative methods;mechanical contact;vectors;vibrations},
 pages = {364--371},
