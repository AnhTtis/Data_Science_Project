@misc{seel2002,
 abstract = {Mistuning of otherwise periodic structures is a

challenging problem to the designers of turbomachinery

systems. Recently, significant advances have been made

in the literature to model mistuning. The vast majority of

these efforts have been applied to bladed disk type

structures. Other periodic structures, such as combustor

components, are also periodic in nature and exhibit

characteristics of mistuning. However, the sector-sector

variation can usually be characterized explicitly in these

cases. The goal of this paper is to develop a general

methodology to model mistuning of periodic structures

where the characteristics of the non periodic features are

well known. The approach resolves the non periodic

features as a set of nonsymmetric reaction forces, which

are expanded as a series of Fourier harmonics, so that the

symmetry of the periodic structure is restored. Modal

sensitivities of the reaction forces are employed which

facilitate the solution of the single sector equations of

motion, based on the tuned mode shapes, without the need

for iteration. The method is validated against the full

system for several example problems.},
 author = {Seeley, C. E. and Liu, Q. and Velagandula, O. and Dede, M. and Mignolet, M.},
 date = {2002},
 title = {Analysis of Cyclic Symmetric Structures with Mistuned Sectors},
 keywords = {ANSYS;combusto;cyclic symmetric;dynamics;finite element;Forced response;harmonic response;liner;periodic;vibration},
 address = {Niskayuna},
 number = {2002GRC148},
 institution = {{Mechanical and e-Engineering Technologies}},
 file = {Seeley_2002:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Seeley_2002.pdf:pdf}
}


@article{Seelecke.2002,
 abstract = {The paper studies the single degree of freedom vibration of a rigid mass suspended by a thin-walled shape memory alloy tube under torsional loading. The behavior is analyzed for the cases of quasiplasticity (low temperatures) and pseudoelasticity (high temperatures) on the basis of an improved version of the M{\"u}ller-Achenbach model. To illustrate the strong hysteresis-induced damping capacity and the non-linear vibration characteristics, both, free and forced vibrations are considered in the first part of the paper. This is done on the basis of an isothermal version of the model, while the second part of the paper focuses on the effect of non-constant temperature caused by the rate-dependent release and absorption of latent heats. {\copyright} 2002 Published by Elsevier Science Ltd.},
 author = {Seelecke, S.},
 year = {2002},
 title = {Modeling the dynamic behavior of shape memory alloys},
 keywords = {Damping capacity;dynamics;hysteresis;Modeling;Shape memory alloys},
 pages = {1363--1374},
 volume = {37},
 number = {8},
 journal = {International Journal of Non-Linear Mechanics},
 doi = {10.1016/S0020-7462(02)00030-6}
}


