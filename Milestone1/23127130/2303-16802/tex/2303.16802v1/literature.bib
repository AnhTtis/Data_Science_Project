@phdthesis{van2004,
 abstract = {This dissertation considers the steady-state dynamic response prediction of

interior structural-acoustic problems. The finite element method (FEM) is

commonly used, but it is restricted to the low frequency range. The wave

based method (WBM) is an alternative deterministic Trefftz based method,

which is able to provide accurate predictions in the mid frequency range. This

dissertation proves the WBM reliability as mid frequency method by improving

its accessibility and its practical applicability. The WBM accessibility

improvement is achieved by the development of an automated model generation

procedure. This allows a detailed performance study, which shows that

the WBM computational efficiency is most pronounced for problems with a

moderately complex geometry. The WBM practical applicability improvement

is achieved by a hybrid coupling with the FEM, which benefits from

the geometrical flexibility of the FEM and the computational efficiency of

the WBM. Numerical validation examples prove the feasibility of the hybrid

method for interior acoustic problems.},
 author = {{Van Hal}, B.},
 year = {2004},
 title = {Automation and performance optimization of the wave based method for interior structural-acoustic problems},
 keywords = {acoustic;computational method;deterministic method;Finite element method;hybrid modelling;mid-frequency;numerical prediction technique;Structural dynamics;structural-acoustic;vibro-acoustic;wave based method},
 address = {Leuven},
 publisher = {Faculteit Toegepaste Wetenschappen, Departement Werktuigkunde, Afdeling Productietechnieken, Machinebouw en Automatisering},
 school = {{Katholieke Universiteit Leuven}},
 type = {Dissertation},
 file = {Vanhal_2004:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Vanhal_2004.pdf:pdf}
}


@misc{van2006,
 abstract = {This paper presents a newly developed hybrid simulation technique for coupled structural-acoustic analysis,

which applies a wave based model for the acoustic cavity and a modally reduced finite element model for

the structural part. The resulting hybrid model benefits from the computational efficiency of the wave based

method, while retaining the finite element method's ability to model the structural part of the problem in great

detail. Application of this approach to the analysis of a cavity-backed plate assembly shows the improved

computational efficiency as compared to classical finite element procedures and illustrates the potential of

the hybrid method as a powerful tool for the analysis of coupled structural-acoustic systems.},
 author = {{Van Genechten}, B. and Pluymers, B. and Vanmaele, C. and Vandepitte, D. and Desmet, W.},
 year = {2006},
 title = {On the coupling of Wave Based models with modally reduced Finite Element models for structural-acoustic analysis},
