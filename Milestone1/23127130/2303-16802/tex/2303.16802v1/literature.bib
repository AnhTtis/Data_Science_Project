 journal = {PAMM Proceedings in Applied Mathematics and Mechanics},
 doi = {10.1002/pamm.200610380},
 file = {Denkena_2006a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Denkena_2006a.pdf:pdf}
}


@misc{denk2007a,
 abstract = {Due to higher quality requirements in industrial applications research activities are increasingly

addressing the interaction between manufacturing process and machine

structure. In a cooperative project between the universities of Hannover and Berlin, the

grinding of slim tools like drills or end mills is investigated in detail because of the expected

strong interaction between the dynamic and static compliance of machine structure,

workpiece, grinding wheel and the generated grinding forces. Especially in the

grinding process of the helical flutes of a twist drill, the low and strongly changing stiffness

of the workpiece affects the static and dynamic behavior of the overall system. In

order to meet the quality standards without time-consuming experiments, a simulation

tool will be developed to predict the interactions during the tool grinding process. The

underlying models are based on a multiscale approach, combining physical phenomena

on different length- and time-scales. This includes the mathematical simulation of the

process forces by the analysis of the microscopic grain engagement, the stepwise generation

of the macroscopic workpiece geometry as well as the simulation of the static

and dynamic deflections under consideration of the changing dynamic compliance. The

simulations are accompanied by experimental dynamic analyses and by measurements

of process forces and workpiece deflections on an industrial tool grinding machine.},
 author = {Denkena, B. and Deichm{\"u}ller, M. and Kr{\"o}ger, M. and Panning, L. and Carstensen, C. and Kilian, S.},
 year = {2007},
 title = {Modeling and Simulation of the Process Machine Interaction during Tool Grinding Processes},
 pages = {391--398},
 howpublished = {Proc. of the 10th CIRP International Workshop on Modeling of Machining Operations, Reggio Calabria},
 file = {Denkena_2007a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Denkena_2007a.pdf:pdf}
}


@article{Denoel.2017,
 abstract = {This paper shows some records and analysis of evidences of vortex induced vibrations of very long stay cables in the atmospheric boundary layer. The considered structure is a 254-m high telecommunication antenna whose structural system is made of a light flexible metallic truss, approx. 10$\times$10m square, which is stayed at five different levels by a set of 20 pairs of stay-cables. The exposure of this antenna is category I with a low-turbulence wind flow which results in a high sensitivity to vortex induced vibrations. The fundamental frequency of the longest cable is 0.3 Hz so that several dozens of regularly spaced natural frequencies could be observed with a sampling frequency of 500 Hz. Sufficiently long recordings allowed to distinctly observe the separate peaks corresponding to the various modes of the cables. Among them many are excited by the detachment of vortices, taking place at different frequencies (because of different cable diameters and the change of wind velocity along the height of the antenna) in the range 35-55 Hz. Referring to the so-called spectral model proposed by Vickery and Clark, the detachment of vortices takes place in a certain bandwidth, whose extent is not completely understood yet. This bandwidth is clearly observed with our long stay-cables because of the large density of mode shapes in the range [0, 200] Hz and because of their regular spacing in the spectral domain. To the author's knowledge this constitutes a first attempt at identifying this parameter of the Vickery and Clark model from full-scale measurements.},
 author = {Deno{\"e}l, Vincent and Andrianne, Thomas},
 year = {2017},
 title = {Real-scale observations of vortex induced vibrations of stay-cables in the boundary layer},
 keywords = {separated by semicolons;Type your keywords here},
 pages = {3109--3114},
 volume = {199},
 issn = {1877-7058},
 journal = {X International Conference on Structural Dynamics, EURODYN 2017},
 doi = {10.1016/j.proeng.2017.09.575}
}


