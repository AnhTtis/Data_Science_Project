@misc{klom2001,
 abstract = {An elemental explanation of the longstanding puzzler, peak

splitting, is advanced and correlated with published

experimental results. The peaks are shown to represent two

singular speeds of a traveling wave, at each of which a portion

of the blades jump from subcritical to supercritical response, or

vice versa. Only one blade, of two with slightly different natural

frequency and spaced approximately 90 deg apart, passes

through full resonant condition in each jump. Interaction with

engine whirl is produced.

Aeroengine vibrations associated with bladed disk flexing/whirl

interaction are examined:

• A nonconservative, either stabilizing or destabilizing,

inertial force due to linear flexing is introduced.

• Previous definition of a nonlinear friction induced limit

cycle, attendant to the interlocked shroud enabling the

high-bypass turbofan, is now reviewed and completed by

explanation of the necessary frictional backward driving

torque.},
 author = {Klompas, N.},
 year = {2001},
 title = {Nature of Vibratory Waves in Bladed Disks},
 howpublished = {Paper 2001-GT-0291, Proc. of ASME TURBO EXPO 2001, June 4--7, New Orleans, Louisiana, USA},
