@article{Font.2006,
 abstract = {Recent studies have investigated the use of plasma actuators for the active control of the boundary layer on turbine blades. Although the overall effects have been quantified through experiments, the exact nature of the plasma and its momentum-transfer mechanisms have not been well characterized. In this study, particle-in-cell and Monte Carlo methods are used to computationally explore the plasma discharge and its interaction with the flow. The plasma composition, its methods of momentum addition, and the physics of its generation are quantified. Comparisons with experiments are made in order to support the findings. Simulations indicate that the plasma is generated through an electron avalanche in a dielectric barrier discharge. The plasma is created as the electrons stream to the dielectric on the first half of the electrode bias cycle and stream back on the second half. Momentum is imparted to the flow on both half-cycles, but the ionization is not equal during both half-cycles. This results in the plasma actuator producing a net force in one direction.},
 author = {Font, G. I.},
 year = {2006},
 title = {Boundary-layer control with atmospheric plasma discharges},
 pages = {1572--1578},
 volume = {44},
 number = {7},
 journal = {AIAA Journal},
 doi = {10.2514/1.18542}
}


@misc{fordxxxx,
 abstract = {The twin-mode model of the forced vtbration of

bladed disk assemblies has been extended to include

harmonics in the circumferential modeshapes.



Blade amplitudes have been calculated for three

related blade disk configurations excited in the

absence of aerodynamic forces by a traveling force

wave. The configurations chosen were: a) mistuned

with harmonics; b ) mistuned without harmonics; and

c ) tuned . The maximum blade amplitude to be found

anywhere on each of these was noted . The relatively

smalI harmonic content (16{\%} of each of two harmonics ) 

of conf iguration a) produced a maximum blade ampliitude

23{\%} above that for configuration b ) ( no harmonics ) 

or 48{\%} above the value for configuration c ) ( tuned ) .

Adding harmonics also increased the amplitude

variations from blade-to-blade within the assembly. 

With harmonics present the ratio of the amplitudes

of the maximum and minimum responding blades was 3.8:1 .



In its present form this extension to the theory

can be applied to vibration tests conducted with an

assembly stationary and with no airflow. 

It can also be applied to the normal operating conditions of

turbomachinery if aerodynamic effects do not

significantly change the vibration characteristics.



The analysis can readily be extended to include

aerodynamic forces.},
