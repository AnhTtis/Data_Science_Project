@article{Vaurigaud.2011,
 abstract = {In this paper, we study a Targeted Energy Transfer (TET) problem between a p degrees-of-freedom (dof) linear master structure and several coupled parallel slave Nonlinear Energy Sink (NES) systems. In detail, each lth dof l=1,2,$\ldots$,p contains nlparallel NES; so the linear structure has (n1+n2+$\cdot$$\cdot$$\cdot$+nl+$\cdot$$\cdot$$\cdot$+np) NES. We are interested to study analytically the TET phenomenon during the first mode of the compound system. To this end, complexification, averaging, and multiple scales methods are used.},
