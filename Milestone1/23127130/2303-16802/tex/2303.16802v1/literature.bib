@misc{grif2002,
 abstract = {The purpose of the work performed under this contract was to develop more rational methods for

prediction the effect of friction damping on the vibratory response of bladed disk assemblies.

The work falls into three categories: 1) Research related to the development of new methods for

analyzing flat friction dampers with spherical contact; 2) Research investigating the importance

of corner radius on performance of dampers with flat friction contact; 3) Research investigating

the relative performance of flat, cylindrical and spherical wedge dampers. The results for each

of these research thrusts are documented in this report.},
 author = {Griffin, J. H. and Koh, K. -H},
 date = {2002},
 title = {Physical models of friction constraint and the effect of friction on bladed disk dynamics},
 file = {Griffin_2002:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Griffin_2002.pdf:pdf}
