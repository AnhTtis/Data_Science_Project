@misc{poud2004,
 abstract = {A new frequency-time domain method is proposed

to analyze the periodic forced response of structural

systems undergoing intermittent contacts and frictional

interactions between some of their components. It is

based on a modification of the Hybrid Frequency-Time

(HFT) method developed by the authors for the study

of the dynamics of systems with external, dry-friction

dampers attached. In the approach presented in this

paper, the dynamics of the structure is still described in

the frequency domain by a set of Fourier coefficients

representing the motion of the nonlinear degrees of

freedom (DOFs) where impact or friction occur, but the

corresponding contact forces are no longer taken into

account in the resulting set of nonlinear equations to

be solved. Instead, this new method is based on the

nonlinear kinematics conditions associated with the

contact forces, that is, it solves directly for the nonlinear

DOFs displacements which satisfy the Coulomb friction

and non-penetration conditions associated with these

nonlinear forces. Therefore, this approach circumvents

the numerical problems associated with the handling

of complicated Fourier transforms stemming from nonsmooth

periodic contact forces, a major hindrance to

the efficient numerical solving of the dynamics of such

systems.

Simple spring-mass systems with displacementdependent,

non-smooth nonlinearities are studied in

this paper, as well as an application of the method to

a more realistic finite element model representing two

example shrouded blades impacting each other. These

examples successfully demonstrate the accuracy and

robustness of the method.},
 author = {Poudou, O. and Pierre, C. and Reisser, B.},
 year = {2004},
 title = {A New Hybrid Frequency-Time Domain Method for the Forced Vibration of Elastic Structures With Friction and Intermittent Contact},
 howpublished = {Proceedings of the 10th International Symposium on Transport Phenomena and Dynamics of Rotating Machinery, March 7-11, Honolulu, HI, USA, pp. 1--14},
 file = {Poudou, Pierre et al 2004 - A New Hybrid Frequency-Time Domain:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Poudou, Pierre et al 2004 - A New Hybrid Frequency-Time Domain.pdf:pdf}
}


