 journal = {Aeronautical Journal},
 doi = {10.1017/aer.2017.29},
 file = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85020544662&doi=10.1017%2faer.2017.29&partnerID=40&md5=fb34833735f42e8c1ee4da96292dc49d}
}


