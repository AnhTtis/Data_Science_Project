@article{wang1991,
 abstract = {The friction damper has been used widely to reduce the resonant vibration of blades. In the past, it has usually been assumed that the friction coefficient of the damper is constant. In this work, the friction coefficient of the damper was assumed to be a function of the sliding velocity of the contact surface. The results show that the maximum resonance response and resonance frequency shift predicted by the model with a variable friction coefficient are significantly different from the results predicted by the traditional model with a constant friction coefficient. The results predicted theoretically agree well qualitatively with an existing experimental result.},
 author = {Wang, J. H. and Shieh, W. L.},
 year = {1991},
 title = {The Influence of a Variable Friction Coefficient on the Dynamic Behavior of a Blade with a Friction Damper},
