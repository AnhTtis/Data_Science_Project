@article{Thomsen.1996,
 abstract = {The potential of using self-arranging sliding mass for vibration damping is considered. Earlier studies have demonstrated the effect of freely sliding mass: vibrations of a structure may decrease when forces of non-linear interaction cause a mass to slide along it. However, when the mass stops sliding at an equilibrium point, a new resonance frequency is created that cannot be damped by further sliding. This study suggests adding passive restoring forces for returning the mass to a prescribed position at small levels of vibration. The effect of this is considered numerically and analytically for a simple study case: a spring-loaded point mass sliding on a tensioned string. The arrangement is shown to damp resonant vibrations, although the damped response may, under certain conditions, be beating or chaotic. Frequency responses are derived analytically for a number of cases, including slow frequency-sweeps across resonance. The suggested device is shown to limit vibrations due to parametric resonance. Sliding mass tends to be most effective in damping high amplitudes, thus complementing other passive or active control strategies that are typically most effective at low amplitudes.},
 author = {Thomsen, J. J.},
 year = {1996},
 title = {VIBRATION SUPPRESSION BY USING SELF-ARRANGING MASS: EFFECTS OF ADDING RESTORING FORCE},
 pages = {403--425},
 volume = {197},
 number = {4},
 journal = {Journal of Sound and Vibration},
