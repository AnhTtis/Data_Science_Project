@misc{zucc2006a,
 abstract = {Resonant vibrations affect fatigue life of vane segments.

Friction damping is employed to reduce vibration amplitude.

When vane segments are assembled, they are twisted so that

lower platforms are in contact. The sum of displacements of

the two ends of the lower platform after twisting is defined

`interlocking'. Different `interlocking' values correspond to

different values of normal contact force. When interlocked

vanes vibrate under external force excitation, energy is

dissipated by friction forces at lower platform contacts

providing damping to the system.

The aim of this paper is the experimental validation of a

numerical code for forced response calculation of interlocked

vane segments. Since friction forces depend on relative

displacements of bodies in contact, the system is nonlinear.

System force response is computed by means of Harmonic

Balance Method (HBM).

Contact model implemented in the code is characterised by

tangential and normal stiffness to take into account local

compliance of the contact area. Gross slip occurs when the

instantaneous ratio of tangential force to normal force is

equal to the friction coefficient. Also effect of microslip is

taken in account.

The experimental set-up used to validate the code is made of

a vane segment fixed at the outer radius to an aluminium

frame and in contact with two supports at the inner radius.

Comparison between the numerical predictions and

experimental results is performed for different values of

interlocking (i.e. force normal to the contact).},
 author = {Zucca, S. and Filippi, S. and Droetti, F. and Gola, M. M.},
 year = {2006},
 title = {Forced Response of Interlocked Vane Segments: Numerical Predictions and Experimental Results},
 keywords = {contact models;dynamics;Friction damping;Turbomachinery},
 howpublished = {Paper ESDA2006-95551, Proc. of ESDA2006, 8th Biennial ASME Conference on Engineering Systems Design and Analysis, July 4-7, Torino, Italy},
 file = {Zucca_2006a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Zucca_2006a.pdf:pdf}
}


@misc{zucc2006b,
 abstract = {In this paper a methodology for forced response calculation of

bladed disks with underplatform dampers is described.

The FE disk model, supposed to be cyclically symmetric, is

reduced by means of Component Mode Synthesis and then

DOFs lying at interfaces are further reduced by means of

interface modes.

Underplatform dampers are modeled as rigid bodies

translating both in the radial and in the tangential direction of

the engine.

Contacts between blade platforms and damper are simulated

by means of contact elements characterized by both tangential

and normal contact stiffness, allowing partial separation of

contact surfaces.

Differential equilibrium equations are turned in non-linear

algebraic equations by means of the Harmonic Balance

Method (HBM).

The methodology is implemented in a numerical code for

forced response calculation of frictionally damped bladed

disks.

Numerical calculations are performed to evaluate the

effectiveness of both the reduced order model and the

underplatform model in simulating the dynamic behavior of

bladed disks in presence of underplatform dampers.},
 author = {Zucca, S. and Borrajo, J. and Gola, M. M.},
 year = {2006},
 title = {Forced Response of Bladed Disks in Cyclic Symmetry With Underplatform Dampers},
 keywords = {bladed disks;cyclic symmetry;Friction damping;reduction technique;underplatform damper},
 howpublished = {Paper GT2006-90785, Proc. of GT2006, ASME Turbo Expo 2006: Power for Land, Sea and Air, May 8-11, Barcelona, Spain},
 file = {Zucca_2006b:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Zucca_2006b.pdf:pdf}
}


@misc{zucc2008a,
 abstract = {Under-platform dampers are used to reduce resonant

stresses in turbine blades to avoid high cycle fatigue failures.

In this paper a model of semi-cylindrical under-platform

damper (i.e. with one flat side and one curved side) for turbine

blades is described.

The damper kinematics is characterized by three degrees of

freedom (DOFs): in-plane translations and rotation.

The model is suitable for the implementation in numerical

codes for the forced response calculation of bladed disks with

semi-cylindrical under-platform dampers.

A numerical test case is presented to show the capabilities of

the model and to analyze the effect of the damper parameters

on the damper behavior and on the system dynamics.

Effect of damper rotation on the forced response of the system

is highlighted, as well as the non-uniqueness of the forced

response of the system due to non-uniqueness of normal preloads.},
 author = {Zucca, S. and Botto, D. and Gola, M. M.},
 year = {2008},
 title = {A kinematic model of semi-cylindrical friction dampers for turbine blades},
 howpublished = {Paper GT2008-51058, Proc. of ASME Turbo Expo 2008: Power for Land, Sea and Air, GT2008, June 9-13, Berlin, Germany},
 file = {Zucca_2008a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Zucca_2008a.pdf:pdf}
}


@unpublished{zucc2008b,
 abstract = {Under-platform friction dampers are used to reduce resonant stresses in turbine blades to avoid high cycle

fatigue failures. In order to predict the response level of bladed disks with underplatform dampers, damper

models are developed and implemented in numerical codes.

In this paper, the effect of static balance equations of the damper on the predicted forced response of bladed

disk is investigated.

In detail it is shown that if the static balance equations of the damper are decoupled by the dynamics balance

equations unique values of static normal loads acting at contact between the blade and the damper cannot be

computed. Only a range of possible values can be defined. The extent of this range is analytically related to

the damper geometry and to the coefficient of friction at the contact.

Non unique values of static normal loads implies non unique hysteresis cycles of friction forces and non

unique friction damping levels.

Implementation of the damper model in a numerical code for the forced response calculation of turbine

bladed disks with under-platform dampers shows that non-uniqueness of static normal loads produces a

range of variability in the forced response of the system.},
 author = {Zucca, S. and Botto, D. and Gola, M. M.},
 year = {2008},
 title = {Range of variability in the dynamics of bladed disks with underplatform dampers.},
 keywords = {bladed disks;Friction damping;passive control of vibration;sliding contacts},
 address = {Torino},
 file = {Zucca_2008b:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Zucca_2008b.pdf:pdf}
}


