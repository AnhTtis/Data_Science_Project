@article{klerk2008,
 abstract = {Four decades after the development of the first dynamic substructuring techniques, there is a necessity to classify

the different methods in a general framework that outlines the relations between them. In this paper, a certain vision

on substructuring methods is proposed, by recalling important historical milestones that allow us to understand

substructuring as a domain decomposition concept. Thereafter, based on the dual and primal assembly of

substructures, a general framework for the classification of the methods is presented. This framework allows us to

indicate how the various classes of methods, proposed along the years, can be derived from a clear mathematical

description of substructured problems. Current bottlenecks in experimental dynamic substructuring, as well as

solutions found in literature, will also be briefly discussed.},
 author = {de Klerk, D. and Rixen, D. J. and Voormeeren, S. N.},
 year = {2008},
 title = {General Framework for Dynamic Substructuring: History, Review and Classification of Techniques},
 pages = {1169--1181},
 volume = {46},
 number = {5},
 journal = {AIAA Journal},
 file = {Klerk, Rixen et al 2008 - General Framework for Dynamic Substructuring:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Klerk, Rixen et al 2008 - General Framework for Dynamic Substructuring.pdf:pdf}
