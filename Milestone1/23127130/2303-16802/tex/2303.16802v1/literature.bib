 file = {Cardona, Lerusse et al 1998 - Fast Fourier nonlinear vibration analysis:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Cardona, Lerusse et al 1998 - Fast Fourier nonlinear vibration analysis.pdf:pdf}
}


@article{carn1957,
 abstract = {In the present paper the static bending of pre-twisted cantilever blading is examined.

The blading is pre-twisted linearly about the centroid of its cross-section to a maximum

angle of n/2 radians and is considered to be mounted encastr6 at the root. The behaviour

of such blading, particularly when bending under a distributed load, is of considerable

practical importance.

By the application of variational calculus, static equilibrium equations are derived from

expressions for the total potential energy of blades subject to either concentrated or

uniformly distributed bending loads. Solution of the equilibrium equations yields

expressions for the static deflexions. Over a range of pre-twist angles between 0 and n/2

radians deflexions are calculated for blades of (1) narrow rectangular cross-section when

subjected to either method of loading, (2) narrow aerofoil cross-section when subjected to

a uniformly distributed load.

Sets of steel cantilever blades of MITOW rectangular cross-section are loaded laterally

at their free ends and the deflexions determined by measurement. The experimental

values of deflexion compare reasonably with the relevant calculated ones. Small

discrepancies bemeen the two sets of results are not yet fully explained and further

investigation of the problem is under consideration.

For blades where the pre-twist angles do not exceed 7r/2 radians, the agreement

obtained between the two sets of deflexions for the concentrated load system is such that

the validity of the theory, when it is applied to other loading systems, can safely be

assumed without further recourse to experiment.},
 author = {Carnegie, W.},
 year = {1957},
 title = {Static bending of pre--twisted cantilever blanding},
 keywords = {Engineering science},
 pages = {873--894},
 volume = {171},
 journal = {ARCHIVE: Proceedings of the Institution of Mechanical Engineers 1847-1982 (vols 1-196)},
 doi = {10.1243/PIME{\textunderscore }PROC{\textunderscore }1957{\textunderscore }171{\textunderscore }071{\textunderscore }02},
 file = {Carnegie_1957:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Carnegie_1957.pdf:pdf}
}


@article{carn1959a,
 abstract = {A theoretical expression is obtained for the work done due to centrifugal effects when a rotating cantilever blade executes small vibrations. The total potential 

energy and kinetic energy of the system are formulated, use being made of previously  derived expressions for the stationary blade. 

By use of Rayleigh's method an euqation for the fundamental frequency of vibration is established and, for the case of a straight blade of uniform and symmetrical

cross-section, a solution is deduced based on an approximate shape for the normal elastic curve. The characteristic differential equations of motion for the rotating system are obtained from the energy expressions by use of variational calculus. Coriolis effects, being small, can be neglected in the analysis.},
 author = {Carnegie, W.},
 year = {1959},
 title = {Vibrations of rotating cantilever blading: Theoretical approaches to the frequency problem based on energy methods},
 pages = {235--240},
 volume = {1},
 number = {3},
 journal = {Journal Mechanical Engineering Science},
 file = {Carnegie_1959a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Carnegie_1959a.pdf:pdf}
}


@article{carn1959b,
 abstract = {This paper reports a study of the vibrations of pre-twisted cantilever blading. The blading

is pre-twisted linearly about the centroid of its cross-section up to an angle of {\~{}} / r2ad and

is considered to be mounted encastrC at the root. Accurate prediction of the frequencies of

vibration of such blading is of considerable practical importance.

Two theoretical approaches to the problem are discussed. The first one, involving a

direct solution of the equations of motion, is not made use of, although the characteristic

equations are derived in an appendix for reference. The second approach is based on the

zpplication of Rayleigh's principle. For blades of uniform rectangular cross-section,

frequency equations are derived for the fundamental mod2 of lateral motion and all the

modes of torsional motion. Similar types of equation are shown to apply to aerofoil crosssection

blading since direct coupling effects between bending and torsion, being extremely

small, can be neglected.

Vibration tests were conducted on sets of bladcs of both uniform rectangular and

uniform aerofoil cross-section pre-twisted between 0 and 7r/2 rad, the objects being to

determine the frequencies and corresponding nodal patterns contained within a specified

frequency band. The effect of pre-twist on the measured frequencies of the lateral, torsional,

and, for the aerofoil cross-section, coupled modes of vibration is shown, with certain

exceptions, to be considerable.

For the modes of motion where a theoretical solution is provided, reasonable agreement

is sho*wn to exist between the calculated and corresponding measured frequencies. Hence,

the use of Rayleigh's method is justified, giving, as it does, relatively simple expressions

for the natural frequencies of vibration.

The torsional frequencies of pre-twisted blades are dependent on the torsional stiffness

values. Hence, a theoretical study of torsional stiffness variation with pre-twist is presented,

the results obtained being confirmed experimentally by a series of simple torsion tests.},
 author = {Carnegie, W.},
 year = {1959},
 title = {Vibrations of pre-twisted cantilever blading},
 keywords = {Engineering science},
 pages = {343--374},
 volume = {173},
 number = {12},
 journal = {ARCHIVE: Proceedings of the Institution of Mechanical Engineers 1847-1982 (vols 1-196)},
 doi = {10.1243/PIME{\textunderscore }PROC{\textunderscore }1959{\textunderscore }173{\textunderscore }038{\textunderscore }02},
 file = {Carnegie_1959b:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Carnegie_1959b.pdf:pdf}
}


