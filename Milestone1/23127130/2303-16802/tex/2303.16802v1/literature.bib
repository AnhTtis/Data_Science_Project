 file = {Levitan_1960:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_mechanik\\Levitan_1960.pdf:pdf}
}


@article{lewa1987,
 abstract = {This paper presents an analytical solution for geometrically non-linear free vibrations of beams with elastically supported ends in the horizontal direction. The equation of motion is obtained by employing Hamilton's principle and assuming that horizontal inertia forces can be neglected. The Ritz method, with a continuum solution and an iterative procedure, are used for determining the frequencies and non-linear modes of vibrations. The orthogonality conditions for these modes are also discussed. Numerical results for various beam boundary conditions are presented and compared with available results.},
 author = {Lewandowski, R.},
 year = {1987},
 title = {Application of the Ritz method to the analysis of non-linear free vibrations of beams},
 pages = {91--101},
