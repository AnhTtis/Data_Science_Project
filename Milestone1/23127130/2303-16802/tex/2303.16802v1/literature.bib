 issn = {1359-8368},
 journal = {Composites Part B: Engineering},
 doi = {10.1016/j.compositesb.2009.10.009}
}


@article{RAGHOTHAMA.1999,
 abstract = {The periodic motions of a non-linear geared rotor-bearing system are investigated by the incremental harmonic balance (IHB) method. A path following procedure using arc length continuation technique is used to trace the bifurcation diagrams. The system exhibits a period doubling route and a quasiperiodic route to chaos in different regions of excitation frequency. The chaotic motions are investigated by numerical integration and the Lyapunov exponents are computed. The periodic solutions and subharmonic solutions obtained by the IHB method compare very well with those obtained by numerical integration.},
