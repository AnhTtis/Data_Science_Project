@misc{pier2000,
 abstract = {This work considers an ongoing experimental investigation of phenomena related to blade

mistuning in bladed disk assemblies. A state-of-the-art facility has been developed which provides

an array of non-intrusive experimental techniques to demonstrate the high sensitivity of bladed

disks to small mistuning and to corroborate the occurrence of phenomena such as mode localization

and forced response amplitude magnification. In addition to documenting such effects, research

focuses chiefly on (1) validation of recently developed reduced-order modeling codes for prediction

of bladed disk forced response, (2) exploration of novel concepts such as intentional mistuning, and

(3) development of techniques for identifying the mistuning characteristics of industrial blisks from

measurements of the complete blisk system response.},
 author = {Pierre, C. and Ceccio, S. L. and Judge, J. and Cross, C. J.},
