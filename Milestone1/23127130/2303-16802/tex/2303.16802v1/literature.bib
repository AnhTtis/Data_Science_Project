 journal = {Computers and Structures},
 doi = {10.1016/j.compstruc.2015.11.003},
 file = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84948441643&doi=10.1016%2fj.compstruc.2015.11.003&partnerID=40&md5=fd23b27680a98744a3012806e8b29484}
}


@misc{boot1972,
 abstract = {Structural darnping within the wheelhead of a grinding machine is an inportant

factor contributing to the stability of the machine as a whole. In

ordler that this phenomenon can be incorporated in vibration analyses, it

is necessary to study the possible sources of energy dissipation within

the wheelhead structure. The damping source studied in this work is that

of a preloaded rnetallic interface, such as that found between the outer

race of the bearings and the shoulder of the bore of the wheelhead body.

Experiments are described which were conducted on a test rig designed to

simulate the interfacial conditions occurring in a typical wheelhead. From

the results of static tests an idealized model is formulated and its response

to dynamic excitation analyzed. The results of this work indicate

that an optinum preload giving maximum frictional damping exists for

metallic interfaces subject to tangential vibrations.},
 author = {Boothroyd, G. and Poli, C. and Migliozzi, L. P.},
 year = {1972},
