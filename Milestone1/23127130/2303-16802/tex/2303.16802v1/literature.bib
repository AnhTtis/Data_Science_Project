@article{Thorin.2018,
 abstract = {In turbomachinery, it is well known that tighter operating clearances improve the efficiency. However, this leads to unwanted potential unilateral and frictional contact occurrences between the rotating (blades) and stationary components (casings) together with attendant thermal excitations. Unilateral contact induces discontinuities in the velocity at impact times, hence the terminology nonsmooth dynamics. Current modeling strategies of rotor--stator interactions are either based on regularizing penalty methods or on explicit time-marching methods derived from Carpenter's forward Lagrange multiplier method. Regularization introduces an artificial time scale in the formulation corresponding to numerical stiffness, which is not desirable. Carpenter's scheme has been successfully applied to turbomachinery industrial models in the sole mechanical framework, but faces serious stability issues when dealing with the additional heat equation. This work overcomes the above issues by using the Moreau--Jean nonsmooth integration scheme within an implicit \textgreek{j}-method. This numerical scheme is based on a mathematically sound description of the contact dynamics by means of measure differential inclusions and enjoys attractive features. The procedure is unconditionally stable opening doors to quick preliminary simulations with time-steps one hundred times larger than with previous algorithms. It can also deal with strongly coupled thermomechanical problems.},
 author = {Thorin, Anders and Gu{\'e}rin, Nicolas and Legrand, Mathias and Thouverez, Fabrice and Almeida, Patricio},
 year = {2018},
 title = {Nonsmooth Thermoelastic Simulations of Blade--Casing Contact Interactions},
 volume = {141},
 number = {2},
 issn = {0742-4795},
 journal = {Journal of Engineering for Gas Turbines and Power},
 doi = {10.1115/1.4040857}
}


@article{thou1996,
 abstract = {Linear approximations of the behaviour of structures are often flawed due to the presence of non-linearities. These can strongly influence the dynamic response; it is essential to detect and identify them. The purpose of this paper is to develop a method to identify the NARMAX on the modal base. By using the modal co-ordinates to express the MARMAX we will be able to reduce the number of parameters to identify and thus make possible its use in a multi-dimensional mechanical context. Moreover, the modal co-ordinates allow us to minimize the non-linearities localization problem. A numerical test will be carried out to illustrate the properties and advantages of this procedure.},
 author = {Thouverez, F. and Jezequel, L.},
 year = {1996},
 title = {Identification of Naramax Models on a Modal Base},
 pages = {193--213},
 volume = {189},
 number = {2},
