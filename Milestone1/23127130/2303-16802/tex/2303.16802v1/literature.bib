@misc{mous2013b,
 abstract = {In the components of the nuclear power plants facilities, vibrating structures with localized contact conditions are frequently

encountered. The linear vibration theory is evidently not sufficient for explaining some phenomenon observed. In this context,

the use of non-linear normal modes seems to be a useful concept for investigating the complexity of nonlinear and non-smooth

dynamics. We choose to limit our study to conservative systems and by then to define nonlinear normal modes like a family of

periodic solutions. For the computation, we improve a method based on the combination of the harmonic balance method and

asymptotic numerical method. Also we propose to regularize the non-smooth forces to fulfill the framework of this numerical

procedure. The first improvement consists to introduce the algorithm Fast-Fourier Transform and its inverse to accelerate some

part of the procedure without any loss of precision. The second improvement consists to dissociate the number of harmonics

between unknown of the system. The method has been implemented into two software, MANLAB for an academic use and

Code Aster, a finite-element solver developed by EDF R{\&}D, for an industrial use. The method will be illustrated using some

examples close to industrial structures.},
 author = {Moussi, E.H and Bellizzi, S. and Cochelin, B. and Nistor, I.},
 year = {2013},
 title = {Computation of Nonlinear Normal modes of structures with elastic stops},
 pages = {1--7},
 howpublished = {Paper 710, Proceedings of the 4th Canadian Conference on Nonlinear Solid Mechanics, July 23-26, Montr{\'e}al,~Canada},
 file = {Moussi, Bellizzi et al 2013 - Computation of Nonlinear Normal modes:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Moussi, Bellizzi et al 2013 - Computation of Nonlinear Normal modes.pdf:pdf}
}


@article{moyr2002,
 abstract = {The high performance bladed disks used in today's turbomachines must meet strict standards in terms of aeroelastic stability and resonant response level. One structural characteristic that can significantly impact on both these areas is that of bladed disk mistuning. To predict the effects of mistuning, computational efficient methods are much needed to make free-vibration and forced-response analyses of full assembly finite element (FE) models feasible in both research and industrial environments. Due to the size and complexity of typical industrial bladed disk models, one must resort to robust and systematic reduction techniques to produce reduced-order models of sufficient accuracy. The objective of this paper is to compare two prevalent reduction methods on representative test rotors, including a modern design industrial shrouded bladed disk, in terms of accuracy (for frequencies and mode shapes), reduction order, computational efficiency, sensitivity to intersector elastic coupling, and ability to capture the phenomenon of mode localization. The first reduction technique employs a modal reduction approach with a modal basis consisting of mode shapes of the tuned bladed disk which can be obtained from a classical cyclic symmetric modal analysis. The second reduction technique uses Craig and Bampton substructure modes. The results show a perfect agreement between the two reduced-order models and the nonreduced finite element model. It is found that the phenomena of mode localization is equally well predicted by the two reduction models. In terms of computational cost, reductions from one to two orders of magnitude are obtained for the industrial bladed disk, with the modal reduction method being the most computationally efficient approach.},
 author = {Moyroud, F. and Fransson, T. and Jacquet-Richardet, G.},
 year = {2002},
 title = {A Comparison of Two Finite Element Reduction Techniques for Mistuned Bladed Disks},
 keywords = {finite element analysis;Gas turbines;rotors;tuning;vibrations},
 pages = {942--952},
 volume = {124},
 number = {4},
 journal = {Journal of Engineering for Gas Turbines and Power},
 doi = {10.1115/1.1415741},
 file = {Moyroud_2002:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Moyroud_2002.pdf:pdf}
}


