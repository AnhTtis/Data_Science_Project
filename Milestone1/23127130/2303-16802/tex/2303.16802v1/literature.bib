@article{ayer2006,
 abstract = {A method for predicting the vibratory response of bladed disks under high engine acceleration rates is developed. The method is based on the Fundamental Mistuning Model, an existing reduced order model for predicting the steady-state vibratory response. In addition, a criterion is developed for a critical engine acceleration rate, above which transient effects play a large role in the response. It is shown that military engines operate at acceleration rates above this critical value and therefore transient effects are important in practice.},
 author = {Ayers, J. P. and Feiner, D. M. and Griffin, J. H.},
 year = {2006},
 title = {A Reduced-Order Model for Transient Analysis of Bladed Disk Forced Response},
 keywords = {blades;discs (structures);engines;military equipment;reduced order systems;Transient analysis;vibrations},
 pages = {466--473},
 volume = {128},
 number = {3},
 journal = {Journal of Turbomachinery},
 doi = {10.1115/1.2185675},
 file = {Ayers_2006:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Ayers_2006.pdf:pdf}
}


@article{Azeez.1999,
 abstract = {This work aims in obtaining the transient response of an overhung rotor undergoing vibro-impacts due to a defective bearing. An overhung rotor clamped on one end, with a flywheel on the other and impacts occurring in between, due to a bearing with clearance, is considered. The variation of this system, popularly known as the Jeffcot rotor, has been considered in previous works, but there, the system has been reduced to a single degree of freedom for ease of analysis. In this work the system is modeled as a continuous rotor including gyroscopic effects and the governing partial differential equations are set up and numerically solved. The method of assumed models is used to discretize the system in order to solve the partial differential equations (PDE) by partially decoupling them and solving numerically. These partially decoupled equations are more accurate and less time consuming than the ones produced by finite elements or other numerical schemes. The most important step in the success of this method is the selection of suitable modes for decoupling the system. It is not simply enough to select orthonormal modes for decoupling the PDEs, but care must be taken to select the modes as close to the actual system as possible. Using this method numerical experiments are run and representative results are presented. The different numerical issues involved are also discussed. An experimental setup was also built to run experiments and validate the results. In the setup a defective bearing is introduced at the flywheel end of the shaft to create radial impacts on the shaft. Laser sensor non-contact probes are used to measure the displacement of the shaft a specified locations. Experimental observations show satisfactory qualitative agreement when compared to the numerical integrations. {\copyright} 1998 Published by Elsevier Science Ltd. All rights reserved.},
 author = {Azeez, M.F.A. and Vakakis, A. F.},
 year = {1999},
 title = {Numerical and experimental analysis of a continuous overhung rotor undergoing vibro-impacts},
 keywords = {Impacts;Jump phenomena;Non-linear dynamics;Rotor},
 pages = {415--435},
 volume = {34},
 number = {3},
 journal = {International Journal of Non-Linear Mechanics},
 file = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0000539076&partnerID=40&md5=14cfb84323858698f3cc39bb036d1a6d}
}


@article{Azevedo.2009,
 abstract = {The premature failure of steam turbine rotor blades, manufactured in forged 12{\%} Cr-NiMoV martensitic stainless steel, was investigated using visual inspection, non-destructive testing, macro and microfractography, microstructural characterization, EDS microanalysis, chemical analysis, micro hardness and tensile testing. The blades belonged to the last stage of a thermoelectric plant steam turbine generator (140 MV A). The results indicated that the failure of the blades was promoted by foreign-particle erosion, which attacked preferentially the low-pressure side of the lower trailing edge of the blades. The resulting wear grooves acted as stress raisers and promoted the nucleation of fatigue cracks, which probably grew during the transition events of the steam turbine operation. Finally, water drop erosion was observed on the blade upper leading edge (low-pressure side). {\copyright} 2009 Elsevier Ltd. All rights reserved.},
 author = {Azevedo, C.R.F. and Sin{\'a}tora, A.},
 year = {2009},
 title = {Erosion-fatigue of steam turbine blades},
 keywords = {blades;Erosion-fatigue;Martensitic steel;Solid-particle erosion;steam turbine},
 pages = {2290--2303},
 volume = {16},
 number = {7},
 journal = {Engineering Failure Analysis},
 doi = {10.1016/j.engfailanal.2009.03.007},
 file = {http://www.scopus.com/inward/record.url?eid=2-s2.0-67649580166&partnerID=40&md5=7a4fefbbdd0ff7b3c1d0041fc78e6a07}
}


