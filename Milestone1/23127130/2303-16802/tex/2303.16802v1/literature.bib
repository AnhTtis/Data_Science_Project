@misc{chan2006,
 abstract = {The complexity of aircraft engine entails large full 3D finite element (FE) models that pose challenges w.r.t. both computational resources and modeling efforts. For the application to vibration analysis, the approach of superelement (SE) in combination with reduction techniques such as component mode synthesis (CMS) has been successfully employed. Besides by increasing the size of FE models to include as much detail as possible, the most practical way for obtaining accurate FE models is by correlating with experimental modal data.

In this paper the correlation process is described for the case study of a full 3D aircraft engine model. Modal testing data have been acquired on individual engine casings, subassembly of casings as well as the full assembly. All the data are used in a step-by-step model validation process. The ``almost axi-symmetric'' casings cause complication in the form of mode pairs and ``rotated'' mode shapes. A method that is based on rotation of mode shapes for the mode pairs is applied to improve the correlation.

The high modal density of the full engine model is exemplified in two ways: (1) large number of modes which are closely spaced in frequency and (2) a mix of ``global'' and ``local'' modes which are complicated by the ``almost axi-symmetric'' cylindrical shell-type casing structure. In order to overcome these challenges, a method of selecting significant modes is introduced.},
 author = {Chan, Dennis S.H},
 year = {2006},
 title = {Correlation of Aircraft Engine Model using Experimental Modal Analysis},
 keywords = {Aircraft engine;correlation;modal analysis;modelling;vibration},
 howpublished = {Paper-ID 338; Proc. of the 7th IFToMM-Conference on Rotor Dynamics, Vienna, Austria, 25-28 September},
 file = {Chan_2006:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Chan_2006.pdf:pdf}
}


@misc{chan2008,
 abstract = {Random mistuning always exists in bladed disk structures. The maximum blade forced response amplitudes are often much larger than those of their perfectly tuned counterparts, which leads to eventual failure via high cycle fatigue (HCF). Therefore, it is of great importance to predict and, ultimately, to reduce the blade forced response levels as a result of random mistuning. In this paper, intentional mistuning is introduced into a simplified 12-bladed disk model by varying the stiffness of the blades in periodic harmonic patterns. The individual and combined effects of intentional mistuning, coupling and damping are examined in the absence and presence of random mistuning through numerical study. It is found that there is some threshold value of intentional mistuning and coupling that leads to maximum mistuning effects and certain relations among intentional mistuning strength, integer harmonics, coupling and damping can suppress the response levels of mistuned bladed disks, which provides useful guidelines for safe and reliable designs of bladed disk systems.},
 author = {Changbo, Y. and Jianjun, W. and Qihan, L.},
 year = {2008},
 title = {On the individual and combined effects of intentional mistuning, coupling and damping on the forced response of bladed disks},
 howpublished = {Paper GT2008-50960, Proc. of ASME Turbo Expo 2008: Power for Land, Sea and Air, Berlin, Germany, June 9-13},
 file = {Changbo_2008:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Changbo_2008.pdf:pdf}
}


@article{Chandra.2015,
 abstract = {A robust time integration scheme for snap-through buckling of shallow arches is proposed. The algorithm is a composite method that consists of three sub-steps. Numerical damping is introduced to the system by employing an algorithm similar to the backward differentiation formulas method in the last sub-step. Optimal algorithmic parameters are established based on stability criteria and minimization of numerical damping. The proposed method is accurate, numerically stable, and efficient as demonstrated through several examples involving loss of stability, large deformation, large displacements and large rotations.},
