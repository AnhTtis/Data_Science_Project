 file = {Jaiswal_1994:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Jaiswal_1994.pdf:pdf}
