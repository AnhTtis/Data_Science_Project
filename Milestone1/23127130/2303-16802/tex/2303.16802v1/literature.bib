}


@misc{kaza2005,
 abstract = {Possibility of active suppression of cascade flutter

was studied both numerically and experimentally. An

active control method using a piezo-electric device was

proposed and studied in the numerical analysis with a

developed flow-structure coupled code. In the control

method, the trailing edge of a blade was actively

oscillated with the piezo-electric device mounted on

the blade. Numerical analysis was carried out in

transonic flow where the instability of blade vibration

was dominated by movement of passage shock wave.

From the results, it was found that the unstable

vibration of the blades could effectively be suppressed

if the phase difference between the blade vibration and

the active trailing edge oscillation was properly

selected. The suppression effect was caused by

alteration of the shock oscillation behavior, which was

induced by the active oscillation of the trailing edge.

The experiment was performed in a linear cascade

tunnel under a subsonic flow condition to confirm the

suppression effect of the control method.},
 author = {Kazawa, J. and Watanabe, T. and Himeno, T.},
 year = {2005},
 title = {Active Control of Cascade Flutter with Piezo-Electric Device},
 howpublished = {Paper ISABE-2005-1254, Proc. of XVII International Symposium On Air Breathing Engines (ISABE)},
 file = {Kazawa_2005:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Kazawa_2005.pdf:pdf}
}


