@article{magn1957,
 abstract = {Es wird gezeigt, da{\ss} einige der in der nichtlinearen Mechanik viel gebrauchten N{\"a}hrungsmethoden (Aequivalente Linearisierung, Harmonische Balance, Beschreibungsfunktion, Energie-Mittelung, Fehlerquadrat-Minimum) als Spezialf{\"a}lle des allgemeinen Ritz-Galerkin schen Variationsverfahren aufgefa{\ss}t werden k{\"o}nnen. Unter Verwendung der Ritz-Galerkinschen Vorschriften wird ein verallgemeinertes Verfahren zur Linearisierung durch nichtharmonische Funktionen angegeben, das f{\"u}r die Berechnung von Relaxationsschwingungen Vorteile bringen kann.



Some approximation methods frequently used in non-linear mechanics (equivalent linearisation, harmonic balance, describing function, mean energy, square deviation minimum) are shown to be special cases of the general variational method due to Ritz and Galerkin. Following the prescriptions given by Ritz-Galerkin a generalized method is derived allowing the linearisation by means of anharmonic functions. It is expected that this method will offer some advantages in the treatment of relaxation oscillations.},
 author = {Magnus, K.},
 year = {1957},
 title = {{\"U}ber den Zusammenhang verschiedener N{\"a}herungsverfahren zur Berechnung nichtlinearer Schwingungen},
 pages = {471--485},
