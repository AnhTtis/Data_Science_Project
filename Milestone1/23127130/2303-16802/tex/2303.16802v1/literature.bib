@misc{peng2005,
 abstract = {This paper presents approaches, results of numerical non-linear forced response analyses

and damping characteristics for compressor shrouded and cantilever vanes. These analyses

include the effects of friction interface forces at joints. For the forced response analyses, a

new multiharmonic balance code is used. This allows structures with friction contact, gap and

interferences to be modelled and calculated with high level of detailing and accuracy. The

effects of normal loads, friction coefficient, contact stiffness, opening and closure of contact

surfaces were all investigated here. The excitation loads considered in the analyses are

aerodynamic loads calculated from unsteady aerodynamic blade surface pressures. These

unsteady aerodynamic pressures are obtained from CFD computations.



Results from the numerical computations are correlated with engine strain gauge data. They

show a very good agreement between predicted mechanical damping Q values and engine

measured Q values. This agreement has been seen across various blade designs analysed

herein.},
 author = {Peng, C. and Petrov, E.},
 year = {2005},
 title = {Prediction of Mechanical Damping for Core Compressor Blades and Vanes},
 howpublished = {Abstract to 10th National Turbine Engine High Cycle Fatigue (HCF) Conference to be held in New Orleans, USA, 8-11 March},
 file = {Peng_2005:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Peng_2005.pdf:pdf}
}


@article{penn2011,
 abstract = {Blade vibration reduction is an important task in high performance turbo machinery for power generation, in order to avoid the risk of blade failure due to the overcoming of fatigue limit. A possible way to obtain this result is a contact related phenomenon, i.e. by physically limiting the vibration amplitude on the blade tip leaving a small gap between the shrouds of adjacent blades. When the relative displacement between adjacent blades exceeds the gap, in a certain vibration mode of the blade row, a contact occurs between the shrouds, the relative motion is restricted and energy is dissipated by friction and impact during the contact. This is called the snubbing mechanism.

In this paper, an original simplified model of bladed disks, in which the snubbing mechanism can occur, is presented and numerical integration in time domain furnishes the time histories of the vibrations of the blades. The level of vibration reduction is then evaluated in some different modes that could be excited for instance by the fluid flow. It is also shown that unlucky combinations of system and excitation parameters can effect also a certain magnification instead of a reduction of the vibration amplitudes.

Experimental results on single blade and blade groups of a steam turbine are used to tune the parameters of the system.},
 author = {Pennacchi, P. and Chatterton, S. and Bachschmid, N. and Pesatori, E. and Turozzi, G.},
 year = {2011},
 title = {A Model to Study the Reduction of Turbine Blade Vibration Using the Snubbing Mechanism},
 keywords = {Nonlinear response;simulation;Snubbing mechanism;Steam turbine blade vibration;vibration suppression},
 pages = {1260--1275},
 volume = {25},
 number = {4},
 journal = {Mechanical Systems and Signal Processing},
 file = {Pennacchi_2011:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Pennacchi_2011.pdf:pdf}
