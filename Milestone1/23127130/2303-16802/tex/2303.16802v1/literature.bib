 journal = {Journal of Engineering for Gas Turbines and Power},
 doi = {10.1115/1.4027762}
}


@article{Bhaumik.2006,
 abstract = {During a test run of an aeroengine, a low-pressure turbine rotor blade had failed. The turbine blades were made of Ni-base superalloy of CM 247 LC grade and fabricated by DS investment casting. The blades were coated with platinum aluminide. Investigation revealed that the blade had failed by fatigue. It was concluded that the coating on the blade had developed cracks due to excessive bending/vibration, which in turn propagated by fatigue leading to the failure. {\copyright} 2005 Elsevier Ltd. All rights reserved.},
