 file = {http://dx.doi.org/10.2514/1.J051812},
 file = {Corral, Gallardo 2014 - Nonlinear Dynamics of Bladed Disks:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Corral, Gallardo 2014 - Nonlinear Dynamics of Bladed Disks.pdf:pdf}
