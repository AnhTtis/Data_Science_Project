@inproceedings{Arsenyev.2015,
 abstract = {The presented work is part of a research project aimed to-

wards multi-disciplinary robust shape optimization of low pres-

sure turbine (LPT) vane clusters. Multi-disciplinary analysis for

vane cluster optimization is used to evaluate design constraints,

involving  3D  aerodynamic  Navier-Stokes  simulation,  transient

thermal analysis, structural analysis and life prediction. The ex-

pense of these simulations combined with high-dimensional de-

sign space, makes the application of gradient-based or stochas-

tic optimizers inefficient. To overcome these issues, a surrogate-

based optimization approach is proposed here. High quality sur-

rogate models are required for accurate description of the con-

straints with life prediction.  Adaptive Global Surrogate-Based

Optimizer,  based  on  Gaussian-Process  (GP)  surrogate  models

and Expected Improvement infill criteria is employed, which al-

lows to efficiently increase the surrogate quality while approach-

ing the optimal solution at the same time. Additional techniques

are introduced to deal with the geometry rebuild failure, as some

combinations of the design parameters may produce infeasible

geometry.  The adaptive optimization method is successfully ap-

plied to the multi-disciplinary problem for the vane cluster shape

optimization. The comparison of the method performance with a

gradient-based optimizer indicates that a much lower number of

true simulations is needed by the proposed method to find an op-

timal design.   Successful optimization results shows the ability

of the method to handle simulation crashes, caused by geometry

rebuild failure.},
 author = {Arsenyev, I. and Duddeck, F. and Fischersworring-Bunk, A.},
 title = {Adaptive surrogate-based multi-disciplinary optimization for vane clusters},
 editor = {ASME},
 booktitle = {Proceedings of the Turbo Expo},
 year = {2015}
}


@misc{asai2009,
 abstract = {In order to reduce vibratory stresses and high-cycle-fatigue damage in turbines, it is strongly required to increase and estimate friction 

damping at contact interfaces, such as blade-disk attachments, shroud interfaces, and platform-type dampers. Studies on analytical 

estimation of friction damping are broadly classified into two approaches:~macroscopig and microscopig ones. in discussing friction 

damping when the interface is subjected to a non-uniform load distribution under micro-slip condition, the microscopig approch has

some advantages. However, there are two major problems; 1) there is few available data about the friction properties, such as 

tangential contact stiffness and micro-slip, 2) particular friction law should be derived to simulate micro-slip behaviour instead of 

Coulomb's friction law which expresses only macro slip.

In this paper, to cope with the above mentioned difficulties for the microscopig approach, an analytical procedure was developed by 

incorporating an original method to evaluate micro-slip and dissipated-energy distribution over the contact interfaces using friction

properties which are determined from hysteresis cycles experimentally obtained by special-fretting-friction tests. The validity of our 

proposed procedure was verified with high accuracy since estimated and tested damping ratios by vibration tests using dovetail

models were in very good agreement quantitatively with different input g levels, contact pressures, and contact angles, \textgreek{J}. Comparing

the effects of \textgreek{J} on damping ratio at the same contact force and input g levels, the highest damping was obtained at \textgreek{J} = 45°, following

by 20°, and the lowest one at \textgreek{J}~=~60° by vibration tests, whose trend was also confirmed to be expressed well qualitatively by our 

analysis. According to special-fretting-friction tests, it was found that tangential contact stiffness, Kt can be decided from the master curve,

p-Kt/Sc X L, where p is contact pressure, Sc is contact area and L is contact length along sliding direction, and micro-slip range, \textgreek{D} \textgreek{d}s can

be obtained from the relation \textgreek{D} \textgreek{d}s / p-Ft/Fn, where Fn is normal contact force and Ft is tangential contact force, since those master curves 

were confirmed to express test data with small scatter bands under different test conditions.},
 author = {Asai, K. and Sakurai, S. and Kudo, T. and Ozawa, N. and Ikeda, T.},
 year = {2009},
 title = {Evaluation of structural damping of blade-disk dovetail models based on dissipation energy on contact surfaces},
 howpublished = {Paper GT2009-59508, Proc. of ASME Turbo Expo 2009, GT2009, June 8-12, Orlando, Florida, USA},
 file = {Asai_2009:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Asai_2009.pdf:pdf}
}


@misc{asch2004a,
 abstract = {The THM 1304 industrial gas turbine is a two shaft

machine incorporating a two stage free power turbine suitable

for mechanical and generator drive applications (Fig. 1). As

part of an ongoing uprating and upgrading program design

modifications were made to the power turbine. The aim was not

only to increase power output and efficiency but also to

improve on the high availability.

The latest design incorporates new blades and vanes,

increasing the aerodynamic efficiency and improving the high

temperature endurance. Additionally, a new single piece casing

and a redesigned mechanical turbine discs arrangement and

shaft leads to a higher performance and optimized

maintainability. The up rated turbine covers the entire nominal

design load range from 9 to 14 MW and extends the available

speed range compared to its predecessor.

Furthermore, compatibility with the existing product range

has been considered.

A test program was carried out on the MAN TURBO test

bed in Oberhausen, Germany to verify the achievement of the

design goals. The program covered not only thermodynamic

and aerodynamic measurements but also temperature and

mechanical measurements. Special emphasis was put on the

validation of the vibration characteristics by means of a

telemetry system. Examples will highlight the development

testing programin detail.

The first production engine went into service at the

WINGAS pipeline compression station in Reckrod, Germany.

Not only the station layout but also the purpose of the station

will be described. Service data registered by the installed

monitoring system within the first 10,000 service hours will be

discussed and the service experience with the new engine will

be presented. During the in house test program the entire

turbine performance map was covered.},
 author = {Aschenbruck, E. and Beukenberg, M. and Blaswich, M. and Bokelmann, H.},
 year = {2004},
 title = {The Upgraded Power Turbine for the Industrial Gas Turbine THM 1304 Development and First Operational Experience},
 howpublished = {Paper GT2004-53785, Proc. of ASME Turbo Expo 2004, Power for Land, Sea, and Air, June 14-17, Vienna, Austria},
 file = {Aschenbruck_2004a:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Aschenbruck_2004a.pdf:pdf}
