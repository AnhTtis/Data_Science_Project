@incollection{hien1993,
 abstract = {A finite element formulation is proposed for first-order sensitivity analysis of nonlinear 

structural systems subject to dynamic loadings. Sensitivity functionals may be defined 

either over a time interval or taken at a specific (critical) time instant. Consistent first-order

expansions about the current values of nodal displacement-type variables are made for 

the system sensitivity functionals and other state functions. Both the direct differentiation

method (DDM) and adjoint system method (ASM) are alternatively employed to evaluate

sensitivity functional increments during the integration time step. The initial-value DDM

problem with nonhomogeneous initial conditions is solved using equivalent homogeneous

equations; advantage is taken of the available (possible iterative) response solution with

no iteration needed at the sensitivity stage of the analysis. The adjoint terminal-value 

problem is described at each time step through a linear system excited by the Dirac delta

impulse. The current version of ASM~is believed to be potentially competitive only for 

path-independent problems (elastic structures under large displacements, in particular).

Computational performance aspects of the procedures are discuessed and compared. 

Numerical algorithms have been worked out and shown to be potentially useful and cost-effective;

they can be readily implemented in existing finite element codes. A number of numerical 

results illustrate the paper.},
 author = {Hien, T. D. and Kleiber, M.},
 title = {Incremental Sensitivity Analsysis in Nonlinear Structural Dynamics},
 pages = {52--67},
 publisher = {{Atlanta Technology Publ}},
 isbn = {1-8837930-2-5},
 editor = {Kleiber, M. and Hisada, T.},
 booktitle = {Design-sensitivity analysis},
 year = {1993},
 address = {Atlanta, Ga.},
 file = {Hien_1993:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_mechanik\\Hien_1993.pdf:pdf}
}


@article{Hilali.2017,
 abstract = {The main objective of this work is to propose some regularization techniques for modeling contact actions in a clutch system and to solve the obtained nonlinear dynamic problem by a high-order algorithm. This device is modeled by a discrete mechanical system with eleven degrees of freedom. In several works, the discontinuous models of the contact actions are replaced by the smoothed functions using the hyperbolic tangent. We propose, in this work, to replace the discontinuous model by a regularized model with new continuous functions that permit us to search the solution under Taylor series expansion. This regularized model approaches better the discontinuous model than the model based on the smoothing functions, especially in the vicinity of the zone of singularities. To solve the equations of motion of discrete mechanical systems, we propose to use a high-order algorithm combining a time discretization, a change of variable based on the previous time, a homotopy transformation and Taylor series expansion in the continuation process. The results obtained by this modeling are compared with those computed by the Newton--Raphson algorithm.},
 author = {Hilali, Youssef and Braikat, Bouazza and Lahmam, Hassane and Damil, Noureddine},
 year = {2017},
 title = {Effect of regularized functions on the dynamic response of a clutch system using a high-order algorithm},
 keywords = {Clutch;High-order approach;Homotopy transformation;No smoothed functions;nonlinear dynamics;Regularization technique;Taylor series},
 pages = {764--778},
 volume = {345},
 number = {11},
 issn = {1631-0721},
 journal = {Comptes Rendus M{\'e}canique},
 doi = {10.1016/j.crme.2017.08.002},
 file = {http://www.sciencedirect.com/science/article/pii/S1631072117301705}
}


