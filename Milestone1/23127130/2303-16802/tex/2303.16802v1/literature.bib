 journal = {PAMM Proceedings in Applied Mathematics and Mechanics},
 doi = {10.1002/pamm.200910092},
 file = {Laborenz_2009b:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_eigene\\Laborenz_2009b.pdf:pdf}
}


