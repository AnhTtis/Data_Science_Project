 issn = {0734-743X},
 journal = {Fifth International Symposium on Impact Engineering},
 doi = {10.1016/j.ijimpeng.2006.12.006}
}


@article{pate2009,
 abstract = {In this paper, a methodology based on shooting technique and Newmark's time integration scheme is proposed for predicting the periodic responses of nonlinear systems directly from solution of second order equations of motion without transforming to double first order equations. The proposed methodology is quite suitable for systems with large number of degrees of freedom such as the banded system of equations from finite element discretization.},
