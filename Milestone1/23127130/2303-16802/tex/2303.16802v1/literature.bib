 author = {Casini, P. and Vestroni, F.},
 year = {2011},
 title = {Characterization of Bifurcating Non-Linear Normal Modes in Piecewise Linear Mechanical Systems},
 keywords = {Damaged systems;internal resonance;Non-linear normal modes;piecewise linear systems},
 pages = {142--150},
 volume = {46},
 number = {1},
 journal = {International Journal of Non-Linear Mechanics},
