@misc{irre1997,
 abstract = {During the operation process in rnany types of fluid flow

machines the rotating blades pass through various resonances e.g.

during run-up or run-down or other transient conditions. Therefore,

for the high cycle fatigue problem of the blades it might be

important to consider the transient vibratory response of the blades

during these passages through resonance and to get knowledge

about tle occuring maximum vibratory stresses.

In the paper, approximate formulas are presented which allow the

estimation of the maximum transient response of the blades.

Thereby, the influence of the change of the natural frequencies due

to the increasing or decreasing centrifugal force field during the runup

or run-down, respectively, is taken into consideration.

Basically, the approximate formulas are based on a linear change of

natural frequencies versus time and on a linear viscous type of

aamping. Extensions to account for parabolic changes which are

rnore realistic for centrifugal effects and for non-linear damping

models e.g. friction damping or turbulence damping are discussed.},
 author = {Irretier, H. D.},
 year = {1997},
 title = {The Maximum Transient Resonance of Rotating Blades With Regard to Centrifugal Force and Nonlinear Damping Effects},
 howpublished = {Proceedings of the ASME Turbo Expo, June 2-5, Orlando, FL, USA, pp. 1--7},
 file = {Irretier_1997:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Irretier_1997.pdf:pdf}
}


