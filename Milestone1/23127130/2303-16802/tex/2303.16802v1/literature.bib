 year = {1990},
 file = {Budak_1990:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_mechanik\\Budak_1990.pdf:pdf}
