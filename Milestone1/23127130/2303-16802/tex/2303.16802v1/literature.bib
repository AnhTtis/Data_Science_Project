@article{lehn2006,
 abstract = {In dieser Arbeit wird die Verwendung von Krylov-Unterraummethoden zur Reduktion flexibler K{\"o}rper in elastischen Mehrk{\"o}rpersystemen vorgeschlagen. Mit diesen Verfahren k{\"o}nnen reduzierte Modelle sehr gut auf den zu untersuchenden Frequenzbereich und die wirkenden Kr{\"a}fte abgestimmt werden. Dadurch ist es m{\"o}glich, f{\"u}r bestimmte Problemklassen bessere Approximationen zu erzeugen als bei Verwendung der in diesem Anwendungsbereich {\"u}blichen Verfahren auf Basis der modalen Reduktion.



In this work, a Krylov-subspace based projection method for the reduction of flexible bodies in flexible multibody systems is proposed. Using this technique, the reduced order model can be adapted very well to the chosen range of the spectrum and the distribution of the forces applied to the body. Thereby, it is possible to generate more accurate approximations for certain classes of problems than by an application of common methods based on modal reduction.},
 author = {Lehner, M. and Eberhard, P.},
 year = {2006},
 title = {Modellreduktion in elastischen Mehrk{\"o}rpersystemen - Model Reduction in Flexible Multibody Systems},
 keywords = {flexible multibody systems;Krylov-subspaces;Model reduction},
 pages = {170--177},
 volume = {54},
 number = {4},
 journal = {Automatisierungstechnik},
 doi = {10.1524/auto.2006.54.4.170},
 file = {Lehner_2006:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Lehner_2006.pdf:pdf}
}


@unpublished{lei2005,
 abstract = {In this paper, a non-local damping model including time and spatial hysteresis effects is

used for the dynamic analysis of structures consisting of Euler-Bernoulli beams and Kirchoff

plates. Unlike ordinary local damping models, the damping force in a non-local model is

obtained as a weighted average of the velocity field over the spatial domain, determined by a

kernel function based on distance measures. The resulting equation of motion for the beam or

plate structures is an integro-partial differential equation, rather than the partial differential

equation obtained for a local damping model. Approximate solutions for the complex

eigenvalues and modes with non-local damping are obtained using the Galerkin method.

Numerical examples demonstrate the efficiency of the proposed method for beam and plate

structures with simple boundary conditions, for non-local and non-viscous damping models,

and different kernel functions.},
 author = {Lei, Y. and Friswell, M. I. and Adhikari, Sondipon},
 year = {2005},
 title = {A Galerkin Method for Distributed Systems with Nonlocal Damping: IJSS Manuscript No. 05-121, revised 8 June 2005},
 address = {College of Aerospace and Material Engineering and National University of Defense Technology and PR China and PR China and Changsha and Hunan and 410073 and P. R. China and Department of Aerospace Engineering and University of Bristol and Queen's Building and University Walk and Bristol BS8 1TR and UK},
 note = {IJSS Manuscript No. 05-121, revised 8 June 2005},
 file = {Lei_2005:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Lei_2005.pdf:pdf}
}


