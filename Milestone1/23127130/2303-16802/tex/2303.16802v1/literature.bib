 keywords = {Aeroelasticity;Axial compressor;Disk;Experimental study;fatigue;Finite element method;Forced vibration;Gas turbines;High frequency;Numerical methods;Testing equipment;Three dimensional model;Turbine blade;Unsteady flow;Wakes},
