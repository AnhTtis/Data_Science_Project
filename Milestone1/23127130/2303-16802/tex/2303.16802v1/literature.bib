@article{szem1980,
 abstract = {The paper presents some new results on the response of the multi-degree-of-freedom nonlinear vibrating systems subject to harmonic excitation. New combination type regions of instability of harmonic solution of the first order have been found theoretically and by the aid of an analog computer. It has been shown that in those regions of the external force frequencies the response of the system is almost periodic, containing several harmonic components with incommensurable frequencies.



Cet article donne quelques resultats nouveaux sur la reponse de systemes vibratoires non lineaires a plusieurs degres de liberte, soumis a une excitation harmonique. De nouvelles regions d'instabilite de la solution harmonique du premier ordre ont ete trouvees theorequement et a l'aide d'un calculateur analogique. On a montre que dans ces regions des frequences de la force externe, la reponse du systeme est presque periodique, contenant plusiers composantes harmoniques avec des frequences incommensurables.



Diese Arbeit gibt einige neue Ergebnisse bezuglich des Verhaltens nichtlinearer Schwingungssysteme mit mehrfachen Freiheitgraden unter harmonischer Erregung. Auf theoretischem Wege und mit Hilfe eines Analogrechners wurden neue zusammengesetzte lnstabilitatgebiete fur harmonische Losungen erster Ordnung gefunden. Es wurde gezeigt, dass im Bereich der Frequenzen der ausseren Kraft das Systemverhalten fast periodisch ist und mehrere harmonische Komponenten mit nichtvergleichbaren Frequenzen enthalt.},
 author = {Szemplinska-Stupnicka, W. and Bajkowski, J.},
 year = {1980},
 title = {Multi-harmonic response in the regions of instability of harmonic solution in multi-degree-of-freedom non-linear systems},
 pages = {1--11},
 volume = {15},
 number = {1},
 journal = {International Journal of Non-Linear Mechanics},
 doi = {10.1016/0020-7462(80)90048-7},
 file = {Szemplinska_1980:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Szemplinska_1980.pdf:pdf}
}


@article{szem1983,
 abstract = {In the study of natural vibrations of non-linear elastic systems it is shown that the mode shape of the vibration can vary with the amplitude as well as the frequency, and that the amplitude frequency relation is strongly affected by constraints imposed on the mode shape in an approximate solution. A method is developed which assumes the approximate solution in the form of a truncated series in which, instead of the set of coefficients, the set of functions of spatial variables is unknown and then determined by a procedure that can be regarded as a generalization of the Ritz method. The problem of variations of the normal mode shapes and of the associated natural frequencies with the amplitude is illustrated by two examples of beams with non-linear boundary conditions, and the amplitude-frequency relation is compared to that corresponding to the a priori assumed linear normal mode solution. Further possible consequences of the mode shape amplitude variations in forced, resonant motion of nonconservative systems are also indicated.



On montre que dans l'{\'e}tude des vibrations naturelles de syst{\`e}mes {\'e}lastiques non lin{\'e}aires la forme de mode de la vibration peut varier avec l'amplitude aussi bien qu' avec la fr{\'e}quence et que la relation amplitude --- fr{\'e}quence est fortement affect{\'e}e par les contraintes impos{\'e}es {\`a} la forme de mode dans une solution approch{\'e}e. On d{\'e}veloppe une m{\'e}thode qui suppose la solution approch{\'e}e sous la forme d'une s{\'e}rie tronqu{\'e}e dans laquelle l'inconnue est l'ensemble des fonctions des variables d'espace au lieu de l'ensemble des coefficients et est ensuite d{\'e}termin{\'e}e par une proc{\'e}dure qui peut {\^e}tre consid{\'e}r{\'e}e comme une g{\'e}n{\'e}ralisation de la m{\'e}thode de Ritz. Le probl{\`e}me des variations avec l'amplitude des formes de modes normaux et des fr{\'e}quences naturelles associ{\'e}es est illustr{\'e} par deus exemples de poutres avec des conditions aux limites non lin{\'e}aires et on compare la relation amplitude fr{\'e}quence {\`a} celle correspondant {\`a} la solution suppos{\'e}e a priori en mode normal lin{\'e}aire. On indique des cons{\'e}quences suppl{\'e}mentaires possibles des variations avec l'amplitude des formes de modes dans le mouvement forc{\'e} en r{\'e}sonance de syst{\`e}mes non conservatifs.



In der Untersuchung der natuerlichen Schwingungen eines nichtlinearen elastischen Systems wird gezeigt, dass sich die Schwingungsform sowohl mit der Amplitude als auch mit der Frequenz veraendern kann, und dass die Beiziehung zwischen Amplitude und Frequenz sehr durch die Bedingungen, die in einer Naeherungsloesung der Schwingungsform aufgezwungen werden, beeinflusst wird. Eine Methode wird entwickelt wobei die Naeherungsloesung in der Form einer abgebrochenen Reihe angenommen wird. Anstatt einer Gruppe von Koeffizienten sind die Unbekannten die Gruppe von Funktionen in der raeumlichen Variablen und diese werden mit Hilfe eines Verfahrens, das als eine Verallgemeinerung der Ritzschen Methode betrachtet werden kann, besitmmt. Das Problem der Abhaengigkeit der normalen Schwingungsformen und der zugehoerigen natuerlichen Frequenzen von der Amplitude wird an Hand zweier Traegern mit nichtlinearen Randbedingungen als Beispiele illustriert. Die Beziehung guer Amplitude und Frequenz wird mit der entsprechenden Beziehung einer Loesung, fuer die von vornherein eine lineare normale Schwingungsform angenommen wird, verglichen. Moegliche Folgen der Amplitudenabhaengigkeit der Schwingungsform fuer erzwungene Resonanzbewegung nichtkonservativer Systeme werden angedeutet.},
 author = {Szemplinska-Stupnicka, W.},
 year = {1983},
 title = {``Non-Linear Normal Modes'' and the Generalized Ritz Method in the problems of vibrations of non-linear Elastic Continuous Systems},
 pages = {149--165},
 volume = {18},
 number = {2},
 journal = {International Journal of Non-Linear Mechanics},
 doi = {10.1016/0020-7462(83)90042-2},
 file = {Szemplinska_1983:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Szemplinska_1983.pdf:pdf}
}


@article{Szolwinski.1999,
 abstract = {This work examines the impact of the interaction between low cycle and high cycle fatigue load spectra on the near-surface stress field associated with conditions of fretting fatigue in the context of a multiaxial fatigue life parameter. Particular focus is placed on analysis of the edge of contact region identified routinely as the site of fretting-induced fatigue damage. Effects of low-cycle fatigue (LCF) - high cycle fatigue (HCF) interaction on the extent of stick and slip are analysed in detail for contacts involving a common titanium engine alloy, Ti-6Al-4V. The effects that this LCF/HCF interaction have on the nucleation of fretting fatigue cracks are then analysed, leading to the subsequent establishment of a relationship between number of LCF cycles to failure and relative HCF frequency and amplitude.},
 author = {Szolwinski, M. P. and Matlik, J. F. and Farris, T. N.},
 year = {1999},
 title = {Effects of HCF loading on fretting fatigue crack nucleation},
 pages = {671--677},
 volume = {21},
 number = {7},
 journal = {International Journal of Fatigue},
 doi = {10.1016/S0142-1123(99)00028-6}
}


