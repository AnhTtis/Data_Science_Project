@misc{berr2003,
 abstract = {Forced vibrations of turbine blades in resonant condition shorten the life and reduce the

reliability of aeronautical engines. To avoid high resonance stresses additional friction

damping is introduced into the bladed disk by means of underplatform dampers. The

underplatform dampers must be designed in order to obtain the highest damping of vibrations

for a certain engine order excitation.

The present paper deals with an analysis of damper kinematics and equilibrium in order to

give design criteria for dampers. The aim is the choice of damper mass, shape and geometry.

The optimisation is faced by means of an energy approach. The analysis highlights the

important parameters in the friction damping design. Moreover the dynamic response of the

non-linear system of a damper between two blades is calculated. The dynamic calculation of

the damped system is used to verify the results obtained by means of the energy approach.},
 author = {Berruti, T. and Firrone, C. M. and Gola, M. M.},
 year = {2003},
 title = {Modelling Underplatform Dampers in Turbine Blade Vibrations},
