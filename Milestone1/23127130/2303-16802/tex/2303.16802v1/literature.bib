 doi = {10.1016/j.ymssp.2012.10.016},
 file = {Blanc, Touzé et al 2013 - On the Numerical Computation:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Blanc, Touzé et al 2013 - On the Numerical Computation.pdf:pdf}
