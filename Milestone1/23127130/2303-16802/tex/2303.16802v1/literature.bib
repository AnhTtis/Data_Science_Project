@misc{dole1989,
 abstract = {The vibrating characteristics of simple mechanical systems, damped by real dry friction or its combination with viscous damping are investigated. Under the simple system we understand a system the vibrating characteristics of which can be simulated in some defined frequency range using a modal model with two degrees of freedom. It is assumed that the normal load acting on the friction contact area does not change during the vibration. Such assumption is fullfilled for single-mode-vibrations of a blade with a flexible blade-to-ground friction damper, or of two bladed turbine packet. The expression {\textquotedbl}real dry friction{\textquotedbl} is introduced here to indicate the deviation from the Coulomb's simplification and from functional dependence of dry friction force on friction velocity only, that is usually considered as a basic source of relaxation self-excited oscillations.~It must be pointed out that those simplifying assumptions are not in agreement with experimental observations. The main feature of thze improved model is that it considers the dependence of the dry-friction-force on time during the stick-slip period of interval occuring within the forced or self-excited oscillation, and its decreace in the subsequent slip state. The solution of the problem is based on the method of Krylov and Bogoliubov. The all necessary parameters of the solved system are defined by the hysteresis loop of general shape, including the relationship both of displacement and velocity of motion and some other relevant parameters. The new model enables the numerical simulation of the considered system of steady state response in the stable regions, and of nonsteady-state response in the instable regions with respect to the memory of the previous state and to the initial shear pre-stress in the contact area.},
 author = {Dolezal, Z.},
 year = {1989},
 title = {The response of the simple systems damped by real dry friction},
 pages = {641--651},
 howpublished = {Paper ISABE 89-7067, Proc. of the International Symposium on Air Breathing Engines},
 file = {Dolezal_1989:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_reibung und kontakt\\Dolezal_1989.pdf:pdf}
}


@misc{domi1983,
 abstract = {Premature cracking of the first stage turbine blades in HPFTP of the SSME~could be alieviated by redesign of the platform friction dampers

