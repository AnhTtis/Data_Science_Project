 abstract = {Vibration present on various levels in many engineering fields and hence vibration mitigation has become a subject of intense study. The nonlinear vibration isolation devices are effective for broad frequency bandwidth and can provide better vibration isolation than linear devices. The need for nonlinearity in stiffness and damping characteristics has motivated researchers to apply the nonlinearity found in mechanisms or materials in the passive vibration control devices.},
 author = {Balaji, P. S. and {Karthik SelvaKumar}, K.},
 year = {2021},
 title = {Applications of Nonlinearity in Passive Vibration Control: A Review},
 pages = {183--213},
 volume = {9},
 number = {2},
 issn = {2523-3939},
 journal = {Journal of Vibration Engineering {\&} Technologies},
 doi = {10.1007/s42417-020-00216-3}
}


