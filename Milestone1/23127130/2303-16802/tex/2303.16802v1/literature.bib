 doi = {10.1016/j.ymssp.2021.107859}
}


@article{dres1959,
 abstract = {Gleiten zwei feste K{\"o}rper entlang einer gemeinsamen Ber{\"u}hrungsfl{\"a}che unter Druck aufeinander, so erleiden die Rauhigkeitserhebungen ihrer Oberfl{\"a}chen elastische und plastische Verformungen; es werden Stoffteilchen abgeschert. Diese Vorg{\"a}nge spielen sich stets beim Gleiten zwischen festen K{\"o}rpern ab, auch wenn noch andere Erscheinungen hinzutreten. Schmierende Zwischenschichten k{\"o}nnen wesentliche {\"A}nderungen mit sich bringen. Eine Absch{\"a}tzung der bei diesen mechanischen Vorg{\"a}ngen auftretenden Kr{\"a}fte f{\"u}hrt zu Reibungszahlen, die im Bereich der von Messungen her bekannten Werte liegen. Die Ruhereibung mu{\ss} von der Gleitreibung unterschieden werden. Bei relativer Ruhe der beiden K{\"o}rper bilden die ineinandergreifenden Rauhigkeiten der Ber{\"u}hrungsfl{\"a}chen einen auf Scherung beanspruchten Festigkeitsverband, der das Gleiten verhindert.},
