@article{wang1995,
 abstract = {The dynamic response and stability of a single-degree-of-freedom system with unsymmetric piecewise-linear/non-linear stiffness are analyzed using the finite element method in the time domain. Based on a Hamilton's weak principle, this method provides a simple and efficient approach for predicting all possible fundamental and sub-periodic responses. The stability of the steady state response is determined by using Floquet's theory without any special effort for calculating transition matrices. This method is applied to a number of examples, demonstrating its effectiveness even for a strongly non-linear problem involving both clearance and continuous stiffness non-linearities. Close agreement is found between available published findings and the predictions of the finite element in time approach, which appears to be an efficient and reliable alternative technique for non-linear dynamic response and stability analysis of periodic systems.},
 author = {Wang, Y.},
 year = {1995},
 title = {Dynamics of Unsymmetric Piecewise-linear/Non-linear Systems Using Finite Elements},
 pages = {155--170},
 volume = {185},
 number = {1},
 journal = {Journal of Sound and Vibration},
 doi = {10.1006/jsvi.1994.0369},
 file = {Wang_1995:D\:\\Wissensdatenbank\\_Literatursammlung\\Citavi_krack\\Citavi Attachments\\Z%3A\\promotion\\Literature\\Datenbank_turbomaschinen\\Wang_1995.pdf:pdf}
}


@article{wang1996,
 abstract = {This paper presents an analytical method for determining the steady state response of an elastic-friction damped single degree of freedom system. The method is based on a procedure that uses solution forms for the response within the intervals where the system possesses constant damping characteristic and combines these solutions to form an exact, periodic motion of the system. Stability of the steady state response is directly determined by using Floquet's theory. The method is applied to a number of examples of practical significance in vibration control, demonstrating its validity and effectiveness.},
 author = {Wang, Y.},
 year = {1996},
 title = {An Analytical Solution for Periodic Response of Elastic-Friction Damped Systems},
 pages = {299--313},
 volume = {189},
 number = {3},
