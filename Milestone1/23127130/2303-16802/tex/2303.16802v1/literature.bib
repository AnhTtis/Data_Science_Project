}


@article{niko2007,
 abstract = {In this paper, robust maximum forced response reduction strategies based on a ``large mistuning'' concept are introduced, including both (i) random and (ii) deterministic approaches. An industrial bladed fan disk serves as an application example for a reliability assessment of the aforementioned strategies using two well-established tools for uncertainty analysis: (i) statistics and (ii) sensitivity and robustness. The feasibility and other practical aspects of implementing large mistuning as a means of preventing excessive forced response levels caused by random mistuning and ensuring the predictability of the response are discussed.},
