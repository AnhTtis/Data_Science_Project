@article{Lester2015,
   abstract = {We demonstrate rapid loading of a small array of optical tweezers with a single $^\{87\}$Rb atom per site. We find that loading efficiencies of up to 90% per tweezer are achievable in less than 170 ms for traps separated by more than $1.7 \,\{m\}$. Interestingly, we find the load efficiency is affected by nearby traps and present the efficiency as a function of the spacing between two optical tweezers. This enhanced loading, combined with subsequent rearranging of filled sites, will enable the study of quantum many-body systems via quantum gas assembly.},
   author = {Brian J Lester and Niclas Luick and Adam M Kaufman and Collin M Reynolds and Cindy A Regal},
