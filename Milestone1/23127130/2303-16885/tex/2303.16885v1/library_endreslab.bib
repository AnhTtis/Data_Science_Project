@article{Chen2018,
   abstract = {We experimentally demonstrate a variation on a Sisyphus cooling technique that was proposed for cooling antihydrogen. In our implementation, atoms are selectively excited to an electronic state whose energy is spatially modulated by an optical lattice, and the ensuing spontaneous decay completes one Sisyphus cooling cycle. We characterize the cooling efficiency of this technique on a continuous beam of Sr, and compare it with the case of a Zeeman slower. We demonstrate that this technique provides similar atom number for lower end temperatures, provides additional cooling per scattering event and is compatible with other laser cooling methods.},
   author = {Chun-Chia Chen and Shayne Bennetts and Rodrigo González Escudero and Florian Schreck and Benjamin Pasquiou},
