@article{belli_mosfire_2018,
	title = {{MOSFIRE} {Spectroscopy} of {Quiescent} {Galaxies} at 1.5 {\textless} z {\textless} 2.5. {II} - {Star} {Formation} {Histories} and {Galaxy} {Quenching}},
	volume = {874},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1810.00008},
	doi = {10.3847/1538-4357/ab07af},
	abstract = {We investigate the stellar populations for a sample of 24 quiescent galaxies at 1.5 {\textless} z {\textless} 2.5 using deep rest-frame optical spectra obtained with Keck MOSFIRE. By fitting templates simultaneously to the spectroscopic and photometric data, and exploring a variety of star formation histories, we obtain robust measurements of median stellar ages and residual levels of star formation. After subtracting the stellar templates, the stacked spectrum reveals the Halpha and [NII] emission lines, providing an upper limit on the ongoing star formation rate of 0.9 +/- 0.1 Msun/yr. By combining the MOSFIRE data to our sample of Keck LRIS spectra at lower redshift, we analyze in a consistent manner the quiescent population at 1 {\textless} z {\textless} 2.5. We find a tight relation (with a scatter of 0.13 dex) between the stellar age and the rest-frame U-V and V-J colors, which can be used to estimate the age of quiescent galaxies given their colors. Applying this age--color relation to large, photometric samples, we are able to model the number density evolution for quiescent galaxies of various ages. We find evidence for two distinct quenching paths: a fast quenching that produces compact post-starburst systems, and a slow quenching of larger galaxies. Fast quenching accounts for about a fifth of the growth of the red sequence at z{\textasciitilde}1.4, and half at z{\textasciitilde}2.2. We conclude that fast quenching is triggered by dramatic events such as gas-rich mergers, while slow quenching is likely caused by a different physical mechanism.},
	number = {1},
	journal = {The Astrophysical Journal},
	author = {Belli, Sirio and Newman, Andrew B and Ellis, Richard S},
	month = sep,
	year = {2018},
	note = {arXiv: 1810.00008},
	keywords = {galaxies, high-redshift, formation, stellar content},
	pages = {17},
	file = {PDF:/Users/lyalcorn/Zotero/storage/V83IWSMJ/1810.00008.pdf:application/pdf},
}

@article{Whitaker2012,
	title = {A large population of massive compact post-starburst galaxies at z {\textgreater} 1: {Implications} for the size evolution and quenching mechanism of quiescent galaxies},
	volume = {745},
	issn = {15384357},
	doi = {10.1088/0004-637X/745/2/179},
	abstract = {We study the growth of the red sequence through the number density and structural evolution of a sample of young and old quiescent galaxies at 0 {\textless} z {\textless} 2. The galaxies are selected from the NEWFIRM Medium-Band Survey in the Cosmic Evolution Survey field. We find a large population of massive young recently quenched ("post-starburst") galaxies at z {\textgreater} 1 that are almost non-existent at z {\textless} 1; their number density is 5 × 10 -5Mpc-3 at z = 2, whereas it is a factor of 10 less at z = 0.5. The observed number densities of young and old quiescent galaxies at z {\textgreater} 1 are consistent with a simple model in which all old quiescent galaxies were once identified as post-starburst galaxies. We find that the overall population of quiescent galaxies have smaller sizes and slightly more elongated shapes at higher redshift, in agreement with other recent studies. Interestingly, the most recently quenched galaxies at 1 {\textless} z {\textless} 2 are not larger, and possibly even smaller, than older galaxies at those redshifts. This result is inconsistent with the idea that the evolution of the average size of quiescent galaxies is largely driven by continuous transformations of larger, star-forming galaxies: in that case, the youngest quiescent galaxies would also be the largest. Instead, mergers or other mechanisms appear to be required to explain the size growth of quiescent galaxies from z = 2 to the present. © 2012. The American Astronomical Society. All rights reserved.},
	number = {2},
	journal = {Astrophysical Journal},
	author = {Whitaker, Katherine E. and Kriek, Mariska and Van Dokkum, Pieter G. and Bezanson, Rachel and Brammer, Gabriel and Franx, Marijn and Labbé, Ivo},
	year = {2012},
	note = {arXiv: 1112.0313},
	keywords = {galaxies: evolution, galaxies: formation, galaxies: high-redshift},
	pages = {1--8},
	file = {PDF:/Users/lyalcorn/Zotero/storage/HMQ8PAUK/Whitaker_2012_ApJ_745_179.pdf:application/pdf},
}

@article{Cole2020,
	title = {Stellar {Kinematics} and {Environment} at z{\textasciitilde}0.8 in the {LEGA}-{C} {Survey}: {Massive}, {Slow}-{Rotators} are {Built} {First} in {Overdense} {Environments}},
	url = {http://arxiv.org/abs/2001.02695},
	abstract = {In this letter, we investigate the impact of environment on integrated and spatially-resolved stellar kinematics of a sample of massive, quiescent galaxies at intermediate redshift (\$0.6{\textless}z{\textless}1.0\$). For this analysis, we combine photometric and spectroscopic parameters from the UltraVISTA and Large Early Galaxy Astrophysics Census (LEGA-C) surveys in the COSMOS field and environmental measurements. We analyze the trends with overdensity (1+\${\textbackslash}delta\$) on the rotational support of quiescent galaxies and find no universal trends at either fixed mass or fixed stellar velocity dispersion. This is consistent with previous studies of the local Universe; rotational support of massive galaxies depends primarily on stellar mass. We highlight two populations of massive galaxies (\${\textbackslash}log M\_{\textbackslash}star/M\_{\textbackslash}odot{\textbackslash}geq11\$) that deviate from the average mass relation. First, the most massive galaxies in the most under-dense regions (\$(1+{\textbackslash}delta){\textbackslash}leq1\$) exhibit elevated rotational support. Similarly, at the highest masses (\${\textbackslash}log M\_{\textbackslash}star/M\_{\textbackslash}odot{\textbackslash}geq11.25\$) the range in rotational support is significant in all but the densest regions. This corresponds to an increasing slow-rotator fraction such that only galaxies in the densest environments (\$(1+{\textbackslash}delta){\textbackslash}geq3.5\$) are primarily (90\${\textbackslash}pm\$10{\textbackslash}\%) slow-rotators.This effect is not seen at fixed velocity dispersion, suggesting minor merging as the driving mechanism: only in the densest regions have the most massive galaxies experienced significant minor merging, building stellar mass and diminishing rotation without significantly affecting the central stellar velocity dispersion. In the local Universe, most massive galaxies are slow-rotators, regardless of environment, suggesting minor merging occurs at later cosmic times \$(z{\textbackslash}lesssim0.6)\$ in all but the most dense environments.},
	author = {Cole, Justin and Bezanson, Rachel and van der Wel, Arjen and Bell, Eric and D'Eugenio, Francesco and Franx, Marijn and Gallazzi, Anna and van Houdt, Josha and Muzzin, Adam and Pacifici, Camilla and van de Sande, Jesse and Sobral, David and Straatman, Caroline and Wu, Po-Feng},
	year = {2020},
	note = {arXiv: 2001.02695},
	keywords = {evolution, galaxies, kinematics and dynamics -},
	file = {PDF:/Users/lyalcorn/Zotero/storage/DFF96JAA/2001.02695.pdf:application/pdf},
}

@article{Jimenez-Andrade2020,
	title = {The redshift and star formation mode of {AzTEC2}: a pair of massive galaxies at \$z=4.63\$},
	url = {http://arxiv.org/abs/2001.06997},
	abstract = {We combine observations from the Atacama Large Millimeter/submillimeter Array (ALMA) and the NOrthern Extended Millimeter Array (NOEMA) to assess the redshift and to study the star formation conditions in AzTEC2: one of the brightest sub-millimeter galaxies (SMGs) in the COSMOS field (\$S\_\{{\textbackslash}rm 1.1mm\}=10.5{\textbackslash}pm1.4\$mJy). Our high-resolution observations confirm that AzTEC2 splits into two components (namely AzTEC2-A and AzTEC2-B) for which we detect [C\${\textbackslash},\$II] and \${\textasciicircum}\{12\}\$CO(5\${\textbackslash}to\$4) line emission, implying a redshift of \$4.626{\textbackslash}pm0.001\$ (\$4.633{\textbackslash}pm0.001\$) for AzTEC2-A (AzTEC2-B) and ruling out previous associations with a galaxy at \$z{\textbackslash}sim1\$. We use the \${\textasciicircum}\{12\}\$CO(5\${\textbackslash}to\$4) line emission and adopt typical SMG-like gas excitation conditions to estimate the molecular gas mass, which is \$M\_\{{\textbackslash}rm gas\}({\textbackslash}alpha\_\{{\textbackslash}rm CO\}/2.5)=2.1{\textbackslash}pm0.4 {\textbackslash}times10{\textasciicircum}\{11\}\{{\textbackslash}rm M\}\_{\textbackslash}odot\$ for AzTEC2-A, and a factor four lower for AzTEC2-B. With the infrared-derived star formation rate of AzTEC2-A (\$1920{\textbackslash}pm100 {\textbackslash},M\_{\textbackslash}odot\{{\textbackslash}rm {\textbackslash}, yr\}{\textasciicircum}\{-1\}\$) and AzTEC2-B (\$710{\textbackslash}pm 35{\textbackslash},M\_{\textbackslash}odot\{{\textbackslash}rm {\textbackslash},yr\}{\textasciicircum}\{-1\}\$), they both will consume their current gas reservoir within \$(30-200){\textbackslash},\$Myr. We find evidence of a rotation-dominated [C\${\textbackslash},\$II] disk in AzTEC2-A, with a de-projected rotational velocity of \$v\_\{{\textbackslash}rm rot\}(i=39{\textasciicircum}{\textbackslash}circ)=660{\textbackslash}pm130\{{\textbackslash}rm {\textbackslash},km{\textbackslash},s\}{\textasciicircum}\{-1\}\$, velocity dispersion \${\textbackslash}lesssim100\{{\textbackslash}rm {\textbackslash},km{\textbackslash},s\}{\textasciicircum}\{-1\}\$, and dynamical mass of \$M\_\{{\textbackslash}rm dyn\}(i=39{\textasciicircum}{\textbackslash}circ)=2.6{\textasciicircum}\{+1.2\}\_\{-0.9\}{\textbackslash}times10{\textasciicircum}\{11\}{\textbackslash},M\_{\textbackslash}odot\$. We propose that an elevated gas accretion rate from the cosmic web might be the main driver of the intense levels of star formation in AzTEC2-A, which might be further enhanced by gravitational torques induced by its minor companion (AzTEC2-B). These results strengthen the picture whereby the population of single-dish selected SMGs is rather heterogeneous, including a population of pairs of massive, highly-active galaxies in a pre-coalescence phase.},
	author = {Jiménez-Andrade, E. F. and Zavala, J A and Magnelli, B and Casey, C M and Liu, D and Romano-Díaz, E. and Schinnerer, E and Harrington, K and Aretxaga, I and Karim, A and Staguhn, J and Burnham, A D and Montaña, A. and Smolčić, V. and Yun, M and Bertoldi, F and Hughes, D},
	year = {2020},
	note = {arXiv: 2001.06997},
	keywords = {evolution, galaxies, high-redshift, formation, ism, starburst, distances and redshifts},
	file = {PDF:/Users/lyalcorn/Zotero/storage/QCDJT4W2/2001.06997v1.pdf:application/pdf},
}

