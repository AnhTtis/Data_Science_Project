	volume = {474},
	issn = {13652966},
	doi = {10.1093/MNRAS/STX2976},
	abstract = {We apply four statistical learning methods to a sample of \$7941\$ galaxies (\$z{\textless}0.06\$) from the Galaxy and Mass Assembly (GAMA) survey to test the feasibility of using automated algorithms to classify galaxies. Using \$10\$ features measured for each galaxy (sizes, colours, shape parameters {\textbackslash}\& stellar mass) we apply the techniques of Support Vector Machines (SVM), Classification Trees (CT), Classification Trees with Random Forest (CTRF) and Neural Networks (NN), returning True Prediction Ratios (TPRs) of \$75.8{\textbackslash}\%\$, \$69.0{\textbackslash}\%\$, \$76.2{\textbackslash}\%\$ and \$76.0{\textbackslash}\%\$ respectively. Those occasions whereby all four algorithms agree with each other yet disagree with the visual classification (`unanimous disagreement') serves as a potential indicator of human error in classification, occurring in \${\textbackslash}sim9{\textbackslash}\%\$ of ellipticals, \${\textbackslash}sim9{\textbackslash}\%\$ of Little Blue Spheroids, \${\textbackslash}sim14{\textbackslash}\%\$ of early-type spirals, \${\textbackslash}sim21{\textbackslash}\%\$ of intermediate-type spirals and \${\textbackslash}sim4{\textbackslash}\%\$ of late-type spirals {\textbackslash}\& irregulars. We observe that the choice of parameters rather than that of algorithms is more crucial in determining classification accuracy. Due to its simplicity in formulation and implementation, we recommend the CTRF algorithm for classifying future galaxy datasets. Adopting the CTRF algorithm, the TPRs of the 5 galaxy types are : E, \$70.1{\textbackslash}\%\$; LBS, \$75.6{\textbackslash}\%\$; S0-Sa, \$63.6{\textbackslash}\%\$; Sab-Scd, \$56.4{\textbackslash}\%\$ and Sd-Irr, \$88.9{\textbackslash}\%\$. Further, we train a binary classifier using this CTRF algorithm that divides galaxies into spheroid-dominated (E, LBS {\textbackslash}\& S0-Sa) and disk-dominated (Sab-Scd {\textbackslash}\& Sd-Irr), achieving an overall accuracy of \$89.8{\textbackslash}\%\$. This translates into an accuracy of \$84.9{\textbackslash}\%\$ for spheroid-dominated systems and \$92.5{\textbackslash}\%\$ for disk-dominated systems.},
	number = {4},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Sreejith, Sreevarsha and Pereverzyev, Sergiy and Kelvin, Lee S. and Marleau, Francine R. and Haltmeier, Markus and Ebner, Judith and Bland-Hawthorn, Joss and Driver, Simon P. and Graham, Alister W. and Holwerda, Benne W. and Hopkins, Andrew M. and Liske, Jochen and Loveday, Jon and Moffett, Amanda J. and Pimbblet, Kevin A. and Taylor, Edward N. and Wang, Lingyu and Wright, Angus H.},
	year = {2018},
	note = {arXiv: 1711.06125v2},
	keywords = {Galaxies: fundamental parameters, Galaxies: structure, Methods: statistical, Galaxies: general},
	pages = {5232--5258},
	file = {PDF:/Users/lyalcorn/Zotero/storage/BHIAF3BN/Sreejith et al. - 2018 - Galaxy and Mass Assembly Automatic morphological classification of galaxies using statistical learning.pdf:application/pdf},
}

@article{Torrey2019,
	title = {The evolution of the mass-metallicity relation and its scatter in {IllustrisTNG}},
	volume = {000},
	issn = {0035-8711},
	doi = {10.1093/mnras/stz243},
	abstract = {The coevolution of galaxies and their metal content serves as an important test for galaxy feedback models. We analyze the distribution and evolution of metals within the IllustrisTNG simulation suite with a focus on the gas-phase mass-metallicity relation (MZR). We find that the IllustrisTNG model broadly reproduces the slope and normalization evolution of the MZR across the redshift range \$0{\textless}z{\textless}2\$ and mass range \$10{\textasciicircum}9 {\textless} M\_*/{\textbackslash}mathrm\{M\}\_{\textbackslash}odot {\textless} 10{\textasciicircum}\{10.5\}\$. We make predictions for the high redshift (\$2{\textless}z{\textless}10\$) metal content of galaxies which is described by a gradual decline in the normalization of the metallicity with an average high redshift (\$z{\textgreater}2\$) evolution fit by \${\textbackslash}mathrm\{d{\textbackslash};log(Z)\}/{\textbackslash}mathrm\{dz\} {\textbackslash}approx - 0.064\$. Our simulations indicate that the metal retention efficiency of the interstellar medium (ISM) is low: a majority of gas-phase metals (\${\textbackslash}sim\$ 85 per cent at \$z=0\$) live outside of the ISM, either in an extended gas disk, the circumgalactic medium, or outside the halo. Nevertheless, the redshift evolution in the simulated MZR normalization is driven by the higher gas fractions of high redshift galaxies, not by changes to the metal retention efficiency. The scatter in the simulated MZR contains a clear correlation with the gas-mass or star formation rate of the system, in agreement with the observed fundamental metallicity relation. The scatter in the MZR is driven by a competition between periods of enrichment- and accretion-dominated metallicity evolution. We expect that while the normalization of the MZR declines with redshift, the strength of the correlation between metallicity and gas-mass at fixed stellar mass is not a strong function of redshift. Our results indicate that the "regulator" style models are best suited for simultaneously explaining the shape, redshift evolution, and existence of correlated scatter with gas fraction about the MZR.},
	number = {November},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Torrey, Paul and Vogelsberger, Mark and Marinacci, Federico and Pakmor, Rüdiger and Springel, Volker and Nelson, Dylan and Naiman, Jill and Pillepich, Annalisa and Genel, Shy and Weinberger, Rainer and Hernquist, Lars},
	year = {2019},
	note = {arXiv: 1711.05261v1},
	keywords = {evolution, galaxies, general, methods, numerical},
	file = {PDF:/Users/lyalcorn/Zotero/storage/4WX86TLR/Torrey et al. - 2019 - The evolution of the mass-metallicity relation and its scatter in IllustrisTNG.pdf:application/pdf},
}

@article{Turner2017a,
	title = {The {KMOS} {Deep} {Survey} ({KDS}) {II}: {The} evolution of the stellar-mass {Tully}-{Fisher} relation since z {\textasciitilde} 4},
	volume = {28},
	url = {http://arxiv.org/abs/1711.03604},
	abstract = {We use KMOS Deep Survey (KDS) galaxies, combined with results from a range of spectroscopic studies in the literature, to investigate the evolution of the stellar-mass Tully-Fisher relation since z {\textasciitilde} 4. We determine the slope and normalisation of the local rotation-velocity -- stellar-mass (Vc - \$M\_\{{\textbackslash}star\}\$) relationship using a reference sample of local spiral galaxies; thereafter we fix the slope, and focus on the evolution of velocity normalisation with redshift. The rotation-dominated KDS galaxies at z {\textasciitilde} 3.5 have rotation velocities {\textasciitilde} -0.1 dex lower than local reference galaxies at fixed stellar mass. By fitting 16 distant comparison samples spanning 0 {\textless} z {\textless} 3 (containing {\textasciitilde} 1200 galaxies), we show that the size and sign of the inferred Vc offset depends sensitively on the fraction of the parent samples used in the Tully-Fisher analysis, and how strictly the criterion of 'rotation dominated' is enforced. Confining attention to subsamples of galaxies that are especially 'disky' results in a consistent positive offset in Vc of {\textasciitilde} +0.1 dex, however these galaxies are not representative of the evolving-disk population at z {\textgreater} 1. We investigate the addition of pressure support, traced by intrinsic velocity dispersion (\${\textbackslash}sigma\_\{int\}\$) to the KDS dynamical mass budget by adopting a 'total' effective velocity of form \$V\_\{tot\} = (Vc{\textasciicircum}\{2\} + 4.0{\textbackslash}sigma\_\{int\}{\textasciicircum}\{2\}){\textasciicircum}\{0.5\}\$. The rotation-dominated and dispersion-dominated KDS galaxies fall on the same locus in the total-velocity versus stellar-mass plane, removing the need for debate over the precise selection threshold for rotation-dominated galaxies. The comparison sample offsets are in the range +0.08 to +0.15 dex in total-velocity zero-point (-0.30 to -0.55 dex in stellar-mass zero-point) from the local Tully-Fisher relation at z {\textgreater} 1, consistent with steady evolution of the ratio of dynamical to stellar mass with cosmic time.},
	number = {November},
	author = {Turner, O. J. and Harrison, C. M. and Cirasuolo, M. and McLure, R. J. and Dunlop, J. and Swinbank, A. M. and Tiley, A. L.},
	year = {2017},
	note = {arXiv: 1711.03604},
	keywords = {galaxies, high-redshift, kinematics and dynamics, - galax-, - galaxies},
	pages = {1--28},
	file = {PDF:/Users/lyalcorn/Zotero/storage/UU3KJART/Turner et al. - 2017 - The KMOS Deep Survey (KDS) II The evolution of the stellar-mass Tully-Fisher relation since z ~ 4.pdf:application/pdf},
}

@article{Ventou2017,
	title = {The {MUSE} {Hubble} {Ultra} {Deep} {Field} {Survey}: {IX}. {Evolution} of galaxy merger fraction since z π 6},
	volume = {608},
	issn = {14320746},
	doi = {10.1051/0004-6361/201731586},
	abstract = {We provide, for the first time, robust observational constraints on the galaxy major merger fraction up to \$z{\textbackslash}approx 6\$ using spectroscopic close pair counts. Deep Multi Unit Spectroscopic Explorer (MUSE) observations in the Hubble Ultra Deep Field (HUDF) and Hubble Deep Field South (HDF-S) are used to identify 113 secure close pairs of galaxies among a parent sample of 1801 galaxies spread over a large redshift range (\$0.2{\textless}z{\textless}6\$) and stellar masses (\$10{\textasciicircum}7-10{\textasciicircum}\{11\} M\_{\textbackslash}odot\$), thus probing about 12 Gyr of galaxy evolution. Stellar masses are estimated from spectral energy distribution (SED) fitting over the extensive UV-to-NIR HST photometry available in these deep Hubble fields, adding Spitzer IRAC bands to better constrain masses for high-redshift (\$z{\textbackslash}geqslant 3\$) galaxies. These stellar masses are used to isolate a sample of 54 major close pairs with a galaxy mass ratio limit of 1:6. Among this sample, 23 pairs are identified at high redshift (\$z{\textbackslash}geqslant 3\$) through their Ly\${\textbackslash}alpha\$ emission. The sample of major close pairs is divided into five redshift intervals in order to probe the evolution of the merger fraction with cosmic time. Our estimates are in very good agreement with previous close pair counts with a constant increase of the merger fraction up to \$z{\textbackslash}approx 3\$ where it reaches a maximum of 20\%. At higher redshift, we show that the fraction slowly decreases down to about 10\% at \$z{\textbackslash}approx6\$. The sample is further divided into two ranges of stellar masses using either a constant separation limit of \$10{\textasciicircum}\{9.5\} M\_{\textbackslash}odot\$ or the median value of stellar mass computed in each redshift bin. Overall, the major close pair fraction for low-mass and massive galaxies follows the same trend. These new, homogeneous, and robust estimates of the major merger fraction since \$z{\textbackslash}approx6\$ are in good agreement with recent predictions of cosmological numerical simulations.},
	journal = {Astronomy and Astrophysics},
	author = {Ventou, E. and Contini, T. and Bouché, N. and Epinat, B. and Brinchmann, J. and Bacon, R. and Inami, H. and Lam, D. and Drake, A. and Garel, T. and Michel-Dansac, L. and Pello, R. and Steinmetz, M. and Weilbacher, P. M. and Wisotzki, L. and Carollo, M.},
	year = {2017},
	note = {arXiv: 1711.00423v1},
	keywords = {Galaxies: high-redshift, Galaxy: evolution, Galaxies: interactions},
	file = {PDF:/Users/lyalcorn/Zotero/storage/V68FKKXK/Ventou et al. - 2017 - The MUSE Hubble Ultra Deep Field Survey IX. Evolution of galaxy merger fraction since z π 6.pdf:application/pdf},
}

@article{Yuan2017,
	title = {The {Most} {Ancient} {Spiral} {Galaxy}: {A} 2.6-{Gyr}-old {Disk} with a {Tranquil} {Velocity} {Field}},
	volume = {850},
	doi = {10.3847/1538-4357/aa951d},
	abstract = {We report an integral-field spectroscopic (IFS) observation of a gravitationally lensed spiral galaxy A1689B11 at redshift \$z=2.54\$. It is the most ancient spiral galaxy discovered to date and the second kinematically confirmed spiral at \$z{\textbackslash}gtrsim2\$. Thanks to gravitational lensing, this is also by far the deepest IFS observation with the highest spatial resolution (\${\textbackslash}sim\$ 400 pc) on a spiral galaxy at a cosmic time when the Hubble sequence is about to emerge. After correcting for a lensing magnification of 7.2 \${\textbackslash}pm\$ 0.8, this primitive spiral disk has an intrinsic star formation rate of 22 \${\textbackslash}pm\$ 2 \$M\_\{{\textbackslash}odot\}\$ yr\${\textasciicircum}\{-1\}\$, a stellar mass of 10\${\textasciicircum}\{9.8 {\textbackslash}pm 0.3\}\$\$M\_\{{\textbackslash}odot\}\$ and a half-light radius of \$r\_\{1/2\}=2.6 {\textbackslash}pm 0.7\$ kpc, typical of a main-sequence star-forming (SF) galaxy at \$z{\textbackslash}sim2\$. However, the H{\textbackslash}alpha{\textbackslash} kinematics show a surprisingly tranquil velocity field with an ordered rotation (\$V\_\{{\textbackslash}rm c\}\$ = 200 \${\textbackslash}pm\$ 12 km/s) and uniformly small velocity dispersions (\$V\_\{{\textbackslash}rm {\textbackslash}sigma, mean\}\$ = 23 \${\textbackslash}pm\$ 4 km/s and \$V\_\{{\textbackslash}rm {\textbackslash}sigma, outer-disk\}\$ = 15 \${\textbackslash}pm\$ 2 km/s). The low gas velocity dispersion is similar to local spiral galaxies and is consistent with the classic density wave theory where spiral arms form in dynamically cold and thin disks. We speculate that A1689B11 belongs to a population of rare spiral galaxies at \$z{\textbackslash}gtrsim2\$ that mark the formation epoch of thin disks. Future observations with JWST will greatly increase the sample of these rare galaxies and unveil the earliest onset of spiral arms.},
	number = {1},
	journal = {The Astrophysical Journal},
	author = {Yuan, Tiantian and Richard, Johan and Gupta, Anshu and Federrath, Christoph and Sharma, Soniya and Groves, Brent A. and Kewley, Lisa J. and Cen, Renyue and Birnboim, Yuval and Fisher, David B.},
	year = {2017},
	note = {arXiv: 1710.11130v1},
	pages = {61},
	file = {PDF:/Users/lyalcorn/Zotero/storage/FD38LUB6/Yuan et al. - 2017 - The Most Ancient Spiral Galaxy A 2.6-Gyr-old Disk with a Tranquil Velocity Field.pdf:application/pdf},
}

@article{Kacprzak2015,
	title = {The absence of an environmental dependence in the mass-metallicity relation at z = 2},
	volume = {802},
	issn = {20418213},
	url = {http://dx.doi.org/10.1088/2041-8205/802/2/L26},
	doi = {10.1088/2041-8205/802/2/L26},
	abstract = {We investigate the environmental dependence of the mass-metallicity relation at z=2 with MOSFIRE/Keck as part of the ZFIRE survey. Here, we present the chemical abundance of a Virgo-like progenitor at z=2.095 that has an established red sequence. We identified 43 cluster (\${\textless}z{\textgreater}=2.095{\textbackslash}pm0.004\$) and 74 field galaxies (\${\textless}z{\textgreater}=2.195{\textbackslash}pm0.083\$) for which we can measure metallicities. For the first time, we show that there is no discernible difference between the mass-metallicity relation of field and cluster galaxies to within 0.02dex. Both our field and cluster galaxy mass-metallicity relations are consistent with recent field galaxy studies at z{\textasciitilde}2. We present hydrodynamical simulations for which we derive mass-metallicity relations for field and cluster galaxies. We find at most a 0.1dex offset towards more metal-rich simulated cluster galaxies. Our results from both simulations and observations are suggestive that environmental effects, if present, are small and are secondary to the ongoing inflow and outflow processes that are governed by galaxy halo mass.},
	number = {2},
	journal = {Astrophysical Journal Letters},
	author = {Kacprzak, Glenn G. and Yuan, Tiantian and Nanayakkara, Themiya and Kobayashi, Chiaki and Tran, Kim V.Y.H. and Kewley, Lisa J. and Glazebrook, Karl and Spitler, Lee and Taylor, Philip and Cowley, Michael and Labbe, Ivo and Straatman, Caroline and Tomczak, Adam},
	year = {2015},
	note = {Publisher: IOP Publishing},
	keywords = {Galaxies: Evolution, Galaxies: High-redshift, Cosmology: Observations, Galaxies: Abundances, Galaxies: Fundamental parameters},
	pages = {L26},
	file = {PDF:/Users/lyalcorn/Zotero/storage/X55MKITM/Kacprzak et al. - 2015 - The absence of an environmental dependence in the mass-metallicity relation at z = 2.pdf:application/pdf},
}

@article{Ventou2019,
	title = {New criteria for the selection of galaxy close pairs from cosmological simulations: evolution of the major and minor merger fraction in {MUSE} deep fields},
	url = {http://arxiv.org/abs/1909.03706},
	abstract = {It is still a challenge to assess the merger fraction of galaxies at different cosmic epochs in order to probe the evolution of their mass assembly. Using the Illustris cosmological simulations, we investigate the relation between the separation of galaxies in a pair, both in velocity and projected spatial separation space, and the probability that these interacting galaxies will merge in the future. From this analysis, we propose a new set of criteria to select close pairs of galaxies along with a new corrective term to be applied to the computation of the galaxy merger fraction. We then probe the evolution of the major and minor merger fraction using the latest MUSE deep observations over the HUDF, HDFS, COSMOS-Gr30 and Abell 2744 regions. From a parent sample of 2483 galaxies with spectroscopic redshifts, we identify 366 close pairs spread over a large range of redshifts (\$0.2{\textless}z{\textless}6\$) and stellar masses (\$10{\textasciicircum}7-10{\textasciicircum}\{11\}M\_\{{\textbackslash}odot\}\$). Using the stellar mass ratio between the secondary and primary galaxy as a proxy to split the sample into major, minor and very minor mergers, we found a total of 183 major, 142 minor and 47 very minor close pairs corresponding to a mass ratio range of 1:1-1:6, 1:6-1:100 and lower than 1:100, respectively. Due to completeness issues, we do not consider the very minor pairs in the analysis. Overall, the major merger fraction increases up to \$z{\textbackslash}approx 2-3\$ reaching 25\% for pairs with the most massive galaxy with a stellar mass \$M{\textasciicircum}*{\textbackslash}geq 10{\textasciicircum}\{9.5\}M\_\{{\textbackslash}odot\}\$. Beyond this redshift, the fraction decreases down to \${\textbackslash}sim 5\$\% at \$z{\textbackslash}approx 6\$. The evolution of the minor merger fraction is roughly constant with cosmic time, with a fraction of 20\% at \$z{\textless}3\$ and a slow decrease between \$3{\textbackslash}leq z {\textbackslash}leq6\$ to 8-13\%.},
	author = {Ventou, E. and Contini, T. and Bouché, N. and Epinat, B. and Brinchmann, J. and Inami, H. and Richard, J. and Schroetter, I. and Soucail, G. and Steinmetz, M. and Weilbacher, P.},
	year = {2019},
	note = {arXiv: 1909.03706},
	keywords = {galaxies, interactions, high-redshift - galaxies, evolution - galaxies},
	file = {PDF:/Users/lyalcorn/Zotero/storage/Z7SX48C9/Ventou et al. - 2019 - New criteria for the selection of galaxy close pairs from cosmological simulations evolution of the major and min.pdf:application/pdf},
}

@article{Overzier2016,
	title = {The realm of the galaxy protoclusters: {A} review},
	volume = {24},
	issn = {09354956},
	doi = {10.1007/s00159-016-0100-3},
	abstract = {The study of galaxy protoclusters is beginning to fill in unknown details of the important phase of the assembly of clusters and cluster galaxies. This review describes the current status of this field and highlights promising recent findings related to galaxy formation in the densest regions of the early universe. We discuss the main search techniques and the characteristic properties of protoclusters in observations and simulations, and show that protoclusters will have present-day masses similar to galaxy clusters when fully collapsed. We discuss the physical properties of galaxies in protoclusters, including (proto-)brightest cluster galaxies, and the forming red sequence. We highlight the fact that the most massive halos at high redshift are found in protoclusters, making these objects uniquely suited for testing important recent models of galaxy formation. We show that galaxies in protoclusters should be among the first galaxies at high redshift making the transition from a gas cooling regime dominated by cold streams to a regime dominated by hot intracluster gas, which could be tested observationally. We also discuss the possible connections between protoclusters and radio galaxies, quasars, and Ly-alpha blobs. Because of their early formation, large spatial sizes and high total star formation rates, protoclusters have also likely played a crucial role during the epoch of reionization, which can be tested with future experiments that will map the neutral and ionized cosmic web. Last, we review a number of promising observational projects that are expected to make significant impact in this growing, exciting field.},
	number = {1},
	journal = {Astronomy and Astrophysics Review},
	author = {Overzier, Roderik A.},
	year = {2016},
	note = {arXiv: 1610.05201v2},
	keywords = {Galaxies: clusters: general, (Cosmology:)large-scale structure, Cosmology, Galaxies: high redshift},
	file = {PDF:/Users/lyalcorn/Zotero/storage/BPYPRLN8/Overzier - 2016 - The realm of the galaxy protoclusters A review.pdf:application/pdf},
}

@article{Magrini2012,
	title = {{LBT}/{LUCIFER} view of star-forming galaxies in the cluster {7C} 1756+6520 at z ∼ 1.4},
	volume = {426},
	issn = {00358711},
	url = {https://academic.oup.com/mnras/article-lookup/doi/10.1111/j.1365-2966.2012.21460.x},
	doi = {10.1111/j.1365-2966.2012.21460.x},
	abstract = {Galaxy clusters are key places to study the contribution of \{{\textbackslash}it nature\} (i.e. mass, morphology) and \{{\textbackslash}it nurture\} (i.e.environment) in the formation and evolution of galaxies. Recently, a number of clusters at z\${\textgreater}\$1, i.e. corresponding to the first epochs of the cluster formation, has been discovered and confirmed spectroscopically. We present new observations obtained with the \{{\textbackslash}sc LUCIFER\} spectrograph at Large Binocular Telescope (LBT) of a sample of star-forming galaxies associated with a large scale structure around the radio galaxy 7C1756+6520 at z=1.42. Combining our spectroscopic data and the literature photometric data, we derived some of the properties of these galaxies: star formation rate, metallicity and stellar mass. With the aim of analyzing the effect of the cluster environment on galaxy evolution, we have located the galaxies in the plane of the so-called Fundamental Metallically Relation (FMR), which is known not to evolve with redshift up to z\$=2.5\$ for field galaxies, but it is still unexplored in rich environments at low and high redshift. We found that the properties of the galaxies in the cluster 7C 1756+6520 are compatible with the FMR which suggests that the effect of the environment on galaxy metallicity at this early epoch of cluster formation is marginal. As a side study, we also report the spectroscopic analysis of a bright AGN, belonging to the cluster, which shows a significant outflow of gas.},
	number = {2},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Magrini, Laura and Sommariva, Veronica and Cresci, Giovanni and Sani, Eleonora and Galametz, Audrey and Mannucci, Filippo and Petropoulou, Vasiliki and Fumana, Marco},
	month = oct,
	year = {2012},
	keywords = {Galaxies: abundances, Galaxies: evolution, Galaxies: formation, Galaxies: clusters: individual: 7C 1756+6520},
	pages = {1195--1203},
	file = {PDF:/Users/lyalcorn/Zotero/storage/6QRJTN4C/Magrini et al. - 2012 - LBTLUCIFER view of star-forming galaxies in the cluster 7C 17566520 at z ∼ 1.4.pdf:application/pdf},
}

@article{Shimakawa2015a,
	title = {An early phase of environmental effects on galaxy properties unveiled by near-infrared spectroscopy of protocluster galaxies at z \&gt; 2},
	volume = {448},
	issn = {1365-2966},
	url = {http://academic.oup.com/mnras/article/448/1/666/1753351/An-early-phase-of-environmental-effects-on-galaxy},
	doi = {10.1093/mnras/stv051},
	abstract = {This work presents the results from our near-infrared spectroscopy of narrow-band selected H\${\textbackslash}alpha\$ emitters (HAEs) in two rich over-densities (PKS 1138-262 at \$z=2.2\$ and USS 1558-003 at \$z=2.5\$) with the Multi-Object Infrared Camera and Spectrograph (MOIRCS) on the Subaru telescope. These protoclusters are promising candidates for the most massive class of galaxy clusters seen today (Paper I). The confirmed HAEs in the protoclusters at \$z{\textgreater}2\$ show high excitation levels as characterized by much higher [OIII]/H\${\textbackslash}beta\$ or [OIII]/H\${\textbackslash}alpha\$ line ratios than those of general galaxies at low-\$z\$. Such a high excitation level may not only be driven by high specific star formation rates (sSFRs) and lower gaseous metallicities, but also be contributed by some other effects. We investigate the environmental dependence of gaseous metallicities by comparing the HAEs in the protoclusters with those in the general field at similar redshifts. We find that the gaseous metallicities of protocluster galaxies are more chemically enriched than those of field galaxies at a given stellar mass in the range of M\$\_{\textbackslash}star{\textbackslash}lesssim10{\textasciicircum}\{11\}\$ M\$\_{\textbackslash}odot\$. This can be attributed to many processes, such as intrinsic (or nature) effects, external (or nurture) effects, and/or some systematic sampling effects. We also find that the offset of the mass-metallicity relation in dense environment becomes larger at higher redshifts. This can be naturally understood by the fact that the in/out-flow rates in star forming galaxies are much higher at higher redshifts. Therefore the environmental dependence of such "feeding" and "feedback" mechanisms in galaxy formation are probably playing major roles in producing the offset of the mass-metallicity relation for the protocluster galaxies at \$z{\textgreater}2\$.},
	number = {1},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Shimakawa, Rhythm and Kodama, Tadayuki and Tadaki, Ken-ichi and Hayashi, Masao and Koyama, Yusei and Tanaka, Ichi},
	month = mar,
	year = {2015},
	keywords = {Galaxies: evolution, Galaxies: formation, Galaxies: clusters: general},
	pages = {666--680},
	file = {PDF:/Users/lyalcorn/Zotero/storage/8T72M6TT/Shimakawa et al. - 2015 - An early phase of environmental effects on galaxy properties unveiled by near-infrared spectroscopy of protocl.pdf:application/pdf},
}

@article{Valentino2015,
	title = {Metal deficiency in cluster star-forming galaxies at {Z} = 2},
	volume = {801},
	issn = {15384357},
	url = {http://dx.doi.org/10.1088/0004-637X/801/2/132},
	doi = {10.1088/0004-637X/801/2/132},
	abstract = {We investigate the environmental effect on the metal enrichment of star-forming galaxies (SFGs) in the farthest spectroscopically confirmed and X-ray detected cluster, CL{\textasciitilde}J1449+0856 at \$z=1.99\$. We combined HST/WFC3{\textasciitilde}G141 slitless spectroscopic data, our 13-band photometry, and a recent Subaru/MOIRCS near infrared spectroscopic follow-up to constrain the physical properties of SFGs in CL{\textasciitilde}J1449+0856 and in a mass-matched field sample. After a conservative active galactic nuclei (AGN) removal, stacking individual MOIRCS spectra of \$6\$ (\$31\$) sources in the cluster (field) in the mass range \$10 {\textbackslash}leq {\textbackslash}mathrm\{log(M/M\_\{{\textbackslash}odot\}\}) {\textbackslash}leq 11\$, we find a \${\textbackslash}sim4{\textbackslash}sigma\$ significant lower [N II]/H\${\textbackslash}alpha\$ ratio in the cluster than in the field. Stacking a subsample of \$16\$ field galaxies with H\${\textbackslash}beta\$ and [O III] in the observed range, we measure a [O III]/H\${\textbackslash}beta\$ ratio fully compatible with the cluster value. Converting these ratios into metallicities, we find that the cluster SFGs are up to \$0.25{\textbackslash},{\textbackslash}mathrm\{dex\}\$ poorer in metals than their field counterparts, depending on adopted calibrations. The low metallicity in cluster sources is confirmed using alternative indicators. Furthermore, we observe a significantly higher H\${\textbackslash}alpha\$ luminosity and equivalent width in the average cluster spectrum than in the field. This is likely due to enhanced specific star formation rate, even if lower dust reddening and/or an uncertain environmental dependence of the continuum-to-nebular emission differential reddening may play a role. Our findings might be explained by the accretion of pristine gas around galaxies at \$z=2\$ and from cluster-scale reservoirs, possibly connected with a phase of rapid halo mass assembly at \$z{\textgreater}2\$ and of high galaxy merging rate.},
	number = {2},
	journal = {Astrophysical Journal},
	author = {Valentino, F. and Daddi, E. and Strazzullo, V. and Gobat, R. and Onodera, M. and Bournaud, F. and Juneau, S. and Renzini, A. and Arimoto, N. and Carollo, M. and Zanella, A.},
	year = {2015},
	note = {Publisher: IOP Publishing},
	keywords = {galaxies: star formation, ISM: abundances, galaxies: clusters: individual (Cl J1449+0856)},
	pages = {132},
	file = {PDF:/Users/lyalcorn/Zotero/storage/E6NIUMKD/Valentino et al. - 2015 - Metal deficiency in cluster star-forming galaxies at Z = 2.pdf:application/pdf},
}

@article{Shimakawa2015,
	title = {Correlation between star formation activity and electron density of ionized gas at z = 2.5},
	volume = {451},
	issn = {13652966},
	doi = {10.1093/mnras/stv915},
	abstract = {In the redshift interval of \$2{\textless}z{\textless}3\$, the physical conditions of the inter-stellar medium (ISM) in star-forming galaxies are likely to be different from those in the local Universe because of lower gaseous metallicities, higher gas fractions, and higher star formation activities. In fact, observations suggest that higher electron densities, higher ionization parameters, and harder UV radiation fields are common. In this paper, based on the spectra of H\${\textbackslash}alpha\$-selected star-forming galaxies at \$z=2.5\$ taken with Multi-Object Spectrometer for InfraRed Exploration (MOSFIRE) on Keck-1 telescope, we measure electron densities (\$n\_e\$) using the oxygen line ratio ( [OII]\${\textbackslash}lambda{\textbackslash}lambda\$3726,3729), and investigate the relationships between the electron density of ionized gas and other physical properties. As a result, we find that the specific star formation rate (sSFR) and the surface density of SFR (\${\textbackslash}Sigma\_{\textbackslash}mathrm\{SFR\}\$) are correlated with the electron density at \$z=2.5\$ for the first time. The \${\textbackslash}Sigma\_{\textbackslash}mathrm\{SFR\}-n\_e\$ relation is likely to be linked to the star formation law in HII regions (where star formation activity is regulated by interstellar pressure). Moreover, we discuss the mode of star formation in those galaxies. The correlation between sSFR and \${\textbackslash}Sigma\_{\textbackslash}mathrm\{SFR\}\$ suggests that highly star-forming galaxies (with high sSFR) tend to be characterized by higher surface densities of star formation (\${\textbackslash}Sigma\_{\textbackslash}mathrm\{SFR\}\$) and thus higher \$n\_e\$ values as well.},
	number = {2},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Shimakawa, Rhythm and Kodama, Tadayuki and Steidel, Charles C. and Tadaki, Ken ichi and Tanaka, Ichi and Strom, Allison L. and Hayashi, Masao and Koyama, Yusei and Suzuki, Tomoko L. and Yamamoto, Moegi},
	year = {2015},
	keywords = {Galaxies: formation, Galaxies: high-redshift, Galaxies: ISM},
	pages = {1284--1289},
	file = {PDF:/Users/lyalcorn/Zotero/storage/K85HWWNC/Shimakawa et al. - 2015 - Correlation between star formation activity and electron density of ionized gas at z = 2.5.pdf:application/pdf},
}

@article{Williams2009,
	title = {Detection of quiescent galaxies in a bicolor sequence from {Z} = 0-2},
	volume = {691},
	issn = {15384357},
	doi = {10.1088/0004-637X/691/2/1879},
	abstract = {We investigate the properties of quiescent and star-forming galaxy populations to z{\textasciitilde}2 with purely photometric data, employing a novel rest-frame color selection technique. From the UKIDSS Ultra-Deep Survey Data Release 1, with matched optical and mid-IR photometry taken from the Subaru XMM Deep Survey and Spitzer Wide-Area Infrared Extragalactic Survey respectively, we construct a K-selected galaxy catalog and calculate photometric redshifts. Excluding stars, objects with uncertain z\_phot solutions, those that fall in bad or incomplete survey regions, and those for which reliable rest-frame colors could not be derived, 30108 galaxies with K{\textless}22.4 (AB) and z{\textless}2.5 remain. The galaxies in this sample are found to occupy two distinct populations in the rest-frame U-V vs. V-J color space: a clump of red, quiescent galaxies (analogous to the red sequence) and a track of star-forming galaxies extending from blue to red U-V colors. This bimodal behavior is seen up to z{\textasciitilde}2. Due to a combination of measurement errors and passive evolution, the color-color diagram is not suitable to distinguish the galaxy bimodality at z{\textgreater}2 for this sample, but we show that MIPS 24um data suggest that a significant population of quiescent galaxies exists even at these higher redshifts. At z=1-2, the most luminous objects in the sample are divided roughly equally between star-forming and quiescent galaxies, while at lower redshifts most of the brightest galaxies are quiescent. Moreover, quiescent galaxies at these redshifts are clustered more strongly than those actively forming stars, indicating that galaxies with early-quenched star formation may occupy more massive host dark matter halos. This suggests that the end of star formation is associated with, and perhaps brought about by, a mechanism related to halo mass.},
	number = {2},
	journal = {Astrophysical Journal},
	author = {Williams, Rik J. and Quadri, Ryan F. and Franx, Marijn and Van Dokkum, Pieter and Labbé, Ivo},
	year = {2009},
	keywords = {galaxies: evolution, galaxies: high-redshift, cosmology: observations, infrared: galaxies},
	pages = {1879--1895},
	file = {PDF:/Users/lyalcorn/Zotero/storage/YAW9XQMI/Williams et al. - 2009 - Detection of quiescent galaxies in a bicolor sequence from Z = 0-2.pdf:application/pdf},
}

