	author = {Lee, Khee-Gan and Krolewski, Alex and White, Martin and Schlegel, David and Nugent, Peter E. and Hennawi, Joseph F. and Müller, Thomas and Pan, Richard and Prochaska, J. Xavier and Font-Ribera, Andreu and Suzuki, Nao and Glazebrook, Karl and Kacprzak, Glenn G. and Kartaltepe, Jeyhan S. and Koekemoer, Anton M. and Le Fèvre, Olivier and Lemaux, Brian C. and Maier, Christian and Nanayakkara, Themiya and Rich, R. Michael and Sanders, D. B. and Salvato, Mara and Tasca, Lidia and Tran, Kim-Vy H.},
	year = {2018},
