}

@article{Rhea2021b,
	title = {{LUCI}: {A} {Python} {Package} for {SITELLE} {Spectral} {Analysis}},
	volume = {5},
	doi = {10.3847/2515-5172/ac2517},
	abstract = {High-resolution optical integral field units (IFUs) are rapidly expanding our knowledge of extragalactic emission nebulae in galaxies and galaxy clusters. By studying the spectra of these objects—which include classic H ii regions, supernova remnants, planetary nebulae, and cluster filaments—we are able to constrain their kinematics (velocity and velocity dispersion). In conjunction with additional tools, such as the BPT diagram, we can further classify emission regions based on strong emission-line flux ratios. LUCI is a simple-to-use python module intended to facilitate the rapid analysis of IFU spectra. LUCI does this by integrating well-developed pre-existing python tools such as astropy and scipy with new machine learning tools for spectral analysis. Furthermore, LUCI provides several easy-to-use tools to access and fit SITELLE data cubes.},
	number = {9},
	journal = {Research Notes of the AAS},
	author = {Rhea, Carter and Hlavacek-Larrondo, Julie and Rousseau-Nepton, Laurie and Vigneron, Benjamin and Guité, Louis-Simon},
