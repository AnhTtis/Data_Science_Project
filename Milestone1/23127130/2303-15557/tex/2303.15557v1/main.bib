@article{Ueda2018,
	title = {A {Cool} {Core} {Disturbed}: {Observational} {Evidence} for the {Coexistence} of {Subsonic} {Sloshing} {Gas} and {Stripped} {Shock}-heated {Gas} around the {Core} of {RX} {J1347}.5–1145},
