@article{Momcheva2016,
	title = {the {3D}-{Hst} {Survey}: {Hubble} {Space} {Telescope} {Wfc3}/{G141} {Grism} {Spectra}, {Redshifts}, and {Emission} {Line} {Measurements} for ∼100,000 {Galaxies}},
	volume = {225},
	issn = {0067-0049},
	doi = {10.3847/0067-0049/225/2/27},
	abstract = {We present reduced data and data products from the 3D-HST survey, a 248-orbit HST Treasury program. The survey obtained WFC3 G141 grism spectroscopy in four of the five CANDELS fields: AEGIS, COSMOS, GOODS-S, and UDS, along with WFC3 \$H\_\{140\}\$ imaging, parallel ACS G800L spectroscopy, and parallel \$I\_\{814\}\$ imaging. In a previous paper (Skelton et al. 2014) we presented photometric catalogs in these four fields and in GOODS-N, the fifth CANDELS field. Here we describe and present the WFC3 G141 spectroscopic data, again augmented with data from GO-1600 in GOODS-N. The data analysis is complicated by the fact that no slits are used: all objects in the WFC3 field are dispersed, and many spectra overlap. We developed software to automatically and optimally extract interlaced 2D and 1D spectra for all objects in the Skelton et al. (2014) photometric catalogs. The 2D spectra and the multi-band photometry were fit simultaneously to determine redshifts and emission line strengths, taking the morphology of the galaxies explicitly into account. The resulting catalog has 98,663 measured redshifts and line strengths down to \$JH\_\{IR\}{\textbackslash}leq 26\$ and 22,548 with \$JH\_\{IR\}{\textbackslash}leq 24\$, where we comfortably detect continuum emission. Of this sample 5,459 galaxies are at \$z{\textgreater}1.5\$ and 9,621 are at \$0.7{\textless}z{\textless}1.5\$, where H\${\textbackslash}alpha\$ falls in the G141 wavelength coverage. Based on comparisons with ground-based spectroscopic redshifts, and on analyses of paired galaxies and repeat observations, the typical redshift error for \$JH\_\{IR\}{\textbackslash}leq 24\$ galaxies in our catalog is \${\textbackslash}sigma\_z {\textbackslash}approx 0.003 {\textbackslash}times (1+z)\$, i.e., one native WFC3 pixel. The \$3{\textbackslash}sigma\$ limit for emission line fluxes of point sources is \$1.5{\textbackslash}times10{\textasciicircum}\{-17\}\$ ergs s\${\textasciicircum}\{-1\}\$ cm\${\textasciicircum}\{-2\}\$. We show various representations of the full dataset, as well as individual examples that highlight the range of spectra that we find in the survey.},
	number = {2},
	journal = {The Astrophysical Journal Supplement Series},
	author = {Momcheva, Ivelina G. and Brammer, Gabriel B. and van Dokkum, Pieter G. and Skelton, Rosalind E. and Whitaker, Katherine E. and Nelson, Erica J. and Fumagalli, Mattia and Maseda, Michael V. and Leja, Joel and Franx, Marijn and Rix, Hans-Walter and Bezanson, Rachel and Cunha, Elisabete Da and Dickey, Claire and Schreiber, Natascha M. Förster and Illingworth, Garth and Kriek, Mariska and Labbé, Ivo and Lange, Johannes Ulf and Lundgren, Britt F. and Magee, Daniel and Marchesini, Danilo and Oesch, Pascal and Pacifici, Camilla and Patel, Shannon G. and Price, Sedona and Tal, Tomer and Wake, David A. and van der Wel, Arjen and Wuyts, Stijn},
	year = {2016},
	note = {arXiv: 1510.02106v2},
	keywords = {evolution, galaxies, methods, techniques, catalogs, spectroscopic, data analysis},
	pages = {27},
	file = {PDF:/Users/lyalcorn/Zotero/storage/6V8EMAMJ/Momcheva et al. - 2016 - the 3D-Hst Survey Hubble Space Telescope Wfc3G141 Grism Spectra, Redshifts, and Emission Line Measurements fo.pdf:application/pdf},
}

@article{Nelson2016,
