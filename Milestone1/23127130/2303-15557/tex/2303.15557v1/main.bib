	doi = {10.1086/177125},
	abstract = {To re-examine the rich cluster \${\textbackslash}Omega\$ value the CNOC Cluster Survey has observed 16 high X-ray luminosity clusters in the redshift range 0.17 to 0.55, obtaining approximately 2600 velocities in their fields. Directly adding all the K and evolution corrected \$r\$ band light to \$M\_r(0)=-18.5\$, about \$0.2L\_{\textbackslash}ast\$, and correcting for the light below the limit, the average mass-to-light ratio of the clusters is \$283{\textbackslash}pm27h{\textbackslash}msun/{\textbackslash}lsun\$ and the average mass per galaxy is \$3.5{\textbackslash}pm0.4{\textbackslash}times10{\textasciicircum}\{12\}h{\textasciicircum}\{-1\}{\textbackslash}msun\$. The clusters are consistent with having a universal \$M\_v/L\$ value (within the errors of about 20{\textbackslash}\%) independent of their velocity dispersion, mean color of their galaxies, blue galaxy content, redshift, or mean interior density. Using field galaxies within the same data set, with the same corrections, we find that the closure mass-to-light, \${\textbackslash}rho\_c/j\$, is \$1160{\textbackslash}pm130h{\textbackslash}msun/{\textbackslash}lsun\$ and the closure mass per galaxy, \${\textbackslash}rho\_c/{\textbackslash}phi({\textgreater}0.2L\_{\textbackslash}ast)\$, is \$13.2{\textbackslash}pm1.9{\textbackslash}times10{\textasciicircum}\{12\}h{\textasciicircum}\{-1\}{\textbackslash}msun\$. Under the assumptions that the galaxies are distributed like the mass and that the galaxy luminosities and numbers are statistically conserved, which these data indirectly support, \${\textbackslash}Omega\_0=0.20{\textbackslash}pm0.04{\textbackslash}pm0.09\$ where the errors are, respectively, the \$1{\textbackslash}sigma\$ internal and an estimate of the \$1{\textbackslash}sigma\$ systematic error resulting from the luminosity normalization.},
	urldate = {2022-08-05},
	journal = {ApJ},
	author = {Carlberg and Yee and Ellingson and Abraham and Gravel and Morris and Pritchet},
	month = may,
	year = {1996},
	note = {arXiv:astro-ph/9509034},
	keywords = {Astrophysics},
	pages = {32},
	file = {arXiv Fulltext PDF:/Users/lyalcorn/Zotero/storage/A6KRKYWX/Carlberg et al. - 1996 - Galaxy Cluster Virial Masses and Omega.pdf:application/pdf;arXiv.org Snapshot:/Users/lyalcorn/Zotero/storage/I8N8GNRX/9509034.html:text/html},
}

@article{hamer_cold_2014,
	title = {Cold gas dynamics in {Hydra}-{A}: evidence for a rotating disc},
	volume = {437},
	issn = {0035-8711, 1365-2966},
