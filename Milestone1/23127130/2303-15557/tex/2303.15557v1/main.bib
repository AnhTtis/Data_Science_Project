@article{mcnamara_energetic_2009,
	title = {An {Energetic} {AGN} {Outburst} {Powered} by a {Rapidly} {Spinning} {Supermassive} {Black} {Hole} or an {Accreting} {Ultramassive} {Black} {Hole}},
	volume = {698},
	issn = {0004-637X, 1538-4357},
	url = {http://arxiv.org/abs/0811.3020},
	doi = {10.1088/0004-637X/698/1/594},
	abstract = {Powering the 10{\textasciicircum}62 erg nuclear outburst in the MS0735.6+7421 cluster central galaxy by accretion implies that its supermassive black hole (SMBH) grew by {\textasciitilde}6x10{\textasciicircum}8 solar masses over the past 100 Myr. We place upper limits on the amount of cold gas and star formation near the nucleus of {\textless}10{\textasciicircum}9 solar masses and {\textless}2 solar masses per year, respectively. These limits imply that an implausibly large fraction of the preexisting cold gas in the bulge must have been consumed by its SMBH at the rate of {\textasciitilde}3-5 solar masses per year while leaving no trace of star formation. Such a high accretion rate would be difficult to maintain by stellar accretion or the Bondi mechanism, unless the black hole mass approaches 10{\textasciicircum}11 solar masses. Its feeble nuclear luminosities in the UV, I, and X-ray bands compared to its enormous mechanical power are inconsistent with rapid accretion onto a {\textasciitilde}5x10{\textasciicircum}9 solar mass black hole. We suggest instead that the AGN outburst is powered by a rapidly-spinning black hole. A maximally-spinning, 10{\textasciicircum}9 solar mass black hole contains enough rotational energy, {\textasciitilde}10{\textasciicircum}62 erg, to quench a cooling flow over its lifetime and to contribute significantly to the excess entropy found in the hot atmospheres of groups and clusters. Two modes of AGN feedback may be quenching star formation in elliptical galaxies centered in cooling halos at late times. An accretion mode that operates in gas-rich systems, and a spin mode operating at modest accretion rates. The spin conjecture may be avoided in MS0735 by appealing to Bondi accretion onto a central black hole whose mass greatly exceeds 10{\textasciicircum}10 solar mass. The host galaxy's unusually large, 3.8 kpc stellar core radius (light deficit) may witness the presence of an ultramassive black hole.},
	number = {1},
	urldate = {2022-08-26},
	journal = {ApJ},
	author = {McNamara, B. R. and Kazemzadeh, F. and Rafferty, D. A. and Birzan, L. and Nulsen, P. E. J. and Kirkpatrick, C. C. and Wise, M. W.},
	month = jun,
	year = {2009},
	note = {arXiv:0811.3020 [astro-ph]},
	keywords = {Astrophysics},
	pages = {594--605},
	file = {arXiv Fulltext PDF:/Users/lyalcorn/Zotero/storage/5ECEJX5Y/McNamara et al. - 2009 - An Energetic AGN Outburst Powered by a Rapidly Spi.pdf:application/pdf;arXiv.org Snapshot:/Users/lyalcorn/Zotero/storage/FTH35X2M/0811.html:text/html},
}

