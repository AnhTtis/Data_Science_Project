	year = {2020},
	note = {arXiv: 1912.01062v1},
	keywords = {Galaxy: evolution, Galaxy: kinematics and dynamics, Galaxy: formation, Largescale structure of the Universe},
	pages = {153--176},
	file = {PDF:/Users/lyalcorn/Zotero/storage/ZI64RU74/1912.01062.pdf:application/pdf},
