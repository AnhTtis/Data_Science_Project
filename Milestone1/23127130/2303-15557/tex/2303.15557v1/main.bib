@article{Theios2019,
	title = {Dust {Attenuation}, {Star} {Formation}, and {Metallicity} in z ∼ 2–3 {Galaxies} from {KBSS}-{MOSFIRE}},
	volume = {871},
	doi = {10.3847/1538-4357/aaf386},
	abstract = {We present a detailed analysis of 317 \$2.0 {\textbackslash}leq z {\textbackslash}leq 2.7\$ star-forming galaxies from the Keck Baryonic Structure Survey (KBSS). Using complementary spectroscopic observations with Keck/LRIS and Keck/MOSFIRE, as well as spectral energy distribution (SED) fits to broadband photometry, we examine the joint rest-UV and rest-optical properties of the same galaxies, including stellar and nebular dust attenuation, metallicity, and star formation rate (SFR). The inferred parameters of the stellar population (reddening, age, SFR, and stellar mass) are strongly dependent on the details of the assumed stellar population model and the shape of the attenuation curve. Nebular reddening is generally larger than continuum reddening, but with large scatter. Compared to local galaxies, high-redshift galaxies have lower gas-phase metallicities (and/or higher nebular excitation) at fixed nebular reddening, and higher nebular reddening at fixed stellar mass, consistent with gas fractions that increase with redshift. We find that continuum reddening is correlated with \$12+{\textbackslash}log({\textbackslash}mathrm\{O\}/{\textbackslash}mathrm\{H\})\_\{{\textbackslash}mathrm\{O3N2\}\}\$ at \$3.0{\textbackslash}sigma\$ significance, whereas nebular reddening is correlated with only \$1.1{\textbackslash}sigma\$ significance. This may reflect the dependence of both continuum reddening and O3N2 on the shape of the ionizing radiation field produced by the massive stars. Finally, we show that H\${\textbackslash}alpha\$-based and SED-based estimates of SFR exhibit significant scatter relative to one another, and agree on average only for particular combinations of spectral synthesis models and attenuation curves. We find that the SMC extinction curve predicts consistent SFRs if we assume the sub-solar (\$0.14Z\_\{{\textbackslash}odot\}\$) binary star models that are favored for high-redshift galaxies.},
	number = {1},
	journal = {The Astrophysical Journal},
	author = {Theios, Rachel L. and Steidel, Charles C. and Strom, Allison L. and Rudie, Gwen C. and Trainor, Ryan F. and Reddy, Naveen A.},
	year = {2019},
	note = {arXiv: 1805.00016v1},
	keywords = {evolution, galaxies, high-redshift, star formation, ism, dust, extinction, abun-, dances, h ii regions},
	pages = {128},
	file = {PDF:/Users/lyalcorn/Zotero/storage/DESB6TKH/Theios et al. - 2019 - Dust Attenuation, Star Formation, and Metallicity in z ∼ 2–3 Galaxies from KBSS-MOSFIRE.pdf:application/pdf},
}

@article{Ashworth2018,
	title = {Theoretical predictions for {IMF} diagnostics in {UV} spectroscopy of star clusters},
	volume = {480},
	issn = {13652966},
	doi = {10.1093/mnras/sty2058},
	abstract = {We explore the possibility of using UV spectroscopy in combination with broad-band photometry as diagnostic tools for understanding the shape of the Initial Mass Function (IMF) in unresolved stellar populations. Building on our previous work, we extend the Stochastically Lighting Up Galaxies code (slug) to include a high-resolution UV spectral synthesiser and equivalent width calculation capabilities. We first gain a qualitative understanding of how UV spectral features behave as the parameters that define a star cluster in slug (mass, age, extinction, and IMF slope alpha) are changed. We then exploit Bayesian inference techniques to recover the alpha values for clusters simulated with slug, using mock observations of these clusters comprised of broad-band photometry and equivalent width measurements of a selection of UV spectral features. We find some improvement when compared to attempts using broad-band photometry alone (with the interquartile range of the alpha posterior PDF shrinking by 32\%), although we still do not yet fully break the known degeneracy between the cluster mass and alpha. Finally, we make predictions about how effective real observations will be by quantifying our ability to constrain alpha as a function of limiting equivalent width. We find that observations sensitive to a modest equivalent width of 9 A are sufficient to improve the recovery of the IMF slope parameter by 32\% (interquartile range of posterior PDF median residuals), moving to 39\% when we include all the significant spectral features in the wavelength range 900-3000 A.},
	number = {3},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Ashworth, G. and Fumagalli, Michele and Adamo, Angela and Krumholz, Mark R.},
	year = {2018},
	note = {arXiv: 1807.11524v2},
	keywords = {Galaxies: clusters: general, Methods: statistical, Mass function, Stars: luminosity function},
	pages = {3091--3104},
	file = {PDF:/Users/lyalcorn/Zotero/storage/AWQFIQFV/Ashworth et al. - 2018 - Theoretical predictions for IMF diagnostics in UV spectroscopy of star clusters.pdf:application/pdf},
}

@article{Korsaga2019,
	title = {{GHASP}: {An} {Hα} kinematics survey of spiral galaxies - {XII}. {Distribution} of luminous and dark matter in spiral and irregular nearby galaxies using {R} c -band photometry},
	volume = {482},
	issn = {13652966},
	doi = {10.1093/mnras/sty2582},
	abstract = {Mass models of 100 nearby spiral and irregular galaxies, covering morphological types from Sa to Irr, are computed using H\${\textbackslash}alpha\$ rotation curves and R\$\_c\$-band surface brightness profiles. The kinematics was obtained using a scanning Fabry-Perot interferometer. One of the aims is to compare our results with those from Korsaga et al. (2018), which used mid-infrared (MIR) WISE W1 (3.4 \${\textbackslash}mu\$m) photometric data. For the analysis, the same tools were used for both bands. Pseudo-Isothermal (ISO) core and Navarro-Frenk-White (NFW) cuspy models have been used. We test Best Fit Models (BFM), Maximum Disc Models (MDM) and models for which M/L is fixed using the B - V colors. Similarly to what was found in the MIR 3.4 \${\textbackslash}mu\$m band, most of the observed rotation curves are better described by a central core density profile (ISO) than a cuspy one (NFW) when using the optical R\$\_c\$-band. In both bands, the dispersion in the (M/L) values is smaller for the fixed M/L fits. As for the W1 photometry, the derived DM halos' parameters depend on the morphological types. We find similar relations than those in the literature, only when we compare our results for the bulge-poor sub-sample because most of previous results were mainly based on late-type spirals. Because the dispersion in the model parameters is smaller and because stellar masses are better defined in that band, MIR photometry should be preferred, when possible, to the optical bands. It is shown that for high-z galaxies, sensible results can still be obtained without full profile decomposition.},
	number = {1},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Korsaga, M. and Amram, P. and Carignan, C. and Epinat, B.},
	year = {2019},
	note = {arXiv: 1804.05820v1},
	keywords = {Galaxies: haloes, Galaxies: spiral, Galaxies: kinematics and dynamics, Galaxies: dwarf, Dark matter},
	pages = {154--174},
	file = {PDF:/Users/lyalcorn/Zotero/storage/ZR2EZM7S/Korsaga et al. - 2019 - GHASP An Hα kinematics survey of spiral galaxies - XII. Distribution of luminous and dark matter in spiral and.pdf:application/pdf},
}

@article{Sweet2018,
	title = {Revisiting the {Stellar} {Mass}–{Angular} {Momentum}–{Morphology} {Relation}: {Extension} to {Higher} {Bulge} {Fraction} and the {Effect} of {Bulge} {Type}},
	volume = {860},
	doi = {10.3847/1538-4357/aabfc4},
	abstract = {We present the relation between stellar specific angular momentum \$j\_*\$, stellar mass \$M\_*\$, and bulge-to-total light ratio \${\textbackslash}beta\$ for THINGS, CALIFA and Romanowsky {\textbackslash}\& Fall datasets, exploring the existence of a fundamental plane between these parameters as first suggested by Obreschkow {\textbackslash}\& Glazebrook. Our best-fit \$M\_*-j\_*\$ relation yields a slope of \${\textbackslash}alpha = 1.03 {\textbackslash}pm 0.11\$ with a trivariate fit including \${\textbackslash}beta\$. When ignoring the effect of \${\textbackslash}beta\$, the exponent \${\textbackslash}alpha = 0.56 {\textbackslash}pm 0.06\$ is consistent with \${\textbackslash}alpha = 2/3\$ predicted for dark matter halos. There is a linear \${\textbackslash}beta - j\_*/M\_*\$ relation for \${\textbackslash}beta {\textbackslash}lesssim 0.4\$, exhibiting a general trend of increasing \${\textbackslash}beta\$ with decreasing \$j\_*/M\_*\$. Galaxies with \${\textbackslash}beta {\textbackslash}gtrsim 0.4\$ have higher \$j\_*\$ than predicted by the relation. Pseudobulge galaxies have preferentially lower \${\textbackslash}beta\$ for a given \$j\_*/M\_*\$ than galaxies that contain classical bulges. Pseudobulge galaxies follow a well-defined track in \${\textbackslash}beta - j\_*/M\_*\$ space, consistent with Obreschkow {\textbackslash}\& Glazebrook, while galaxies with classical bulges do not. These results are consistent with the hypothesis that while growth in either bulge type is linked to a decrease in \$j\_*/M\_*\$, the mechanisms that build pseudobulges seem to be less efficient at increasing bulge mass per decrease in specific angular momentum than those that build classical bulges.},
	number = {1},
	journal = {The Astrophysical Journal},
	author = {Sweet, Sarah M. and Fisher, David and Glazebrook, Karl and Obreschkow, Danail and Lagos, Claudia and Wang, Liang},
	year = {2018},
	note = {arXiv: 1804.07083v1},
	keywords = {evolution, galaxies, cd, elliptical and lenticular, kinematics and dynamics, spiral, corresponding author, au, bulges, com, funda-, mental parameters, sarah, sarah sweet, sarahsweet},
	pages = {37},
	file = {PDF:/Users/lyalcorn/Zotero/storage/8IJ8YGDQ/Sweet et al. - 2018 - Revisiting the Stellar Mass–Angular Momentum–Morphology Relation Extension to Higher Bulge Fraction and the Ef.pdf:application/pdf},
}

@article{Obreja2018,
	title = {Introducing galactic structure finder: {The} multiple stellar kinematic structures of a simulated {Milky} {Way} mass galaxy},
	volume = {477},
	issn = {13652966},
	doi = {10.1093/MNRAS/STY1022},
	abstract = {We present the first results of applying Gaussian Mixture Models in the stellar kinematic space of normalized angular momentum and binding energy on NIHAO high resolution galaxies to separate the stars into multiple components. We exemplify this method using a simulated Milky Way analogue, whose stellar component hosts: thin and thick discs, classical and pseudo bulges, and a stellar halo. The properties of these stellar structures are in good agreement with observational expectations in terms of sizes, shapes and rotational support. Interestingly, the two kinematic discs show surface mass density profiles more centrally concentrated than exponentials, while the bulges and the stellar halo are purely exponential. We trace back in time the Lagrangian mass of each component separately to study their formation history. Between z{\textasciitilde}3 and the end of halo virialization, z{\textasciitilde}1.3, all components lose a fraction of their angular momentum. The classical bulge loses the most ({\textasciitilde}95\%) and the thin disc the least ({\textasciitilde}60\%). Both bulges formed their stars in-situ at high redshift, while the thin disc formed {\textasciitilde}98\% in-situ, but with a constant SFR{\textasciitilde}1.5M\$\_\{{\textbackslash}rm{\textbackslash}odot\}\$/yr\${\textasciicircum}\{{\textbackslash}rm-1\}\$ over the last {\textasciitilde}11 Gyr. Accreted stars (6\% of total stellar mass) are mainly incorporated to the thick disc or the stellar halo, which formed ex-situ 8\% and 45\% of their respective masses. Our analysis pipeline is freely available at https://github.com/aobr/gsf.},
	number = {4},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Obreja, Aura and Macciò, Andrea V. and Moster, Benjamin and Dutton, Aaron A. and Buck, Tobias and Stinson, Gregory S. and Wang, Liang},
	year = {2018},
	note = {arXiv: 1804.05576v1},
	keywords = {Galaxies: fundamental parameters, Galaxies: kinematics and dynamics, Galaxies: structure, Galaxies: stellar content -methods: numerical},
	pages = {4915--4930},
	file = {PDF:/Users/lyalcorn/Zotero/storage/LVLKKFSG/Obreja et al. - 2018 - Introducing galactic structure finder The multiple stellar kinematic structures of a simulated Milky Way mass gal.pdf:application/pdf},
}

