	keywords = {evolution, galaxies, high-redshift, formation, surveys, high-redshift surveys, color figures, online-only material},
	file = {PDF:/Users/lyalcorn/Zotero/storage/6L9MKW4S/Brammer et al. - 2012 - 3D-HST A wide-field grism spectroscopic survey with the hubble space telescope.pdf:application/pdf;PDF:/Users/lyalcorn/Zotero/storage/RZL6PXGY/Brammer et al. - 2012 - 3D-HST A wide-field grism spectroscopic survey with the hubble space telescope(2).pdf:application/pdf},
}

@article{Nelson2012,
	title = {Spatially resolved {Halpha} maps and sizes of 57 strongly star-forming galaxies at z{\textasciitilde}1 from {3D}-{HST}: evidence for rapid inside-out assembly of disk galaxies},
	volume = {747},
	issn = {20418205},
	url = {http://arxiv.org/abs/1202.1822},
	doi = {10.1088/2041-8205/747/2/L28},
	abstract = {We investigate the build-up of galaxies at z{\textasciitilde}1 using maps of Halpha and stellar continuum emission for a sample of 57 galaxies with rest-frame Halpha equivalent widths {\textgreater}100 Angstroms in the 3D-HST grism survey. We find that the Halpha emission broadly follows the rest-frame R-band light but that it is typically somewhat more extended and clumpy. We quantify the spatial distribution with the half-light radius. The median Halpha effective radius r\_e(Halpha) is 4.2+-0.1 kpc but the sizes span a large range, from compact objects with r\_e(Halpha) {\textasciitilde} 1.0 kpc to extended disks with r\_e(Halpha) {\textasciitilde} 15 kpc. Comparing Halpha sizes to continuum sizes, we find {\textless}r\_e(Halpha)/r\_e(R){\textgreater}=1.3+-0.1 for the full sample. That is, star formation, as traced by Halpha, typically occurs out to larger radii than the rest-frame R-band stellar continuum; galaxies are growing their radii and building up from the inside out. This effect appears to be somewhat more pronounced for the largest galaxies. Using the measured Halpha sizes, we derive star formation rate surface densities. We find that they range from {\textasciitilde}0.05 Msun yr{\textasciicircum}\{-1\} kpc{\textasciicircum}\{-2\} for the largest galaxies to {\textasciitilde}5 Msun yr{\textasciicircum}\{-1\} kpc{\textasciicircum}\{-2\} for the smallest galaxies, implying a large range in physical conditions in rapidly star-forming z{\textasciitilde}1 galaxies. Finally, we infer that all galaxies in the sample have very high gas mass fractions and stellar mass doubling times {\textless} 500 Myr. Although other explanations are also possible, a straightforward interpretation is that we are simultaneously witnessing the rapid formation of compact bulges and large disks at z{\textasciitilde}1.},
	number = {2},
	journal = {Astrophysical Journal Letters},
	author = {Nelson, Erica June and van Dokkum, Pieter G. and Brammer, Gabriel and Schreiber, Natascha Forster and Franx, Marijn and Fumagalli, Mattia and Patel, Shannon and Rix, Hans-Walter and Skelton, Rosalind E. and Bezanson, Rachel and Da Cunha, Elisabete and Kriek, Mariska and Labbe, Ivo and Lundgren, Britt and Quadri, Ryan and Schmidt, Kasper B.},
	month = feb,
	year = {2012},
	note = {arXiv: 1202.1822},
	keywords = {galaxies: evolution, galaxies: formation, galaxies: high-redshift},
	file = {PDF:/Users/lyalcorn/Zotero/storage/HX62X9SW/Nelson et al. - 2012 - Spatially resolved Hα maps and sizes of 57 strongly star-forming galaxies at z 1 from 3D-HST Evidence for rapid.pdf:application/pdf},
}

