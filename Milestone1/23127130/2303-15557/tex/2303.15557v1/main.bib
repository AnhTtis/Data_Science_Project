	year = {2006},
	pages = {835--849},
	file = {PDF:/Users/lyalcorn/Zotero/storage/LP63HLNP/Alonso‐Herrero et al. - 2006 - Near‐Infrared and Star‐forming Properties of Local Luminous Infrared Galaxies.pdf:application/pdf},
}

@article{Papovich2009,
