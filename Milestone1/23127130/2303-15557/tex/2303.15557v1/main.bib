	year = {2011},
	keywords = {galaxies: high-redshift, cosmology: observations},
	file = {PDF:/Users/lyalcorn/Zotero/storage/PNRETP8I/Koekemoer et al. - 2011 - Candels The cosmic assembly near-infrared deep extragalactic legacy survey - The hubble space telescope observ.pdf:application/pdf},
}

@article{Darvish2018,
