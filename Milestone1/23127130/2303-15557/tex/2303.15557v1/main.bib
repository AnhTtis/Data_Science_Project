@article{Outini2019,
	title = {Forward modeling of galaxy kinematics in slitless spectroscopy},
	url = {http://arxiv.org/abs/1910.07803},
	abstract = {Slitless spectroscopy has long been considered as a complicated and confused technique. Nonetheless, with the advent of Hubble Space Telescope (HST) instruments characterized by a low sky background level and a high spatial resolution (most notably WFC3), slitless spectroscopy has become an adopted survey tool to study galaxy evolution from space. We investigate its application to single object studies to measure not only redshift and integrated spectral features, but also spatially resolved quantities such as galaxy kinematics. We build a complete forward model to be quantitatively compared to actual slitless observations. This model depends on a simplified thin cold disk galaxy description -- including flux distribution, intrinsic spectrum and kinematic parameters -- and on the instrumental signature. It is used to improve redshifts and constrain basic rotation curve parameters, i.e. plateau velocity \$v\_\{0\}\$ (in \${\textbackslash}rm km.s{\textasciicircum}\{-1\}\$) and central velocity gradient \$w\_\{0\}\$ (in \${\textbackslash}rm km.s{\textasciicircum}\{-1\}.arcsec{\textasciicircum}\{-1\}\$). The model is tested on selected observations from 3D-HST and GLASS surveys, to estimate redshift and kinematic parameters on several galaxies measured with one or more roll angles. Our forward approach allows to mitigate the self-contamination effect, a primary drawback of slitless spectroscopy, and therefore has the potential to increase precision on redshifts. In a limited sample of well-resolved spiral galaxies from HST surveys, it is possible to significantly constrain galaxy rotation curve parameters. This proof-of-concept work is promising for future large slitless spectroscopic surveys such as EUCLID and WFIRST.},
	author = {Outini, Mehdi and Copin, Yannick},
	year = {2019},
	note = {arXiv: 1910.07803},
	keywords = {galaxies, high-redshift - galaxies, kinematics and dynamics -, evolution - instrumentation, spectrographs},
	pages = {1--13},
	file = {PDF:/Users/lyalcorn/Zotero/storage/LM8KFIV4/1910.07803.pdf:application/pdf},
}

@article{Cannarozzo2019,
	title = {The cosmic evolution of the stellar mass-velocity dispersion relation of early-type galaxies},
	volume = {23},
	url = {http://arxiv.org/abs/1910.06987},
	abstract = {Taking advantage of a Bayesian hierarchical inference formalism, we study the evolution of the observed correlation between central stellar velocity dispersion \${\textbackslash}sigma\_0\$ and stellar mass \$M\_*\$ of massive (\$M\_*{\textbackslash}gtrsim 10{\textasciicircum}\{10.5\}{\textbackslash},{\textbackslash}mathrm\{M\_{\textbackslash}odot\}\$) early-type galaxies (ETGs) out to redshift \$z{\textbackslash}approx 2.5\$. Collecting ETGs from state-of-the-art literature samples, we consider both a fiducial (\$0{\textbackslash}lesssim z{\textbackslash}lesssim 1.2\$) and an extended (\$0{\textbackslash}lesssim z{\textbackslash}lesssim 2.5\$) samples. Based on the fiducial sample, we find that at \$z{\textbackslash}lesssim 1.2\$ the \$M\_*-{\textbackslash}sigma\_0\$ relation is well represented by \${\textbackslash}sigma\_0{\textbackslash}propto M\_*{\textasciicircum}\{{\textbackslash}beta\}(1+z){\textasciicircum}\{{\textbackslash}zeta\}\$, with \${\textbackslash}beta{\textbackslash}simeq 0.23\$ (at given \$z\$, \${\textbackslash}sigma\_0\$ increases with \$M\_*\$ as a power law with slope similar to the classical Faber-Jackson relation) and \${\textbackslash}zeta{\textbackslash}simeq 0.26\$ (at given \$M\_*\$, \${\textbackslash}sigma\_0\$ decreases for decreasing \$z\$, for instance by \${\textbackslash}simeq 20{\textbackslash}\%\$ from \$z=1\$ to \$z=0\$). The analysis of the extended sample suggests that over the wider redshift range \$0{\textbackslash}lesssim z{\textbackslash}lesssim 2.5\$ the slope \${\textbackslash}beta\$ could be slightly steeper at higher \$z\$ (\${\textbackslash}beta{\textbackslash}simeq 0.2+0.2{\textbackslash}log(1+z)\$ describes the data as well as constant \${\textbackslash}beta{\textbackslash}simeq 0.23\$) and that the redshift-dependence of the normalisation could be stronger (\${\textbackslash}zeta{\textbackslash}simeq 0.42\$). The intrinsic scatter in \${\textbackslash}sigma\_0\$ at given \$M\_*\$ is \${\textbackslash}simeq 0.07\$ dex for the fiducial sample at \$z{\textbackslash}lesssim 1.2\$, but is found to increase with redshift in the extended sample, being \${\textbackslash}simeq 0.11\$ dex at \$z=2\$. Our results suggest that, on average, the velocity dispersion of individual massive (\$M\_*{\textbackslash}gtrsim 5{\textbackslash}times 10{\textasciicircum}\{11\}M\_{\textbackslash}odot\$) ETGs decreases with time while they evolve from \$z{\textbackslash}approx 1\$ to \$z{\textbackslash}approx 0\$.},
	author = {Cannarozzo, Carlo and Sonnenfeld, Alessandro and Nipoti, Carlo},
	year = {2019},
	note = {arXiv: 1910.06987},
	keywords = {evolution, galaxies, formation, cd, elliptical and lenticular, kinematics and dynamics, fundamental parameters},
	pages = {1--23},
	file = {PDF:/Users/lyalcorn/Zotero/storage/WXBJE7CN/1910.06987.pdf:application/pdf},
}

@article{Suess2019,
	title = {Half-mass radii of quiescent and star-forming galaxies evolve slowly from 0 {\textless} z {\textless} 2.5: implications for galaxy assembly histories},
	url = {http://arxiv.org/abs/1910.06984},
	abstract = {We use high-resolution, multi-band imaging of {\textasciitilde}16,500 galaxies in the CANDELS fields at 0 {\textless} z {\textless} 2.5 to study the evolution of color gradients and half-mass radii over cosmic time. We find that galaxy color gradients at fixed mass evolve rapidly between z{\textasciitilde}2.5 and z{\textasciitilde}1, but remain roughly constant below z{\textasciitilde}1. This result implies that the sizes of both star-forming and quiescent galaxies increase much more slowly than previous studies found using half-light radii. The half-mass radius evolution of quiescent galaxies is fully consistent with a model which uses observed minor merger rates to predict the increase in sizes due to the accretion of small galaxies. Progenitor bias may still contribute to the growth of quiescent galaxies, particularly if we assume a slower timescale for the minor merger growth model. The slower half-mass radius evolution of star-forming galaxies is in tension with cosmological simulations and semi-analytic galaxy models. Further detailed, consistent comparisons with simulations are required to place these results in context.},
	author = {Suess, Katherine A. and Kriek, Mariska and Price, Sedona H. and Barro, Guillermo},
	year = {2019},
	note = {arXiv: 1910.06984},
	file = {PDF:/Users/lyalcorn/Zotero/storage/84C9GL4C/1910.06984.pdf:application/pdf},
}

