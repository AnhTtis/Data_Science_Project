	keywords = {Galaxies: formation, Galaxies: high-redshift, Infrared: galaxies, Galaxies: starburst},
	pages = {1506--1518},
	file = {PDF:/Users/lyalcorn/Zotero/storage/GJLSVP3Q/Papovich et al. - 2009 - Paschen-α emission in the gravitationally lensed galaxy SMM.pdf:application/pdf},
