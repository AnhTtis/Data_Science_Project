	pages = {21},
	file = {PDF:/Users/lyalcorn/Zotero/storage/TDPI8QHE/Schreiber et al. - 2018 - The SINSzC-SINF Survey of z ∼ 2 Galaxy Kinematics SINFONI Adaptive Optics–assisted Data and Kiloparsec-sca.pdf:application/pdf},
}

@article{Girard2018,
	title = {{KMOS} {LENsing} {Survey} ({KLENS}): {Morpho}-kinematic analysis of star-forming galaxies at z ∼ 2},
	volume = {613},
	issn = {14320746},
	doi = {10.1051/0004-6361/201731988},
	abstract = {We present results from the KMOS lensing survey-KLENS which is exploiting gravitational lensing to study the kinematics of 24 star forming galaxies at \$1.4{\textless}z{\textless}3.5\$ with a median mass of \${\textbackslash}rm log(M\_{\textbackslash}star/M\_{\textbackslash}odot)=9.6\$ and median star formation rate (SFR) of \${\textbackslash}rm 7.5{\textbackslash},M\_{\textbackslash}odot{\textbackslash},yr{\textasciicircum}\{-1\}\$. We find that 25\% of these low-mass/low-SFR galaxies are rotation dominated, while the majority of our sample shows no velocity gradient. When combining our data with other surveys, we find that the fraction of rotation dominated galaxies increases with the stellar mass, and decreases for galaxies with a positive offset from the main sequence. We also investigate the evolution of the intrinsic velocity dispersion, \${\textbackslash}sigma\_0\$, as a function of the redshift, \$z\$, and stellar mass, \${\textbackslash}rm M\_{\textbackslash}star\$, assuming galaxies in quasi-equilibrium (Toomre Q parameter equal to 1). From the \$z-{\textbackslash}sigma\_0\$ relation, we find that the redshift evolution of the velocity dispersion is mostly expected for massive galaxies (\${\textbackslash}rm log(M\_{\textbackslash}star/M\_{\textbackslash}odot){\textgreater}10\$). We derive a \${\textbackslash}rm M\_{\textbackslash}star-{\textbackslash}sigma\_0\$ relation, using the Tully-Fisher relation, which highlights that a different evolution of the velocity dispersion is expected depending on the stellar mass, with lower velocity dispersions for lower masses, and an increase for higher masses, stronger at higher redshift. The observed velocity dispersions from this work and from comparison samples spanning \$0{\textless}z{\textless}3.5\$ appear to follow this relation, except at higher redshift (\$z{\textgreater}2\$), where we observe higher velocity dispersions for low masses (\${\textbackslash}rm log(M\_{\textbackslash}star/M\_{\textbackslash}odot){\textbackslash}sim 9.6\$) and lower velocity dispersions for high masses (\${\textbackslash}rm log(M\_{\textbackslash}star/M\_{\textbackslash}odot){\textbackslash}sim 10.9\$) than expected. This discrepancy could, for instance, suggest that galaxies at high-\$z\$ do not satisfy the stability criterion, or that the adopted parametrisation of the specific star formation rate and molecular properties fail at high redshift.},
	journal = {Astronomy and Astrophysics},
	author = {Girard, M. and Dessauges-Zavadsky, M. and Schaerer, D. and Cirasuolo, M. and Turner, O. J. and Cava, A. and Rodríguez-Muñoz, L. and Richard, J. and Pérez-González, P. G.},
	year = {2018},
	note = {arXiv: 1801.05816v1},
	keywords = {Galaxies: evolution, Galaxies: high-redshift, Galaxies: kinematics and dynamics},
	pages = {1--21},
	file = {PDF:/Users/lyalcorn/Zotero/storage/MK43UDB6/Girard et al. - 2018 - KMOS LENsing Survey (KLENS) Morpho-kinematic analysis of star-forming galaxies at z ∼ 2.pdf:application/pdf},
}

@article{Ren2018,
	title = {The {Cosmic} {Web} around the {Brightest} {Galaxies} during the {Epoch} of {Reionization}},
	volume = {856},
	doi = {10.3847/1538-4357/aab094},
	abstract = {The most luminous galaxies at high-redshift are generally considered to be hosted in massive dark-matter halos of comparable number density, hence residing at the center of overdensities/protoclusters. We assess the validity of this assumption by investigating the clustering around the brightest galaxies populating the cosmic web at redshift \$z{\textbackslash}sim8-9\$ through a combination of semi-analytic modeling and Monte Carlo simulations of mock Hubble Space Telescope WFC3 observations. The innovative aspect of our approach is the inclusion of a log-normal scatter parameter \${\textbackslash}Sigma\$ in the galaxy luminosity versus halo mass relation, extending to high-\$z\$ the conditional luminosity function framework extensively used at low redshift. Our analysis shows that the larger the value of \${\textbackslash}Sigma\$, the less likely that the brightest source in a given volume is hosted in the most massive halo, and hence the weaker the overdensity of neighbors. We derive a minimum value of \${\textbackslash}Sigma\$ as a function of redshift by considering stochasticity in the halo assembly times, which affects galaxy ages and star formation rates in our modeling. We show that \${\textbackslash}Sigma\_\{min\}(z){\textbackslash}sim0.15-0.3\$, with \${\textbackslash}Sigma\_\{min\}\$ increasing with redshift as a consequence of shorter halo assembly periods at higher redshifts. Current observations (\$m\_\{AB\}{\textbackslash}sim27\$) of the environment of spectroscopically confirmed bright sources at \$z{\textgreater}7.5\$ do not show strong evidence of clustering and are consistent with our modeling predictions for \${\textbackslash}Sigma{\textbackslash}geq{\textbackslash}Sigma\_\{min\}\$. Deeper future observations reaching \$m\_\{AB\}{\textbackslash}sim28.2-29\$ would have the opportunity to clearly quantify the clustering strength, and hence to constrain \${\textbackslash}Sigma\$, investigating the physical processes that drive star formation in the early Universe.},
	number = {1},
	journal = {The Astrophysical Journal},
	author = {Ren, Keven and Trenti, Michele and Mutch, Simon J.},
	year = {2018},
	note = {arXiv: 1802.06802v1},
	pages = {81},
	file = {PDF:/Users/lyalcorn/Zotero/storage/F6IZ4BPB/Ren, Trenti, Mutch - 2018 - The Cosmic Web around the Brightest Galaxies during the Epoch of Reionization.pdf:application/pdf},
}

@article{Garrison-Kimmel2018,
	title = {The origin of the diverse morphologies and kinematics of {MilkyWay}-mass galaxies in the {FIRE}-2 simulations},
	volume = {481},
	issn = {13652966},
	doi = {10.1093/mnras/sty2513},
	abstract = {We use hydrodynamic cosmological zoom-in simulations from the FIRE project to explore the morphologies and kinematics of fifteen Milky Way (MW)-mass galaxies. Our sample ranges from compact, bulge-dominated systems with 90\% of their stellar mass within 2.5 kpc to well-ordered disks that reach \${\textbackslash}gtrsim15\$ kpc. The gas in our galaxies always forms a thin, rotation-supported disk at \$z=0\$, with sizes primarily determined by the gas mass. For stars, we quantify kinematics and morphology both via the fraction of stars on disk-like orbits and with the radial extent of the stellar disk. In this mass range, stellar morphology and kinematics are poorly correlated with the properties of the halo available from dark matter-only simulations (halo merger history, spin, or formation time). They more strongly correlate with the gaseous histories of the galaxies: those that maintain a high gas mass in the disk after \$z{\textbackslash}sim1\$ develop well-ordered stellar disks. The best predictor of morphology we identify is the spin of the gas in the halo at the time the galaxy formed 1/2 of its stars (i.e. the gas that builds the galaxy). High-\$z\$ mergers, before a hot halo emerges, produce some of the most massive bulges in the sample (from compact disks in gas-rich mergers), while later-forming bulges typically originate from internal processes, as satellites are stripped of gas before the galaxies merge. Moreover, most stars in \$z=0\$ MW-mass galaxies (even \$z=0\$ bulge stars) form in a disk: \${\textbackslash}gtrsim\$60-90\% of stars begin their lives rotationally supported.},
	number = {3},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Garrison-Kimmel, Shea and Hopkins, Philip F. and Wetzel, Andrew and El-Badry, Kareem and Sanderson, Robyn E. and Bullock, James S. and Ma, Xiangcheng and van de Voort, Freeke and Hafen, Zachary and Faucher-Giguère, Claude André and Hayward, Christopher C. and Quataert, Eliot and Kereš, Dušan and Boylan-Kolchin, Michael},
	year = {2018},
	note = {arXiv: 1712.03966v1},
	keywords = {Galaxies: evolution, Galaxies: formation, Galaxies: spiral, Galaxies: structure, Cosmology: theory, Galaxies: bulges},
	pages = {4133--4157},
	file = {PDF:/Users/lyalcorn/Zotero/storage/SY3RYHK7/Garrison-Kimmel et al. - 2018 - The origin of the diverse morphologies and kinematics of MilkyWay-mass galaxies in the FIRE-2 simulation.pdf:application/pdf},
}

