	file = {PDF:/Users/lyalcorn/Zotero/storage/E4MGWG2F/McNamara, O'Connell - 1993 - Blue lobe galaxies in the cooling flow clusters Abell 1795 and Abell 2597.pdf:application/pdf},
}

