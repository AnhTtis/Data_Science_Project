@article{Tacconi2013,
	title = {Phibss: {Molecular} gas content and scaling relations in z ∼ 1-3 massive, main-sequence star-forming galaxies},
	volume = {768},
	issn = {15384357},
	doi = {10.1088/0004-637X/768/1/74},
	abstract = {We present PHIBSS, the IRAM Plateau de Bure high-z blue sequence CO 3-2 survey of the molecular gas properties in normal star forming galaxies (SFGs) near the cosmic star formation peak. PHIBSS provides 52 CO detections in two redshift slices at z{\textasciitilde}1.2 and 2.2, with log(M*(M\_solar)){\textgreater}10.4 and log(SFR(M\_solar/yr)){\textgreater}1.5. Including a correction for the incomplete coverage of the M*-SFR plane, we infer average gas fractions of {\textasciitilde}0.33 at z{\textasciitilde}1.2 and {\textasciitilde}0.47 at z{\textasciitilde}2.2. Gas fractions drop with stellar mass, in agreement with cosmological simulations including strong star formation feedback. Most of the z{\textasciitilde}1-3 SFGs are rotationally supported turbulent disks. The sizes of CO and UV/optical emission are comparable. The molecular gas - star formation relation for the z=1-3 SFGs is near-linear, with a {\textasciitilde}0.7 Gyrs gas depletion timescale; changes in depletion time are only a secondary effect. Since this timescale is much less than the Hubble time in all SFGs between z{\textasciitilde}0 and 2, fresh gas must be supplied with a fairly high duty cycle over several billion years. At given z and M*, gas fractions correlate strongly with the specific star formation rate. The variation of specific star formation rate between z{\textasciitilde}0 and 3 is mainly controlled by the fraction of baryonic mass that resides in cold gas.},
	number = {1},
	journal = {Astrophysical Journal},
	author = {Tacconi, L. J. and Neri, R. and Genzel, R. and Combes, F. and Bolatto, A. and Cooper, M. C. and Wuyts, S. and Bournaud, F. and Burkert, A. and Comerford, J. and Cox, P. and Davis, M. and Förster Schreiber, N. M. and García-Burillo, S. and Gracia-Carpio, J. and Lutz, D. and Naab, T. and Newman, S. and Omont, A. and Saintonge, A. and Shapiro Griffin, K. and Shapley, A. and Sternberg, A. and Weiner, B.},
	year = {2013},
	keywords = {galaxies: evolution, galaxies: high-redshift, galaxies: ISM, ISM: molecules},
