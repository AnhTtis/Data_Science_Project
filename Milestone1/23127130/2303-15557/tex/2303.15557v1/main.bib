	pages = {1241--1258},
	file = {PDF:/Users/lyalcorn/Zotero/storage/PARP74IZ/Welker et al. - 2017 - The rise and fall of stellar across the peak of cosmic star formation history Effects of mergers versus diffuse s.pdf:application/pdf},
}

@article{Krumholz2015,
