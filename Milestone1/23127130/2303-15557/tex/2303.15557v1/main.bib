	journal = {The Astrophysical Journal},
	author = {Forrest, Ben and Tran, Kim-Vy H. and Broussard, Adam and Allen, Rebecca J. and Apfel, Miranda and Cowley, Michael J. and Glazebrook, Karl and Kacprzak, Glenn G. and Labbé, Ivo and Nanayakkara, Themiya and Papovich, Casey and Quadri, Ryan F. and Spitler, Lee R. and Straatman, Caroline M. S. and Tomczak, Adam},
