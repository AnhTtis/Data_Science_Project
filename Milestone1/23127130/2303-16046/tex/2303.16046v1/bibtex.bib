	month = {mar},
	number = {2},
	pages = {98},
	publisher = {The American Astronomical Society},
	title = {QUANTITATIVE EXAMINATION OF A LARGE SAMPLE OF SUPRA-ARCADE DOWNFLOWS IN ERUPTIVE SOLAR FLARES},
	url = {https://dx.doi.org/10.1088/0004-637X/730/2/98},
	volume = {730},
