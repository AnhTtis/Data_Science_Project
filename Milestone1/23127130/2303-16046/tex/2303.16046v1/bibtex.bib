	journal = {The Astrophysical Journal},
	month = {oct},
	number = {1},
	pages = {33},
	publisher = {American Astronomical Society},
	title = {{MULTIPLE} {PLASMA} {EJECTIONS} {AND} {INTERMITTENT} {NATURE} {OF} {MAGNETIC} {RECONNECTION} {IN} {SOLAR} {CHROMOSPHERIC} {ANEMONE} {JETS}},
	url = {https://doi.org/10.1088/0004-637x/759/1/33},
	volume = {759},
