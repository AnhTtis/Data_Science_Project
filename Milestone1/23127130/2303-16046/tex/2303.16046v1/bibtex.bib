	publisher = {American Astronomical Society},
	title = {{ON} A {CORONAL} {BLOWOUT} {JET}: {THE} {FIRST} {OBSERVATION} {OF} A {SIMULTANEOUSLY} {PRODUCED} {BUBBLE}-{LIKE} {CME} {AND} A {JET}-{LIKE} {CME} {IN} A {SOLAR} {EVENT}},
