	month = {jan},
	number = {2},
	pages = {L26},
	publisher = {The American Astronomical Society},
	title = {A Magnetic Reconnection Event in the Solar Atmosphere Driven by Relaxation of a Twisted Arch Filament System},
	url = {https://dx.doi.org/10.3847/2041-8213/aaa88c},
	volume = {853},
	year = {2018},
