@unpublished{Edtmair:2022aa,
	abstract = {We prove the strong Viterbo conjecture for all smooth convex domains in $\mathbb{R}^4$ which are sufficiently close to the round ball with respect to the $C^3$-topology. This strengthens a result of Abbondandolo-Bramham-Hryniewicz-Salom{\~a}o establishing a systolic inequality for such domains. For arbitrary dynamically convex star-shaped domains in $\mathbb{R}^4$, we prove that the cylindrical embedding capacity agrees with the minimal symplectic area of a disk-like global surface of section of the Reeb flow on the boundary of the domain. By a result of Hryniewicz-Hutchings-Ramos, this implies that the cylindrical embedding capacity agrees with the first ECH capacity for all dynamically convex domains in $\mathbb{R}^4$.},
	author = {Edtmair, Oliver},
	date-added = {2023-03-06 18:31:34 +0100},
	date-modified = {2023-03-06 18:32:10 +0100},
	note = {arXiv:2206.07847},
	title = {Disk-like surfaces of section and symplectic capacities},
